/* CSS Document */
/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{max-width:640px;min-width:320px; margin:0 auto;font-size:14px; font-family:Microsoft Yahei,"微软雅黑"; background-color: #fff; color: #372221;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#001d0b; font-size: 14px;}
.a,input,button,div{-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{ float: left;}
.fr{ float: right}
.cnt{text-align: center}

section{background-color: #96ecec}
::-webkit-input-placeholder { color:#becaca;}
:-moz-placeholder {color:#becaca;}
::-moz-placeholder {color:#becaca;}
:-ms-input-placeholder {color:#becaca;}

.mtop{ margin-top: 35px;}
.mtop1{ margin-top: 20px;}
.mbot{margin-bottom: 8px;}
.ptop{ padding-top: 10px;}
.ptop1{padding-top: 45px;}
.pbot{padding-bottom: 25px;}

.banner a.detail,.notice-icon,.mod-title span,.mod-title em.d-gift,.mod-ft-l,.mod-ft-r,.prize_list li,.clo a{background: url("../images/vacation_second_images/common_icon.png") no-repeat; background-size:144px 180px;}

.topbar{width:100%;height:51px; background-color: #0294f0; border-bottom:1px solid #0367a6;}
.topbar a{float:left; width:22px; height:22px;margin:15px 0px 0px 8px; background: url("../images/vacation_second_images/btn_back.png") 0 0 no-repeat; background-size: contain;}
.topbar h2{float:left; height:26px; padding:15px 0px 10px 8px;font-size:20px; color:#fff;}
.topbar h2.login-hd{ width: 100%;padding-left:0px;text-align: center;}

.banner{position: relative; width: 100%;min-width: 320px; margin:0 auto; text-align: center;}
.banner img{ width: 100%;vertical-align: bottom;}
.banner a.detail{ display: block;width:88px; height:31px;line-height:30px;position: absolute;bottom:6px;right:2%; background-position: 0 -32px;background-size:136px 170px; font-size: 16px; color: #f0436e;}
/*公告*/
.notice{width: 100%;height: 26px; margin-top:5px;line-height: 26px;background-color:#0294f0; color: #fff; overflow: hidden; }
.notice-icon{ float: left; display: inline; width: 20px;height:20px;margin:2px 0px 0px 5px; background-position: -1px -75px}
.notice-list{}
.notice-list span{color: #fff;padding-right: 20px;}
.notice-list span em{ padding-left: 10px;}

.wth{width: 94%; margin:0 auto;}
.wrap{width: 100%;background:url("../images/vacation_second_images/wrap_bg.png") 0 0 repeat-x #26e3c3; background-size:contain;}
.white-wrap{width: 94%; margin:10px auto 30px auto; background-color: #fff;}

/*抽奖*/
.rotate {width: 100%;padding:15px 0px 10px;background: url(../images/vacation_second_images/rotate_bg.png) no-repeat 0 0;background-size:100% auto;}
.rotate-con {position: relative;width:85%;margin: 0 auto;}
.rotate-pointer {position: absolute;top: 50%;left: 50%;width: 40%;height: 40%;margin-top: -22%;margin-left: -20%;background: url(../images/vacation_second_images/pointer.png) no-repeat 0 0;background-size: 100% 100%; cursor: pointer;-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}

.prompt{padding: 5px 8px; background-color: #fff668; border: 1px solid #ff7a76;border-radius: 5px; color: #ff5959;line-height: 18px;}

.mod{position:relative;width:94%; margin-left:auto;margin-right:auto;padding: 40px 0px 10px; background-color: #ffffe7; border:3px solid #99f2d6; border-bottom: none; border-radius: 10px 10px 0px 0px;}
.mod-title{width:86%; position: absolute;top:-20px;left:50%;margin-left: -43%;padding:10px 0px;background-color: #ffffe7;border-top:3px solid #99f2d6;border-radius:30px  }
.mod-title span{position:absolute;display: block;width:62px; height: 55px;top:-18px;}
.mod-title span.cloud-l{background-position: 0 -102px;left: -15px;}
.mod-title span.cloud-r{background-position: -62px -102px;right:-15px;}
.mod-title em{display: block;width:144px;height:28px;margin:0 auto;}
.mod-title em.d-gift{background-position: 0 0;}
.mod-title em.prize_q{width:114px; background: url("../images/vacation_second_images/prize_q.png") 0 0 no-repeat; background-size:114px auto ;}
.mod-con{padding:0px 10px; color: #afafaf}
.mod-con p{ word-break: break-all; line-height: 22px;}
.mod-con p a{ color: #666666; font-size: 16px; text-decoration: underline}
.mod-ft{ position: relative;width:94%; margin: 0 auto;}
.mod-ft span{float: left; display:block;width: 15px;height: 18px;z-index:2 }
.mod-ft span.mod-ft-l{background-position:0 -163px;margin-left: -100%;}
.mod-ft span.mod-ft-r{background-position:-15px -163px;margin-left: -15px;}
.bor-m{float: left;width: 100%;}
.bor-m em{display: block;height: 15px;margin: 0px 15px; border-bottom:3px solid #99f2d6;background-color:#ffffe7;}

/*软件*/
.soft-list{width: 95%; margin: 0 auto;}
.soft-list li{ float: left; display: inline; width: 25%;margin:7px 0px;text-align: center;}
.soft-list li p{}
.soft-list li .soft-icon{text-align: center;}
.soft-list li .soft-icon img{ width: 60px;height: 60px; margin: 0 auto;}
.soft-list li .soft-name{margin-top: 10px;height: 20px; line-height: 20px; overflow: hidden;}
.soft-list li .soft-name a{ color: #372221;}
.soft-list li .soft-size{ color: #f46600;height: 22px; line-height: 22px;overflow: hidden;}
.soft-list li a.downlode{ display: block; width:64px;height: 25px; line-height: 26px; margin: 13px auto 10px auto; background-color: #ff727a; border-radius: 3px;box-shadow: 0 1px 0px #ed5958; color: #fff;cursor: pointer;}

/*详情*/
.part{margin-top:10px;padding:10px; background-color: #fff;}
.part h4{font-size: 16px;padding-bottom: 6px;}
.part p{line-height:25px;font-size: 14px; color: #92a6a7}
.part p span{ padding-right: 20px;}
/*结束*/
.awards{padding:15px;}
.awards-hd{padding-bottom:15px;font-size:16px;text-align: center;color: #92a6a7}
.awards-list{padding: 10px;border-top: 1px solid #cee1e1;line-height: 25px;}
.awards-list dt{}
.awards-list dd{ color: #708b95; clear: both;overflow:hidden}
.awards-list dd em{ float: left}
.awards-list dd span{ float: right}
/*邮寄地址*/
.form-item{padding:15px 0px;}
.form-item label{ float: left;width:70px; }
.form-item .textbox{margin-left: 70px;}
.form-item .textbox input{width: 98%;font-family: "Microsoft Yahei"; color:#92a6a7;background: none; border:none;font-size: 14px;}
/*按钮*/
.btns{ width: 100%;}
.btns a{ display: block;height: 30px;line-height: 31px; margin:0 auto; border-radius:3px;color:#fff;text-align: center;}
.btns a.short-btn{width: 40%;background-color: #45dac1;box-shadow: 0 1px 0px #149c78;}
.btns a.long-btn{width: 100%;background-color: #ff727a;box-shadow: 0 1px 0px #ed5958;}
.btns a.long-btn2{width: 100%;height: 40px;line-height: 41px;background-color: #ff648c;box-shadow: 0 1px 0px #e34f75;}

/*获奖查询*/
.prize_list{}
.prize_list li{padding:15px 0px 10px 20px;background-position:-132px -149px; border-bottom: 1px solid #eaeeef}
.prize_list li .hd{padding-bottom:5px;font-size:16px;color: #1e1e1e}
.prize_list li .bd{ position: relative;}
.prize_list li .bd p{color: #ff9000;padding-top: 5px;}

.no_prize{padding: 100px 0px 200px; }
.no_prize p{padding-top: 20px; text-align: center;font-size: 16px;color: #383838;}
.no_prize p img{width: 150px;margin: 0 auto;}

/*提示框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{ position:absolute;width: 66%;padding:3%;left:50%;top:50%;margin-left:-36%; background-color: #97f0d4; border-radius: 10px; box-shadow:inset 0 0px 10px #fff;z-index: 99999;}
.tip-mid{padding: 7px;background-color: #26e3c3;border-radius: 10px; box-shadow:0 0px 10px #fff;}
.tip-con{position:relative;padding: 40px 10px 30px 10px;background-color: #ffffe7;border-radius: 10px; box-shadow:0 0px 10px #006255; color: #673618}
.tip-con h4{ position: absolute;top:-71px;left:50%;margin-left:-50px;z-index:100000 }
.tip-con h4 img.happy{width:100px ;height:100px;}
.tip-con h4 img.cry{width:120px ;height:95px;}
.tip-con p{ line-height: 20px; text-align: center;}
.tip-con p span{color: #ef481c}
.clo{ position: absolute;right: 10px;top: 10px;}
.clo a{ display: block; width:17px; height:17px; background-position:-29px -77px;text-indent: -9999px;}
.prompt2{padding-bottom:10px;color: #4e1600;}
.prompt2 span{color: #ff5959;font-weight:bold;padding: 0px 3px;}
/*尾部*/
.footer{max-width:640px;}
.footer img{width: 100%;margin-top: -1px;}
