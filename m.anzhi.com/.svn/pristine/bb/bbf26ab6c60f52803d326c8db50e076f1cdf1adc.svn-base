function trim(str)
{
	//删除左右两端的空格
	return str.replace(/(^\s*)|(\s*$)/g, ""); //把空格替换为空
}

function save(){
	<!--{if $out['is_login'] == 2}-->
		location.href="<!--{$out.login_url}-->";
		return false;
	<!--{/if}-->
	var alternate  = $('input[name="alternate"]:checked').val();
	var area  = $('#area').val();
	var team_name = trim($("#team_name").val());
	if(team_name == "") {
		$('#team_name_err').show().find("span").html('请输入正确战队名称').focus();
		$("#team_name").focus();
		return false;
	}
	if (team_name.indexOf("\\") >= 0) {
		$('#team_name_err').show().find("span").html('请输入正确战队名称').focus();
		$("#team_name").focus();
		return false;	   
	}	
	var pattern = new RegExp("[`~!@#$%^&*()=|{}':;',\\[\\]<>/?~！@#￥……&*（）_+\|《》.|{}【】‘；：”“'。，、？\" ]");
	if(pattern.test(team_name)){
		$('#team_name_err').show().find("span").html('请输入正确战队名称').focus();
		$("#team_name").focus();
		return false;
	}
	
	var len_name = GetCharLength(team_name);
	if(len_name > 30){
		$('#team_name_err').show().find("span").html('请输入正确战队名称');
		$("#team_name").focus();
		return false;
	}
	$('#team_name_err').hide();	
	var is_err = check_form(1);
	var is_err2 = check_form(2);
	var is_err3 = check_form(3);
	var is_err4 = check_form(4);
	if(!is_err || !is_err2 || !is_err3 || !is_err4 ){
		//$("#lxname2").focus();
		return false;
	}	
	if(alternate == 1){	
		var is_err5 = check_form(5);
		var len = 5;
		if(!is_err5){
			$("#lxname5").focus();
			return false;
		}		
	}else if(alternate == 2){
		var is_err5 = check_form(5);
		var is_err6 = check_form(6);
		var len = 6;
		if(!is_err5 || !is_err6 ){
			$("#lxname6").focus();
			return false;
		}			
	}else{
		var len = 4;
	}

	var data_str = '';
	for(var i=1;i<=len;i++){
		var lxname = trim($("#lxname"+i).val());
		var identity = trim($("#identity"+i).val());
		var mobile_phone = $("#mobile_phone"+i).val();
		var qq = $("#qq"+i).val();
		var school = $("#school"+i).val();
		var student = $("#student"+i).val();
		var gender  = $('input[name="gender'+i+'"]:checked').val();
		if(!gender) gender = 1;
		data_str += "&lxname"+i+"="+lxname+"&identity"+i+"="+identity+"&mobile_phone"+i+"="+mobile_phone+"&qq"+i+"="+qq+"&school"+i+"="+school+"&student"+i+"="+student+"&gender"+i+"="+gender;
	}
	
	$.ajax({
		url: '<!--{$out.prefix}-->.php',
		data:"info=1&team_name="+team_name+"&alternate="+alternate+"&type="+type+"&pkg="+pkg+"&softname="+softname+data_str+"&area="+area,
		type:"post",
		dataType: 'json',
		success:function(data) {
			if(data.code == 1){
				var idkey = qq_group[$("#school1").val()]['idkey'];
				var title = qq_group[$("#school1").val()]['title'];
				var url_g = "//shang.qq.com/wpa/qunwpa?idkey="+idkey;
				$("#add_qq_group").attr("href",url_g);
				$("#add_qq_group_img").attr("alt",title).attr("title",title);
				pop_tips('','','2',1);
				//showOpenNew('dialog2');
			}else if(data.code == 2){
				location.href=data.msg;
				return false;
			}else{
				alert("提交失败");
				return false;
			}
		},
	});
}

function check_form(id){
	var is_err = true;
	var lxname = trim($("#lxname"+id).val());
	if(lxname == "") {
		$("#lxname"+id+"_err").show().find("span").html('请填写正确姓名');
		is_err = false;
	}
	var name_len = lxname.length;
	if(name_len > 10){
		$('#lxname'+id+'_err').show().find("span").html('请填写正确姓名');
		is_err = false;
	}
	if(is_err) $('#lxname'+id+'_err').hide();
	
	var gender  = $('input[name="gender'+id+'"]:checked').val();
	if(gender == 'undefined' || !gender){
		$('#gender'+id+'_err').show().find("span").html('请选择性别');
		is_err = false;
	}
	if(is_err) $('#gender'+id+'_err').hide();
	
	var identity = trim($("#identity"+id).val());
	if(identity == "") {
		$('#identity'+id+'_err').show().find("span").html('请填写正确的身份证号');
		is_err = false;
	}
	if(is_err) $('#identity'+id+'_err').hide();
	
	var mobile_phone = $("#mobile_phone"+id).val();
	if(mobile_phone == "") {
		$('#mobile_phone'+id+'_err').show().find("span").html('请填写正确的手机号');
		is_err = false;
	 }
	var re = /^(13|14|15|17|18)\d{9}$/
	if (!re.test(mobile_phone)){
		$('#mobile_phone'+id+'_err').show().find("span").html('请填写正确位数的手机号');
		is_err = false;
	}
	if(is_err) $('#mobile_phone'+id+'_err').hide();
	
	var qq = trim($("#qq"+id).val());
	if(qq == "") {
		$('#qq'+id+'_err').show().find("span").html('请填写正确的QQ号');
		is_err = false;
	}
	if(is_err) $('#qq'+id+'_err').hide();
	
	var school = trim($("#school"+id).val());
	if(school == "") {
		$('#school'+id+'_err').show().find("span").html('请选择所在学校');
		is_err = false;
	}
	if(is_err) $('#school'+id+'_err').hide();
	
	var student = trim($("#student"+id).val());
	if(student == "") {
		$('#student'+id+'_err').show().find("span").html('请填写正确学生证号');
		is_err = false;
	}
	if(is_err)  $('#student'+id+'_err').hide();	
	return is_err;
}


//计算汉字两个字符  ----- js 方法
function GetCharLength(str) {
	var iLength = 0;  //记录字符的字节数
	for (var i = 0; i < str.length; i++)  //遍历字符串中的每个字符
	{
		if (str.charCodeAt(i) > 255)   //如果当前字符的编码大于255
		{
			iLength += 2;    //所占字节数加2
		}
		else {
			iLength += 1;   //否则所占字节数加1
		}
	}
	return iLength;   //返回字符所占字节数
}

function isChina(s) {
	var patrn = /[\u4E00-\u9FA5]|[\uFE30-\uFFA0]/gi;
	if (patrn.exec(s)) {
		return false;
	} else {
		return true; //没有汉字
	}
}



