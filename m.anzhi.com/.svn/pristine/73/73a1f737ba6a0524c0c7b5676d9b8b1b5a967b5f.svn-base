/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{background:#85c35b; font-size:16px; font-family:Microsoft Yahei,"微软雅黑"; line-height:20px; color:#dbbcff}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none; font-size: 16px; outline: none; color:#dbbcff}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{float:left;}
.fr{float:right;}
.banner{width:100%; position: relative;}
.banner img{width:100%}
.banner a{display: block; width:4.82rem; height:1.6rem; background:url(../images/btn_03.png) no-repeat; background-size: cover; position: absolute; bottom:2rem; right:0.74rem;  text-indent: -9999px; overflow: hidden;}
.part{width:100%; position: relative;}
.part img{width:100%}
.part4 .part4_icon{display:block; position: absolute; left:50%; top:2.8rem; width:3.98rem; height:3.98rem; margin-left:-1.99rem;}
.part4 a{display: block; width:4.34rem; height:2.1rem; background:url(../images/btn_02.png) no-repeat; background-size: cover; position: absolute; top:7.65rem; left:50%; margin-left:-2rem;  text-indent: -9999px; overflow: hidden;}
.footer{position: fixed; width:100%; height:2.52rem; background:rgba(0,0,0,.4); left:0; bottom:0}
.footer img{display: block; position: absolute; left:18px; top:10px; width:1.8rem; height:1.8rem}
.footer p{margin: 0.5rem 3.2rem 0 2.8rem; color:#fff; font-size: 18px;}
.footer p span{display:block; padding-top:2px; font-size: 14px;}
.footer i{display: block; width:3rem; height:1.28rem; background:#a0d202; line-height: 1.28rem; color:#ffffff; position: absolute; right:0.51rem; top:0.72rem; font-style: normal; text-align: center;}
.footer a.clickarea{position: absolute; left:0; top:0; bottom:0;right:0; z-index: 99999;}
