function gbcount(message)
{
	var max;
	max = 15;
	if (message.value.length > max) {
		message.value = message.value.substring(0,max);
		alert("名称不能多于15个字符");
		return false;
	}
}
function showDiv(id){
	//console.log(id);
		if(id==0){
			$('[id^=pic]').hide();
			$('#pic' + id).hide();
			$('#olgame').show();
			return;
		}else if(id == 1){
			$('#pic1').css("display","");
			$('#pic2').css("display","");
			$('#olgame').hide();
			return;
		}
	}
