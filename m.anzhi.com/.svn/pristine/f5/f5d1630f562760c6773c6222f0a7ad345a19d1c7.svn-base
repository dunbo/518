/* CSS Document */
/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{max-width:640px;min-width:320px; margin:0 auto;font-size:16px; font-family:Microsoft Yahei,"微软雅黑"; background: #eaeaea; color: #626262;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#626262; font-size: 16px;}
.a,input,button,div{-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{ float: left;}
.fr{ float: right}
.cnt{text-align: center}
.wordwrap{word-wrap:break-word;word-break:break-all;}
.space{height:0px; overflow:hidden}
.ptop10{ padding-top: 10px;}
.btns{display:block; width:76px; height:30px; margin:10px auto; background:#23b4ff; border-radius:4px; color:#fff; font-size:16px; text-align:center; line-height:30px;}
/*.banner{margin:9px 9px 0;}*/
.banner img{ width: 100%;vertical-align: bottom;}
.app{ min-height:73px; position:relative}
.app_img{width:88px; height:88px; position:absolute; left:50%; top:-35%; margin-left:-44px}
.app img{display:block; width:78px; height:78px; margin:0 auto; border:5px solid #eaeaea; border-radius:23px;}
.app_txt{width:90%; margin:0 auto; padding-top:48px; font-size:15px; text-align:center}
.app_txt h3{font-size:18px; color:#ff4800;}
.soft_title{position:relative}
.soft-list{padding:5px 0 0; }
.soft-list li{ float: left; display: inline; width: 25%;margin:7px 0px;text-align: center;}
.soft-list li .soft-icon{text-align: center;}
.soft-list li .soft-icon img{ width: 60px;height: 60px; margin: 0 auto; border-radius:8px}
.soft-list li .soft-name{margin-top: 10px;height: 20px; color:#ff4800; line-height: 20px; overflow: hidden; white-space:nowrap; text-overflow:ellipsis;}
.soft-list li .soft-name a{ color: #737373;}

.title h3{display:inline-block; width:102px; height:22px; padding-left:12px; background:url(../images/title_bg.png) no-repeat; background-size:100% 100%; font-size:16px; color:#fff; line-height:22px;}
.lottery_info{ padding:0 0 8px;}
.lottery_info p{float:left; padding:15px  0 0 12px; color:#737373; font-size:17px}
.lottery_info a{margin-right:8px; display:inline}
.swiper-container {width: 96%; margin:10px auto 0;}
.swiper-slide {width:33.3%;}
.swiper-slide img{width:100%;display:block}

.prize_list{padding:12px}
.btns2{width:314px; margin:0 auto}
.btns2 a{float:left; width:137px; height:36px; margin:10px; background:#23b4ff; text-align:center; line-height:36px; font-size:16px; border-radius:2px; color:#fff}
.btns2 a.gray{background:#a6a6a6}
.form-items {margin:0 12px 12px}
.form-items li{ margin-top: 30px; padding:0 10px; border:1px solid #7d7d7d; clear: both; overflow: hidden;}
.form-items li label{ float: left;width:90px;padding-top: 10px; color:#482608;}
.form-items li span{display:block;margin-left: 90px;}
.form-items li input{ width:90%;padding:10px 6px;border:none;color: #000;  font-family: Microsoft Yahei,"微软雅黑"; background-color:#fff; border-radius:2px}
.form-items li textarea{width:90%; height:48px; padding:10px 6px; border:none; font-family: Microsoft Yahei,"微软雅黑"; background-color:#fff;}
.form_error{display:block; padding:8px 0 0 90px; color:red}

.btns_wrap{margin-top:15px; padding:25px 12px 15px; background:url(../images/title_bg.png) repeat-x; position:relative}
.btns_wrap a{height:30px; line-height:30px; border-radius:2px; text-align:center; line-height:30px; color:#fff; font-size:16px}
.btns_wrap a.disabled{background:rgba(35,180,255,.5)}

.share_btn{ width:110px; height:30px; background:#ffb83b; border-radius:15px; margin:10px auto; display:block; line-height:30px; text-align:center; font-size:16px}
.return_top{width:36px; height:36px; background:url(../images/return_icon.png) no-repeat; background-size:100% 100%; position:fixed; bottom:70px; right:5%; cursor:pointer; z-index:999999}


.end{padding:12px; line-height:26px}

.end_btn{text-decoration:underline; color:#fff}
.yellow{color:#ffb83b}

.ft_module{ width:100%; position:fixed; bottom:0;left:0px; z-index:999999; background: url(../images/fudong_bg.png) repeat;}
.ft_btnA {color: #FFFFFF; padding:5px 2px 5px 4px;}
.ft_btnA, .ft_btnA dl {float:left; margin-top:3px;}
.ft_btnA dl.ft_margin15 {margin:0px 0 0 5px;}
.ft_btnA dl p.font18 {font-size:16px; margin-bottom:2px;}
.ft_btnA dl p.font14 {font-size:12px;}
.ft_btnB { float:right; margin-right:5px; margin-top:8px;}
.ft_btnB .down_button { margin-top:5px; padding:5px; text-align:center; cursor:pointer; color:#FFFFFF; background-color:#7FC31E;}
.ft_btnB .down_button a {font-size: 14px;}
.ft_close{float:right;/* border-left:1px solid #898989;*/ height:100%;}
.ft_close a{display:inline-block; background:url(../images/close_menu.png) no-repeat 0 0px;width:20px; height:20px; cursor:pointer}

/*提示框*/
#body-bg{position:absolute; top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-webkit-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{ position:absolute;width: 90%; left:50%;top:50%; background:#fff; border-radius:15px; margin-left:-45%; z-index: 99999; color:#818181}
.tip-con{ margin: 15px 0; padding: 10px 0 15px; border-bottom:1px solid #e6e6e6; text-align:center;}
.tip-con img{width:144px; height:116px; margin:0 auto}
.tip-con p{padding-top:5px}
.btns_close{display:block; height:28px; margin:0 0 15px; color:#19d5fd; line-height:28px; text-align:center; font-size:16px}
.btns_close2{display:block; width:30px; height:30px; background:url(../images/tip_clo3.png) no-repeat; background-size:100% 100%; position:absolute; right:10px; top:5px; cursor:pointer}
.wrap{margin-top:8px; padding-bottom:10px; background:#fff;}