@charset "utf-8";
/* CSS Document */
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td,hr,button,article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section {margin:0;padding:0;}
body,button,input,select,textarea{font:12px/21px Microsoft YaHei,tahoma,arial,\5b8b\4f53;}
body{background-color:#ebebeb;}
ol,ul{list-style:none}
img{border:0}
.clearfix{zoom:1}
.clearfix:after,.clearfix:before{content:"\0020";display:block;height:0;overflow:hidden}
.clearfix:after{clear:both}
.clear { clear: both; height: 0; line-height: 0; font-size: 0; overflow: hidden;}


@media all and (max-width:330px){
	#weixin-main{margin:0 auto; width:260px;}
	.weixin-top img{ width:99.8%;text-align:center}
	.weixin-part-module{width:240px;}
	.weixin-explain{width:240px;}
	.weixin-activity-bg{ background-color:#fff; box-shadow:0 2px #d8d8d8!important;}
	.weixin-activity{width:240px; background-color:#fff; box-shadow:0 2px #d8d8d8;}
	}
@media screen and (min-width:330px) and (max-width:478px){
	#weixin-main{margin:0 auto; width:280px;}
	.weixin-top img{ width:99.8%;text-align:center}
	.weixin-part-module{width:260px;}
	.weixin-explain{width:260px;}
	.weixin-activity{width:260px; background-color:#fff; box-shadow:0 2px #d8d8d8;}
	}
@media screen and ( max-width: 478px ) {
	#weixin-main{margin:0 auto; width:320px;}
	.weixin-top img{ width:99.8%;text-align:center}
	.weixin-part-module{width:300px;}
	.weixin-explain{width:300px;}
	.weixin-activity{width:300px; background-color:#fff; box-shadow:0 2px #d8d8d8;}
}
@media screen and (min-width:478px) and (max-width:667px){
	#weixin-main{margin:0 auto; width:380px;}
	.weixin-top img{ width:99.8%;text-align:center}
	.weixin-part-module{width:360px;}
	.weixin-explain{width:360px;}
	.weixin-activity-bg{ background-color:#fff; box-shadow:0 2px #d8d8d8!important;}
	.weixin-activity{width:360px; background-color:#fff; box-shadow:0 2px #d8d8d8;}
	}
.weixin-top{}
.weixin-part-module{border-radius:4px;margin:4px 0px 0 0px;padding:14px 10px 15px 10px;}
.weixin-part1{padding:14px 10px 23px 10px; background-color:#fff; box-shadow:0 2px #d8d8d8!important;}
.weixin-part1 p.weixin-title{font-size:16px;color:#606060;}
.weixin-part1 p.weixin-affiliate{height:36px; margin-top:10px;}
.weixin-part2{ background-color:#fff; box-shadow:0 2px #d8d8d8;}
.weixin-part2 p.weixin-title{font-size:18px;color:#606060; text-align:center; color:#248be6;}
.weixin-part1 p input.weixin-inputtext{width:67%; border:1px solid #d5d7d7; border-radius:4px;font-size:16px;line-height:34px; float:left; padding:0px 2px;}
.weixin-part1 p input.weixin-inputsubmit{width:80px;background:url(../images/weixin_bg.png) no-repeat scroll -217px -10px;font-size:16px; line-height:36px; color:#e7f4ff; border:none; float: right;}
.weixin-explain{margin:10px 0px 0 0px; border-radius:4px; padding:14px 10px 11px 10px; color:#606060; font-size:14px;}
.weixin-jx{ background-color:#fff; box-shadow:0 2px #d8d8d8;}
.weixin-je{ background-color:#fff; box-shadow:0 2px #d8d8d8;}
.weixin-activity{margin:10px 0px 0 0px; border-radius:4px; padding:14px 10px 10px 10px; color:#606060; font-size:14px; background-color:#fff; box-shadow:0 2px #d8d8d8;}
.weixin-explain h4,.weixin-activity h4{font-size:16px;background:url(../images/weixin_bg.png) no-repeat scroll -10px -54px; position:absolute; width:150px; margin-top:-16px; color:#fff; font-weight:normal; margin-left:-10px; line-height:32px; padding-left:5px;}
.weixin-content{padding-top:22px;}
.weixin-content ul{/*padding-left:15px;*/}
.weixin-content ul li{list-style:decimal outside none; font-size:14px; line-height:25px;}
.weixin-content > p{ line-height:25px;}






