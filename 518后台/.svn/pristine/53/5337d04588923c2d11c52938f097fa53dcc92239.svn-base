{// 加载头部公共文件 }
<include file="../Public/header" />
<style type="text/css">
<!--
.tb{ border-top:1px solid #666666;border-left:1px solid #666666;}
.tb th,.tb td{border-right:1px solid #666666; border-bottom:1px solid #666666;}
.shaixuan input,select{width:85%;}
-->
</style>
<div class="main" >
<div class="content">
<center>
	<form action="__URL__/admin_permission_copy_do" method="post" id="copyForm">
		<table class="tb" style="width:65%;">
			<tr>
				<td>权限来源：</td>
				<td><input type="text" name="c_name" required placeholder="权限来源用户的姓名"/></td>
				<td>权限赋给：</td>
				<td><input type="text" name="t_name" required placeholder="权限赋给谁？"/></td>
			</tr>
			<tr>
				<td colspan=4 style="text-align:center;"><button type="button" id="startCopy">开始复制···</button></td>
			</tr>
		</table>
	</form>
	<script>
		$(function(){
			$("#startCopy").click(function(){
				if(confirm("确认复制权限？"))
				{
					$("#copyForm").submit();
				}
			});
		})
	</script>
</center>
</div>
</div>
<!-- 主页面结束 -->
