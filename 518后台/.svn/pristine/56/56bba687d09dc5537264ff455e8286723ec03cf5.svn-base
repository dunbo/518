<body>
    <center><h2>编辑采集站点：</h2></center>
    <form method="post" action="__URL__/edit_website" id="add_website">
        <div name="div_basic">
            <h3>1，基本信息配置：</h3>
            <div class="form_list_one">
                <label><font color="red">*</font>网站名称：</label>
                <input type="text" class="input_text" name="website_name" id="website_name" value='{$website.website_name}'/>
            </div>
            <div class="clear"></div>
            <div class="form_list_one">
                <label>优先级：</label>
                <input type="text" class="input_text" name="priority" id="priority"  value='{$website.priority}'/>
            </div>
            <div class="clear"></div>
            <div class="form_list_one">
                <label>备注：</label>
                <textarea style="width:500px" name="remark" id="remark">{$website.remark}</textarea>
            </div>
        <div class="clear"></div>
        </div>
        <div name="div_list">
            <h3>2，入口配置：</h3>
            <div class="form_list_one">
                <label><font color="red">*</font>入口方式：</label>
                <select style="width:200px" name="entrance_type" id="entrance_type" onchange="entrance_type_change(this.value)">
                    <option value="1" <if condition="$website.entrance_type eq 1">selected</if>>列表</option>
                    <option value="2" <if condition="$website.entrance_type eq 2">selected</if>>api</option>
                </select>
            </div>
            <div class="form_list_one">
                <label>入口url或api：</label>
                <textarea style="width:500px" name="fake_list_url" id="fake_list_url">{$website.fake_list_url}</textarea>
            </div>
            <div class="clear"></div>
            <div class="form_list_one">
                <label><font color="red">*</font>是否保存cookie：</label>
                <select style="width:200px" name="cookie_open" id="cookie_open">
                    <option value="0" <if condition="$website.cookie_open eq 0">selected</if>>否</option>
                    <option value="1" <if condition="$website.cookie_open eq 1">selected</if>>是</option>
                </select>
            </div>
            <br/>
            <div name="entrance_type_1_div" id="entrance_type_1_div" <if condition="$website.entrance_type eq 1"><else/>style="display:none;"</if>>
                <div class="form_list_one">
                    <label>页码的dom：</label>
                    <textarea style="width:500px;height:26px" name="max_page_dom_str" id="max_page_dom_str">{$website.max_page_dom_str}</textarea>
                </div>
                <div class="form_list_one">
                    <label>页码的value：</label>
                    <input type="text" class="input_text" name="max_page_dom_value" id="max_page_dom_value" value='{$website.max_page_dom_value}'/>
                </div>
                <div class="form_list_one">
                    <label>页码的正则：</label>
                    <textarea style="width:500px;height:26px" name="max_page_dom_reg" id="max_page_dom_reg">{$website.max_page_dom_reg}</textarea>
                </div>
                <br/>
                <div class="form_list_one">
                    <label>cell的dom：</label>
                    <textarea style="width:500px;height:26px" name="cell_dom_str" id="cell_dom_str">{$website.cell_dom_str}</textarea>
                </div>
                <br/>
                <div class="form_list_one">
                    <label>详情页url的dom：</label>
                    <textarea style="width:500px;height:26px" name="detail_url_dom_str" id="detail_url_dom_str">{$website.detail_url_dom_str}</textarea>
                </div>
                <div class="form_list_one">
                    <label>详情页url的value：</label>
