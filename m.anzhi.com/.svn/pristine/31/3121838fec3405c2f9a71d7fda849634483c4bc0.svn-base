/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,
textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto;font-size:15px; font-family:Microsoft Yahei,"微软雅黑"; 
background:#471376;color: #d859ef;
 overflow-x: hidden;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#d859ef; font-size: 14px; outline: none;}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.cl{overflow:hidden;}
.clear{clear:both;overflow:hidden;}
.fl{float:left}
.fr{float:right}
.center{text-align: center;}
.banner{width: 100%; margin: 0 auto; position: relative;}
.banner img{ width: 100%;}
.notice{width:100%; height:0.6rem; line-height: 0.64rem; position: absolute; left:0; bottom:0.3rem;}
.notice_list{margin: 0 1.5rem 0 1.8rem; color:#ffce0a; font-size: 0.26rem;}
.wrap{padding:0 0.25rem; padding:0 0 0.08rem; position: relative;}
.user_wrap{height:1.1rem; padding:0.48rem 0.3rem 0 0.6rem; background:url(../images/login_bg.jpg) no-repeat; 
background-size:100% 100%}
.user_info{position: relative;}
.user_info ul{margin: 0 1.9rem 0 0.1rem; line-height: 0.42rem; font-size: 0.26rem;}
.user_info ul li i{float:left; height:0.32rem; line-height: 0.4rem; font-style: normal;}
.user_info ul li:nth-of-type(1){font-size: 0.28rem;}
.user_info ul li:nth-of-type(2){padding-top:0.1rem; font-size: 0.24rem; color:#ffe0f9}
.user_info ul li:nth-of-type(2) span{color:#fffc00}
.btns{display: block; width:1.38rem; height:0.54rem; border:1px solid #81239c; border-radius: 0.1rem;
 box-shadow: 0 0 8px rgba(234,55,255,.5); text-align: center; line-height: 0.56rem; font-size: 0.26rem; color:#e956d7}
.user_info a.btns{ position: absolute; right:0.05rem; top:50%; margin-top:-0.32rem}
.user_info_a{float:left; width:1.2rem; height:0.32rem; margin-left:0.1rem; border:1px solid #673dfe;
 border-radius: 15px; color:#673dfe; line-height: 0.38rem; font-size: 0.22rem; text-align: center;}
.titles{width:3.52rem; height:1.16rem; margin: 0.4rem auto 0; background:url(../images/title_bg.png) no-repeat; 
background-size:100% 100%; text-align: center; line-height: 1.2rem;}
.titles span{text-shadow:#ff6ab4 1px 0 0,#ff6ab4 0 1px 0,#ff6ab4 -1px 0 0,#ff6ab4 0 -1px 0; color:#23008a; font-weight: bold; font-size: 0.32rem;}
.ball_wrap{height:11.97rem; padding:0 0.25rem 0 0.6rem; background:url(../images/bg1.jpg) no-repeat;
 background-size:100% 100%; overflow:hidden;}
.ball_info{margin-top:0.3rem; font-size: 0.27rem;}
.ball_info p,.prompt_02,.select_no{margin-bottom:0.15rem; height: 0.4rem; font-size: 0.27rem;}
.select_no{margin-bottom: 0.1rem;}
.prompt_02{margin-bottom: 0.2rem;}
.ball_info p span,.prompt_02 span{color:#ffeb0a}
.ball_info ul{margin-bottom: 0.37rem;}
.ball_info ul li{display: inline-block; width:0.68rem; height:0.68rem; margin-right:0.1rem;
 background:url(../images/ball_bg1.png) no-repeat;
 background-size: 100% 100%; line-height: 0.71rem; text-align: center; font-size: 0.3rem; color:#311456; font-weight: bold;}
.ball_info ul.select_ball_blue li,.ball_info2 ul li:last-of-type{background:url(../images/ball_bg2.png) no-repeat; background-size: 100% 100%; margin-right: 0;}
.ball_info2 ul li:last-of-type.gray{background:url(../images/ball_bg3.png) no-repeat; background-size: 100% 100%;}
.ball_select{width:4.82rem; margin: 0 auto; position: relative;}
.absolute_bg1{display: block; width:6.4rem; height: 4.72rem; background:url(../images/absolute_bg1.png) no-repeat;
background-size:100% 100%; position: absolute; left:-0.97rem; top:0; z-index: 1;}
.ball_select img{display: block; width:4.82rem}
.ball_select a{display: block; position: absolute; text-indent: -9999rem; z-index: 2;}
.begin{width:1.34rem; height:1.34rem; bottom:0; left:1.7rem; border.7-radius: 50%;}
.refresh{width:0.9rem; height:0.9rem; border-radius: 50%; bottom:0.22rem; right:0.33rem}
.clean{width:0.9rem; height:0.9rem; border-radius: 50%; bottom:0.22rem; left:0.31rem}
.soft_wrap{height:4.67rem; padding:0 0.25rem 0 0.49rem; background:url(../images/bg2.jpg) no-repeat; background-size: 100% 100%; overflow: hidden;}
.soft_list{width: 100%;margin: 0.25rem auto 0;}
.soft_list li{float: left;display:inline;width: 21%;margin: 0px 2%;}
.soft_icon{width:1.04rem; padding:0.07rem 0.1rem 0.04rem 0.06rem; background:url(../images/soft_bg1.png) no-repeat; background-size:100% 100%}
.soft_list li a{display: block;width: 92%; padding:4%;}
.soft_list li p{text-align: center;color: #7e3500}
.soft_list li .soft_icon img{width:100%; margin: 0 auto; border-radius: 0.2rem;}
.soft_list li .soft_name{}
.soft_list li .soft_name,.soft_list li .soft_size{width:100%;height:0.5rem;margin-top:0.01rem; margin-bottom:0.12rem; line-height:0.54rem;text-overflow:ellipsis;white-space:nowrap;overflow: hidden; font-size: 0.22rem; color:#ef59dc}
.soft_list li a.soft_btn{display:block; width:1.21rem; height:0.65rem; padding:0; border:none; background: url(../images/btns_03.png) 0 0 no-repeat;background-size: 100% 100%;  margin:0 auto; text-align:center; line-height:0.69rem; font-size: 0.22rem; font-weight: bold; color:#ffeb0a;  text-shadow:#000 1px 0 0,#000 0 1px 0,#000 -1px 0 0,#000 0 -1px 0;}
.change_btn{position: absolute; right:0.3rem; top:1.1rem; width:0.98rem; height:0.26rem; background:url(../images/change_btn.png) no-repeat;
background-size: 100% 100%; text-indent: -9999rem;}

.rule_wrap{height:10.15rem; padding:0 0.3rem 0 0.6rem; background:url(../images/bg3.jpg) no-repeat; background-size: 100% 100%; overflow: hidden;}
.detail_ctn{margin-top:0.15rem; font-size: 0.26rem;}
.detail_ctn li{position:relative; line-height:0.45rem; text-align: justify;}
.rule_wrap2{height:17.27rem; padding:0 0.3rem 0 0.6rem; background:url(../images/bg5.jpg) no-repeat; background-size: 100% 100%; overflow: hidden;}
.rule_wrap2 .detail_ctn li{line-height:0.5rem;}
.rule_wrap3{height:6.22rem; padding:0 0.3rem 0 0.6rem; background:url(../images/bg6.jpg) no-repeat; background-size: 100% 100%;}
.rule_wrap3_inner{width:5.5rem; margin: 0 auto; padding:0.15rem 0 0.1rem; background:url(../images/bg7.jpg) no-repeat; background-size: 100% 100%;}
.rule_wrap3 table{width:90%; margin: 0 auto; font-size: 0.26rem;}
.rule_wrap3 table td,.rule_wrap3 table th{height:0.55rem; text-align: center;}
.rule_wrap3 table th{color: #ffeb0a;}
.tips_img{display: block; width:1.27rem; margin: 0.6rem auto 0.5rem; position: relative; left:-0.09rem}
.select_bg1{padding:0.2rem 0 0.1rem 0.28rem; background:url(../images/bg4.jpg) no-repeat; background-size:100% 100%; overflow: hidden;}
.tab_title{height:1.78rem; background:url(../images/tab_title1.png) no-repeat; background-size:100% 100%; overflow: hidden;}
.tab_title2{background:url(../images/tab_title2.png) no-repeat; background-size:100% 100%;}
.tab_title3{background:url(../images/tab_title3.png) no-repeat; background-size:100% 100%;}
.tab_title4{background:url(../images/tab_title4.png) no-repeat; background-size:100% 100%;}
.tab_title ul{width:5.6rem; margin: 0.65rem auto 0;}
.tab_title ul li{float:left; width:2.6rem; height:1.04rem; text-indent: -9999px;}
.tab_title ul li:nth-of-type(1){margin-right:0.4rem;}
.tab_title ul li a{display:block; width:2.6rem; height:1.04rem; font-size: 0.32rem; line-height: 0.69rem;}
.ball_list li{display: inline-block; width:0.68rem; height:0.68rem; margin-bottom: 0.3rem; margin-right:0.18rem;
 background:url(../images/ball_bg1.png) no-repeat; background-size: 100% 100%; line-height: 0.68rem; text-align: center; font-size: 0.3rem; color:#311456;
 font-weight: bold; cursor: pointer}
 .ball_list li:nth-of-type(7n){margin-right: 0;}
 .ball_list li.gray{background:url(../images/ball_bg3.png) no-repeat; background-size: 100% 100%;}
 .btns2{display: block; width:3.82rem; height:0.8rem; background:url(../images/btns_01.png) no-repeat;
 background-size: 100% 100%; margin: 0 auto 0.3rem; text-indent: -9999px;}
.select_bg2{height:3.39rem; padding:0 0.2rem 0 0.3rem; background:url(../images/bg8.jpg) no-repeat;overflow: hidden;}
.ball_list_blue li{background:url(../images/ball_bg2.png) no-repeat; background-size: 100% 100%;}
.select_bg2 .ball_info ul{margin-bottom: 0.2rem;}
.select_bg1 .ball_info ul{margin-bottom: 0.2rem;}
.select_bg2 .ball_info li,.select_bg1 .ball_info li{margin-right:-0.08rem}
.select_bg1 .ball_info{margin-top:0}
.select_bg1 .ball_info:last-of-type{padding-bottom:0.1rem;}
.select_bg1 .ball_info .ball_list li{margin-bottom:0;}
.ball_control{float: left;}
.ball_control span,.ball_control em{float: left; margin-left:0.1rem; display: inline; color:#ffeb00}
.ball_control span{width:0.25rem; height:0.54rem; margin-top:0.1rem; cursor: pointer;}
.ball_control span.reduce_btn{background:url(../images/reduce_btn.png) no-repeat center center; background-size: 0.25rem 0.25rem}
.ball_control span.add_btn{background:url(../images/add_btn.png) no-repeat center center; background-size: 0.25rem 0.25rem}
.ball_control em{width:0.6rem; margin-top:0.07rem}
.select_bg2 .btns2{margin-top:0.4rem}
.ball_control_pre{display: inline-block; width:0.65rem; color:#ffe6fc}
.wrap2{width:5.63rem; margin: 0 auto 0.2rem; padding:0.2rem 0 0.2rem 0.18rem; border-radius: 0.1rem; background:url(../images/wrap_bg.png) no-repeat; background-size: 100% 100%;}
.ball_time{margin-bottom:0.2rem; font-size: 0.28rem; color:#ffeb0a; text-align: center;}
.wrap2 .ball_info li{margin-right: 0;}
.prompt_01{font-size: 0.26rem; color:#ee59dc}
.wrap2 .ball_info:last-of-type{padding-bottom:0rem;}
.wrap2:first-of-type{margin-top:0.2rem}
.rule_wrap a{color:#ffe537; text-decoration: underline; font-size: 0.26rem;}
.rule_wrap a.share_btn{margin-top:0.95rem; text-decoration: none;}

/*提示框*/
.wrap3{margin-top:0.35rem; padding:0.03rem; background: -webkit-linear-gradient(top,rgba(227,84,250,1),rgba(156,5,255,1)); 
background: -o-linear-gradient(top,rgba(227,84,250,1),rgba(156,5,255,1)); 
background: -moz-linear-gradient(top,rgba(227,84,250,1),rgba(156,5,255,1)); 
background:linear-gradient(top, rgba(227,84,250,1), rgba(156,5,255,1)); border-radius: 0.15rem;}
.wrap_inner{background:#2d1352; border-radius: 0.1rem;}
.wrap_inner2{margin:1px; background:#46268a url(../images/tips_bg.png) no-repeat center 0;
background-size: 4.7rem 1.94rem;
 border:0.03rem solid #120821; border-radius: 0.1rem; position: relative;}
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{position:absolute; width:80%; height:auto; left:50%; top:50%; margin-left:-40%; z-index: 99999;}
.tip_hd h4{height:0.76rem; background: -webkit-linear-gradient(left,rgba(217,88,204,1),rgba(97,58,221,1)); background: -o-linear-gradient(left,rgba(217,88,204,1),rgba(97,58,221,1)); background: -moz-linear-gradient(left,rgba(217,88,204,1),rgba(97,58,221,1)); background:linear-gradient(left, rgba(217,88,204,1), rgba(97,58,221,1)); line-height:0.78rem; font-size:0.3rem; text-align: center; color:#ffffff}
.tip-mid{padding:0.15rem 0}
.tip-con{width:90%;  margin:0px auto;}
.tip-con p{padding:0.85rem 0 0.2rem; font-size: 0.28rem; line-height: 0.46rem; color:#ee59dc; text-align: center;}
.tip-con p span{color:#ff3131}
.tip-box2 .tip-con p span{color:#ee59dc}
.clo{ position: absolute; right: 0.12rem; top:0.12rem;}
.clo a{ display: block; width:0.36rem; height:0.36rem;background:url("../images/tip_clo.png") 0 0 no-repeat;background-size:100% auto;text-indent: -9999px;}
.tip-box a{margin-top:0}
.btns4{width:90%; margin: 0.1rem auto 0.2rem;}
.btns4 a{width:1.85rem; height:0.57rem; background:url(../images/btns_02.png) no-repeat; background-size: 100% 100%;
text-align: center; line-height: 0.57rem; color:#ffd237; font-size: 0.23rem;text-shadow:#000 1px 0 0,#000 0 1px 0,#000 -1px 0 0,#000 0 -1px 0;}

.form-items{padding:0.85rem 0 0;}
.form-items li{position: relative;}
.form-items li span{display: block; margin: 0 0 0.33rem 0; padding:0.04rem; background:rgba(225,47,240,.2); border-radius: 0.1rem;}
.form-items input{display: inline-block; width:100%; height:0.62rem; background:#fff; border:none; text-indent: 0.1rem; background:#fffef5; border-radius: 0.09rem; font-size: 0.24rem; color:#6f2f00; line-height: 0.57rem;}
.form-items span.input_short{float:left; width:40%}
.close_btn{display: block; width:0.41rem; height:0.41rem; background:url(../images/tip_clo.png) no-repeat; background-size:100% 100%; position: absolute; right:-0.08rem; top:-0.1rem; cursor: pointer;}
.soft_wrap .titles{background:url(../images/title_bg1.png) no-repeat; background-size:100% 100%; text-indent: -9999px;}
.rule_wrap .titles,.rule_wrap2 .titles{background:url(../images/title_bg2.png) no-repeat; background-size:100% 100%; text-indent: -9999px;}
.btns2_red{background:url(../images/btns_05.png) no-repeat; background-size:100% 100%;}
.btns2_blue{background:url(../images/btns_06.png) no-repeat; background-size:100% 100%;}
.btns_submit{background:url(../images/btns_07.png) no-repeat; background-size:100% 100%;}
.btn_disabled{background:url(../images/btns_04.png) no-repeat; background-size:100% 100%;}
.btns_return{background:url(../images/btns_08.png) no-repeat; background-size:100% 100%;}