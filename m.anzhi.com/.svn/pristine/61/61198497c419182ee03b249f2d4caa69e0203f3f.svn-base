/* CSS Document */
body,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
fieldset,img {border: 0;}
a{text-decoration:none; color:#000000;}
.clear{clear:both;}
ul,li{list-style:none;}
body{ font-family:'微软雅黑'; background:#e5e5e5; font-size:14px; line-height:20px; color:#000000;}

#main{width:98%; margin:0 auto;}

.uninstall_des{height:72px; margin:10px 0; padding-left:80px; background:url(../images/logo_2.png) no-repeat;}
.uninstall_des p{padding:5px 0 0 0; font-size:16px; line-height:30px;}
.uninstall_list{width:100%; position:relative}
.uninstall_list input{position:absolute; left:-1000px; clip:rect(0,0,0,0); z-index:-1;}
.uninstall_form .uninstall_list_checkbox{ display:block; width:99%; height:34px; margin:0 auto 6px; background:#eff2f2; border-radius:5px; color:#575757; font-size:16px; font-family:"微软雅黑"; text-align:center; line-height:34px; cursor:pointer}
.uninstall_list_checkbox.checked{background:#008aea; color:#ffffff;}
.uninstall_form p{ padding:5px 0 8px 5px; color:#666666; font-size:14px;}
.uninstall_form textarea{ display:block; width:97%; height:50px; padding:5px 1%; margin:0 auto; border:1px solid #bdbdbd; border-radius:5px; background:#ffffff;}
.uninstall_form_btn input{ display:block; width:80%; height:42px; margin:10px auto; border:none; border-radius:5px; background:#008aea; color:#ffffff; font-size:21px; font-family:"微软雅黑"}

#main1{padding-top:220px; background:url(../images/icon_01.png) no-repeat center 60px;}
#main1 p{text-align:center; line-height:30px; color:#686868; font-size:18px;}
