<include file="../Public/header"/>

<link rel="stylesheet" type="text/css" href="/Public/css/tablelist.css" />
<script type="text/javascript" src='__PUBLIC__/js/jquery.js'></script>
<link rel="stylesheet" href="/Public/css/thickbox.css" type="text/css" media="screen" />
<script type="text/javascript" src='/Public/js/thickbox-compressed.js'></script>

<body>
    <table width="99%" class="table_list" cellpadding="0" cellspacing="0" style="margin-top:4px;">
		<thead>
			<tr align="center">
				<th width="4%" height="26" bgcolor="#E3E2FE"><strong>序 号</strong></th>
				<th width="6%" bgcolor="#E3E2FE"><strong>软件名称</strong></th>
				<th width="8%" bgcolor="#E3E2FE"><strong>软件包名</strong></th>
				<th width="10%" bgcolor="#E3E2FE"><strong>APK</strong>包</th>
				<th width="6%" bgcolor="#E3E2FE"><strong>抓取网站</strong></th>
				<th width="7%" bgcolor="#E3E2FE"><strong>抓取时间</strong></th>
				<th width="7%" bgcolor="#E3E2FE"><strong>忽略时间</strong></th>
				<th width="7%" bgcolor="#E3E2FE"><strong>忽略原因</strong></th>
			</tr>
		</thead>
		<tbody>
            <volist name="Datalist" id="Datalist">
				<tr <elt name="Datalist.new_sver" value="$Datalist.version_code"> bgcolor="#E0E0E0" <else/><eq name="Datalist.category" value="1">bgcolor="#FFFF00"</eq></elt> >
					<td height="20" align="center"> {$Datalist.id}</td>
					<td>{$Datalist.new_sname}</td>
                    <td align="center"><a target="blank" href="__URL__/../../Dev/Soft/softlist/package/{$Datalist.package}">{$Datalist.package}</a></td>
					<td><a href="http://downapk.anzhi.com/<?php echo $Datalist['new_sapk']?>">{$Datalist.new_sapk}</a></td>
					<td align="center">{$Datalist.new_sfromweb}</td>
					<td align="center">{$Datalist.new_sdate|date='Y-m-d H:i:s',###|default='&nbsp;'}</td>
					<td align="center">{$Datalist.ignore_time|date='Y-m-d H:i:s',###|default='&nbsp;'}</td>
					<td align="center">
                        <a href="__URL__/show_full_content?id={$Datalist.id}&&type=ignore_reason&&width=300&&height=200" class="thickbox">{$Datalist.ignore_reason}</a>
                    </td>
				</tr>
			</volist>
        </tbody>
    </table>
    <div style="width:92%; padding-top:4px; text-align:right">{$page}</div>
</body>