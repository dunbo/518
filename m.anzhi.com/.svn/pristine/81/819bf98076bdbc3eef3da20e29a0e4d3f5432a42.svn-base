@charset "utf-8";
/* CSS Document */

html {color:#000;background:#fff;-webkit-text-size-adjust: none; }
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,img,form,fieldset,legend,input,textarea,p,blockquote,th,td,hr,button,article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section,small,a{margin:0;padding:0;}
/*body,button,input,select,textarea{font:12px/21px Microsoft YaHei,tahoma,arial,\5b8b\4f53;}*/
ul,li{list-style:none;}
a{ text-decoration:none;}
a:hover{ text-decoration:none;}
.clear { clear: both; height: 0; line-height: 0; font-size: 0; overflow: hidden;}
/*main*/
.main{margin:0 auto;width:320px; max-width:480px; min-width:320px;}
.main_part{/*width:320px;*/ margin:0 auto;}
.banner{width:320px; margin:0 auto;}

/*gundong*/
#kuaixun { width:320px; margin:0 auto;/*display:inline;*/ }
#kuaixun p.gameimg{float:left; height:24px; background-color:#f00202;}
#kuaixun p.gameover{height:24px;
background-color:#f00202;
color:#fff;
font:14px/24px Microsoft YaHei,tahoma,arial,\5b8b\4f53;
text-align:center;}
#hjmd-gundong{width:288px; float:right; height:24px; margin:0 auto;background:#f00202; line-height:24px;}
#hjmd-gundong marquee{}
#hjmd-gundong marquee a {color:#fff; padding:0 5px;text-decoration:none; }
#hjmd-gundong marquee a font.mdan{padding:0 10px; color:#CCC;}
/**/
#kuaixun marquee a,
#kuaixun marquee a font.mdan,
.app-ldraw-surplus,
.app-ldraw-hdsm,
.app_part2_title,
.part5-module p{font:14px/24px Microsoft YaHei,tahoma,arial,\5b8b\4f53;}
.app-ldraw-part{width:300px; margin:0 auto; padding:8px 0px; overflow:hidden;}
.app-ldraw-surplus{float:left; color:#ff1d1d;}
.app-ldraw-hdsm{float:right; color:#fc8200;}
.app-ldraw-hdsm a.explan{color:#fc8200; }
.app_part2,
.act_part3{border-radius:4px;-moz-border-radius:4px;-webkit-border-radius:4px;box-shadow:0 2px 0 #e9bb88;}
/*app downloads*/
.app_part2{width:300px; margin:0px auto 0px auto;background-color:#ffe8cf;}
.app_part2_title{text-align:center; color:#ff1d1d; padding:10px 0px 8px 0;}
.app_down_module{border-bottom:1px solid #FFCD98;overflow:hidden;padding-bottom:5px;}
.app_down_module li{float:left; padding:10px 7px 10px 8px;}
.app_down_module li dl dt,.app_down_module li dl dd{font:13px/21px Microsoft YaHei,tahoma,arial,\5b8b\4f53;text-align:center;}
.app_down_module li dl dd p.name{font:13px/21px Microsoft YaHei,tahoma,arial,\5b8b\4f53; color:#000;}
.app_down_module li dl dd p.size{font:13px/21px Microsoft YaHei,tahoma,arial,\5b8b\4f53;color:#fc8200; text-align:center;}
/*20140923-1459*/
.download_bg{background:url(/images/button.png) no-repeat scroll 0px 0px; text-align:center; width:57px;}
.download_bg:hover{background:url(/images/button.png) no-repeat scroll 0px 0px; text-align:center;}
.download_bg a{font-size:12px; line-height:28px; color:#fff;text-decoration:none; text-shadow:0px 1px 1px #840000; width:57px; display:inline-block;}
.download_bg a:hover{text-decoration:none;}
/*add 20140924-1103*/
.version-low{padding:0px 10px 10px;}
.version-low p{ font-size:14px; font-family:Microsoft YaHei,tahoma,arial,\5b8b\4f53; color:#1d9dff;}
/*20140923-17:47*/
.app_change{width:282px; margin:0px auto;}
.app_change p a{font:16px/30px Microsoft YaHei,tahoma,arial,\5b8b\4f53; color:#fff;text-shadow:0px 1px 1px #840000; width:130px; display:inline-block;}
.app_change p{text-align:center; background:url(/images/button_change.png) no-repeat scroll center 12px; /*height:58px;*/ padding:14px 0px 18px;}
/*Activity Description*/
.act_part3{width:300px; margin:10px auto 0 auto;background-color:#ffe8cf;}
.act_part3_title_module{border-bottom:1px solid #ffcd98;}
.act_part3_title{font:16px/35px  Microsoft YaHei,tahoma,arial,\5b8b\4f53; color:#ff1d1d; margin-left:10px;}
.act_part3 .act_sides{padding:10px; font-size:14px; color:#7a7a7a; line-height:24px;}
.act_sides > p{font:14px/28px  Microsoft YaHei,tahoma,arial,\5b8b\4f53; color:#fc8200;}
.part4-title{font:16px/35px  Microsoft YaHei,tahoma,arial,\5b8b\4f53; color:#ff1d1d; text-align:center; border-bottom:1px solid #ffcd98;}
.part4-module,
.part5-module{padding:10px;}
.part6-module{padding:0px 10px;}
.part4-module h1{font:18px/30px Microsoft YaHei,tahoma,arial,\5b8b\4f53; color:#ff1d1d; text-align:center;}
.part4-module p.part4-cue{padding-left:10px;color:#fc8200;font:14px/28px Microsoft YaHei,tahoma,arial,\5b8b\4f53;}
.part4-module p input.phone-number{width:270px; background-color:#ffbe78;border-radius:4px;-moz-border-radius:4px;-webkit-border-radius:4px;border:none; height:41px;line-height:27px;font:14px/24px Microsoft YaHei,tahoma,arial,\5b8b\4f53;color:#fff; padding:0 5px;}
.part4-module p.submit-module,.part5-module p.submit-module{text-align:center;padding:15px 0 5px;}
.part4-module p input.submit-number,.part5-module p input.submit-number{width:129px; height:36px; border:none;background:url(/images/button_change_h.png) no-repeat center;font:16px/26px Microsoft YaHei,tahoma,arial,\5b8b\4f53;color:#fff;  cursor:pointer;text-shadow:0px 1px 1px #840000; padding-bottom:4px;}
/*part5*/
.part5-module p{ color:#fc8200; line-height:20px !important; letter-spacing:0.1mm;}
/*part6*/
.part6-module-btn{}
.part6-module-btn li{border-bottom:1px solid #ffcd98; color:#fc8200;font:14px/24px Microsoft YaHei,tahoma,arial,\5b8b\4f53; padding:5px 5px;}
.part6-module-btn li.part6-module-border{ border:none!important;}
/*app-downam*/
.app-downam-part4{width:300px;padding:6px 0px; margin:0 auto;}
.app-downam-part4 p{text-align:center;color:#fc8200;}
.app-downam-part4 p.down-nam{font:14px/24px Microsoft YaHei,tahoma,arial,\5b8b\4f53;}
.app-downam-part4 p.ldraw-more{font:16px/24px Microsoft YaHei,tahoma,arial,\5b8b\4f53;font-weight:bold;}
/*top_part7-20140923-10:46*/
.top_part7{width:282px; margin:10px auto;}
.top_part7 p a{font:16px/36px Microsoft YaHei,tahoma,arial,\5b8b\4f53; color:#fff; cursor:pointer; text-shadow:0px 1px 1px #840000; width:130px; display:inline-block;}
.top_part7 p{background:url(/images/button_change.png) no-repeat scroll center 0px; text-align:center;}

/*choujianggame*/
#gameContent{width:310px; height:300px;font:16px/24px Microsoft YaHei,tahoma,arial,\5b8b\4f53; color:#fff;text-shadow:0px 1px 1px #840000;overflow:hidden;margin:0 auto; position:relative;}
#gameContent table{/*float:right;*/border-collapse: separate;/*border-spacing:3.2px;*/ background:url(/images/cj_bg.png) no-repeat scroll -2px -4px;}
#gameContent td{width:100px;height:100px;text-align:center;vertical-align:middle;}/*border:1px solid #ffdd99; background:#fff9ed;*/
#my_click {width:80px;height:80px;text-align:center;vertical-align:middle; border-radius:20px;-moz-border-radius:20px;-webkit-border-radius:20px;}/*border:1px solid #ffdd99; background:#fff9ed;*/
#gameContent p{position:absolute;left:102px;top:100px;width:103px;height:100px;padding-left:0;z-index:999; cursor:pointer; border-radius:20px;-moz-border-radius:20px;-webkit-border-radius:20px;}
#gameContent p a{ display:block;width:103px;height:100px; border-radius:20px;-moz-border-radius:20px;-webkit-border-radius:20px;}
#gameContent p.waitGame{/*background:url("lottery.jpg") no-repeat -200px 0;*/}
#gameBtn{ z-index:999;display:block;width:100px;height:100px;}
#gameBtn:hover{cursor:pointer;/*background:url("lottery.jpg") no-repeat -100px 0;*/}
#gameContent td h1{font:28px/30px Microsoft YaHei,tahoma,arial,\5b8b\4f53; font-weight:bold; color:#fff600;}
