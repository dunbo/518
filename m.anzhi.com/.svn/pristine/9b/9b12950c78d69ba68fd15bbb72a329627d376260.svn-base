/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto;font-size:14px; font-family:Microsoft Yahei,"微软雅黑"; background-color: #6fa2cf; color: #550202;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#550202; font-size: 14px; outline: none;}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.cl{overflow:hidden;}
.clear{clear:both;overflow:hidden;}

.cnt{text-align: center;}
.fl{float: left;}
.fr{float: right}
.wth{width: 95%;}
.mtop{margin-top: 15px;}

.notice_icon,.slideBox .prev,.slideBox .next{ background: url("../images/common_icon.png") 5px 3px no-repeat; background-size:60px 90px;}

.banner{width: 100%; margin: 0 auto;}
.banner img{ width: 100%;}
/*结束*/
.end{width:100%;position: absolute; top:0px; left: 0;padding: 5px 0px; text-align: center; background-color: #ff9914; font-size: 16px; line-height: 25px;}
.end p{ padding: 0px 5px;}

.mod{margin: 0px 8px 20px 8px;}
.mod-t{height:24px; background:url(../images/mod_bg1.png) no-repeat; background-size:100% 24px}
.mod-b{height:24px; background:url(../images/mod_bg3.png) no-repeat; background-size:100% 24px}
.mod-m{padding:0 15px 0 18px; background:url(../images/mod_bg2.png) repeat-y; background-size:100% auto}
.mod p{line-height: 22px;}
.mod_hd{position:relative;margin-bottom:7px;color: #550202;font-size: 20px; font-weight:bold}
.mod_hd span{ float: left;margin-top: 5px;}
.mod_hd .chang_btn{ float: right; width:80px; height:35px; margin: 0px auto 0; background:url(../images/btns_bg2.png) no-repeat; background-size:100% 100%; color:#fff;cursor: pointer;text-align: center; line-height:35px}
.mod_hd .btn_myprize{position:absolute;right:0px; top:1px;padding: 1px 10px; background-color: #fff668;color: #ff5959;border-radius: 6px;-webkit-border-radius: 6px;-moz-border-radius: 6px;}
.mod_hd.mtop{margin-top: 15px;}
.col{margin: 5px auto;}
.prompt{margin-top: 10px;}
/*活动规则*/
.rule{color:#550202; font-size:18px; line-height:28px}
.rule h4{ margin-bottom:5px; text-align:center; font-weight:bold}
.rule_small{ margin-top:5px; font-size:16px}

/**/
.main{position:relative;width: 100%;background: url("../images/main_bg.jpg") 0 0 no-repeat; background-size: 100% auto; z-index:2}

/*软件*/
.soft-list{width: 100%; margin: 0px auto;}
.soft-list li{ float: left; display: inline; width: 25%;margin-top:7px;text-align: center;}
.soft-list li p{}
.soft-list li .soft-icon{text-align: center;}
.soft-list li .soft-icon img{ width: 56px;height: 56px; margin: 0 auto;}
.soft-list li .soft-name{margin-top: 10px;height: 20px; line-height: 20px; overflow: hidden;}
.soft-list li .soft-name a{ color: #;}
.soft-list li .soft-size{ color: #f46600;height: 22px; line-height: 22px;overflow: hidden;}
.soft-list li a.downlode{ display: block; width:56px;padding:6px 0px 5px; margin: 10px auto; background:url(../images/btns_bg.png) no-repeat; background-size:100% 100%; color:#ff5959;cursor: pointer;font-size:13px;text-align: center;}
/*返利*/

.rebate li{margin-bottom:2px;padding:0px 8px;line-height:30px;color:#fff; font-size:13px; background-color: #57b0fb;border-radius: 15px;-webkit-border-radius: 15px;-moz-border-radius: 15px;text-shadow: 1px -1px 1px #0a0204;-webkit-text-shadow: 1px -1px 1px #0a0204;-moz-text-shadow: 1px -1px 1px #0a0204;letter-spacing: 1px;clear: both; overflow: hidden}
.rebate li span{float: left;width:55%}
.split{margin:12px 0 8px;}
.split img{width: 100%;}
/*流程图*/
.slideBox{ position:relative;width: 90%;  margin:10px auto 0px auto;}
.slideBox .prev,.slideBox .next{ position:absolute; display:block; width:30px; height:50px; left:-18px; top:50%;margin-top:-25px;background-position:4px -34px; text-align:center;cursor: pointer;}
.slideBox .next{ left:auto; right:-18px;background-position:-32px -34px;}
.slideBox .bd{margin:0px 12px;border:2px solid #e4c9a0; }
.slideBox .bd li{text-align:center;}
.slideBox .bd li img{ vertical-align:top; width:100%;}
.copyright{margin-top:10px;}

/*尾部*/
.footer{width: 100%; margin-top:-150px; position:relative;}
.footer img{width: 100%;}
.person{position:absolute; left:0; bottom:-295px; z-index:10000; display:block; width:292px; height:318px; background:url(../images/ft_bg2.png) no-repeat}
