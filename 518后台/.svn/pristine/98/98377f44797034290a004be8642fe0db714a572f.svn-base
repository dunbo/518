
<div>
	<h3>修改分类</h3>
	<form action="__URL__/doedittype/typeid/{$typeid}/" method="post">
    <div>分类：{$result.typename|default='&nbsp;'}</div>
    <div>修改<input type="text" name="typename" id="typename" value=""/></div>
    <div>
        <input type="submit" name="sub" value="提交" style="display:inline-block;margin:10px 30px" onclick="return validata()" class="search_btn"/>
		<a href="javascript:;" class="search_btn" style="display:inline-block;" onclick="tb_remove()">取消</a>
	<div>	
	</form>

</div>
<script type="text/javascript">
function validata(){
	if($("#typename").val().trim() == ''){
		alert('分类名称不能为空');
		return false;
	}
	return true;
}
</script>
