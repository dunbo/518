<div class="newbox" id="checked_newbox">
	<h6>审核通过：</h6>
	<div class="newbox_cnt">
		<form method="post" action="" class="checked_newbox_form">
			<input type="hidden" name="balance_period" value="{$balance_period}" />
			<input type="hidden" name="settlement_patternss" value="{$settlement_patterns}" />
			<input type="hidden" name="username" value="{$username}" />
			<input type="hidden" name="charge" value="{$charge}" />
			<input type="hidden" name="salvation" value="{$salvation}" />
			<table cellpadding="0" cellspacing="0">
				<tr>
					<td>户名：</td>
					
					<td><input type="text" class="inputtext" name="account_name" readonly value="{$result[0]['account_name']}" /><input type="hidden" name="id" value="{$result[0]['id']}" /></td>
				</tr>
				<tr>
					<td>所属银行：</td>
					<td><input type="text" class="inputtext" name="bank_name" readonly value="{$result[0]['bank_name']}"  /></td>
				</tr>
				
				<tr>
					<td>开户行：</td>
					<td><input type="text" class="inputtext" name="opening_bank" readonly  value="{$result[0]['opening_bank']}" /></td>
				</tr>
				<tr>
					<td>开户行所在地：</td>
					<td><input type="text" class="inputtext" name="bank_addr" readonly value="{$result[0]['bank_addr']}" /></td>
				</tr>
				<tr>
					<td>银行账号：</td>
					<td><input type="text" class="inputtext" name="bank_account" readonly value="{$result[0]['bank_account']}" /></td>
				</tr>
			</table>
		</form>
		<div class="open_close_btn">
			<a href="__URL__/operation_check_do/id/{$result[0]['id']}/check/1">确认</a>
			<a href="javascript:;" onclick="tb_remove();">取消</a>
			<div class="clear"></div>
		</div>
	</div>
</div>
