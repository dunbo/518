<script language="javascript">
function selectAll() {	//全选
	$("[name='id[]']").each(function(){
		$(this).attr('checked',true);
	});
}
function selectRev() {	//反选
	$("[name='id[]']").each(function(){
		if($(this).is(':checked')) {
			$(this).removeAttr('checked');
		} else {
			$(this).attr('checked',true);
		}
	});
}
function batchPass() {	//批量通过
	if(confirm('是否通过选中开发者的审核？')) {
		var arr = new Array();
		var i = 0;
		$("[name='id[]']").each(function(){
			if($(this).is(':checked')) {
				arr[i] = $(this).val()
				i++;
			}
		});
		var str = arr.join(',');
		if(str=='') {
			alert('请选择要通过的开发者！');
			return false;
		}
		var url = '/index.php/Admin/User/auditforuser_confirm/uid/'+str+'/state/0/list_type/audit';
		self.location.href = url;
		if(window.ActiveXObject) window.event.returnValue = false;
	}
}
function batchReject() {	//驳回审核
	var arr = new Array();
	var i = 0;
	$("[name='id[]']").each(function(){
		if($(this).is(':checked')) {
			arr[i] = $(this).val()
			i++;
		}
	});
	var str = arr.join(',');
	if(str=='') {
		alert('请选择要驳回的开发者！');
		return false;
	}
	var url = '/index.php/Admin/User/auditforuser_confirm/uid/'+str+'/state/-1/list_type/audit';
	popup('reject_div');
	$('#denyurl').val(location.pathname);
	$('#reject_form')[0].action = url;
}

$('#reject_reason').click(function(){
	if($(this).val()=='请输入其他驳回原因：') {
		$(this).val('');
		$(this).css({'color':'#000'});
	}
});
function reject(url) {
	popup('reject_div');
	$('#denyurl').val(location.pathname);
	$('#reject_form')[0].action = url;
}
function reject_chk() {
	var rt = '';
	$('input:checkbox[name="reject[]"]:checked').each(function(){
		rt += $(this).val()+"<br />";
	});
	if($('#reject_reason').val()!='请输入其他驳回原因：') {
		rt += $('#reject_reason').val();
	}
	if(!rt) {
		alert('请选择或填写驳回原因！');
		return false;
	}
	return true;
}
</script>
<div id="bgdiv" style="position:absolute;display:none; left:0px; top:0px; width:100%; height:100%; filter:alpha(opacity=40);opacity:0.4; background-color:#000000; z-index:1000;"></div>
<script language="javascript">
function popup(id) {
	var ids=['reg','login','bgdiv'];
	for(var i in ids) {
		popclose(ids[i]);
	}

	$("#bgdiv").css("display","block");
	$("#bgdiv").css({width:$(document).width()+"px",height:$(document).height()+"px"});

	$("#"+id).css({display:"block",position:"absolute",zIndex:"10001"});
	$("#"+id).css("top",($(window).height()-$("#"+id)[0].offsetHeight)/2+$(document).scrollTop()+"px");
	$("#"+id).css("left",($(window).width()-$("#"+id)[0].offsetWidth)/2+$(document).scrollLeft()+"px");
}
function popclose(id) {
	$("#"+id).css({display:"none"});
	$("#bgdiv").css({display:"none"});
}
</script>

