/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto; background:#fff; font-size:15px; font-family:Microsoft Yahei,"微软雅黑"; line-height:20px; color:#000000}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none; font-size: 14px; outline: none; color:#000000}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{float:left;}
.fr{float:right;}
.main{position:relative; background:#052647}

.main2{background:#00517c; overflow:hidden}
/*有通用的地方*/
.wrap{background:#052647; margin:45px 6.5%; border-radius:5px; position:relative; z-index:1; color:#482608}
.titles{padding:5px 0; color:#aec9e3; font-size:18px; font-weight:bold}
.btns{display:block; width:113px; height:34px; background:url(../images/btns.png) no-repeat; background-size:100% 100%; text-align:center; line-height:32px; color:#fff; font-weight:bold; font-size:15px}
.title{display:inline-block; margin:18px 0 0; padding:6px 20px 6px 15px; background:#1c6dbf; border-radius:0 15px 15px 0; color:#fff; text-align:center; position:relative}
.title h3{color:#fff; font-size:18px}
.banner{width: 100%; margin: 0 auto; position:relative;  z-index:2}
.banner img{ width: 100%;}
/*index*/
.rule{ padding:13px 15px 15px; border-bottom:1px solid #1e5184;}
.rule p{ padding:3px 8px; line-height: 22px; position:relative; z-index:2; color:#aec9e3}
.rule p span{color:#c80000;}
.active_time{display:inline-block}
.lottery_wrap{ padding:22px 15px 10px; border-bottom:17px solid #00517b;}
.prize_index{position:relative;}
.prize_index p{color:#fff; font-weight:15px}
.prize_index p span{padding:0 1px; color:#fff; font-size:18px; font-weight:bold}
.prize_index a{display:block; position:absolute; right:0; top:-6px; width:110px; height:36px; background:url(../images/btns_03.png) no-repeat; background-size:100% 100%; text-align:center; text-indent:-9999px}
.prize_index p.prize_tips{color:#ffea00}
/*转盘抽奖*/
.rotate {width:90%; margin:15px auto 0; position: relative; overflow}
.rotate-con{width:100%;}
.rotate img{width:100%;}
.rotate-pointer {position: absolute;top: 50%;left: 50%;width: 40%;height: 40%;margin-top: -20.5%;margin-left: -20%;background: url(../images/pointer.png) no-repeat 0 0;background-size: 100% 100%; cursor: pointer;-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.pointer-disabled{background: url(../images/pointer_disabled.png) no-repeat 0 0;background-size: 100% 100%; z-index:11}

.btns1 a{display:block; margin:10px auto;}
.soft{padding:14px 10px 10px; margin-top:-25px; position:relative; background:#00517b; z-index:2}
.soft_icon{width: 64px;height: 64px; position:absolute; left:10px; top:14px}
.soft_icon img{width: 64px;height: 64px;}
.soft_info{margin:0 0 0 68px; color:#fff}
.soft_info h3{font-size:20px; font-weight:bold}
.soft_info p{ padding:8px 0 0 2px; line-height:17px; font-size:13px}
.soft_info p span{padding-left:5px; color:#ff3d40}
.soft_info p em{color:#ffd45f;}
.soft a.download{position:absolute; right:10px; bottom:24px; display: block; width:60px;height: 32px; padding-left:19px; line-height: 33px; margin: 10px auto; background:url(../images/btns.png) no-repeat; background-size:100%100%; background-clip: padding-box; color:#d0eefb;cursor: pointer; text-align: center; font-size:16px;}
.soft a.registered{width:79px; padding-left:0; background:#616161; border-radius:20px; color:#fff;}

.imgs_wrap{padding:20px;}
.imgs_wrap ul li{float:left; width:30.3%; margin:0 1.5%}
.imgs_wrap ul li img{display:block; width:100%;}
.imgs_wrap p{padding:20px 0 0; text-align:center; color:#fff}


/*我的奖品*/
.my_prize{padding:15px 0 8%; margin:0 6%; position:relative; color:#e1b976}
.my_prize_list2{padding:5px 0;}
.my_prize_list2 li{ position:relative; font-size:16px}
.my_prize_list2 p{ padding:6px 0;}
.form-items li{ margin-top: 30px; clear: both; overflow: hidden;}
.form-items li label{ float: left;width:90px;padding-top: 10px; color:#e1b976;}
.form-items li span{display:block;margin-left: 90px;}
.form-items li input{ width:90%;padding:10px 6px;border:none;color: #000;  font-family: Microsoft Yahei,"微软雅黑"; background-color:#fff; border-radius:2px}
.form_error{display:block; padding:8px 0 0 90px; color:red}
.split{height:11px; background:url(../images/line_bg.png) no-repeat; background-size:100% 100%;  margin: 8px 0px 15px; overflow:hidden;}
.my_prize .btns{background:url(../images/btns_01.png) no-repeat; background-size:100% 100%; margin-top:40px}
/*结束页*/
#end_wrap {padding-bottom:30px}

/*提示框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-webkit-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{ position:absolute; width:295px; height:210px; left:50%; top:50%; margin-left:-147px; z-index: 99999; background:#052647; border:1px solid #015079; border-radius:10px; font-size:15px;}
.tip-con{ margin: 15px 15px 0 15px; padding:20px 30px 10px 20px; color:#fff;}
.tip_title h6{ padding:2px 0 20px; min-height:20px; color:#fff; font-size:18px; font-weight:bold; line-height:18px;text-align:center}
.tip-con p{padding:0 0 20px 20px; line-height: 22px; word-wrap:break-word;word-break:break-all; font-size:15px; color:#fff}
.tips_btns a{display:block; width:98px; height:34px; background:url(../images/btns_01.png) no-repeat; background-size:100% 100%; margin:0 auto 0; line-height:35px; text-align:center; color:#fff}
.footer{width:100%; height:42px; background:#00517b;  text-align:center; position:relative; color:#fff; line-height:42px;}
.bold{font-weight:bold;}
.red{color:#ab0500}
.prompt_01{color:#e1b976}

