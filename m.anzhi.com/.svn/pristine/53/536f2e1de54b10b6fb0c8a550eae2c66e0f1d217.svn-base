/* CSS Document */
/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{max-width:640px;min-width:320px; margin:0 auto;font-size:0.14rem; font-family:Microsoft Yahei,"微软雅黑"; background: #330257; color: #fff;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#fff; font-size: .14rem;}
.a,input,button,div{-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{ float: left;}
.fr{ float: right}
.banner{width:100%; position: relative;}
.banner img{ width: 100%; display:block;}
.main{padding:0 0.3rem; background:url(../images/main_bg.jpg) no-repeat; background-size:100% auto; overflow: hidden;}
.main2{padding:0 0.3rem; background:url(../images/main_bg2.jpg) no-repeat; background-size:100% auto; overflow: hidden;}
.relive{width:2.91rem; height:0.89rem; margin: 0 auto; background:url(../images/relive_bg.png) no-repeat; background-size:100% 100%; position: relative;}
.relive span{position: absolute; right:0.15rem; top:0.2rem; width:0.55rem; height:0.4rem; line-height: 0.4rem; font-size: 0.4rem; color:#ef4f3f;text-shadow:#000 1px 0 0,#000 0 1px 0,#000 -1px 0 0,#000 0 -1px 0;}
.begin{width:5.8rem; height:2.65rem; margin-top:0.35rem; padding-top:0.55rem; background:url(../images/bg1.png) no-repeat; background-size:100% 100%; overflow: hidden; position: relative;}
.begin img{width:100%;}
.begin img.begin_img01{margin-top:0.2rem}
.user_prize{width:1.35rem; height:0.48rem; position: absolute; right:0; top:0.05rem; text-indent: -9999px;}
.begin_txt{padding:0.15rem 0 0.4rem; font-size: 0.44rem;}
.begin_txt span{float: left; width:2.6rem; text-align: center;}
.begin_txt em{float:right; width:3rem; text-align: center;}
.begin_txt i{font-size:0.28rem; font-style:normal; color:#c5a8ff}
.begin_txt2{padding:0.25rem 0 0.15rem}
.begin_txt2 p{text-align: center;}
.btns{display: block; width:5.3rem; height:0.8rem; margin: 0 auto; background:url(../images/btn_01.png) no-repeat; background-size: 100% 100%; text-indent: -9999px;}
.btns2{margin: 0.24rem auto 0;}
.btns2 a{height:0.7rem; text-indent: -9999px;}
.btns2 .fl{width:3.6rem; background:url(../images/btn_02.png) no-repeat; background-size:100% 100%; }
.btns2 .fr{width:2.12rem; background:url(../images/btn_03.png) no-repeat; background-size:100% 100%;}
.btns3{margin: 0.8rem auto;}
.btns3 a{display:block; width:2.5rem; height:0.59rem; border:0.02rem solid #491cb1; border-radius: 10px; text-indent: -9999px;}
.btns3 a.fl{background:url(../images/txt_01.png) no-repeat center center; background-size:1.02rem 0.38rem}
.btns3 a.fr{background:url(../images/txt_02.png) no-repeat center center; background-size:1.32rem 0.38rem}
.rule_wrap{padding-top:1.18rem; font-size: 0.26rem; line-height: 0.36rem;}
.rule_wrap img{width:1.31rem}
.rule li{position: relative; margin-top:0.2rem}
.rule li span{position: absolute; left: 0; width:0.4rem}
.rule li p{padding-left:0.38rem}
.return_btn{margin:1.3rem auto 0; background:url(../images/txt_04.png) no-repeat center center; background-size:0.7rem 0.37rem}
.tips img{margin: 0 auto;}
.update_wrap,.prize_wrap,.end_wrap{padding-top:1.18rem}
.update_wrap .tips img{width:4.15rem;}
.update_wrap .btns{background:url(../images/btn_04.png) no-repeat; background-size:100% 100%}
.btns3 a.rule_btn{margin:1rem auto 0; border:none; background:url(../images/txt_02.png) no-repeat center center;background-size:1.32rem 0.38rem}
.prize_wrap img{width:4.23rem}
.update_wrap .btns3{margin-bottom: 0.3rem;}
.end_wrap .tips img{width:2.36rem;}
.end_wrap .btns{margin:0.3rem auto 1.3rem; background:url(../images/btn_05.png) no-repeat; background-size:100% 100%}
.footer img{width:3.5rem; margin: 0 auto 0.6rem;}
/*礼券流量*/
.tab_title{width:5.54rem; height:0.77rem; margin: 0 auto; padding:0.12rem 0.16rem 0; background:url(../images/tab_bg1.png) no-repeat; background-size:100% 100%;}
.tab_title li{float:left; width:2.7rem; height:0.6rem; background:rgba(47,1,145,.2); border-radius: 0.1rem; text-align: center; line-height: 0.6rem; font-size: 0.28rem; cursor: pointer;}
.tab_title li:nth-of-type(1){margin-right:0.1rem;}
.tab_title li span{display: block; width:1.01rem; height:0.35rem; margin: 0.13rem auto 0; text-indent: -9999px;}
.tab_title li:nth-of-type(1) span{background:url(../images/tab_txt_04.png) no-repeat center center; background-size:0.68rem 0.35rem;}
.tab_title li:nth-of-type(2) span{background:url(../images/tab_txt_01.png) no-repeat center center; background-size:0.68rem 0.35rem;}
.tab_title li.current{background:url(../images/tab_bg2.png) no-repeat; background-size:100% 100%; box-shadow: 0 0 8px #b02a9d;}
.tab_title li:nth-of-type(1).current span{background:url(../images/tab_txt_03.png) no-repeat center center; background-size:0.68rem 0.35rem;}
.tab_title li:nth-of-type(2).current span{background:url(../images/tab_txt_02.png) no-repeat center center; background-size:0.68rem 0.35rem;}
.tab_title2 li:nth-of-type(1) span{background:url(../images/tab_txt_07.png) no-repeat center center; background-size:1.01rem 0.35rem;}
.tab_title2 li:nth-of-type(2) span{background:url(../images/tab_txt_05.png) no-repeat center center; background-size:1.01rem 0.35rem;}
.tab_title2 li:nth-of-type(1).current span{background:url(../images/tab_txt_06.png) no-repeat center center; background-size:1.01rem 0.35rem;}
.tab_title2 li:nth-of-type(2).current span{background:url(../images/tab_txt_08.png) no-repeat center center; background-size:1.01rem 0.35rem;}
.tab_cnt{padding-bottom:0.1rem; display: none;}
.wrap{margin-top:0.35rem; padding:0.05rem; background: -webkit-linear-gradient(left,rgba(217,88,204,1),rgba(97,58,221,1)); background: -o-linear-gradient(left,rgba(217,88,204,1),rgba(97,58,221,1)); background: -moz-linear-gradient(left,rgba(217,88,204,1),rgba(97,58,221,1)); background:linear-gradient(left, rgba(217,88,204,1), rgba(97,58,221,1)); border-radius: 0.1rem;}
.wrap_inner{background:#300354; padding:0.02rem;  border-radius: 0.1rem;}
.wrap_inner2{margin:1px; background:#46268a; border-radius: 0.09rem; }
.wrap_inner3{padding:0.01rem; background: -webkit-linear-gradient(left,rgba(217,88,204,1),rgba(97,58,221,1)); background: -o-linear-gradient(left,rgba(217,88,204,1),rgba(97,58,221,1)); background: -moz-linear-gradient(left,rgba(217,88,204,1),rgba(97,58,221,1)); background:linear-gradient(left, rgba(217,88,204,1), rgba(97,58,221,1)); border-radius: 0.09rem;}
.wrap_inner4{background:#48239b; border-radius: 0.14rem 0.14rem 0.09rem 0.09rem; overflow: hidden;}
.tab_date{width:100%; height:0.48rem; background:url(../images/tab_bg3.png) no-repeat; background-size:100% 100%; font-size: 0.25rem; line-height: 0.48rem; text-indent: 0.1rem; text-shadow:#6429c9 1px 0 0,#6429c9 0 1px 0,#6429c9 -1px 0 0,#6429c9 0 -1px 0;}
.tab_table{width:94%; margin: 0.1rem auto; font-size: 0.24rem;}
.tab_table td{padding:0.1rem; text-align: center;}
.tab_table td.left{text-align: left;}
.tab_table a{display: block; width:1.51rem; height:0.65rem; margin-top:0.1rem; background:url(../images/btn_06.png) no-repeat; background-size: 100% 100%; text-indent: -9999px;}
.tab_table2 a{background:url(../images/btn_07.png) no-repeat; background-size:100% 100%; text-indent: 0; line-height: 0.55rem; font-size: 0.24rem; text-shadow:#000 1px 0 0,#000 0 1px 0,#000 -1px 0 0,#000 0 -1px 0;}
.soft_list{width: 96%;margin: 0 auto;}
.soft_list li{float: left;display:inline; width: 27.3%; margin: 0.3rem 3% 0;}
.soft_list li p{text-align: center;}
.soft_list li .soft-icon img{width:76%; margin: 0 auto; border-radius: 18%;}
.soft_list li .soft-name{width:100%; padding: 0.15rem 0 0.13rem; line-height:0.24rem; font-size:0.24rem; text-overflow:ellipsis; white-space:nowrap; overflow: hidden}
.soft_list li a.downlode{display:block; width:1.1rem; height:0.65rem; background:url(../images/btn_09.png) no-repeat; background-size:100% 100%; margin:0 auto; text-align:center; line-height:0.58rem; font-size: 0.24rem; text-shadow:#000 1px 0 0,#000 0 1px 0,#000 -1px 0 0,#000 0 -1px 0;}
.change_btn{display: block; width:1.6rem; height:0.42rem; margin: 0.25rem auto 0.35rem; background:url(../images/btn_08.png) no-repeat; background-size:100% 100%; text-indent: -9999px;}
.relive2 img{width:100%}
.relive2 p{height:0.6rem; padding-left:0.15rem; line-height:0.72rem; font-size: 0.24rem;}
.process_wrap{height:0.55rem;  position: relative; overflow: hidden;}
.process{width:3.43rem; height: 0.4rem; margin-top:0.07rem; background:url(../images/bg2.png) no-repeat; background-size:100% 100%; position:relative}
.process span{display: block; width:1.21rem; height:0.22rem; margin: 0.08rem 0.09rem 0; background:url(../images/bg3.png) no-repeat; background-size:100% 100%; border:1px solid #fff188; border-radius: 15px; position: absolute; left:0; top:0}
.process em{display: block; width:0.8rem; height:0.27rem; position: absolute; left:3.6rem; top:0rem; font-size: 0.24rem; line-height: 0.34rem;}
.process_wrap a{display: block; width:1.51rem; height:0.65rem; background:url(../images/btn_07.png) no-repeat; background-size:100% 100%; text-align: center; line-height: 0.55rem; font-size: 0.3rem; color:#fff; position: absolute; right:0; top:0; text-shadow:#000 1px 0 0,#000 0 1px 0,#000 -1px 0 0,#000 0 -1px 0;}
.process_wrap a.get_unabled{background:url(../images/btn_18.png) no-repeat; background-size:100% 100%;}
.answer_wrap{width:5.3rem; margin: 0 auto; padding:0.7rem 0 0.1rem; position: relative;}
.question{font-size: 0.3rem; text-align: justify; color:#d1bbff; text-shadow:#330155 1px 0 0,#330155 0 1px 0,#330155 -1px 0 0,#330155 0 -1px 0; line-height: 0.42rem;}
.answer ul{padding-top:0.28rem;}
.answer ul li{width:5.3rem; height:0.7rem; margin-bottom: 0.32rem; background:#fff; border-radius: 0.3rem; color:#323d47; line-height: 0.72rem; font-size: 0.28rem; overflow: hidden; cursor: pointer; position: relative;}
.answer ul li span{position: absolute; left:0; top:0; right:0; bottom:0; padding:0 0.3rem;  z-index: 2;}
.answer ul li.current span{background: -webkit-linear-gradient(top,rgba(166,133,240,1),rgba(235,34,112,1)); background: -o-linear-gradient(top,rgba(166,133,240,1),rgba(235,34,112,1)); background: -moz-linear-gradient(top,rgba(166,133,240,1),rgba(235,34,112,1)); background:linear-gradient(top, rgba(166,133,240,1), rgba(235,34,112,1)); color:#fff}
.answer ul li em{position:absolute; top:0; left:0; width:0%; height: 100%; background:#dbcdf9; z-index: 1;}
.answer ul li.current2 span{color:#323d47;}
.answer ul li.current2 em{background: -webkit-linear-gradient(top,rgba(166,133,240,1),rgba(235,34,112,1)); background: -o-linear-gradient(top,rgba(166,133,240,1),rgba(235,34,112,1)); background: -moz-linear-gradient(top,rgba(166,133,240,1),rgba(235,34,112,1)); background:linear-gradient(top, rgba(166,133,240,1), rgba(235,34,112,1));}
.answer ul li i{position: absolute; right:0.3rem; top:0; font-style: normal; z-index: 3;}
.answer_static{width:1rem; height:1rem; background: -webkit-linear-gradient(left,rgba(217,88,204,1),rgba(97,58,221,1)); background: -o-linear-gradient(left,rgba(217,88,204,1),rgba(97,58,221,1)); background: -moz-linear-gradient(left,rgba(217,88,204,1),rgba(97,58,221,1)); background:linear-gradient(left, rgba(217,88,204,1), rgba(97,58,221,1)); border-radius: 50%; position: absolute; left:50%; margin-left:-0.5rem; top:-0.55rem}
.answer_static_inner{width:0.9rem; height:0.9rem; background:#fff; margin: 0.05rem auto; border-radius: 50%; overflow: hidden;}
.user_info{padding:0.23rem 0 0.38rem 0.25rem}
.user_info .relive{float: left;}
.user_num{float:right; width:1.5rem; font-size: 0.28rem; color:#c5a8ff; line-height: 0.9rem;}
.answer_static_inner span{display:block; width:0.8rem; margin: 0 auto; line-height: 0.85rem; font-size: 0.24rem; color:#80878d; text-align: center;}
.answer_static_inner em,.answer_static_inner i{display: block; width:0.46rem; height:0.46rem; margin: 0.22rem auto;}
.answer_static_inner em{background:url(../images/static_01.png) no-repeat center center; background-size: 100% 100%;}
.answer_static_inner i{background:url(../images/static_02.png) no-repeat center center; background-size: 100% 100%;}
.flow_p{padding-top:0.25rem; color:#ff50f4; font-size: 0.24rem;}

/*提示框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{position:absolute; width:90%; height:auto; left:50%; top:50%; margin-left:-45%; z-index: 99999;}
.tip_hd h4{height:0.76rem; background: -webkit-linear-gradient(left,rgba(217,88,204,1),rgba(97,58,221,1)); background: -o-linear-gradient(left,rgba(217,88,204,1),rgba(97,58,221,1)); background: -moz-linear-gradient(left,rgba(217,88,204,1),rgba(97,58,221,1)); background:linear-gradient(left, rgba(217,88,204,1), rgba(97,58,221,1)); line-height:0.78rem; font-size:0.3rem; text-align: center; color:#ffffff}
.tip-mid{padding:0.15rem 0}
.tip-con{width:90%;  margin:0px auto;}
.tip-con p{padding:0.45rem 0 0.8rem; font-size: 0.26rem; line-height: 0.46rem; color:#e93500; text-align: center;}
.tip-con p span{color:#ff3131}
.tip_icon1{width:2.17rem; margin: 0.2rem auto 0.15rem;}
.tip_txt1{width:3.16rem; margin: 0 auto;}
.tip-box3 .tip_icon1{width:2.33rem}
.tip-box2 .tip_txt1{width:2.95rem}
.tip-box3 .tip_txt1{width:2.33rem}
.tip-box4 .tip_txt1{width:1.99rem; margin-bottom: 0.4rem;}
.clo{ position: absolute; right: 0.12rem; top:0.12rem;}
.clo a{ display: block; width:0.36rem; height:0.36rem;background:url("../images/tip_clo.png") 0 0 no-repeat;background-size:100% auto;text-indent: -9999px;}
.tip-box a{margin-top:0}
.btns4{margin: 0.25rem 0.2rem 0;}
.btns4 a{width:2.4rem; height:0.8rem; text-indent: -9999px;}
.btns4 a.fl{background:url(../images/btn_10.png) no-repeat; background-size: 100% 100%;}
.btns4 a.fr{background:url(../images/btn_11.png) no-repeat; background-size: 100% 100%;}
.tip-box2 .btns{margin-top:0.28rem; background:url(../images/btn_12.png) no-repeat; background-size:100% 100%}
.tip-box3 .btns{margin-top:0.28rem; background:url(../images/btn_16.png) no-repeat; background-size:100% 100%}
.tip-box6 .btns{background:url(../images/btn_17.png) no-repeat; background-size:100% 100%}
.tip-box7 .btns{background:url(../images/btn_15.png) no-repeat; background-size:100% 100%}
.tip-box5 .btns4 a.fl{background:url(../images/btn_13.png) no-repeat; background-size: 100% 100%;}
.tip-box5 .btns4 a.fr{background:url(../images/btn_14.png) no-repeat; background-size: 100% 100%;}
.tip-box5 img{width:3.41rem; margin: 0 auto;}
.tip-box5 .tip-con{position:relative}
.tip-box5 .tip-con p{width:70%; padding:0.45rem 0 0 15%; position: absolute; bottom:0; line-height: 0.32rem; font-size: 0.24rem; color:#c5a8ff; text-shadow:#000 1px 0 0,#000 0 1px 0,#000 -1px 0 0,#000 0 -1px 0;}

.form-items{padding-top:0.3rem;}
.form-items li{position: relative; margin-bottom:0.33rem}
.form-items label{position: absolute; left:0; top:0; width:1rem; height:0.66rem; font-size: 0.24rem; color:#c5a8ff; line-height: 0.66rem; text-shadow:#000 1px 0 0,#000 0 1px 0,#000 -1px 0 0,#000 0 -1px 0; font-weight: bold;}
.form-items li span{display: block; margin: 0 0 0 1rem; padding:0.04rem; background: -webkit-linear-gradient(left,rgba(217,88,204,1),rgba(97,58,221,1)); background: -o-linear-gradient(left,rgba(217,88,204,1),rgba(97,58,221,1)); background: -moz-linear-gradient(left,rgba(217,88,204,1),rgba(97,58,221,1)); background:linear-gradient(left, rgba(217,88,204,1), rgba(97,58,221,1)); border-radius: 0.1rem;}
.form-items input{display: inline-block; width:100%; height:0.62rem; background:#fff; border:none; text-indent: 0.1rem; background:#fffef5; border-radius: 0.09rem; font-size: 0.24rem; color:#6f2f00; line-height: 0.57rem;}
.form-items span.input_short{float:left; width:40%}
.form-items li i{float: right; width:35%; height:0.68rem; background: -webkit-linear-gradient(left,rgba(217,88,204,1),rgba(97,58,221,1)); background: -o-linear-gradient(left,rgba(217,88,204,1),rgba(97,58,221,1)); background: -moz-linear-gradient(left,rgba(217,88,204,1),rgba(97,58,221,1)); background:linear-gradient(left, rgba(217,88,204,1), rgba(97,58,221,1)); border-radius: 0.15rem; text-align: center; line-height: 0.38rem; font-size: 0.18rem; cursor: pointer; font-style: normal;}
.form-items li i em{display: block; width:1.43rem; height:0.32rem; margin: 0.18rem auto; background:url(../images/txt_06.png) no-repeat center; background-size:100% 100%;}
.close_btn{display: block; width:0.84rem; height:0.84rem; background:url(../images/close.png) no-repeat; background-size:100% 100%; position: absolute; right:0.15rem; top:0.5rem; cursor: pointer;}
