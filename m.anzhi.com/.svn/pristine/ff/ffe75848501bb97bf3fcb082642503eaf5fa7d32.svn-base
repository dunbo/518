.num_01 em{display:block; width:40px; height:40px;  background:url(../images/sign_bg1.png) no-repeat; background-size:100% 100%; position:absolute; left:50%; top:50%; margin-left:-20px; margin-top:-20px}
.num_01 em.sign_no{background:url(../images/sign_bg2.png) no-repeat; background-size:100% 100%;}

.detail{ padding:20px 10px; color:#c4c4c4; font-size:16px; line-height:26px}
.detail li h6{font-size:16px; color:#ffec81}
.detail li img{display:block; max-width:100%; margin:10px 0;}


/*提示框*/
#body-bg{position:absolute; top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-webkit-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{ position:absolute;width: 90%; height:240px; background:#303030; left:50%;top:50%; margin-left:-45%; z-index: 99999; font-size:15px; color:#ffeda4}
.tip-title{height:50px; background:#1f1f1f; text-align:center; line-height:50px}
.tip-title h3{color:#797979}
.tip-con{ margin: 15px;padding:20px 6%;}
.tip-con p{font-size:20px; line-height: 28px; text-align:center;word-wrap:break-word;word-break:break-all;}
.tip-con img{width:152px; margin:0 auto;}
.btns_false{display:block; width:150px; height:46px; margin:30px auto; border-radius:4px; background-image: -webkit-linear-gradient(top, rgba(255, 93, 56, 1) 0%, rgba(255, 0, 0, 1) 100%); background-image: -moz-linear-gradient(top, rgba(255, 93, 56, 1) 0%, rgba(255, 0, 0, 1) 100%); background-image: -o-linear-gradient(top, rgba(255, 93, 56, 1) 0%, rgba(255, 0, 0, 1) 100%); background-image: -linear-gradient(top, rgba(255, 93, 56, 1) 0%, rgba(255, 0, 0, 1) 100%); text-align:center; line-height:46px; color:#fff; font-size:24px; font-weight:bold}
@keyframes fadeIn{
	0%{opacity:0}
	100%{opacity:1}
}
@-webkit-keyframes fadeIn{
	0%{opacity:0}
	100%{opacity:1}
}