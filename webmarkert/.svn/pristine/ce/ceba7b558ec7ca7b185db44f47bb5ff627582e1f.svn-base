function bad(){
	var feedback_content = $("textarea[name=feedback_content]").val();
	$.ajax({
		url: '/feedback.php',
		data: 'softid=1111&version_code=93&feedbacktype=5&feedback_content='+feedback_content,
		dataType:'json',
		type: 'POST',
		success: function(msg){
			if(feedback_content == ''){
				alert('请输入具体原因');
				return false;	
			}
			alert(msg['msg']);
			window.location=msg['url'];
		}
	});
}
</script>

