/* CSS Document */
/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{max-width:640px;min-width:320px; margin:0 auto;font-size:16px; font-family:Microsoft Yahei,"微软雅黑"; background: url(../images/body_bg.png) repeat; color: #fff;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#fff;}
.a,input,button,div{-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{ float: left;}
.fr{ float: right}
.cnt{text-align: center}
.underline{text-decoration:underline;}
.mtop{ margin-top: 35px;}
.mtop1{ margin-top: -20px;}
.mtop2{margin-top:-23px;}
.mbot{margin-bottom: 8px;}
.ptop{ padding-top: 10px;}
.ptop1{padding-top: 45px;}
.pbot{padding-bottom: 25px;}


.banner{position: relative; width: 100%;min-width: 320px; margin:0 auto; text-align: center;}
.banner img{ width: 100%;vertical-align: bottom;}

.btns{display:block; width:120px; height:40px; margin:20px auto 10px; background:#ffd800; border-radius:12px; color:#066508; font-size:16px; letter-spacing:2px; text-align:center; line-height:40px; font-weight:bold; color:#ea5e00}
.btns_01{width:108px; background:url(../images/btn_01.png) no-repeat; background-size:108px 40px; color:#fff; box-shadow:2px 2px 3px rgba(0,0,0,.3);}
.noclick_02{background:url(../images/noclick_02.png) no-repeat; background-size:108px 40px;}
.footer_link{padding:10px 0; font-size:18px; letter-spacing:3px; text-align:center; color:#460904;}
.footer_link a{color:#460904;}

/*公告*/
.notice{width: 100%;height: 60px; margin:8px 0 0; line-height: 53px;background:url(../images/notice_bg.png) no-repeat; background-size:100% 100%; color: #fff; overflow: hidden; }
.notice-list{width:60%; margin:0 auto}
.notice-list span{color: #fff;padding-right: 20px;}
.notice-list span em{ padding-left: 10px;}
.introduce{margin:40px 15px 0; background:#f59fbb; padding:10px 0; border:10px solid #fdc8da; border-radius:10px; font-size:16px; line-height:26px; text-align:center; position:relative;}
.introduce span{position:absolute; left:-30px; top:-35px; width:63px; height:76px; background:url(../images/bg_01.png) no-repeat; background-size:63px 76px}



.tips_p{padding-left:20px; padding-right:20px; text-align:center;}
.wrap_tips{width:75%; margin:10px auto 0; background:#ffb5b9; border:2px solid #fff;  border-radius:8px;}
.wrap_tips img{ margin:10px auto 5px;}
.wrap_tips2 img{width:90%; margin:10px auto 5px;}
.prompt a {padding:5px 15px; background:#fd3558; border-radius:10px}

.person2 img{display:block; margin:0 auto;}
.introduce2{margin-top:15px; background:#e83133; border-color:#ff7779;}
.wth{width: 94%; margin:0 auto;}
.white-wrap{width: 94%; margin:10px auto 30px auto; background-color: #fff;}

.main{margin:0 10px 10px;}
.wrap{ background:#fd3558; border-radius:8px; margin:20px 15px 0; box-shadow:inset 2px 2px 20px rgba(255,148,167,.75); position:relative;}
.tips_sim{padding:5px 10px; background:none}
.mod_hd{position:relative}
.mod_hd img{width:100%;}
.mod_hd span{display:block; width:134px; height:68px; background:url(../images/mod_hd6.png) no-repeat; background-size:100% 100%; position:absolute; left:10px; top:10px}
.mod_hd2 img{width:268px; margin:0 auto}
.mod_hd3 img,.mod_hd5 img{width:90%; margin:0 auto}
.mod_hd5 img{margin-top:-10px}
.mod_bd{min-height:180px; background:#ff4b6c; color:#fff}
.mod_hd4 img{margin:10px auto}

/*软件*/
.soft-p{padding:8px 0; font-size:16px; text-shadow: 0 0 2px #87134d;} 
.soft-list{background:#fd3558; margin: 0 auto; padding:6px 5px 10px;}
.soft-list .wrap{margin-top:15px}
.soft-list li{ float: left; display: inline; width: 50%;margin:8px 0px; position:relative;}
.soft-list li .soft-icon{position:absolute; left:10px; top:8px; width:60px; height:60px;}
.soft-list li .soft-icon img{ width: 60px;height: 60px;}
.soft-txt{margin:0 0 0 70px;}
.soft-list li .soft-name{ margin:11px 8px 8px; height: 20px; line-height: 20px; color:#fff; overflow: hidden;}
.soft-list li .soft-name a{ color: #fff;}
.soft-list li a.downlode{float:left; width:56px;height: 25px; line-height: 26px; margin: 0 8px; background: #ffd800; border-radius: 3px; color: #ea5e00;cursor: pointer; text-align:center; font-size:14px}
.soft-list .btns{width:100px; height:35px; background:#812cd8; margin-right:15px; color:#fff; line-height:35px}


.prize-title,.end-title{padding:38px 0 0 0; font-size:18px;}
.prize-title span,.end-title span{padding:0 10px; color:#fff}
.prize-text{padding:10px 10px 0; font-size:18px; line-height:28px}
.prize_btns{display:block; padding:4px 10px; background:#ffd800; border-radius:4px; position:absolute; right:15px; top:10px; color:#ea5e00;}
.prize_no{ margin:0; padding:80px 10px; text-align:center}


.detail_list,.prize_list{width:92%; margin:30px auto; padding:10px; background:#fd3558; border-radius: 8px;}
.detail_list h4{font-size: 18px; font-weight: bold; padding-bottom: 6px; color:#fff;}
.detail_list  p{font-size: 16px; line-height: 25px}
.active_ctn li{position:relative; color:#fff}
.active_ctn span{width:25px; position:absolute; top:3px}
.active_ctn p{margin-left:25px;}

.end{padding:10px 10px 8px; font-size:18px; line-height:28px}
.end a{ display:block; padding:30px 0 10px 30px; font-size:18px; text-decoration:underline;}
.end em{display:block; padding:30px 0 0; font-size:16px; color:#fff;}

/*邮寄地址*/
.form1{width:90%; margin:0 auto;}
.form_title{margin:15px auto 0;}
.form-item{ margin:8px 0 0;  font-size:16px;}
.form-item label{ float: left;width:70px; padding:12px 0;}
.form-item .textbox input{float:left; width:63%; padding:12px 15px; background:#fff; border-radius:4px; border:none; font-family: "Microsoft Yahei"; color:#898787;}
.prompt2{padding:5px 0 0;}
.footer img{width:100%}

/*提示框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{position:absolute;width: 90%;left:50%;top:50%;margin-left:-45%; background:url(../images/dialog_bg.png) no-repeat; background-size:100% 100%; z-index: 99999;}
.tip-box2{background:#fd3558; border-radius:10px}
.tip_img{width:90%; padding:52px 0 10px; background:url(../images/mod_hd3.png) no-repeat; background-size:100% 100%; position:absolute; left:50%; margin-left:-45%; top:-90px; color:#ff0000; text-align:center; font-size:20px}
.tip_hd{width: 90px; height:58px; margin:0 auto 10px; }
.tip-mid{padding-bottom:4px;border-radius: 0px 0px 15px 15px;}
.tip-con{padding:25px 75px 35px 45px;margin:0px 4px}
.tip-con p{line-height: 25px; text-align: center;font-size: 18px;}
.clo{ position: absolute;right: -7px;top: 10px;}
.clo a{ display: block; width:38px; height:38px;background:url("../images/tip_clo.png") 0 0 no-repeat;background-size:100% auto;text-indent: -9999px;}
.person_02{width:96px; height:177px; background:url(../images/person_02.png) no-repeat; background-size:100% 100%; position:absolute; right:-5px; bottom:15px;}
.person_03{background:url(../images/person_03.png) no-repeat; background-size:100% 100%;}
.prize_info{padding:20px; position:relative; word-wrap:break-word; word-break:break-all}
.prize_info_l{ width:80px; height:80px; padding:10px; background:#ac2940; border-radius:10px; position:absolute; left:20px; top:20px; box-shadow:inset 0 0 12px #fff;}
.prize_info_l img{display:block; width:80px; height:80px;}
.prize_info_r{margin-left:100px; padding:20px 0 0;  height:80px; text-align:center; font-size:18px}
.prize_info_r span{font-size:24px;}
.prize_des{padding:15px 0 0;}
.prize_info_btn{width:300px; margin:0 auto 20px;}
.prize_info_btn a{float:left; margin:0 5px;}
.prize_info_btn a.btns_down{width:160px;}
.btns_two{width:260px; margin:0 auto;}
.btns_two a{float:left; margin-left:10px; display:inline;}


/*九宫格抽奖*/
.lottery{ width:320px; background:#fd3558; margin:30px auto 15px; padding:10px 0; border-radius:8px; position:relative;}
.person{width:78px; height:128px; background:url(../images/person_01.png) no-repeat; background-size:78px 128px; position:absolute; left:-5px; top:-20px; z-index:99999}
#gameContent{ position:relative;width:280px; height:330px;padding: 5px; margin:0 auto; border-radius:8px; font-size: 16px;color:#fff;background-color: #901932}
#gameContent table{width:280px;height: 330px;}
#gameContent table td{width:31%;height:33%; padding:0 1%; text-align:center;vertical-align:middle;}
#gameContent table td .td_w{position: relative;}
#gameContent table td img{width:99%; margin:0 auto}
#gameContent table td .active{display:none;position:absolute;left:-5px;top:-4px;width:98px;height:98px;background:url("../images/select.png") 0 0 no-repeat;color: #ffe66f;background-size: contain;z-index: 4}
#gameContent table td.current div{display:block;}
#gameContent table td#my_click{font-size:32px;color:#ffe1df;}
#my_button {position:absolute;left: 103px;top:121px;z-index:999;}
#my_button a{display:block;width:83px;height:99px;text-align: center;text-indent: -9999px; background:url(../images/lottery_btn.png) no-repeat; background-size:100% 100%;}
#my_button a.gray_btn{background:url(../images/lottery_disabled.png) no-repeat; background-size:100% 100%;s}

.plane_txt{width:90%; margin:0 auto}
.plane_txt img{width:100%;}
.plane{width:160px; height:210px; background:url(../images/plane.png) no-repeat; background-size:100% 100%; position:absolute; left:50%; margin-left:-80px; bottom:60px; }
.plane_btn{position:absolute; bottom:20px; left:50%; margin-left:-32px}
.plane_btn .btns{width:64px; height:32px; border-radius:0; background:url(../images/btn_02.png) no-repeat; background-size:100% 100%; line-height:32px; color:#572502;}
.index_rule{width:120px; position:absolute; bottom:3px; left:50%; margin-left:-60px; text-align:center; }
.fly_away_first {-webkit-transform:translateY(10px); transform:translateY(10px); -webkit-transition: -webkit-transform 0.4s ease-out; transition: transform 0.4s ease-out;}
.fly_away {animation:plane_ac 1.5s ease-in; -webkit-animation:plane_ac 1.5s ease-in;}
@keyframes plane_ac{
	0%{opacity:1; transform:translate(0, 10px) scale(1); }
	60%{opacity:1; transform:translate(0, -220px) scale(1); }
	100%{opacity:0; transform:translate(0,-420px) scale(0.7);}
}
@-webkit-keyframes plane_ac{
	0%{opacity:1; -webkit-transform:translate(0, 10px) scale(1); }
	60%{opacity:1; -webkit-transform:translate(0, -220px) scale(1); }
	100%{opacity:0; -webkit-transform:translate(0,-420px) scale(0.7);}
}

