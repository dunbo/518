<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>提示信息</title>
<link type="text/css" href="http://dev.anzhi.com/css/common.css" rel="stylesheet" />
<link type="text/css" href="http://dev.anzhi.com/css/page4.css" rel="stylesheet" />
<script type="text/javascript" src="/js/jquery-1.4.2.min.js"></script>
<script type="text/javascript" src="/js/anzhi.js?Ao9Ye30"></script>
</head>
<body>
<script type="text/javascript">
	function dev_alert(msg,sec,url){//操作信息提示框
	$('#alert_div').remove();
	var str='';
	str+= '<div id="alert_div" class="newbox" style="display:block;top:76.5px;left:434.5px;min-height:60px;_height:60px"><div class="newbox_tips"><p>'+msg+'</p></div>';
	if(sec==0){
		str += '<div class="open_close_btn" style="text-align:center;">';
		str += '<a id="gosub" onclick="closeBtn(\'alert_div\');" href="javascript:;">关闭</a>';
		str += '</div>';
	}
	$('body').append(str);
    showOpenNew('alert_div');
    if(!!url){
    	if(url!='-1'){
    		setTimeout(function(){
    			top.location.href = ''+url+'';
    		},2000);
         
    	}else{
    		setTimeout(function(){
    			window.history.go(-1);
    		},2000);
        }
     }
     if(!!sec){
     	setTimeout(function(){
     		closeBtn('alert_div');
     		//window.location.reload();
		},(sec+1)*1000);
     }else{
        setTimeout(function(){
        	closeBtn('alert_div');
        	//window.location.reload();
		},2000);
	 }
}
function closeBtn(id){
	var oMark = document.getElementById('mark');
			var oDiv = document.getElementById(id);
			oDiv.style.display = 'none';	
			document.body.removeChild(oMark);
	}

</script>

<script type="text/javascript">
dev_alert('<!--{$msg}-->','2','<!--{$url}-->');
</script>
</body>
</html>
