<script type="text/javascript" src='/Public/js/jquery-1.4.2.min.js'></script>
<script type="text/javascript" src="/Public/js/My97DatePicker/WdatePicker.js"></script>


<div>
	<h3>添加CPD月消耗</h3>
	<form action="__URL__/add_month_expend" method="post" onsubmit="return check_value()" enctype="multipart/form-data" >
	<table>
	    <tr>
			<td style="text-align:right; width:80px;"><font style="color:red;">*</font>公司：</td>
			<td align="left">
				<input type="text" name="company" id="company" style="margin:10px; width:300px; height:30px;" />
			</td>
		</tr>
		<tr>
			<td style="text-align:right; width:80px;"><font style="color:red;">*</font>月份：</td>
			<td align="left">
				<input style="margin:10px; width:300px; height:30px;" class="Wdate" type="text" onClick="WdatePicker({dateFmt:'yyyyMM',startDate:'%y%M'})" value="" name="month" id="month">
			</td>
		</tr>
		<tr>
			<td style="text-align:right; width:80px;"><font style="color:red;">*</font>产品：</td>
			<td align="left">
				<input type="text" name="product" id="product" style="margin:10px; width:300px; height:30px;" />
			</td>
		</tr>
		<tr>
			<td style="text-align:right; width:80px;"><font style="color:red;">*</font>包名：</td>
			<td align="left">
				<input type="text" name="package" id="package" style="margin:10px; width:300px; height:30px;" />
			</td>
		</tr>
		<tr>
			<td style="text-align:right; width:80px;"><font style="color:red;">*</font>计费下载量：</td>
			<td align="left">
				<input type="text" name="download" id="download" style="margin:10px; width:300px; height:30px;" />
			</td>
		</tr>
		<tr>
			<td style="text-align:right; width:80px;"><font style="color:red;">*</font>单价：</td>
			<td align="left">
				<input type="text" name="price" id="price" style="margin:10px; width:300px; height:30px;" />
			</td>
		</tr>
		<tr>
			<td style="text-align:right; width:80px;"><font style="color:red;">*</font>消耗金额：</td>
			<td align="left">
				<input type="text" name="cost" id="cost" style="margin:10px; width:300px; height:30px;" />
			</td>
		</tr>
	</table>
    <div>
		<input style="display:inline; margin:0 50px 10px 75px;" type="submit" class="search_btn" value="确定" />
		<input style="display:inline;"type="button" class="search_btn" value="取消" onclick="tb_remove();" />
		</div>	
	</form>
</div>
<script>
function check_value()
{
	return true;
