/* CSS Document */
/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{max-width:640px;min-width:320px; margin:0 auto;font-size:16px; font-family:Microsoft Yahei,"微软雅黑"; background: #008dff; color: #ab7e55;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#ab7e55; font-size: .14rem;}
.a,input,button,div{-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{ float: left;}
.fr{ float: right}
.banner{width:100%; position: relative;}
.banner img{ width: 100%; display:block;}
.notice{width:100%; height:0.98rem; background:url(../images/notice_bg.png) no-repeat; background-size: 100% 100%; line-height: 0.6rem;}
.notice_list{margin: 0 1.42rem; color:#fff; font-size: 0.26rem;}
.wrap{margin:0 0.3rem 0.35rem; padding:0.3rem 0; background:#fefaf2; border-radius: 8px; position: relative;}
.wrap_bg_l,.wrap_bg_r{display: block; width:0.17rem; height:0.76rem; background:url(../images/line_01.png) no-repeat; background-size: 100% 100%; position: absolute; bottom:-0.56rem; z-index: 2;}
.wrap_bg_l{left:0.2rem}
.wrap_bg_r{right:0.2rem}
.gift_wrap{margin-top:-0.2rem}
.gift_box{width:}
.gift_box li{float:left; width:1.6rem; margin: 0 0.16rem; display: inline;}
.gift{width:1.26rem; height:1.15rem; margin:0 auto; position:relative;}
.gift_box li .gift1{display: block; width:1.26rem; height:1.15rem; position: absolute; left:0; top:0;z-index: 2;}
.gift_box li:nth-of-type(2) .gift1{animation: light 0.7s ease infinite; -webkit-animation: light 0.7s ease infinite;}
.light{display: block; position: absolute; width:2.08rem; height:2.08rem; background:url(../images/light.png) no-repeat; background-size: 100% 100%; left:50%; margin-left:-1.04rem; top:50%; margin-top:-1.04rem; z-index: 1; animation: light 0.7s ease infinite;  -webkit-animation: light 0.7s ease infinite;}
.gift_open1{display: block; width:1.53rem; height:1.51rem; background:url(../images/gift_open1.png) no-repeat; background-size:100% 100%; position: absolute; left:50%; margin-left:-0.785rem; top:30%; margin-top:-0.755rem; display: none; z-index: 3;}
.gift_open2{display: block; width:1.71rem; height:1.58rem; background:url(../images/gift_open2.png) no-repeat; background-size:100% 100%; position: absolute; left:50%; margin-left:-0.855rem; top:30%; margin-top:-0.754rem; display: none; z-index: 3;}
.gift_open3{display: block; width:2.7rem; height:1.84rem; background:url(../images/gift_open3.png) no-repeat; background-size:100% 100%; position: absolute; left:50%; margin-left:-1.35rem; top:30%; margin-top:-0.92rem; display: none; z-index: 3;}
.gift_03 .gift_open1{background:url(../images/gift_open7.png) no-repeat; background-size:100% 100%;}
.gift_03 .gift_open2{background:url(../images/gift_open8.png) no-repeat; background-size:100% 100%;}
.gift_03 .gift_open3{background:url(../images/gift_open9.png) no-repeat; background-size:100% 100%;}
.gift_04 .gift_open1{background:url(../images/gift_open4.png) no-repeat; background-size:100% 100%;}
.gift_04 .gift_open2{background:url(../images/gift_open5.png) no-repeat; background-size:100% 100%;}
.gift_04 .gift_open3{background:url(../images/gift_open6.png) no-repeat; background-size:100% 100%;}
.gift_box li p{padding:0.07rem 0 0.11rem; font-size: 0.26rem; line-height: 0.36rem; text-align: center;}
.gift_box li p span{color:#0391ff}
.gift_btn,.user_info a{display: block; width:1.6rem; height:0.51rem; background:url(../images/btns_01.png) no-repeat; background-size: 100% 100%; text-align: center; line-height: 0.53rem; font-size: 0.24rem; color:#fff}
.gift_btn_disabled{background:url(../images/btns_04.png) no-repeat; background-size: 100% 100%;}
.user_info{position: relative;}
.user_info ul{margin: 0 1.9rem 0 0.2rem; line-height: 0.42rem;}
.user_info ul li:nth-of-type(1){font-size: 0.3rem;}
.user_info ul li:nth-of-type(2){padding-top:0.05rem; font-size: 0.26rem;}
.user_info ul li:nth-of-type(2) span{color:#0391ff}
.user_info a{position: absolute; right:0.25rem; top:50%; margin-top:-0.255rem}
.user_info2 a{}
.title{width:1.39rem; height:0.55rem; margin-left:-0.15rem; background:#f9e603; border:0.02rem solid #863900; border-radius: 8px; color:#6f2f00; font-size: 0.28rem; line-height: 0.57rem; text-align: center;}
.show_item li{float:left; width:33.3%; margin-top: 0.16rem; text-align: center; font-size: 0.26rem; color:#4e2801;}
.show_item li span{padding:0.04rem 0.06rem; cursor: pointer;}
.show_item li.current span{border-bottom:0.04rem solid #2874ff; color:#2874ff;}
.show_cnt{width:100%}
.swiper-container {width: 96%; margin:0.3rem auto 0;}
.swiper-slide {width:33.3%;}
.swiper-slide img{width:100%;display:block}
.swiper-slide p{padding-top:0.1rem; font-size:0.26rem; text-align: center;}
.rule_wrap{margin-bottom: 0;}
.rule{padding:0.15rem 0.2rem 0; text-align: justify; line-height: 0.42rem; font-size: 0.26rem;}
.rule li span{color:#0391ff}
.share_wrap{height:1.11rem; padding-top:0.33rem; background:url(../images/share_bg.png) no-repeat; background-size:100% 100%;}
.share_wrap a{display: block; width:3.63rem; height:0.72rem; margin: 0 auto; background:url(../images/btns_02.png) no-repeat; background-size:100% 100%; text-align: center; line-height: 0.72rem; color:#fff; font-size: 0.34rem;}
.end_wrap{padding-bottom:0.28rem; text-align: center; color:#fff; font-size: 0.26rem; line-height: 0.4rem;}
.end_wrap img{display: block; width:2.46rem; margin: 0 auto;}
.end_wrap p span{display:block; font-size: 0.30rem; line-height: 0.48rem;}
.awards_list{padding:0.3rem}
.slide_up{overflow:hidden; height:3.68rem; position: relative;}
.slide_up dd{width:98%; height:0.46rem; margin:0 auto; overflow:hidden; font-size: 0.26rem; line-height: 0.46rem;}
.slide_up span{float:left}
.slide_up em{float:right; color:#0391ff}
.prize_wrap{padding-bottom: 0;}
.my_prize{padding:0.25rem}
.my_prize ul li{font-size: 0.24rem; line-height: 0.48rem;}
.my_prize ul li p span{color:#0391ff}

#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{position:absolute; width:90%; height:auto; padding:0 0; left:50%; top:48%; margin-left:-45%; z-index: 99999;}
.tip_hd h4{height:0.36rem; margin-top:0.28rem; line-height:0.36rem; font-size:0.36rem; text-align: center; color:#4e2801}
.tip-con{width:85%;  margin:0px auto; }
.tip-mid img{width:60%; margin:0.3rem auto 0; max-width: 2.7rem;}
.tip-con p{padding:0.15rem 0; font-size: 0.26rem; line-height: 0.46rem;}
.tip-con p span{color:#0391ff}
.clo{ position: absolute; right: 0.12rem; top:0.12rem;}
.clo a{ display: block; width:0.36rem; height:0.36rem;background:url("../images/tip_clo.png") 0 0 no-repeat;background-size:100% auto;text-indent: -9999px;}
.tip-box a{margin-top:0}
.btns2{margin: 0 0.2rem;}
.tip-box .btns2{width:85%; margin: 0.05rem auto 0.4rem;}
.btns2 a{width:2.02rem; height:0.58rem; margin:0 0.1rem; background:url(../images/btns_03.png) no-repeat; background-size:100% 100%; line-height: 0.58rem; text-align: center; color:#fff; font-size: 0.26rem;}
.form-items input{display: inline-block; width:3.9rem; height:0.54rem; text-indent: 0.1rem; background:#dec8b1; border:none; border-radius: 4px; font-size: 0.24rem; color:#fff}
.tip-box .form-items{padding-top:0.4rem; padding-bottom: 0.25rem;}
.tip-box .form-items li input{width:100%}
#tip-box3 .tip-con p{text-align: center;}
.stars,.stars2,.stars3{display: block; width:0.31rem; height:0.32rem; background:url(../images/stars.png) no-repeat; background-size:100% 100%; position: absolute;}
.user_wrap .stars{right:-0.3rem; top:-0.28rem}
.user_wrap .stars2{left:-0.3rem; bottom:-0.4rem}
.show_wrap .stars{left:63%; top:-0.2rem; z-index: -1;}
.show_wrap .stars2{left:-0.3rem; bottom:-0.4rem}
.rule_wrap .stars{right:-0.3rem; top:40%}
.rule_wrap .stars2{left:-0.3rem; top:53%}
.prize_wrap .stars{right:-0.3rem; bottom:30%}
.prize_wrap .stars2{left:-0.3rem; bottom:10%}
.prize_wrap .stars3{left:-0.3rem; top:-0.3rem}
@keyframes light{
	0%{transform: scale(0.9,0.9);}
	70%{transform: scale(1.1,1.1);}
	90%{transform: scale(1,1);}
	100%{transform: scale(1,1);}
}
@-webkit-keyframes light{
	0%{-webkit-transform: scale(0.9,0.9);}
	70%{-webkit-transform: scale(1.1,1.1);}
	90%{-webkit-transform: scale(1,1);}
	100%{-webkit-transform: scale(1,1);}
}