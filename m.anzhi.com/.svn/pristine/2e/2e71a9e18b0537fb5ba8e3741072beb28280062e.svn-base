/* CSS Document */

.app_header_detail_anzhi{padding:10px 10px; color:#555555;}
.app_info_page{width:100%; position:relative;}
.app_info_page .app_img{float:left; width:48px; height:48px; margin-right:10px; display:inline;}
.app_btn{display:block; width:75px; height:26px; background:#71b32b; border-radius:3px; color:#ffffff; text-align:center; line-height:26px; position:absolute; right:8px; top:22px; }
.app_txt h3{font-size:18px; font-weight:normal; white-space:nowrap; overflow:hidden}
.app_score{float:left; padding-top:4px;}
.app_header_detail_anzhi ul{margin-top:5px;}
.app_header_detail_anzhi ul li{float:left; width:50%; line-height:22px;}
.app_header_detail_anzhi ul#jiaobiao_icon li{float:left; height:25px; background:url(../images/jiaobiao_01.png) no-repeat; padding-left:30px; width:100px; line-height:25px;}
.app_header_detail_anzhi ul#jiaobiao_icon li#jiaobao_icon1{ color:#69b124;}
.app_header_detail_anzhi ul#jiaobiao_icon li#jiaobao_icon2{ background:url(../images/jiaobiao_03.png) no-repeat 0 4px; padding-left:35px; color:#69b124;}
.app_header_detail_anzhi ul#jiaobiao_icon li#jiaobao_icon3{ background:url(../images/jiaobiao_02.png) no-repeat; padding-left:33px; color:#f4ac05;}
.gufang_tupiao{float:left; width:28px; height:19px;  margin:3px 0 0 0; display:inline;}

.app_cnt{border-top:3px solid #8cb340; color:#555555}
.appcnt_bg{height:30px; background:#f4f7f6; color:#669900; line-height:30px; padding:0 10px;}
.appcntlist{ padding:8px 10px; font-size:16px;}

.softcnt_img img{display:block; width:226px; height:308px; margin:0 auto;}
#mylove{width:100%; margin:0 auto;}
#mylove li{float: left; width: 20%;  height: 90px; padding: 11px 0 0; border-bottom: 1px solid #c4df9b;}
#mylove li img {display: block; width: 48px; height: 48px; margin: 0 auto;}
#mylove li span {display: block; height: 24px; line-height: 24px; overflow: hidden; text-align: center; text-overflow: ellipsis; white-space: nowrap;}

.comment li{padding:10px; border-bottom:1px solid #c4df9b;}
.comment_top{height:26px; line-height:26px; white-space:nowrap;}
.comment_top span{float:left; font-size:14px; color:#898989}
.comment_top em{float:right; margin-top:4px;}
.comment_top em img{float:left;}
.comment li p{font-size:18px; padding-top:4px;}

.toggle_btn{height:24px; padding-right:18px; padding:5px; position:relative;}
.toggle_btn span{display:block;  height:24px; padding-right:18px; background: url(../images/arrow_01.png) no-repeat right 2px; position:absolute; right:10px; top:6px; color:#336600; font-size:15px;}
.toggle_btn span.up{background: url(../images/arrow_03.png) no-repeat right 2px;}

#jietu{padding:10px; background:#cccccc; border-top:1px solid #a3a3a3;}
.scroll_detail{width:226px; height:308px; margin:0 auto; position:relative;}
.mod_02{float:left; width:226px;}
.mod_02 img{display:block; width:226px; height:308px;}
.dotModule_2 {padding:0 5px;height:11px;line-height:6px;-webkit-border-radius:11px;background:rgba(45,45,45,0.5); position:absolute; bottom:5px; left:90px;}
#slide_02_dot {text-align:center;}
#slide_02_dot span {display:inline-block;margin:0 3px;width:5px;height:5px;vertical-align:middle;background:#f7f7f7;-webkit-border-radius:5px;}
#slide_02_dot .selected {background:#66ff33;}
/*score*/
.score{padding:15px 10px; position:relative; min-height:129px;}
.score_left{ width:100px; height:115px; background:#fff; border:1px solid #669900; border-radius:10px; text-align:center; position:absolute; top:15px; left:10px;}
.score_left h6{height:30px; background:#669900; border-radius:10px 10px 0 0; color:#ffffff; font-size:20px; line-height:30px; font-weight:normal}
.score_left span{display:block;}
.fontbig{font-size:40px; color:#669900; padding:15px 0 10px 0}
.score_left em{font-style:normal;}
.score ul{margin-left:112px; padding-top:5px;}
.score ul li{margin-top:2px; font-size:12px}
.score ul li span{display:inline-block;}
.score_stars{padding-right:5px}
.comment_num{}
.comment_bg_box{display:inline-block; width:90px; height:7px; vertical-align:top;}
.comment_bg{background:#669900; height:7px;}
.comment_bg5{width:100%;}
.comment_bg4{width:80%;}
.comment_bg3{width:60%;}
.comment_bg2{width:40%;}
.comment_bg1{width:20%;}
/*suggest*/
#suggest{padding:0 10px 60px;}
#suggest p{ padding-top:8px; font-size:16px; color:#555555; line-height:30px;}
.jinggao{ display:inline-block; width:24px; height:20px; background:url(../images/jinggao.png) no-repeat;}
#telphone,#suggest select{width:60%; padding:6px 0; border:1px solid #959595; border-radius:5px;  padding-left:10px;}
#suggest select{height:30px; line-height:30px; padding:6px 0;}
#suggest textarea{width:80%; height:80px; border:1px solid #959595; border-radius:5px;}
.suggest_sub input{width:80px; height:35px; margin-top:20px; border:none; border-bottom:1px solid #4a6940; border-radius:5px; background:#71b32b; line-height:35px; text-align:center; color:#fff; font-size:18px; font-family:"微软雅黑"}

#sl_left{position:absolute; left:-45px; top:120px; width:40px; height:40px; background:url(../images/arrow_05.png) no-repeat center center;}
#sl_right{position:absolute; right:-45px; top:120px; width:40px; height:40px; background:url(../images/arrow_04.png) no-repeat center center;}