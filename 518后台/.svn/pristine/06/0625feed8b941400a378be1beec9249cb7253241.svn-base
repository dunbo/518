function cut_table(obj){
	if(confirm('是否确认删除？')) {
		$(obj).parent().parent().remove();
		var ii = 0;
		$(".add_tr").each(function(){
			ii++;
			$(this).html("图文第"+ section[ii] +"段");
		});	
		i = ii;
		var feature_graphic_id = $(obj).val();
		if(feature_graphic_id){
			var feature_graphic_strid = $('#feature_graphic_strid').val();
			$('#feature_graphic_strid').val(feature_graphic_id+','+feature_graphic_strid);
		}
	}
}
