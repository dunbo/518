/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto; background:#427ae3; font-size:14px; font-family:Microsoft Yahei,"微软雅黑"; line-height:20px; color:#000000}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none; font-size: 14px; outline: none; color:#000000}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{float:left;}
.fr{float:right;}
.main{position:relative; background:url(../images/bg1.png) repeat-y; background-size:contain}
.maininner{padding:0 0 20px; background:url(../images/bg2.png) no-repeat; background-size:100% 100%; position:relative; overflow:hidden}
/*有通用的地方*/
.wrap{background:#2c3e66; margin:90px 6.5% 0; padding:20px 0 0;  border-radius:15px; position:relative; border-radius:15px; z-index:1; color:#c89a41}
.decorate_01{width:100%}

.titles{width:82px; height:30px; margin:2px 0 0; background:url(../images/title_01.png) no-repeat; background-size:100% 100%}
.rule_title2{ background:url(../images/title_02.png) no-repeat; background-size:100% 100%}
.active_title{width:68px; margin:0 0 0 25px; background:url(../images/title_03.png) no-repeat; background-size:100% 100%}
.awards_title{padding:16px 10px 0; color:#fff; font-size:16px; font-weight:bold; text-align:center}
#end_wrap .titles{width:106px; background:url(../images/title_05.png) no-repeat; background-size:100% 100%}
.btns{display:block; width:113px; height:34px; background:url(../images/btns.png) no-repeat; background-size:100% 100%; text-align:center; line-height:32px; color:#fff; font-weight:bold; font-size:15px}
.title{padding:10px 0 15px 0; position:relative}
.title h3{font-weight:bold;color:#fff;}

.banner{width: 100%; margin: 0 auto; position:relative;  z-index:2}
.banner img{ width: 100%;}
.person2{position:absolute; right:4%; bottom:-8px; width:165px; height:72px; background:url(../images/person2.png) no-repeat; background-size:100% 100%; z-index:1}
/*index*/
.rule{ margin:-22px 25px 0; padding:13px 15px 66px;  background:#2e4169; border-radius:5px; position:relative;}
.rule p{ padding:3px 8px; line-height: 22px; position:relative; z-index:2; color:#e0bd79}
.rule p span{color:#c80000;}
.active_time{display:inline-block}
.notice{ margin:15px 25px; padding:2px; background:#323232; border-radius:10px; color:#b9b9b9}
.notice_list{height:28px; padding:0 15px; line-height:28px;}
#login_wrap,.awards_wrap{margin:12px 25px 0; padding:8px 0 5px; border:1px solid rgba(255,255,255,.3); background:rgba(255,255,255,.15); border-radius:4px; }
.logined{ padding:0 10px; position:relative}
.logined_r{width:78px; height:26px; position:absolute; right:5px; top:16px;}
.logined_r a{display:block; width:78px; height:26px; padding:0 0 0 8px; background:url(../images/btns_02.png) no-repeat; background-size:80px 26px; text-align:center; line-height:27px; color:#fff; font-weight:bold; font-size:13px}
.logined_l p{ padding:3px 0 2px; color:#fff}
.login{padding-top:15px; padding-bottom:20px}
.login .logined_r a{width:80px; height:26px; padding:0; background:url(../images/btns_01.png) no-repeat; background-size:80px 26px;}
.star_list{margin: 6px 0px 10px;}
.star_list span{float: left;width:22px;height:22px;line-height:24px;background: url("../images/stars.png") 0 0 no-repeat;background-size: 32px 96px;text-align: center; font-size:12px}
.star_list span.star_gold{background-position:0 1px; color:#915825}
.star_list span.star_gray{background-position:0 -26px;color: #606060}
.star_list span.star_lightgray{background-position:0 -51px;color: #d0d0d0}
.star_list em{float: left;width:6px;height:22px;background: url("../images/stars.png") 0 0 no-repeat;background-size: 32px 96px;}
.star_list em.spot_gold{background-position:-26px 5px;}
.star_list em.spot_gray{background-position:-26px -22px;}
.star_list em.spot_lightgray{background-position:-26px -46px;}
.lottery_wrap{margin:20px 25px 0; padding:0 0 10px}
.prize_index{position:relative; margin:10px 0 0}
.prize_index p span{padding:0 1px; color:#de0000; font-size:16px}
.prize_index a{display:block; position:absolute; left:51%; top:62%; width:83px; height:25px; background:url(../images/btns_03.png) no-repeat; background-size:100% 100%; text-align:center; text-indent:-9999px}
.prize_index p.prize_tips{color:#ffea00}
.lottery_person{position:absolute; right:-25px; top:0; display:block; width:26%}
/*转盘抽奖*/
.rotate {width:90%; margin:15px auto 0; position: relative; overflow}
.rotate-con{width:100%;}
.rotate img{width:100%;}
.rotate-pointer {position: absolute;top: 50%;left: 50%;width: 46%;height: 46%;margin-top: -22.1%;margin-left: -22.8%;background: url(../images/pointer.png) no-repeat 0 0;background-size: 100% 100%; cursor: pointer;-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.pointer-disabled{background: url(../images/pointer_disabled.png) no-repeat 0 0;background-size: 100% 100%; z-index:11}
.lottery_txt{ padding:10px 0 5px; color:#fff; text-align:center}
/*九宫格抽奖*/
.lottery2{width:300px; margin:0 auto 10px; position:relative;}
.lottery2 img{display:block; width:94px; height:67px; margin:0 auto; border-radius:2px}
.lottery2 table{border-collapse: separate; width:100%;}
.lottery2 td{width:94px;height:67px; padding:3px; position:relative}
.lottery2 td span{position:absolute; left:4px; top:3px; width:92px; height:70%;}
#my_click {width:94px;height:67px; cursor:pointer;}
.lottery2 p{position:absolute; width:94px;height:67px; left:103px; top:76px; display:block;}
.lottery2 p a{display:block; width:94px; height:67px;}
.waitGame{background:url(../images/lottery_disabled.png) no-repeat; background-size:100% 100%}
.active_ctn{margin:0 0 10px 25px}
.active_ctn li{position:relative; line-height:22px; color:#21335b}
.active_ctn span{width:20px; position:absolute; top:1px;}
.active_ctn p{margin-left:20px;}

.btns1 a{display:block; margin:10px auto;}
.soft{padding:14px 10px 10px; margin:12px 25px 0; position:relative;border:1px solid rgba(255,255,255,.3); background:rgba(255,255,255,.15); border-radius:4px;}
.soft .titles{width:97px; height:14px; background:url(../images/title_04.png) no-repeat; background-size:100% 100%}

.soft_list{padding:5px 0;}
.soft_list li{ float: left; display: inline; width: 90%; margin:10px 5% 0;}
.soft_list li .soft_icon{ width: 56px;height: 56px;}
.soft_list li .soft_icon img{ width: 56px;height: 56px;}
.soft_list li .soft_name{ margin-top: 10px;height: 20px; line-height: 20px; overflow: hidden; color:#2c3e66; white-space:nowrap; overflow:hidden; text-overflow:ellipsis}
.soft_list li .soft_name a{ color: #2c3e66;}
.soft_list li a.downlode{position:absolute; right:10px; bottom:54px; display: block; width:62px;height: 24px; line-height: 24px; margin: 10px auto; background:url(../images/btns.png) no-repeat; background-size:100%100%; background-clip: padding-box; border-radius:25px; border:1px solid #ffea00; line-height:24px; color:#fff;cursor: pointer; text-align: center; font-weight:bold}

#active_ctn li{margin-top:20px; color:#cb3731; font-weight:bold}

/*我的奖品*/
.my_prize{padding:15px 0 8%; margin:0 6%; position:relative; color:#e1b976}
.my_prize_list li{line-height:25px; font-size:16px; position:relative;}
.my_prize_list2{padding:5px 0;}
.my_prize_list2 li{ position:relative; font-size:16px}
.my_prize_list2 p{ padding:6px 0;}
.form-items li{ margin-top: 30px; clear: both; overflow: hidden;}
.form-items li label{ float: left;width:90px;padding-top: 10px; color:#c89a41;}
.form-items li span{display:block;margin-left: 90px;}
.form-items li input{ width:90%;padding:10px 6px;border:none;color: #000;  font-family: Microsoft Yahei,"微软雅黑"; background-color:#fff; border-radius:2px}
.form_error{display:block; padding:8px 0 0 90px; color:red}
.split{height:11px; background:url(../images/line_bg.png) no-repeat; background-size:100% 100%;  margin: 8px 0px 15px; overflow:hidden;}
.my_prize .btns{background:url(../images/btns_01.png) no-repeat; background-size:100% 100%; margin-top:40px}
.person3{position:absolute; left:10%; top:-46px; width:152px; height:86px; background:url(../images/person3.png) no-repeat; background-size:100% 100%}
.person4{position:absolute; left:8%; bottom:0; width:88px; height:82px; background:url(../images/person4.png) no-repeat; background-size:100% 100%}
.person5{position:absolute; right:7%; bottom:0; width:82px; height:82px; background:url(../images/person5.png) no-repeat; background-size:100% 100%}
/*结束页*/
.awards_wrap{margin:12px 5.5% 0}
.awards{padding:5% 20px 4%; position:relative; color:#2c3e66}
.awards_list{line-height: 26px; text-align:center}
.slide_up{overflow:hidden;height:130px;position: relative;}

/*提示框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-webkit-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{ position:absolute; width:295px; height:320px; left:50%; top:50%; margin-left:-147px; z-index: 99999; background:url(../images/dialog_bg.png) no-repeat; background-size:100% 100%; font-size:15px;}
.tip-con{ margin: 15px 15px 0 15px; padding:125px 30px 10px 20px; color:#440e0e;}
.tip_title h6{ padding:2px 0 15px; min-height:20px; text-shadow: -1px 3px 1px #002b4a;  color:#fff; font-size:17px; font-weight:bold; line-height:18px;text-align:center}
.tip-con p{padding:0 0 15px 20px; line-height: 22px; word-wrap:break-word;word-break:break-all; font-size:14px; color:#ffffff}
.tips_btns a{display:block; width:82px; height:28px; background:url(../images/btns_01.png) no-repeat; background-size:100% 100%; margin:0 auto 0; line-height:29px; text-align:center; color:#fff}
#tip-box3 .tip-con,#tip-box4 .tip-con{padding-top:145px}
#tip-box3  .tips_btns a,#tip-box4  .tips_btns a{margin-top:16px;}

.footer{width:80%; height:24px; margin:14px auto 0;  text-align:center; position:relative; color:#fff;}
.bold{font-weight:bold;}
.red{color:#ab0500}
.prompt_01{color:#c89a41}
.mtop10{margin-top: 10px;}
