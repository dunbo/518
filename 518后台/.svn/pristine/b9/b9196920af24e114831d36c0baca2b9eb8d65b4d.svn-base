	  </tr>
	  <volist name="rebateList" id ="vo">
		<tr>
			<td>{$vo.rebate_name}</td>
			<td>{$vo.discount}%</td>
			<td>{$vo.gold_coefficient}</td>
		</tr>
	  </volist>
	  
	</table>
