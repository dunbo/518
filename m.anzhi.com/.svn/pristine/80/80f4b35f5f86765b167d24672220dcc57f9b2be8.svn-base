.form_wrap{width:986px; background: rgba(0,0,0,.5); margin: 0 auto; padding:20px; color:#fff; font-size: 18px; position: absolute; left: 50%; top:380px; margin-left: -512px;}
.form_info{margin-bottom:20px; font-size: 21px;}
.form_item_group{padding-top:22px}
.form_item_group h4 img{display: block; width:354px; margin: 0 auto 20px;}
.form_item{margin-bottom: 30px;}
.form_item .form_label{float: left; width:130px; height: 52px; line-height: 52px; text-align: right; overflow: hidden;}
.form_item label em{padding-right:10px; color:#ff3000; vertical-align: middle;}
.input_area{float: left; width:550px; height:52px; margin: 0 10px; display: inline;}
.input_area input.inputtext{width:520px; height:52px; padding:0 15px; background:#83899b; border-radius:5px; border:none; color:#fff; font-size: 18px;}
.error{float:left; padding-left:20px; background:url(../images/error_bg.png) no-repeat left 20px; line-height: 52px; color:#ff3000;}
/*单选按钮组样式*/
.label_group{float:left; margin: 13px 10px 0 10px; cursor: pointer;}
.radio_icon{display:inline-block; position: relative; width:18px; height:18px; top:3px}
.radio_inner{display: inline-block; width: 16px; height: 16px; background-color: #000; border: 2px solid #aab1c7; border-radius: 16px; position: relative; left: 0; top: 0;}
.radio_inner:after{content: " "; width: 16px; height: 16px;border: 2px solid #aab1c7; background: #aab1c7; position: absolute; left: -2px;  top: -2px; border-radius: 16px; opacity: 0; transform: scale(0); transition:all .2s}
.radio_icon .radio_input{width:100%; height:100%; position: absolute; left:0; top:0; bottom:0; right:0; opacity: 0; cursor: pointer;}
.radio_txt{display: inline-block; height:18px; margin-left:5px; line-height: 18px;}
.label_selected .radio_inner:after{opacity: 1; transform: scale(1);}
.form_btn{display: block; width:267px; height:68px; margin:50px auto 30px; border:none; background:url(../images/btn_03.png) no-repeat; text-indent: -9999px;}
.form_tips{margin: -10px 0 30px; padding-left:140px; color:#a3b5d8}

.select_group{position: relative;}
.select_group select{margin-top:12px}
.form_arrow{display:block; width:42px; height:42px; position:absolute; right:0; top:3px;}
.arrow_down{background:url(../images/arrow_down.png) no-repeat;}
.arrow_up{background:url(../images/arrow_up.png) no-repeat;}
.select_list{width:548px; position:absolute; left:0; top:53px; z-index:1000; right:0; background:#8b93a9; border:1px solid #232127; border-top:none; display: none; border-radius: 0 0 4px 4px;}
.select_list li{ padding:13px 2%; margin:0 auto; border-bottom:1px solid #232127; color:#fff;}
.select_list li:last-child{border-bottom:none}
#mask{width:100%; height:100%; background:rgba(0,0,0,.7); position: absolute; left:0; top: 0; z-index: 99;}
.dialog{width:952px; position: absolute; left:50%;  top:0; z-index: 100; display: none;}
.dialog_title{width:952px; height:129px; position: relative}
.dialog_title img{display: block; width:575px; height:129px; margin: 0 0 0 150px;}
.dialog_title span{display:block; width:32px; height:32px; background:url(../images/close.png) no-repeat center center; position: absolute; right:70px; top:50px; cursor: pointer;}
.dialog_body{margin-top:-60px}
.dialog_body ul{width:952px}
.dialog_body ul li{float:left; cursor: pointer;}
.dialog_body ul li.game_one{display: block; width:445px; height: 596px; background:url(../images/game_style1.png) no-repeat}
.dialog_body ul li.game_one:hover{background:url(../images/game_style1h.png) no-repeat}
.dialog_body ul li.game_four{display: block; width:506px; height: 596px; background:url(../images/game_style2.png) no-repeat}
.dialog_body ul li.game_four:hover{background:url(../images/game_style2h.png) no-repeat}
