{// 加载头部公共文件 }
<include file="../Public/header" />
<link rel="stylesheet" type="text/css" media="all"
	href="__PUBLIC__/js/calendar-win2k-cold-1.css" title="win2k-cold-1" />
<script type="text/javascript" src="__PUBLIC__/js/calendar_bak.js"></script>
<script type="text/javascript" src="__PUBLIC__/js/calendar-zh.js"></script>
<script type="text/javascript" src="__PUBLIC__/js/calendar-setup.js"></script>
<script type="text/javascript" src="__PUBLIC__/js/result.js"></script>
<script type="text/javascript" src="__PUBLIC__/js/action.js"></script>
<script type="text/javascript" src="__PUBLIC__/js/Util/swfobject.js"></script>
<br/>
<center>
<form id="upload" method='post' action="" >
<table width="944" border="1">
  <tr>
    <td colspan="3">渠道软件编辑：</td>
  </tr>
  <tr>
    <td width="239">广告软件包名：</td>
    <td width="382"><input name="package" type="text"  id="package" disabled value="{$featuresoftlist.package}" /></td>
    <td width="301"><span id="truename_span" style='color:#6666FF' >不可重复</span></td>
  </tr>

  <tr>
    <td>软件名称:</td>
    <td><input type="text" name="softname" id="softname" disabled   value="{$featuresoftlist.softname}" /></td>
    <td></td>
  </tr>
  <tr>
    <td>渠道软件排序：</td>
    <td><input name="rank" type="text"  id="rank" value="{$featuresoftlist.rank}" /></td>
    <td><span id="truename_span" style='color:#6666FF' >专题软件排序,修改后，系统会自动修改其他软件</span><input type="hidden" id="id" name="id" value="{$featuresoftlist.id}"/></td>
  </tr>
	<tr>
		<td>渠道推广开始时间：</td>
		<td><span	id="WebCalendar3" style="border: 1px solid #7F9DB9; align: absmiddle; cursor: hand; display: inline-block; width: 124px; padding: 1px">
	<input id="fromdate" name="fromdate" style="cursor: hand; width: 100px; border: none 0px black;" value="{$featuresoftlist.start_time|date="Y-m-d",###|default='&nbsp;'}" size="15" type="text"><img
			src="__PUBLIC__/js/calendar.gif"
			onclick="return showCalendar('fromdate', 'y-m-d');"
			style="margin: 1px; cursor: hand;" width="16px" align="absmiddle"
			height="15px"></span></td>
		<td>&nbsp;</td>
	</tr>
	<tr>
		<td>渠道推广结束时间：</td>
		<td>
			<span id="WebCalendar3"
			style="border: 1px solid rgb(127, 157, 185); padding: 1px; display: inline-block; width: 124px;">
		<input id="todate" name="todate"
			style="border: 0px none black; width: 100px;" value="{$featuresoftlist.end_time|date="Y-m-d",###|default='&nbsp;'}"
			size="15" type="text"><img src="__PUBLIC__/js/calendar.gif"
			onclick="return showCalendar('todate', 'y-m-d');"
		style="margin: 1px;" width="16px" align="absmiddle" height="15px"></span>
		</td>
		<td>&nbsp;</td>
	</tr>	
  <tr>
    <td colspan="3"><input type="submit" value='提交'/><a href="__URL__/channels_recommend_check/{$url}">返回</a></td>
 </tr>
</table>
</form>
</center>
{// 加载尾部公共文件 }
<include file="../Public/footer" />