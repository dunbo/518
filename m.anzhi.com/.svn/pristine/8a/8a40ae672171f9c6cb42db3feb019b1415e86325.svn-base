/* CSS Document */
/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{max-width:640px;min-width:320px; margin:0 auto;font-size:14px; font-family:Microsoft Yahei,"微软雅黑"; background-color: #3f8f38; color: #3b1f05;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#3b1f05; font-size: 14px;}
a,input,div{-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
::-webkit-input-placeholder { color:#898787;}
:-moz-placeholder {color:#898787;}
::-moz-placeholder {color:#898787;}
:-ms-input-placeholder {color:#898787;}

.submit_btn a,.mod_hd .chang_btn,.slideBox .prev,.slideBox .next{background: url("../images/common_icon.png") 0 0 no-repeat; background-size:50px 100px;}

.banner{}
.banner img{width: 100%;}
.wrap{width: 100%;padding-bottom: 20px; background-color:#48cc9b }
.main{position:relative;padding: 0 14% 15% 16%; background: url("../images/main_bg.png") no-repeat 0 0 ; background-size: 100% 100%;z-index: 10}
.prompt{margin-bottom:3px;color: #fff600;font-size: 14px; line-height: 22px;}
.prompt1{color: #fff600;margin: -5px 0px 0px 3px;}
.form-item{ margin-bottom: 10px;}
.form-item input{width:95%;padding:0px 8px;height: 30px;border: 1px solid #747172;border-radius: 6px; background-color: #fff;font-family: "Microsoft yahei"; color: #898787;font-size: 14px;}
.form-item span{ float: right; line-height: 30px; color: #fff;}
.input_box{margin-right:50px;}
.submit_btn{}
.submit_btn a{ display: block;width:100px;height:30px; margin: 0 auto; background-color:#fff609;border:2px solid #885720; border-radius:6px;text-align: center; text-indent: -9999px;background-position:27px -50px;}
.mod{position:relative;margin:15px;padding:30px 10px 10px 10px;background-color: #bd8e4d;border-radius: 15px; -webkit-border-radius: 15px; box-shadow: 1px 1px 5px #666;}
.line{width: 320px; position: absolute;top:-35px;left: 50%;margin-left: -160px;z-index: 5}
.line img{width: 320px;}
.prize{ position: relative; margin:-15px 2px 0px 2px;}
.prize img{ width: 100%;}
.prize h4{position: absolute;top:5px;left: 0px; width: 100%;text-align: center; color: #4a230c; font-size: 16px;z-index: 1}
.mod_hd{margin:4px 0px 5px 11px;color: #4a230c;font-size: 18px;}
.mod_hd p{margin-left: -15px;text-align: center}
.mod_hd .chang_btn{ float: right;width:70px;height:25px; background-color:#fff609;border:2px solid #885720; border-radius:20px;text-align: center; text-indent: -9999px;background-position:13px -80px;}
.mod_hd.mtop{margin-top: 15px;}
.rule p{line-height: 22px; color: #fff;}
.rule p span{color: #fa5252}

/*流程图*/
.slideBox{ position:relative;width:100%;margin:0px auto;}
.slideBox .prev,.slideBox .next{ position:absolute; display:block; width:15px; height:46px; left:0; top:50%;margin-top:-23px;text-align:center;cursor: pointer;}
.slideBox .next{ left:auto; right:0;background-position:-31px 0px;}
.slideBox .bd{margin:0px 20px;border:2px solid #e4c9a0; }
.slideBox .bd li{text-align:center;}
.slideBox .bd li img{ background:url(../images/loading.gif) center center no-repeat;  vertical-align:top; width:100%;}

/*软件*/
.soft-list{width: 100%; margin: 0px auto 5px auto;padding-bottom:5px;}
.soft-list li{ float: left; display: inline; width: 25%;margin:7px 0px;text-align: center;}
.soft-list li p{}
.soft-list li .soft-icon{text-align: center;}
.soft-list li .soft-icon img{ width: 56px;height: 56px; margin: 0 auto;}
.soft-list li .soft-name{margin-top: 10px;height: 20px; line-height: 20px; overflow: hidden;}
.soft-list li .soft-name a{ color: #000000;}
.soft-list li a.downlode{ display: block; width:56px;padding:6px 0px 5px; margin: 10px auto; background-color:#118056; border:2px solid #ffffff;border-radius:5px;-webkit-border-radius:5px;-moz-border-radius:5px;color:#ffffff;cursor: pointer;font-size:13px;text-align: center;}

/*活动结束*/
.end_hd{text-align: center;color: #fff;font-size: 20px;margin-bottom: 5px;}
.end_bd{color: #fff;margin-bottom: 20px; font-size: 16px; text-indent: 2em}
/*获奖页*/
.awards{padding-bottom:15px;color: #fff;}
.awards-hd{padding-bottom:5px;font-size:16px;}
.awards-list{line-height: 22px;}
.slide_up{overflow:hidden;height:88px;position: relative;}
/*尾部*/
.copyright{margin: 5px 0px 0px 30px;font-size: 14px; color: #fff600}
.footer{}
.footer img{width: 100%;}
