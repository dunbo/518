/* CSS Document */
body,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
fieldset,img {border: 0;}
a{text-decoration:none; color:#252525;}
.az_clear{clear:both;}
ul,li{list-style:none;}
body{ font-family:MicroSoft YaHei,SimSun,sans-serif; color:#252525; font-size:14px; line-height:20px; min-width:320px;}

.az_logo_wp{ display: inline-block;float: left; margin-left: 25px; margin-top:0px; vertical-align: top;}
.az_logo{background:url("../images/logo-wp.gif") no-repeat scroll center center rgba(0, 0, 0, 0);display: block;height: 40px;overflow: hidden;text-indent: 100%;white-space: nowrap;width: 50px;}
#az_logowrap{height:45px; padding:10px 10px 5px 8px; position:relative; background:#f6f6f6;}
#az_logo{position:absolute; left:9px; top:9px; width:100px; height:43px; background:url(../images/logo.png) no-repeat; background-size:100px 43px; -webkit-background-size:100px 43px; display:block; text-indent:-9999px}
#az_search{height:38px; margin:0 0 0 38%; border:1px solid #EFEFEF; background:#fff; position:relative;}
#az_search .az_search_txt{margin-right:38px; padding:10px 0px 10px 10px; width:80%; border:none; background:none; text-indent:0.5em;  font-size:16px; }
.az_search_btn{position:absolute; right:12px; top:8px}

#az_header{ background-color:#fff;}
.az_mainnav{ height:50px; margin:0px auto 20px; overflow:hidden; background:#fff; border-top:1px solid #efefef; border-bottom:1px solid #efefef;}
.az_mainnav li{float:left; width:19.6%; height:50px; line-height:48px; font-size:18px; border-right:1px solid #efefef; color:#3e3e3e;}
.az_mainnav li a{display:block; padding-top:2px; color:#3e3e3e; text-align:center }
.az_mainnav li.az_current1 a{ height:50px; line-height:41px; background:#f6f6f6;  border-top:3px solid #7fc31e;}

.az_subnav{ height:36px; line-height:36px; width:80%; text-align:center; margin:0px auto 20px;}
.az_subnav table{ border:2px solid #7fc31e; }
.az_subnav td{ text-align:center; font-size:16px;}
.az_subnav td a{display:block; height:36px; color:#7fc31e;}
.az_subnav td.az_current2{ height:36px; background:#7fc31e;}
.az_subnav td.az_current2 a{ display:block; color:#ffffff;}

.az_more{width:60%; height:30px; background:#fff; border:1px solid #dbdbdb; text-align:center; line-height:30px; color:#669900; margin:10px auto;}
.az_more a{display:block; width:100%; height:30px; text-align:center; line-height:30px; color:#669900; font-size:18px;}

.az_app-items-list{ background-color:#fff; /* width:98%; margin:0 auto;*/}
.az_app-items-list li{/*width:100%;*/ min-height:57px; margin:10px; line-height:36px; border:1px solid #efefef; position:relative; background-color:#fff;}

/*专题*/
.az_app-zt-list{ width:98%; margin:0 auto;}
.az_app-zt-list li{  width:98%;  margin:0 auto 10px; border:1px solid #efefef;}
.az_app-zt-list li h2{ padding:10px 10px 0 10px; width:98%; height:28px; line-height:28px; font-weight:normal; font-size:18px; color:#7fc31e; white-space:nowrap; overflow:hidden;  text-overflow:ellipsis;}
.az_app-zt-list li p{ height:18px; overflow:hidden; text-overflow: ellipsis; white-space: nowrap; padding:10px; display:block; width:98%; line-height:24px; font-size:14px;}


.az_items-info{padding: 10px 98px 10px 10px;}
.az_app-items-list h2{float:left; width:48%; padding:10px;  font-weight:normal; font-size:16px; white-space:nowrap; overflow:hidden; color:#000; text-overflow:ellipsis;}
.az_items_size{float:right; font-size:14px; color:#7fc31e;}
.az_items_size span{ padding-right:3px;}
.az_imme-down-btn{ display:block; width:68px; height:32px; background:#7fc31e; color:#ffffff; font-size:14px; text-align:center; line-height:32px; position:absolute; right:10px; top:12px; z-index:99999;}
.az_down-btn{ display:block; width:68px; height:32px; background:#7fc31e; color:#ffffff; font-size:14px; text-align:center; line-height:32px; position:absolute; right:10px; top:12px; z-index:99999;}
.az_clickarea{height:100%;  display:block; width:100%; position:absolute; left:0; top:0;}

/*底部*/
#az_footer{ padding:8px 0px; border-top:1px slild #c4df9b; text-align:center; border-top:1px solid #efefef; color:#444444; line-height:23px; height:100px;}
#az_footer p a,.az_topbtn{padding:0 9px;}
.az_topbtn{cursor:pointer;}
#az_footer a{color:#444444}

/*浮动底部下载*/
.az_adrift_module{ width:100%;padding:0px 0px; background: url(../images/fudong_bg.png) repeat; position:fixed;bottom:0; z-index:999999;}
.az_adrift_btnA{color:#ffffff;}
.az_adrift_btnA,.az_adrift_btnA dl{float:left;}
.az_adrift_btnA dl.az_adrift_margin15{margin-left:8px;}
.az_adrift_btnA dl p{/* line-height:21px;margin-top:5px;*/}
.az_adrift_btnA dl p.az_font18{font-size:16px; margin-top:2px;}
.az_adrift_btnA dl p.az_font14{font-size:12px;}
.az_adrift_btnB{float:right; margin-right:20px;}
.az_adrift_btnB .az_down_button{background-color:#7fc31e;  color:#ffffff;text-align:center; padding:5px; margin-top:5px; cursor:pointer;}
.az_adrift_btnB .az_down_button a{font-size:14px;}

/*提示模块*/
.az_prompt{margin-top:20px; margin-bottom:20px;overflow:hidden;}
.az_prompt_module{margin:0 auto; width:300px; font-size:12px;}
.az_prompt_module dl{ float:left; background-image:url(../images/prompt_bg.png);}
.az_prompt_module dl.az_btn_A{background-position:10px -102px;}
.az_prompt_module dl.az_btn_B{background-position:0 -51px; background-repeat:repeat-x; line-height:26px; color:000000; padding:10px 0px;}
.az_prompt_module dl.az_btn_B a{color:#7fc31e; padding:0 10px;text-decoration:underline;}
.az_prompt_module dl.az_btn_C{ background-position:0 0px;}
.az_prompt_module dl.az_btn_A,.az_prompt_module dl.az_btn_C{ background-repeat:no-repeat; width:26px; height:46px;}


.ft_module{ width:100%; position:fixed; bottom:0; z-index:999999; background: url(../images/fudong_bg.png) repeat;}
.ft_btnA {color: #FFFFFF; padding:5px 2px 5px 4px;}
.ft_btnA, .ft_btnA dl {float:left; margin-top:3px;}
.ft_btnA dl.ft_margin15 {margin:0px 0 0 5px;}
.ft_btnA dl p.font18 {font-size:16px; margin-bottom:2px;}
.ft_btnA dl p.font14 {font-size:12px;}
.ft_btnB { float:right; margin-right:5px; margin-top:8px;}
.ft_btnB .down_button { margin-top:5px; padding:5px; text-align:center; cursor:pointer; color:#FFFFFF; background-color:#7FC31E;}
.ft_btnB .down_button a {font-size: 14px;}
.ft_close{float:right;/* border-left:1px solid #898989;*/ height:100%;}
.ft_close a{display:inline-block; background:url(../images/close_menu.png) no-repeat 0 0px;width:20px; height:20px; cursor:pointer}