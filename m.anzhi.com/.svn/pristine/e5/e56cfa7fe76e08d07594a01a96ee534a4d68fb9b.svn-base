<script type="text/javascript">
var curl=location.href; 
if( curl.indexOf("fool") >= 0 ) {
	 $("html,body").animate({scrollTop:$(".lottery_wrap").offset().top},100);
}
var flags={canMove:true};
function showOpenBox(obj){
	$('input').blur();
	setTimeout(function(){
		var bg_h=$(window).height() + $(document).scrollTop(),
        top_h=  $(obj).height()/2 - $(document).scrollTop();
		$(obj).css("margin-top",-top_h+"px").show();
		$('#body-bg').css("height",bg_h+"px").show();
		flags.canMove=false;
		window.onresize = function(){
			var bg_h=$(window).height() + $(document).scrollTop(),
				top_h=  $(obj).height()/2 - $(document).scrollTop();
			$('#body-bg').css("height",bg_h+"px");
			$(obj).css("margin-top",-top_h+"px");	
		}
	},200)
}

var sid = '<!--{$out.sid}-->';
var aid = '<!--{$out.aid}-->';
var prefix = "<!--{$out.prefix}-->";
var lottery_num = "<!--{$out.lottery_num}-->";
var deduction_lottery_num = "<!--{$out.deduction_lottery_num}-->";
$(document).ready(function () {
	$('.prize_btn').bind('mousedown',function(){
		$(this).addClass('prize_btnh');	
		$('.prize_btn').bind('mouseup',function(){
			$(this).removeClass('prize_btnh');	
		})
	})
	setTimeout(function(){
		//go_from 1排行榜（无软件名称、状态按钮） 2抽奖页面（有软件名称、和状态按钮）
		var go_from =2;
		my_soft(aid,sid,8,go_from);
	}, 500);
});
function onLoginComplete(sid,tip){
	if(tip == "登录成功"){
		sid_str = sid.replace("PHPSESSID=", "");
		location.href="/lottery/"+prefix+"/index.php?sid="+sid_str+"&aid=<!--{$out.aid}-->"+"&new=1";	
	}
}

var is_user_winning = "<!--{$out.is_user_winning}-->";
function my_snatch_record(){
	if(is_user_winning == 2){
		pop_tips("",'您暂时没有砸蛋记录',"2",1);	
		return false;
	}else{
		location.href="/lottery/"+prefix+"/userinfo.php?types=1&sid="+sid+"&aid="+aid+"&new=1";
	}
}
$(document).ready(function(){
	$('.zadan').click(function(){
		<!--{if $out['is_login'] == 2}-->
			login('<!--{$out.login_url}-->','<!--{$out.version_code}-->');
			return false;
		<!--{/if}-->
		if(parseInt(lottery_num) <= 0 ){
			pop_tips("","您的锤子不足,请您充值后在砸蛋！","2",1);			
			return false;	
		}	
		var type = $(this).attr('id');
		if(type == 'ten' && parseInt(lottery_num) < 10){
			pop_tips("","您的锤子不足,请您充值后在砸蛋！","2",1);			
			return false;			
		}else if(type == 'hundred' && parseInt(lottery_num) < 100){
			pop_tips("","您的锤子不足,请您充值后在砸蛋！","2",1);			
			return false;	
		}
		if(type == 'one'){
			var num = 1;
			var objBox = '#tip-box4';
		}else if(type == 'ten'){
			var num = 10;
			var objBox = '#tip-box5';
		}else{
			var num = 100;
			var objBox = '#tip-box6';
		}		
		
		showOpenBox(objBox);
		
		setTimeout(function(){
			if(type == 'one'){
				lottery_click(1);
			}else if(type == 'ten'){
				lottery_click(10);
			}else{
				lottery_click(100);
			}
			$(this).unbind();
		},1000)
	})
})
//抽奖
function lottery_click(num){
	var pkg_str = open_pkg_arr.join(";");	
	$.ajax({
		url: '/lottery/'+prefix+'/lottery.php',
		data: 'sid='+sid+'&aid='+aid+'&pkg='+pkg_str+'&num='+num,
		type: 'post',
		dataType: 'json',				
		success: function(data){
			if(data.code == 1){
				is_user_winning =1;
				location.href="/lottery/"+prefix+"/userinfo.php?types=2&sid="+sid+"&aid="+aid+"&new=1";
			}else if(data.code == 2){
				location.href=data.url+"?sid="+sid+"&aid="+aid+"&new=1";
			}else{
				//未中奖未刷新页面中的
				 lottery_num = parseInt(lottery_num) - parseInt(num);
				 deduction_lottery_num = parseInt(deduction_lottery_num) + parseInt(num);
				 $('#lottery_num').html(lottery_num);
				 $('#lottery_num_one').html(lottery_num);
				 $('#deduction_lottery_num').html(deduction_lottery_num);
				
				pop_tips("",data.msg,"2",1);
				$('.tip-box_bg').hide();
			}
		}
	});
}
//幸运奖励
var luk_num = "<!--{$out.luk_num}-->";
function luk_prize(){
	<!--{if $out['is_login'] == 2}-->
		login('<!--{$out.login_url}-->','<!--{$out.version_code}-->');
		return false;
	<!--{/if}-->
/*	
	if(luk_num < 50){
		pop_tips("",'您的幸运值不足',"2",1);	
		return false;	
	}
	*/
	location.href="/lottery/"+prefix+"/luk_prize.php?sid="+sid+"&aid="+aid+"&new=1";
}
</script>