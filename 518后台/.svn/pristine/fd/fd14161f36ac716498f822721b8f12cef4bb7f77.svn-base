		}); 
	});	         
}

function set_radio(){
	$("input[type=radio]").each(function(i){
	 var text = $(this).val();
	 var date = $(this).attr("date");
	 var n = $(this).attr("name");
	 if(date){
		 console.log(n +':'+ date);
		 if(date =='on'){
			 $("."+n+"_on").show();
			 $("."+n+"_off").hide();
			 $(this).attr("checked",true);
		 }else if(date =='off'){
			 $("input[type=radio]").each(function(i){
				 var n1 = $(this).attr("name");
				 if($(this).val() == 'off' && n1 == n){ 
					 $(this).attr("checked",true); 
					 $("."+n1+"_on").hide();
					 $("."+n1+"_off").show();
				 }
			 });
		 }else{}
	 }
	});	         
}

//删除图片
function removeImg(obj){
	$(obj).parent().html('');
	var name = $(obj).attr('id');
	//console.log(name);
	$("input[name="+name+"]").val('');
	$("input[name="+name+"]").attr("value","");
}

//图片上传判断
function onUploadImgChange(sender,img,obj,width){
    var newimg = new Image();
    newimg.src = check_img(sender);
    newimg.title = "点击删除";
    newimg.id = img;
    
    if( $("#"+img).val() && !sender.value.match( /.jpg|.gif|.png|.bmp|.jpeg/i ) )
	{        
        alert('图片格式无效！');   
        return false;       
    } 
    // 加载完成执行
    newimg.onload = function () {
		var w = newimg.width;
		var h = newimg.height;
		var fileSize = 0;
	    
	    if($(sender)[0].files[0].size){
	    	fileSize = $(sender)[0].files[0].size; //获得文件大小；
	    }
	    
	    if(width == 640){
	    	if(w != width){			 
				error_msg('图片宽度必须'+width);
				$(sender).val("");
				$(sender).attr("value","");
				$(set_img(sender)).html('');
				return false;    			
			}	
		}
	    
		if(0 < fileSize && fileSize > 204800){			 
			error_msg('图片大于200K');
			//console.log($(sender).attr('name'));
			$(sender).val("");
			$(sender).attr("value","");
			return false;    			
		}
		
		//绑定事件并写入文件 
		var str ="<img src='" + newimg.src +"' title='安智_流量活动_图片' ";
			str += "/>";
			str +="<hr/><button class='imgbutton' onclick='removeImg(this);' id='" + img +"' >立即删除</button>";
		$(set_img(sender)).html(str);
    }
} 

function set_img(sender){
	//var par=sender.parentNode; //原生js得到父节点 
    return nextObj = $($(sender).parent()).next();
  	console.log(nextObj);
}


function check_img(obj){
	   var imgSrc = '';
	    if (obj.files && obj.files[0]) {
	        if (getOs() == 'Chrome') {
	            imgSrc = window.webkitURL.createObjectURL(obj.files[0]);
	        } else {
	            imgSrc = window.URL.createObjectURL(obj.files[0]);
	        }
	    } else {
	        //IE下，使用滤镜
	        obj.select();
	        //解决IE9下document.selection拒绝访问的错误
	        if (navigator.appName == "Microsoft Internet Explorer" && navigator.appVersion.match(/9./i) == "9.") {
	            obj.blur();
	        }
	        var imgSrc = document.selection.createRange().text;
	        document.selection.empty();
	    }
		return imgSrc;
}

function getOs()
{ 
   var OsObject = ""; 
   if(isIE = navigator.userAgent.indexOf("MSIE")!=-1) { 
        return "MSIE"; 
   }
   if(isFirefox=navigator.userAgent.indexOf("Firefox")!=-1){ 
        return "Firefox"; 
   }
   if(isChrome=navigator.userAgent.indexOf("Chrome")!=-1){ 
        return "Chrome"; 
   }
   if(isSafari=navigator.userAgent.indexOf("Safari")!=-1) { 
        return "Safari"; 
   }
   if(isOpera=navigator.userAgent.indexOf("Opera")!=-1){ 
        return "Opera"; 
   } 
} 
