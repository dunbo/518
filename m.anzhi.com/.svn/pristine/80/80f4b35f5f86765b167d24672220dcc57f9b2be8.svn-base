body,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td,em,i{margin:0; padding:0;  font-weight:normal; font-family:"微软雅黑",Arial,sans-serif;}
body{background:#252b3b; font-size:14px; color:#666;}
fieldset,img {border: 0;}
em,i{ font-style:normal;}
a{text-decoration:none; color:#666;}
a:hover{text-decoration:none;}
.clear{clear:both; /*height:0;*/}
ul,li{list-style:none;}
input::-webkit-input-placeholder {color: #dfdfdf;font-size: 14px;}
input:-moz-placeholder {color: #dfdfdf; font-size: 14px;}
input::-moz-placeholder {color: #dfdfdf; font-size: 14px;}
input:-ms-input-placeholder {color: #dfdfdf; font-size: 14px;}
