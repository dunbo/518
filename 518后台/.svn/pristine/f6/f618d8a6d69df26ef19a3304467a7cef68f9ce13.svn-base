        $("#add_website").submit();
    }
    
    function get_detail_type_change(get_detail_type) {
        if (get_detail_type == 1) {
            $("#get_detail_type_1_div").css('display', '');
            $("#get_detail_type_2_div").css('display', 'none');
        } else {
            $("#get_detail_type_1_div").css('display', 'none');
            $("#get_detail_type_2_div").css('display', '');
        }
