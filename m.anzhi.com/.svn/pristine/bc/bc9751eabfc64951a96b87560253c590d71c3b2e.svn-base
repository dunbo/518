/* CSS Document */
/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{max-width:640px;min-width:320px; margin:0 auto;font-size:0.14rem; font-family:Microsoft Yahei,"微软雅黑"; background: #fff; color: #090909;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#414141; font-size: .14rem;}
.a,input,button,div{-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{ float: left;}
.fr{ float: right}
.fixed{width:6.4rem; height: 1.75rem; position: fixed; left:50%; top:0; margin-left:-3.2rem; z-index: 2}
.fixed a{display: block; width:0.82rem; height:1.29rem; background:url(../images2/tab_bg1.png) no-repeat; background-size:100% 100%; text-indent: -9999rem; position: absolute; top:0}
.fixed a.gift_btn{background:url(../images2/tab_bg2.png) no-repeat; background-size:100% 100%; left:1.6rem}
.fixed a.game_btn{left:0.36rem;}
.fixed a.gift_btn2{background:url(../images2/tab_bg4.png) no-repeat; background-size:100% 100%; left:1.6rem}
.fixed a.game_btn2{background:url(../images2/tab_bg3.png) no-repeat; background-size:100% 100%;left:0.36rem;}
.banner{width:100%; position: relative;}
.banner img{ width: 100%; display:block;}
.banner a{display: block; width:1.57rem; height:0.45rem; background:url(../images2/btns_05.png) no-repeat; background-size:100% 100%; position: absolute; left:0.18rem; bottom:0.8rem; text-align: center; line-height: 0.47rem; font-size: 0.24rem; color:#fff}
.app{padding:0.15rem 0.22rem 0; background:url(../images2/bg1.jpg) no-repeat; background-size:100% 100%}
.part2 .app{background:url(../images2/bg2.jpg) no-repeat; background-size:100% 100%}
.part3 .app{background:url(../images2/bg3.jpg) no-repeat; background-size:100% 100%}
.part4 .app{background:url(../images2/bg4.jpg) no-repeat; background-size:100% 100%}
.part5 .app{background:url(../images2/bg5.jpg) no-repeat; background-size:100% 100%}
.part6 .app{background:url(../images2/bg6.jpg) no-repeat; background-size:100% 100%}
.part6 .app{padding-bottom: 1.2rem;}
.infor{padding-bottom:0.25rem; color:#322c71; font-size: 0.26rem; line-height: 0.36rem; text-align: justify;}
.soft-list li{margin:0 0 0.3rem; position: relative;}
.soft-list li img{float: left; width:1.1rem; height:1.1rem; border:2px solid #615cb7; border-radius: 19px;}
.soft-list li h3{float: left; width:3rem;white-space: nowrap;  text-overflow: ellipsis;overflow: hidden;margin:0.05rem 0 0 0.13rem; font-size:0.32rem; color:#322c71;}
.soft-list li p{float: left; width:3rem; margin:0.15rem 0 0 0.15rem; font-size:0.26rem; color:#9793bf; line-height: 0.36rem;}
.soft-list li a{  width:1.56rem; height:0.56rem; background:url(../images2/btns_01.png) no-repeat; background-size: 100% 100%; position: absolute; right:0; top:50%; margin-top:-0.28rem; text-align: center; line-height: 0.6rem; font-size: 0.24rem; color:#ffffff}

.detail{min-height: 7.2rem; background: url(../images2/bg7.jpg) no-repeat; background-size:100% 100%}
.detail .app{padding:0.3rem 0 0; background: none;}
.footer{margin-top:-0.7rem; height:1.84rem; background:url(../images2/footer_bg.jpg) no-repeat; background-size: 100% 100%;}
.footer2{height:1.42rem; background:url(../images2/footer_bg2.jpg) no-repeat; background-size: 100% 100%;}
.title{width:3.72rem; height:1.41rem; margin: 0 auto 0.25rem; background:url(../images2/title_bg.png) no-repeat; background-size:100% 100%;}
.prize .soft-list li{padding-bottom:0.45rem; background:url(../images2/line.png) no-repeat left bottom; background-size:100% auto}
.prize .soft-list li h3{width:4.45rem; margin-bottom: 0.1rem;}
.prize .soft-list li p{width:4.45rem}
.prize .soft-list li a{top:40%; right:0.2rem}
.soft-list li a.btns_over{background: url(../images2/btns_02.png) no-repeat; background-size: 100% 100%;}
.soft-list li a.btns_end{background: url(../images2/btns_03.png) no-repeat; background-size: 100% 100%;}
.tips_p img{display: block; width:1.92rem; height:1.7rem; margin: 0.5rem auto 0;}
.tips_p p{padding:0.28rem 0; text-align: center; color:#322c71; font-size: 0.28rem; line-height: 0.28rem;}
.return_btn{display:block; width:3.78rem; height:0.6rem; margin: 0 auto; background:url(../images2/btns_04.png) no-repeat; background-size: 100% 100%; text-align: center; line-height: 0.6rem; font-size: 0.28rem; color:#ffffff}
.app2{padding:0.3rem 0.2rem 0; background:url(../images2/bg8.jpg) no-repeat; background-size:100% 100%;}
.app2 .soft-list li{float:left; width:50%}
.app2 .soft-list li h3{width:1.6rem; white-space:nowrap; text-overflow:ellipsis; overflow:hidden}
.app2 .soft-list li p{width:1.6rem; font-size: 0.24rem;}