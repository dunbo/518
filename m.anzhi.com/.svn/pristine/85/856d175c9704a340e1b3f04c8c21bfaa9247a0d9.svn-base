/* CSS Document */
/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{max-width:640px;min-width:320px; margin:0 auto;font-size:14px; font-family:Microsoft Yahei,"微软雅黑"; background: #f34fa2; color: #fff;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#fff;}
.a,input,button,div{-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{ float: left;}
.fr{ float: right}
.cnt{text-align: center}
.underline{text-decoration:underline;}

.mtop{ margin-top: 35px;}
.mtop1{ margin-top: 20px;}
.mtop2{margin-top:10px;}
.mbot{margin-bottom: 8px;}
.ptop{ padding-top: 10px;}
.ptop1{padding-top: 45px;}
.pbot{padding-bottom: 25px;}


.banner{position: relative; width: 100%;min-width: 320px; margin:0 auto; text-align: center;}
.banner img{ width: 100%;vertical-align: bottom;}

/*公告*/
.notice{width: 100%;height: 26px; line-height: 26px;background:#8449df; color: #fff; overflow: hidden; }
.notice-icon{ float: left; display: inline; width: 25px;height:21px;margin:2px 0px 0px 5px; background:url(/images/aprilstrip/icon_01.png) no-repeat; background-size:25px 21px;}
.notice-list span{color: #fff;padding-right: 20px;}
.notice-list span em{ padding-left: 10px;}
.introduce{margin:40px 20px 0; background:#f59fbb; padding:10px 0; border:10px solid #fdc8da; border-radius:10px; font-size:16px; line-height:26px; text-align:center; position:relative;}
.introduce span{position:absolute; left:-30px; top:-35px; width:63px; height:76px; background:url(/images/aprilstrip/bg_01.png) no-repeat; background-size:63px 76px}


.person{width:318px; margin:20px auto 0;}
.person li{float:left; width:145px; margin:0 7px; display:inline;}
.person li img{width:145px; height:228px;}
.btns{display:block; width:121px; height:37px; margin:20px auto 10px; background:url(/images/aprilstrip/btn_00.png) no-repeat; background-size:121px 37px; color:#066508; font-size:16px; letter-spacing:2px; text-align:center; line-height:32px; font-weight:bold;}
.btns_01{background:url(/images/aprilstrip/btn_01.png) no-repeat; background-size:121px 37px; color:#0a647b;}
.btns_02{background:url(/images/aprilstrip/btn_02.png) no-repeat; background-size:121px 37px; color:#964d29;}
.btns_03{width:193px; height:50px; background:url(/images/aprilstrip/btn_03.png) no-repeat; background-size:193px 50px; color:#964d29; line-height:41px}
.noclick_01{background:url(/images/aprilstrip/noclick_01.png) no-repeat; background-size:193px 50px; color:#6d6866}
.noclick_02{background:url(/images/aprilstrip/noclick_02.png) no-repeat; background-size:121px 37px; color:#964d29}
.footer_link{padding:15px 0; font-size:18px; letter-spacing:3px; text-align:center;}

.tips_p{padding:0 0 10px; text-shadow: 0 0 2px #87134d;}
.wrap_tips{width:75%; margin:10px auto 0; background:#ffb5b9; border:2px solid #fff;  border-radius:8px;}
.wrap_tips img{margin:10px auto 5px;}
.wrap_tips2 img{margin:10px auto 5px;}
.prompt {text-shadow: 0 0 2px #87134d;}

.swipe_wrap{background:url(/images/aprilstrip/swipe_bg.png) no-repeat; background-size:contain; position:relative;}
#boy,#girl{width:200px; margin:0 auto;}
#girl{padding-top:10px;}
.swipe_wrap img{display:block; margin:0 auto;}
.arrow{animation:start 1.5s infinite ease-in-out; -webkit-animation:start 1.5s infinite ease-in-out; width:34px; height:64px; background:url(/images/aprilstrip/arrow.png) no-repeat; background-size:34px 64px; position:absolute; left:50%; margin-left:-17px; bottom:-40px;}
.swipe_wrap a{position:absolute; right:10px; top:10px; font-size:18px; letter-spacing:3px; text-align:center;}

.person2 img{display:block; margin:0 auto;}
.introduce2{margin-top:15px; background:#e83133; border-color:#ff7779;}
.wth{width: 94%; margin:0 auto;}
.white-wrap{width: 94%; margin:10px auto 30px auto; background-color: #fff;}

.main{margin:8px 6px;}
.wrap{border:2px solid #fff; background:#ffb5b9; border-radius:8px; margin:10px 0 0;}
.tips_sim{padding:5px 10px; background:none}

/*软件*/
.soft-p{padding:8px 0; font-size:16px; text-shadow: 0 0 2px #87134d;} 
.soft-list{background:#ffb5b9; margin: 0 auto; padding:6px 5px 0;}
.soft-list li{ float: left; display: inline; width: 25%;margin:7px 0px;text-align: center;}
.soft-list li .soft-icon{text-align: center;}
.soft-list li .soft-icon img{ width: 60px;height: 60px; margin: 0 auto;}
.soft-list li .soft-name{margin-top: 6px;height: 20px; line-height: 20px; overflow: hidden;}
.soft-list li .soft-name a{ color: #372221;}
.soft-list li .soft-size{ color: #fe0000;height: 20px; line-height: 20px;overflow: hidden;}
.soft-list li a.downlode{ display: block; width:64px;height: 25px; line-height: 26px; margin: 6px auto 8px auto; background-color: #968cfc; border:2px solid #241c7a; border-radius: 3px; color: #fff;cursor: pointer;}


.prize-title,.end-title{padding:20px 0 0 0; font-size:28px;}
.prize-title span,.end-title span{padding:0 10px; color:#890422}
.prize-title span.spec{color:#c41e44; font-size:20px}
.prize-text{padding:10px 10px 0; font-size:18px; line-height:28px}


.detail_list{padding:10px; background:#ffb5b9; border: 2px solid #fff; border-radius: 8px;}
.detail_list h4{font-size: 18px; font-weight: bold; padding-bottom: 6px; color:#890422;}
.detail_list  p{font-size: 16px; line-height: 25px}
.active_ctn li{position:relative; color:#890422;}
.active_ctn span{width:25px; position:absolute; top:3px}
.active_ctn p{margin-left:25px;}


.end{padding:10px 10px 8px; font-size:18px; color:#505050; line-height:28px}
.end a{ display:block; padding:15px 0 10px; font-size:18px; text-decoration:underline;}
.end em{font-size:16px; color:#890422;}

/*邮寄地址*/
.form1{width:90%; margin:0 auto;}
.form_title{margin:15px auto 0;}
.form-item{ margin:8px 0 0;  font-size:16px;}
.form-item label{ float: left;width:70px; padding:12px 0;}
.form-item .textbox input{float:left; width:63%; padding:12px 15px; background:#fff; border-radius:4px; border:1px solid #747172; font-family: "Microsoft Yahei"; color:#898787;}
.prompt2{padding:5px 0 0;}

/*提示框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{position:absolute;width: 90%;left:50%;top:50%;margin-left:-45%;border:2px solid #fff; background:#ffb5b9; border-radius:8px; z-index: 99999;}
.tip_hd{width: 90px; height:138px; margin:0 auto 10px; padding:0 20px; background:#ffd630; border-radius:8px;}
.tip_hd img{width: 90px; height:138px; margin:15px auto 0;}
.tip-mid{padding-bottom:4px;border-radius: 0px 0px 15px 15px;}
.tip-con{padding:0px 25px 0 25px;margin:0px 4px}
.tip-con p{line-height: 25px; text-align: center;font-size: 18px;}
.clo{ position: absolute;right: -7px;top: -8px;}
.clo a{ display: block; width:38px; height:38px;background:url("/images/aprilstrip/tip_clo.png") 0 0 no-repeat;background-size:100% auto;text-indent: -9999px;}


/*转盘抽奖*/
.rotate_tips{padding:0 0 8px; text-align:center;}
.rotate {width: 91%; margin:0 auto;position: relative;}
.rotate-pointer {position: absolute;top: 50%;left: 50%;width: 36%;height: 36%;margin-top: -19%;margin-left: -18%;background: url(/images/aprilstrip/pointer.png) no-repeat 0 0;background-size: 100% 100%; cursor: pointer;-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.pointer-disabled{background: url(/images/aprilstrip/pointer_disabled.png) no-repeat 0 0;background-size: 100% 100%; z-index:11}
.rotate_list{width:60px; height:78px; position:absolute; left:0; top:0; left:50%; top:50%; z-index:10}


.redcolor{ color:#f6323a;}
.yellow,.end .yellow{color:#fcff00;}

@-webkit-keyframes start {
	0%,30% {opacity: 0;-webkit-transform: translate(0,-10px);}
	60% {opacity: 1;-webkit-transform: translate(0,0);}
	100% {opacity: 0;-webkit-transform: translate(0,8px);}
}
@keyframes start {
	0%,30% {opacity: 0;transform: translate(0,-10px);}
	60% {opacity: 1;transform: translate(0,0);}
	100% {opacity: 0;transform: translate(0,8px);}
}
