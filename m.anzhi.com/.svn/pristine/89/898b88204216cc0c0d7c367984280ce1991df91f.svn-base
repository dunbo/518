/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto; background:#1e0329; font-size:15px; font-family:Microsoft Yahei,"微软雅黑"; line-height:20px; color:#1d1f21}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none; font-size: 14px; outline: none; color:#1d1f21}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{float:left;}
.fr{float:right;}
.main{position:relative; padding:0 12px 12px; background:url(../images/bg1.jpg) no-repeat; background-size:100% auto}
/*有通用的地方*/
.wrap{min-height:60px; padding:8px; margin-bottom:20px; background:url(../images/repeat_bg1.jpg) no-repeat; background-size:100% 100%; border:1px solid #4d1f8c}
.wrap_inner{border:1px solid #8c3fed; position:relative}
.wrap_icon{position:absolute;width:7px; height:7px; background:url(../images/icon_02.png) no-repeat; background-size:7px 7px;}
.wrap_lt{ left:-1px; top:-1px;}
.wrap_rt{ right:-1px; top:-1px; background:url(../images/icon_03.png) no-repeat; background-size:7px 7px;}
.wrap_lb{ left:-1px; bottom:-1px; background:url(../images/icon_04.png) no-repeat; background-size:7px 7px;}
.wrap_rb{ right:-1px; bottom:-1px; background:url(../images/icon_05.png) no-repeat; background-size:7px 7px;}

.titles{width:82px; height:24px; margin:8px 0 0; padding:0 0 0 24px; background:url(../images/icon_01.png) no-repeat 0 2px; background-size:17px 17px; color:#fdfbff; font-size:18px}

.btns{display: block; width:92px;height: 28px; margin-left:auto; margin-right:auto; line-height: 28px; background:url(../images/btns_01.png) no-repeat; background-size:100%100%; color:#bd2610;cursor: pointer; text-align: center; font-weight:bold; font-size:16px}
.title img{width:90%; margin:10px auto}

.banner{width: 100%; margin: 0 auto; position:relative;  z-index:2}
.banner img{ width: 100%;}
/*index*/
.rule{padding:13px 15px;}
.rule p{padding:3px 8px; line-height: 22px; position:relative; z-index:2; color:#ebccff}
.rule p span{color:#c80000;}
.active_time{display:inline-block}
.notice{margin:15px 0; padding:2px; background:#27074e; color:#ebccff}
.notice_list{height:28px; padding:0 15px; line-height:28px;}
.logined{padding:12px 10px; position:relative}
.logined_r{position:absolute; right:10px; top:18px;}
.logined_l p{padding:3px 0 2px; color:#fff}
.login{padding-top:18px; padding-bottom:20px}

.star_list{margin: 8px 0px 10px;}
.star_list span{float: left;width:36px;height:41px;line-height:43px;background: url(../images/star_01.png) 0 0 no-repeat;background-size: 36px 41px; text-align: center; color:#4a055b; font-weight:bold; font-size:12px;}
.star_list span.star_light{background: url("../images/star_02.png") 0 0 no-repeat;background-size: 36px 41px; color: #e9e0f1}
.star_list em{float: left;width:6px;height:22px;background: url("../images/stars.png") 0 0 no-repeat;background-size: 32px 96px;}
.star_list em.spot_gold{background-position:-26px 5px;}
.star_list em.spot_gray{background-position:-26px -22px;}
.star_list em.spot_lightgray{background-position:-26px -46px;}
.lottery_wrap{margin:30px 25px 0; padding:0 0 10px}
.prize_index{position:relative;}
.prize_index p{padding-left:25px; color:#ebccff}
.prize_index p span{padding:0 1px; color:#ffe200; font-size:18px}
.prize_index a{ position:absolute; right:10px; top:22%;}
.prize_index .titles{width:auto; margin-bottom:6px; font-weight:bold; color:#ebccff}
/*九宫格抽奖*/
.lottery_txt {color:#ebccff; padding:10px 0 5px; text-align:center;}
.lottery2{width:300px; margin:10px auto; position:relative;}
.lottery2 img{display:block; width:94px; height:71px; margin:0 auto; border-radius:2px}
.lottery2 table{border-collapse: separate; width:100%;}
.lottery2 td{width:94px;height:71px; padding:3px; position:relative}
.lottery2 td span{position:absolute; left:4px; top:3px; width:92px; height:70%;}
#my_click {width:94px;height:71px; cursor:pointer;}
.lottery2 p{position:absolute; width:94px;height:71px; left:103px; top:80px; display:block;}
.lottery2 p a{display:block; width:94px; height:71px;}
.waitGame{background:url(../images/lottery_disabled.png) no-repeat; background-size:100% 100%}
.active_title{padding-bottom:10px; background:none; font-weight:bold; color:#ebccff; font-size:20px}
.active_ctn{margin:0 0 0 25px}
.active_ctn li{position:relative; line-height:22px; color:#ebccff}
.active_ctn span{width:18px; height:18px; background:#f6ff00; border-radius:50%; position:absolute; top:3px; text-align:center; color:#2c0c40; line-height:18px; box-shadow:0px 0px 4px #ffff0a}
.active_ctn p{margin-left:25px;}
.share_btn{display:block; width:92px; height:28px; margin:20px auto 8px; background:#7d2cbc; border-radius:5px; text-align:center; line-height:29px; color:#1e0329;}
.btns1 a{display:block; margin:10px auto;}
.soft{padding:10px 12px; position:relative}
.soft .titles{width:114px; color:#ebccff; font-weight:bold}
.soft_list{padding:5px 0;}
.soft_list li{ float: left; display: inline; width: 90%; margin:10px 5% 0;}
.soft_list li .soft_icon{ width: 56px;height: 56px; margin-left:2px}
.soft_list li .soft_icon img{ width: 56px;height: 56px;}
.soft_list li .soft_name{ margin-top: 10px;height: 20px; line-height: 20px; overflow: hidden; color:#bbe6ff; white-space:nowrap; overflow:hidden; text-overflow:ellipsis}
.soft_list li .soft_name a{ color: #bbe6ff;}
.soft_list li a.download{position:absolute; right:10px; bottom:54px; }

#active_ctn li{margin-top:20px; color:#cb3731; font-weight:bold}

/*我的奖品*/
.my_prize{padding:15px 0 8%; margin:0 6%; position:relative; color:#ebccff}
.my_prize_list li{line-height:25px; font-size:15px; position:relative;}
.my_prize_list li a {display:block; width: 120px; height: 31px; margin:8px auto 0; background: url(../images/btns_01.png) no-repeat; background-size:100% 100%; /*text-indent:-9999px*/}
.my_prize_list2{padding:5px 0;}
.my_prize_list2 li{ position:relative; font-size:15px}
.my_prize_list2 p{ padding:6px 0;}
.form-items{width:90%; padding:0 4% 30px; margin-top:20px; background:rgba(34,3,46,.6); border:1px solid #b880f1}
.form-items li{ margin-top: 30px; clear: both; overflow: hidden;}
.form-items li label{ float: left;width:90px;padding-top: 10px; color:#ebccff;}
.form-items li span{display:block;margin-left: 90px;}
.form-items li input{ width:90%;padding:10px 6px;border:none;color: #000;  font-family: Microsoft Yahei,"微软雅黑"; background:#ae95c4; border-radius:2px}
.form_error{display:block; padding:8px 0 0 90px; color:red}
.split{height:11px; background:url(../images/line_bg.png) no-repeat; background-size:100% 100%;  margin: 8px 0px 15px; overflow:hidden;}
.my_prize .btns{margin:40px auto 0}
/*结束页*/
.awards_wrap{padding:12px 15px}
.awards{padding:5% 20px 4%; position:relative; color:#fff}
.awards_list{line-height: 26px; text-align:center}
.slide_up{overflow:hidden;height:130px;position: relative;}

/*提示框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-webkit-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{ position:absolute; width:300px; height:246px; left:50%; top:50%; margin-left:-150px; z-index: 99999; background:url(../images/dialog_bg.jpg) no-repeat; background-size:100% 100%; font-size:15px;}
.tip_title h6{ padding:2px 0 20px; min-height:20px; color:#fff; font-size:18px; font-weight:bold; line-height:18px;text-align:center}
.tip-con{ margin: 15px 15px 0 15px; padding:30px 30px 10px 20px; color:#440e0e;}
.tip-con p{padding:0px 0 30px 20px; line-height: 22px; word-wrap:break-word;word-break:break-all; font-size:15px; color:#ebccff}

.footer{width:100%; height:38px; background:#16001e;  text-align:center; position:relative; color:#a663d1; line-height:38px}
.bold{font-weight:bold;}
.red{color:#ab0500}
.prompt_01{color:#fdfbff}
.prompt_02{padding:7px 0 0 90px; color:#ff0000}
.mtop10{margin-top: 10px;}
