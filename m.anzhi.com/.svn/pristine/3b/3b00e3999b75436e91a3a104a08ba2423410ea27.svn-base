/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto;background:url(../images/bg1.jpg) repeat-y; background-size:contain; font-size:15px; font-family:Microsoft Yahei,"微软雅黑"; line-height:25px; color:#4e4e4e}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none; font-size: 14px; outline: none; color:#4e4e4e}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}

.banner img{ width: 100%;}
.main{padding:0 15px 30px; position:relative; z-index:2}
.description h6{padding:0 0 15px; font-weight:bold; text-align:center; font-size:18px}
.btn_01{display:block; width:175px; height:42px; margin:-45px auto 20px; background:url(../images/btn_01.png) no-repeat; background-size:100% 100%; text-indent:-9999px}
.title{margin-top:16px; margin-bottom:10px}
.title h3{font-size:16px; color:#2b8bff; font-weight:bold}
.wrap{margin-top:4px; overflow:hidden}
.wrap h6{padding:10px 0 5px; font-size:16px; font-weight:bold; text-align:center}
.wrap h6 span{color:#550303}
.wrap img{width:100%; margin:10px 0 15px}
.btn_02{display:block; width:114px; height:40px; margin:15px auto 0; border-radius:3px; background:#ff7b75; line-height:40px; text-align:center; color:#fff; font-size:18px; letter-spacing:1.5px}
.disabled{background:#717171; color:#313131; font-weight:bold; text-indent:0; font-size:20px; line-height:42px; text-align:center; letter-spacing:1px}
