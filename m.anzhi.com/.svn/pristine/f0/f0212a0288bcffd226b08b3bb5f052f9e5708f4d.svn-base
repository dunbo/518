/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto;font-size:14px; font-family:Microsoft Yahei,"微软雅黑";background-color: #7a1f10; color: #fc6c23;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color: #fc6c23; font-size: 14px; outline: none;}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.cl{overflow:hidden;}
.clear{clear:both;overflow:hidden;}

.cnt{text-align: center;}
.fl{float: left;}
.fr{float: right}
.wth{width: 95%;}
.mtop{margin-top: 15px;}

section{background:url("http://img4.anzhi.com/static/activity/flyback/body_bg.png") 0 0 repeat-y;background-size: 100% auto;}
.btn_log a,.soft-list li a.downlode,.mod_hd .btn_change,.mod_hd .btn_myprize,.mod_hd span,.btn_submit a{background:url("http://img4.anzhi.com/static/activity/flyback/btns.png") 0 0 no-repeat;background-size: 170px 170px;}

.banner{width: 100%; margin: 0 auto;}
.banner img{ width: 100%;}
/*结束*/
.end{width:100%;position: absolute; top:0px; left: 0;padding: 5px 0px; text-align: center; color:#fff; background-color: #ff9914; font-size: 16px; line-height: 25px;}
.end p{ padding: 0px 5px;}

.mod{position:relative;margin: 20px auto;padding: 10px;border:3px dotted #6f1102;-webkit-border-radius: 20px;-moz-border-radius:20px;border-radius: 20px;}
.mod.mtop{margin-top: 35px;}
.mod p{line-height: 22px;}
.mod_hd{position:relative;margin-bottom:8px;color: #fff;}
.mod_hd span{ float: left;height:37px;line-height: 36px;font-size: 16px;color: #ffb555;font-weight: bold;}
.mod_hd span.hd_l{width:40px;background-position: 0 0;}
.mod_hd span.hd_m{background-position: -40px 0px;}
.mod_hd span.hd_r{width:18px;background-position: -148px 0px;}
.mod_hd .btn_change{ float: right;width:85px;height:32px;line-height:32px;background-position: 0px -108px; color:#7a3300;cursor: pointer;text-align: center;font-size: 16px;font-weight: bold}
.mod_hd .btn_myprize{display:block;position:absolute;right:0px; top:3px;width:100px;height:32px;line-height:30px;background-position: 0px -74px; color:#ffca2f;cursor: pointer;text-align: center;font-size: 16px;font-weight: bold}
.col{margin: 5px auto;}
.prompt{margin-top: 10px;}
/*活动规则*/
.rule .mod_hd{position: absolute;top:-20px;left: -2%;}
.rule{padding-top: 15px;}
.rule dl{clear: both;overflow: hidden;line-height: 24px;}
.rule dl dt{float: left;}
.rule dl dd{margin-left:16px;}
/*公告*/
.notice{position:relative;width: 100%;height: 30px; margin-bottom:10px;line-height: 30px;background:url("http://img4.anzhi.com/static/activity/flyback/notice_bg.png") 0 0 no-repeat;background-size: 100% 100%; color: #fff;  }
.notice_icon{position: absolute;top:-70px;left:0px;width: 100px;}
.notice_icon img{width: 100%;}
.notice_list{width: 80%;margin: 0 auto;overflow: hidden;}
.notice_list span{color: #fff;padding-right: 20px;}

/**/
.main{position:relative;width: 90%;margin: 0 auto;}

/*软件*/
.soft{padding:10px;background-color: #fec520;-webkit-border-radius: 20px;-moz-border-radius: 20px;border-radius: 20px;}
.soft-list{padding:10px;background-color: #6f1102;-webkit-border-radius: 20px;-moz-border-radius: 20px;border-radius: 20px;}
.soft-list li{ float: left; display: inline; width: 25%;margin-top:7px;text-align: center;}
.soft-list li p{}
.soft-list li .soft-icon{text-align: center;}
.soft-list li .soft-icon img{ width: 56px;height: 56px; margin: 0 auto;}
.soft-list li .soft-name{margin-top: 10px;height: 20px; line-height: 20px; overflow: hidden;}
.soft-list li .soft-name a{ color: #ca5b49;}
.soft-list li .soft-size{ color: #f46600;height: 22px; line-height: 22px;overflow: hidden;}
.soft-list li a.downlode{ display: block; width:61px;height: 22px;line-height: 22px; margin: 10px auto; background-position: 0px -147px; color:#ffffff;cursor: pointer;font-size:13px;text-align: center;}
/*登录以后*/
.login{position:relative;padding: 15px 0px;}
.login ul{margin-right:110px;line-height: 22px; }
.login ul.ends{padding: 15px 0px; font-size: 16px;}
.btn_log{position: absolute;top:50%;right:0px;margin-top: -16px;}
.btn_log a{display: block;width: 100px;height: 32px; line-height: 32px;font-size:16px;font-weight:bold;text-align: center; color: #7a3300; background-position:0px -38px ;}
/*九宫格抽奖*/
.lottery{background-color: #6f1102;border-radius: 20px;}
.lottery_inner{position:relative;}
.lottery img{width: 100%;margin: 0 auto;}
.lottery table{border-collapse: separate; width:100%;margin: 0 auto;}
.lottery td{padding:1%;text-align:center;vertical-align:middle;background-color: #db5f4c;border-radius: 30%;}
.lottery p{position:absolute; width:30%;height:30%; left:35%; top:35%;}
.lottery p.waitGame{background: url("http://img4.anzhi.com/static/activity/flyback/lottery_btn_gray.png") 0 0 no-repeat;background-size: 100% 100%;}
.lottery p a{display:block; width:100%; height:100%;background: url("http://img4.anzhi.com/static/activity/flyback/lottery_btn.png") 0 0 no-repeat;background-size: 100% 100%;}

/*联系信息*/
.form-items{margin-bottom:30px;}
.form-items li{ margin-top: 20px; clear: both; overflow: hidden;}
.form-items li label{ float: left;width:120px;padding-top: 7px;}
.form-items li span{display:block;margin-left: 120px;padding:6px;background-color:#fff;}
.form-items li input{width:100%;color: #000;border: none; font-family: Microsoft Yahei,"微软雅黑";background-color:#fff;}
.form-items li.error{padding-left:120px;margin-top: 5px;color: #fc6c23}
.btn_submit{margin: 15px 0px 10px 0px;}
.btn_submit a{ display: block;width: 100px;margin:0 auto;height: 32px; line-height: 32px;font-size:16px;font-weight:bold;text-align: center; color: #7a3300; background-position:0px -38px ;}

/*获奖名单*/
.awards_list{line-height: 22px;}
.awards_list p{padding-bottom: 112px}
.slide_up{overflow:hidden;height:132px;position: relative;}

/*提示框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color:rgba(0,0,0,0.5);z-index: 99998;}
.tip-box{ position:absolute;width: 90%; left:50%;top:50%;margin-left:-45%;padding: 40px 0px 10px 0px; z-index: 99999;background-color: #fec520;border-radius: 20px;}
.tip_clo{position:absolute;right: -10px;top:-10px;z-index: 100000}
.tip_clo a{display: block;width: 40px;height: 40px;background: url("http://img4.anzhi.com/static/activity/flyback/close_icon.png") 0 0 no-repeat;background-size: 100% 100%;text-indent: -9999px;}
.tip-con{position: relative; margin: 0px 10px;padding: 15px; border-radius: 20px;background-color: #6f1102; color:#e78474;z-index: 100000}
.tip-con p{ line-height: 25px;}
.tip-con h4{font-size: 16px;color: #e78474;font-weight: bold;text-align: center}
.tip-icon{position: absolute;top:-28px;left:50%;width:260px;margin-left:-120px;z-index: 99999}
.tip-icon img{width: 100%;}
/*我的奖品*/
.tip_prompt{margin-top: 10px;padding:15px 10px; background-color: rgba(255,255,255,0.2);color: #ffcdc5}
.tip_prompt p{line-height: 25px;}
.tip_prompt p span{color: #fff}
.my_prize_list{margin-top: 10px;padding-bottom:5px;border-bottom: 1px solid #b95849 }
.my_prize_list li{margin:0px 0px 8px 0px;padding:5px; color:#ffcdc5;background-color: rgba(255,255,255,0.2);text-align: center}
/*尾部*/
.footer{width: 100%;padding:15px 0px;background: url("http://img4.anzhi.com/static/activity/flyback/ft_bg.png") 0 0 no-repeat;background-size: 100% 100%;}
.footer p{text-align: center;color: #5d0e00}
