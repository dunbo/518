 #searchBtn {
    display: inline-block;
    margin-bottom: 0;
    margin-right: 5px;
    padding: 4px 10px;
    font-weight: 400;
    text-align: center;
    vertical-align: middle;
    cursor: pointer;
    background-image: none;
    border: 1px solid transparent;
    white-space: nowrap;
    font-size: 14px;
    border-radius: 4px;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    vertical-align: top;
    float: right;
}
.searchBar {
    width: 100%;
    height: 30px;
    margin: 10px 0 15px 0;
    padding: 0;
    border-bottom: 1px solid #eee;
}
#selet {
    margin-right: 5px;
    margin-top: 8px;
    float: right;
}
#selets {
    margin-right: 25px;
    margin-top: 5px;
    float: right;
}
.buttips{
    font-size: 15px;
    text-align: center;
    background-color: #cac8c8;
    color: red;
    height: 30px;
    line-height: 30px;

}
#queueList li .icon {
    cursor: pointer;
    width: 113px;
    height: 113px;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 2;
    border: 0;
    background-repeat: no-repeat;
}
#queueList li .icon:hover {
    width: 107px;
    height: 107px;
    border: 3px solid #1094fa;
}
#queueList li.selected .icon {
    background-image: url(images/success.png);
    background-image: url(images/success.gif)\9;
    background-position: 75px 75px;
}
#queueList li.success .icon {
    background-image: url(images/upload.png);
    background-position: 75px 75px;
}

#queueList li.selected .icon:hover {
    width: 107px;
    height: 107px;
    border: 3px solid #1094fa;
    background-position: 72px 72px;
}
#queueList li.success .icon:hover {
    width: 113px;
    height: 113px;
    border: 0px;
}
#queueList li.error .icon:hover {
    width: 113px;
    height: 113px;
    border: 0px;
}
