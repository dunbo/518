/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto; background:#fff6c3; font-size:16px; font-family:Microsoft Yahei,"微软雅黑"; line-height:20px; color:#636363}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none; font-size: 16px; outline: none; color:#636363}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{float:left;}
.fr{float:right;}
.main{position:relative; padding:0 6px 8px;}
/*有通用的地方*/
.wrap{min-height:20px; padding:15px 0; margin-bottom:20px; position:relative}
.banner{width: 100%; margin: 0 auto; position:relative;}
.banner img{ width: 100%;}
/*index*/
.notice{padding:5px 0 14px; position:relative}
.notice-icon{width:16px; height:20px; left:5px; top:9px;  background: url(../images/icon_01.png) no-repeat 0 0; position:absolute; background-size:16px 20px;}
.notice_list{height:28px; padding:0 15px 0 34px; line-height:28px; font-size:16px}

/*开宝箱*/
.lottery_wrap{margin-bottom:0}
.lottery_info{ padding:12px 0 12px;}
.lottery_info p{text-align:center; color:#356712}
.lottery_info p a{color:#356712}
.lottery_btn3{width:116px; height:52px; margin:15px auto 0; background:url(../images/btns_05.png) no-repeat; background-size:100% 100%; display:block; text-indent:-9999px}
.lottery_disabled{background:url(../images/btns_03.png) no-repeat; background-size:100% 100%;}
.gift{height:240px; position:relative;}
.lottery_btn1{position:absolute; right:10px; top:0; width:94px; height:23px; background:url(../images/txt_02.png) no-repeat; background-size:100% 100%; text-indent:-9999px}
.txt1 span{padding:0 2px; color:#fa5f25; font-size:20px}
.gift_box{width:238px; height:168px; background:url(../images/gift_box.png) no-repeat; background-size:100% 100%; position:absolute; left:50%; margin-left:-119px; bottom:8px; cursor:pointer; z-index:4; -webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.gift_box_disabled{ background:url(../images/gift_box3.png) no-repeat; background-size:100% 100%;}
.gift_box2{width:312px; height:193px; background:url(../images/gift_box2.png) no-repeat; background-size:100% 100%; position:absolute; left:50%; margin-left:-156px; bottom:8px; z-index:2; opacity:0}
.light{width:200px; height:136px; background:url(../images/light.png) no-repeat; background-size:100% 100%; position:absolute; left:50%; margin-left:-100px; top:0px; z-index:3; opacity:0}
.light_open{-webkit-animation:light .6s .1s linear forwards; animation:light .6s .1s linear forwards}
.open{-webkit-animation:open .5s .0s linear forwards; animation:open .5s .0s linear forwards}
.share_btn{display:block; width:93px; height:23px; margin:0 auto; background:url(../images/share_btn.png) no-repeat; background-size:100% 100%; text-indent:-9999px}
.soft_wrap{ padding-bottom:15px}
.soft-list{padding:5px 0 0; margin:10px 0 0;}
.soft-list li{ float: left; display: inline; width: 25%;margin:7px 0px;text-align: center;}
.soft-list li .soft_icon{text-align: center;}
.soft-list li .soft_icon img{ width: 60px;height: 60px; margin: 0 auto; border:4px solid #6eac49; border-radius:20px}
.soft-list li .soft_name{margin-top: 10px;height: 20px; color:#173700; line-height: 20px; overflow: hidden; text-overflow:ellipsis; white-space:nowrap}
.soft-list li .soft_name a{ color: #173700;}
.soft-list li a.download{ display: block; width:70px;height: 30px; line-height: 30px; margin: 13px auto 0; background:url(../images/btns_04.png) no-repeat; background-size:100% 100%; color: #fff;cursor: pointer; font-weight:bold;}
/*我的奖品*/
.prize_list{ margin:20px 0 0; padding:5px 0 20px; background:#fedd7c; border:2px solid #9b6813; border-radius:15px; color:#636363; position:relative}
.prize_list_inner{width:90%; margin:0 auto;}
.prize-title{padding:10px 10px 15px; font-size:18px; color:#f66032}
.prize-text{padding:6px 10px 10px; background:#fceaa0; line-height:28px; font-size: 16px;}
.prize-text li{padding:0 0 0 50px; position:relative; line-height:30px}
.prize-text li span{position:absolute; left:0; top:0; width:50px; height:30px;}
.prize_btns{display:block; width:40px; height:20px; background:url(../images/txt_01.png) no-repeat; background-size:100% 100%; position:absolute; right:8%; top:16px; text-indent:-9999px}

/*detail 活动规则*/
.detail_wrap{ margin:0 0 20px; padding:15px 10px; position:relative; color:#646464; font-size:16px}
.detail_list{margin:10px 0 0}
.detail_list h4{padding:5px 10px 6px; font-size: 18px; font-weight: bold;}
.detail_list  p{padding:0 10px; line-height: 25px;}
.detail_ctn{ margin-bottom:10px;}
.detail_ctn li{position:relative; padding:0 0 0 10px; line-height:22px;}
.detail_ctn span{width:14px; position:absolute; top:1px;}
.detail_wrap .prize-text li{padding:0 0 0 70px;}
.prize-text li span{width:70px;}
.detail_wrap .line1{margin:12px 0 8px}
.tips_p img{display:block; width:136px; height:120px; margin:10px auto 40px; padding:15px 0; overflow:hidden}
.tips_p p{text-align:center;}
.tips_p a,.tip-box a{display:block; margin:0 auto 20px; width:127px; height:52px; background:url(../images/update_btn.png) no-repeat; background-size:100% 100%; text-indent:-9999px}
.tips_p a.end_btn{background:url(../images/end_btn.png) no-repeat; background-size:100% 100%;}
.rule_btn{display:block; width:70px; height:28px; margin:0 auto; border-bottom:1px solid #fa5f25; text-align:center; color:#fa5f25; font-size:16px; font-weight:bold; line-height:28px;}
.footer{padding:0 0 20px; color:#636363; text-align:center}
/*提示框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{position:absolute; width:90%; height:160px; padding:20px 0; background:#fedf4c; border:2px solid #fdbc33; border-radius:20px; left:50%; top:50%; margin-left:-45%; z-index: 99999;}
.tip-mid{padding:30px 0 20px;}
.tip-con{width:80%; padding:8px 15px;margin:0px auto; background:url(../images/dialog_bg2.png) no-repeat; background-size:100% 100%;}
.tip-mid img{width:160px; margin:-50px auto 0}
.tip-con p{line-height: 22px; text-align: center; color:#f66032;}
.clo{ position: absolute; right: -1px; top:-1px;}
.clo a{ display: block; width:36px; height:36px;background:url("../images/tip_clo.png") 0 0 no-repeat;background-size:100% auto;text-indent: -9999px;}
.tip-box a{margin-top:0}
#tip-box3 a.btns{margin-top:20px}
#tip-box1 a.btns{background:url(../images/btns_02.png) no-repeat; background-size:100% 100%;}
#tip-box2 a.btns{background:url(../images/btns_01.png) no-repeat; background-size:100% 100%;}
#tip-box3 a.btns{background:url(../images/btns_03.png) no-repeat; background-size:100% 100%;}
#tip-box4 a.btns{background:url(../images/btns_06.png) no-repeat; background-size:100% 100%;}
#tip-box5 a.btns{background:url(../images/btns_07.png) no-repeat; background-size:100% 100%;}
.form_title{width:116px; height:48px; background:url(../images/form_bg.png) no-repeat; background-size:100% 100%; position:absolute; left:50%; margin-left:-58px; top:-24px; text-align:center; line-height:48px; color:#fff6b6; font-size:18px; font-weight:bold}
/*邮寄地址*/
.form1{width:90%; margin:12px auto 0; font-size:14px;}
.form-item{ margin:12px 0 0; }
.form-item label{ float: left;width:55px; padding:5px 0; color:#173700}
.form-item .textbox input{float:left; width:60%; padding:5px 12px; background:#fff6b6; border:2px solid #f9d530; border-radius:8px;font-family: "Microsoft Yahei"; color:#636363;}
.prompt1{color:#f66032; font-size:18px; text-align:center; font-weight:bold}
.prompt2{padding:5px 0 0;}
.prompt3{padding-top:6px; text-align:center; color:#ff620c; font-size:14px}
/*抓精灵*/
.game1{width:100%; height:100%; background:url(../images/bg2.png) no-repeat; background-size:100% 100%; position:relative}
.game1_01{position:absolute; left:0; top:3%; width:100%;}
.game1_01 img{display:block; width:100%}
.begin_btn,.lottery_go{width:130px; height:62px; margin:15px auto 0; background:url(../images/btns_09.png) no-repeat; background-size:100% 100%; display:block; text-indent:-9999px; position:absolute; left:50%; bottom:20%; margin-left:-65px;}
.lottery_go{background:url(../images/btns_06.png) no-repeat; background-size:100% 100%;}
.page_btn{width:60px; height:23px; background:url(../images/page_01.png) no-repeat; background-size:100% 100%; display:block; text-indent:-9999px; position:absolute; left:50%; bottom:1.2%; margin-left:-30px;}
.mark{width:100%; height:100%; background:rgba(0,0,0,.4); position:absolute; left:0; top:0; display:none}
.mark img {position:absolute; width: 110px; height:130px; left:50%; top:50%; margin-left:-55px; margin-top:-65px; animation:num_ac .4s; -webkit-animation:num_ac .4s; z-index:99}
.mark img.go{width:92px; height:47px; margin-left:-46px; margin-top:-23px}
.game2{width:100%; height:100%; background:url(../images/bg3.png) no-repeat; background-size:100% 100%; position:relative; overflow:hidden}
.spirte_wrap{ width: 100px; height: 100px; background-image: url(../images/1.png); background-repeat: no-repeat; background-position: center; background-size:100% 100%; position: absolute; left:0px; top:0px; z-index: 200;-webkit-tap-highlight-color:rgba(255,0,0,0);}
.addnum{width:100px; height:20px; background:#fff; border-radius:15px; position:absolute; right:-10px; top:-10px; text-align:center; line-height:20px;animation:sprite_ac .6s; -webkit-animation:sprite_ac .6s; font-size:14px; z-index:100}
.addnum em{color:#ff0505; font-style:normal}
.game2 p{width:100%; text-align:center; position:absolute; left:0; bottom:1%}
.game2 p span{padding:0 2px; color:#fa5f25; font-size:20px; font-weight:bold}
@keyframes num_ac{
	0%{transform:scale(3)}
	100%{transform:scale(1)}
}
@-webkit-keyframes num_ac{
	0%{-webkit-transform:scale(3)}
	100%{-webkit-transform:scale(1)}
}
@keyframes sprite_ac{
	0%{transform:scale(2)}
	100%{transform:scale(1)}
}
@-webkit-keyframes sprite_ac{
	0%{-webkit-transform:scale(2)}
	100%{-webkit-transform:scale(1)}
}
@keyframes open {
    0%{opacity:0;}
    100%{opacity:1;}
}
@-webkit-keyframes open {
      0%{opacity:0;}
    100%{opacity:1;}
}
@keyframes light{
    0%{transform:scale(0.1,0.1);opacity:0;}
	70%{transform:scale(1,1); opacity:1;}
	90%{transform:scale(1.2,1.2); opacity:1;}
    100%{transform:scale(1,1);opacity:0;}
}
@-webkit-keyframes light{
    0%{-webkit-transform:scale(0.1,0.1);opacity:0;}
	70%{-webkit-transform:scale(1,1); opacity:1;}
	90%{-webkit-transform:scale(1.2,1.2); opacity:1;}
    100%{-webkit-transform:scale(1,1);opacity:0;}
}