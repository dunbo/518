<include file="../../Public/header"  />
<link type="text/css" rel="stylesheet" href="/Public/css/tablelist.css" />
<script type="text/javascript" src='/Public/js/thickbox-compressed.js' ></script>
<link type="text/css" rel="stylesheet" href="/Public/css/thickbox.css" />
<style>
a {
color:blue !important;
}
</style>
<body style="overflow-x: hidden; overflow-y: auto; ">

<div class="tags_s{$_GET['tagid']}">

<div>{$parent.tagname|default=' '}（共{$parent.count|default='0'}个） </div>
<table border="0" cellpadding="0" cellspacing="0" class="table_list">
	<thead>
		<tr>
			<th style="width:50px;"><a href="__URL__/secondtaglist/tagid/{$_GET['tagid']}/rankorder/{$rankorder}?width=1000" class="thickbox">优先级↓↑</a></th>
			<th style="width:70px;"><a href="__URL__/secondtaglist?tagid={$_GET['tagid']}&width=1000&idorder={$idorder}" class="thickbox">标签ID↓↑</a></th>
			<th style="width:100px;">标签名称</th>
			<th >title</th>
			<th style="width:200px;">描述</th>
			<th >关键字</th>
			<th style="width:50px;">TDK配置</th>
		</tr>
	</thead>
	<volist name="result" id="tags">
	<tr>
		<td>{$tags.rank|default=''}</td>
		<td>{$tags.tagid|default=' '}</td>
		<td>{$tags.tagname|default='&nbsp;'}</td>
		<td>{$tags.title|default='&nbsp;'}</td>
		<td><div style="max-height:100px;overflow-y:auto">{$tags.description|default='&nbsp;'}</div></td>
		<td>{$tags.keywords|default='&nbsp;'}</td>
		<td><a href="__URL__/edit?tagid={$tags.tagid}" class="thickbox">编辑</a></td>
	</tr>
	</volist>
	<tr><td colspan="7" > <a style="margin: auto;" href="__URL__/taglist/" class="search_btn">返回上级列表</a></td>
    
	</tr>
</table>
<script>
</script>
</div>