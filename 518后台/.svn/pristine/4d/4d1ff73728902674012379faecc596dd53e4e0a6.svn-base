<script type="text/javascript" src="__PUBLIC__/school/js/jquery-1.6.2.min.js"></script>
<script type="text/javascript">
$(document).ready(function(){
	$.ajax({
		url: '__URL__/apk_upload',
		type: 'GET',
		//dataType: 'json',
		var str = '';
		success: function(result_json){
			alert("fjdjfhjsdfhjdshf");
			if(result_json == 0){
				str = "<span style='color: #f00'>所上传apk在市场中不存在，可进行软件压缩包上传！</span><br /><br /><span>3 秒钟后自动跳转至压  缩包上传页面  ！</span>";
			}else if(result_json == 1){
				str = "<span style='color: #f00'>所上传apk低于市场中存在的版本，不可进行软件压缩包上传！</span><br /><br /><span>3 秒钟后自动跳转至压  缩包上传页面  ！</span>";
			}else if(result_json == 2){
				str = "<span style='color: #f00'>所上传apk与市场中存在的版本相同，可进行软件替包上传！</span><br /><br /><span style='color: #f00'>替包操作，请慎重！确保与开发者进行沟通后，同意有我们对此软件进行替换后再执行此操作！！！</span>";
			}else if(result_json == 3){
				str = "<span style='color: #f00'>所上传apk高于市场中存在的版本，可进行软件更新上传！</span><br /><br /><span style='color: #f00'>更新操作，请慎重！确保与开发者进行沟通后，同意有我们对此软件进行替换后再执行此操作！！！</span>";
			}
			$('#prompt').html(str);
		}	
	})
})

</script>
<div class="main" >
<div class="content">
<center>
<form method='post' action='' enctype="multipart/form-data" name="push_form">
APK上传：<input size='50' type='file' name='apkname' onChange="javascript:push_form.submit();"/><input type="hidden" name="key" id="key" value="{$apk_dir}" >&nbsp;&nbsp;&nbsp;
<br /><strong>请选择想要上传的APK文件</strong><br /><br />
</form>
<div>
	<div id="prompt"></div>
	<div></div>
</div>
</center>
</div>
</div>
