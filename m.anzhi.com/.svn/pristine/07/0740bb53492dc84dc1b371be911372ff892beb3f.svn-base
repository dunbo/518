<script type="text/javascript">
var sid='<!--{$out.sid}-->';
var aid='<!--{$out.aid}-->';
TouchSlide({ 
	slideCell:"#focus_01",
	titCell:".focus_dotted ul", //开启自动分页 autoPage:true ，此时设置 titCell 为导航元素包裹层
	mainCell:"#focus_slide ul", 
	autoPage:true, //自动分页
	pnLoop:"false",
	autoPlay:true,
	interTime:3000
});


function onLoginComplete(sid,tip){
	if(tip == "登录成功"){
		sid_str = sid.replace("PHPSESSID=", "");
		location.href="/lottery/xy2/yd_index.php?sid="+sid_str+"&aid=<!--{$out.aid}-->";
	}
}

function sign(){
	$.ajax({
		url: '/lottery/xy2/yd_sign.php',
		data: 'sid='+sid+'&aid='+aid,
		type: 'post',
		dataType: 'json',				
		success: function(data){
                    if(data==-1){
                        alert('活动已结束');
                    }else if(data==2){
                        alert('页面已失效,签到失败，请重新进入活动页面');
                    }else if(data==1){
                        showOpenBox('#tip-box3')
                    }
		}
	});
}
</script>
