/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto; background:#2f83cb; font-size:15px; font-family:Microsoft Yahei,"微软雅黑"; line-height:20px; color:#1d1f21}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none; font-size: 14px; outline: none; color:#1d1f21}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{float:left;}
.fr{float:right;}
.main{position:relative; padding:0 12px 12px; background-image: -webkit-linear-gradient(top, rgba(54, 135, 202, 1) 0%, rgba(47, 131, 203, 1) 100%); background-image: -moz-linear-gradient(top, rgba(54, 135, 202, 1) 0%, rgba(47, 131, 203, 1) 100%); background-image: -o-linear-gradient(top, rgba(54, 135, 202, 1) 0%, rgba(47, 131, 203, 1) 100%); background-image: -linear-gradient(top, rgba(54, 135, 202, 1) 0%, rgba(47, 131, 203, 1) 100%);}
.main2{padding:0;}
.main_inner{padding-bottom:40%; background:url(../images/footer.png) no-repeat left bottom; background-size:100% auto}
/*有通用的地方*/
.wrap{min-height:20px; padding:15px; margin-bottom:20px; position:relative}
.btns{display: block; width:102px;height: 33px; margin-left:auto; margin-right:auto; line-height: 33px; background:url(../images/btns_01.png) no-repeat; background-size:100%100%; color:#fff;cursor: pointer; text-align: center; font-weight:bold; font-size:16px; text-shadow:-1px -1px 1px rgba(0,0,0,.5)}
.banner{width: 100%; margin: 0 auto; position:relative;}
.banner img{ width: 100%;}
/*index*/
/*活动规则*/
.rule{color:#fff;}
.rule p{line-height: 24px; color:#d0f0ff}
.rule p.rule_title{ margin-top:6px; font-weight:bold; color:#d0f0ff; font-size:18px}
.rule a{float:right; padding:1px 7px; border:1px solid #5fb6fa; border-radius:10px; display:inline; color:#5fb6fa; text-align:center;}

.wrap2{border:6px solid #dbf0fa; margin:-10px 15px 20px; padding:0; background:#3483c3; border-radius:10px}
.wrap_inner{margin:0 0 6px; background:#4aa8e4; border-radius:4px 4px 8px 8px}
.logined{padding:12px 10px; position:relative}
.logined_r{position:absolute; right:10px; top:8px;}
.logined_l p{padding:3px 0 2px; color:#fff}
.login{padding-top:15px; padding-bottom:20px}

.notice{margin:0 15px 20px; padding:2px; background:#dbf0fa; border-radius:8px; color:#2f83cb}
.notice_list{height:28px; padding:0 15px; line-height:28px;}

/*转盘抽奖*/
.lottery_info{ padding:0 0 12px;}
.lottery_info p{float:left; padding:5px 0 0 12px; font-size:15px; color:#d0f0ff}
.lottery_info a{margin-right:8px; display:inline;background: #0f6abb; border-bottom: 4px solid #0a579b; border-radius: 15px; color:#9fcfec; line-height:34px}
.rotate_tips{padding:0 0 8px; text-align:center;}
.rotate {width: 300px; margin:0 auto;position: relative;}
.rotate-pointer {position: absolute;top: 50%;left: 50%;width: 46%;height: 46%;margin-top: -23%;margin-left: -23%;background: url(../images/pointer.png) no-repeat 0 0;background-size: 100% 100%; cursor: pointer;-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.pointer-disabled{background: url(../images/pointer_disabled.png) no-repeat 0 0;background-size: 100% 100%; z-index:11}
.rotate_list{width:60px; height:78px; position:absolute; left:0; top:0; left:50%; top:50%; z-index:10}
.rotate-con{position:relative;}
.rotate-con i{display:block; width:152px; height:118px; position:absolute;}
.rotate-con i.l{left:-10px; top:-10px; background:url(../images/icon_01.png) no-repeat; background-size:100% 100%}
.rotate-con i.r{right:-10px; bottom:-10px; background:url(../images/icon_02.png) no-repeat; background-size:100% 100%}

.soft_wrap{border:1px solid #052c4f}
.soft{padding:10px 12px; position:relative}
.soft .titles{width:114px; height:16px; background:url(../images/txt1.png) no-repeat; background-size:100% 100%;}
.soft_list{padding:5px 0;}
.soft_list li{ float: left; display: inline; width: 90%; margin:10px 5% 0;}
.soft_list li .soft-icon{ width: 56px;height: 56px;padding:0 12px;}
.soft_list li .soft-icon img{ width: 56px;height: 56px;}
.soft_list li .soft-name{ width:80px; margin-top: 10px;height: 20px; line-height: 20px; overflow: hidden; color:#bbe6ff; white-space:nowrap; overflow:hidden; text-overflow:ellipsis; text-align:center}
.soft_list li .soft-name a{margin-left:25px; color: #bbe6ff;}
.soft_list li a.downlode{display: block; width:112px;height: 40px; margin-left:auto; margin-right:auto; line-height: 36px; background:url(../images/btns_03.png) no-repeat; background-size:100%100%; color:#fffbbf;cursor: pointer; text-align: center; font-weight:bold; font-size:16px; text-shadow:-1px -1px 1px rgba(0,0,0,.5); position:absolute; right:10px; bottom:54px; -webkit-tap-highlight-color:rgba(255,0,0,0);}
.share_btn{display:block; width:136px; height:38px; background:#0e6abb; border:1px solid #052c4f; margin:30px auto 0; border-radius:20px; line-height:38px; text-align:center; color:#9fcfec; font-size:16px; font-weight:bold}
.soft_wrap i{display:block; width:9px; height:6px; background:url(../images/icon_03.png) no-repeat; background-size:100% 100%; position:absolute; top:-3px;}
.soft_wrap i.l{left:-1px}
.soft_wrap i.r{right:-1px}
.wrap3{margin:60px 15px 20px;background:#d0f0ff; padding:5px 5px 30px;  border-radius:10px}
.wrap3_inner{background:#3483c3; padding-bottom:6px; border-radius:5px}
/*我的奖品*/
.my_prize{padding:15px 15px 0; background:#4aa8e4; position:relative; color:#d0f0ff; border-radius:5px}
.my_prize_list li{line-height:25px; font-size:15px; position:relative;}
.my_prize_list li a {display:block; width: 106px; height: 32px; margin:8px auto 0; background: #0f6abb; border-bottom:4px solid #0a579b; border-radius:15px; text-align:center; line-height:32px; color:#9fcfec; font-size:16px; font-weight:bold}

.form-items{width:90%; padding:0 4% 30px; margin-top:20px;}
.form-items li{ margin-top: 30px; clear: both; overflow: hidden;}
.form-items li label{ float: left; width:90px; padding-top: 8px; color:#d0f0ff;}
.form-items li span{display:block; margin-left: 90px;}
.form-items li input{ width:90%; padding:8px 6px; border:none; color: #000; font-family: Microsoft Yahei,"微软雅黑"; background:#fff;}
.form_error{display:block; padding:8px 0 0 90px; color:red}
.split{height:8px; background:url(../images/line_bg.png) repeat-x; background-size:auto 100%;  margin: 35px 0px 15px; overflow:hidden;}

.btns2{width:242px; margin:20px auto 0}
.btns2 a.btns{float:left; width:97px; height:30px; margin-left:10px; margin-right:10px; line-height:30px}
.title{padding:5px 0 15px; text-align:center}
/*结束页*/
.awards_wrap{background:#d0f0ff; border-radius:15px; padding:12px 15px 30px;}
.awards_wrap .titles{padding-top:10px; font-size:18px; font-weight:bold; color:#2f83cb; text-align:center}
.awards{padding:3% 20px 3%; position:relative; color:#2f83cb;}
.awards_list{line-height: 26px; text-align:center}
.slide_up{margin-top:20px; overflow:hidden;height:130px;position: relative;}
.footer{width:100%; height:38px; padding-top:62px; background:url(../images/footer.png) no-repeat; background-size:100% 100%;  text-align:center; position:relative; color:#2f83cb;}
.bold{font-weight:bold;}
.red{color:#ab0500}
.prompt_01{color:#d0f0ff}
.prompt_02{padding:7px 0 0 90px; color:#ff0000}
.prompt_03{color:#001e33}

/*提示框*/
#body-bg{position:absolute; top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-webkit-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{ position:absolute;width: 90%; left:50%;top:50%; margin-left:-45%; z-index: 99999; font-size:15px; color:#ffd274; font-size:16px}
.tip-box .wrap{background:url(../images/tips_bg.png) no-repeat; background-size:100% 100%}
.tip-con{ margin: 15px;padding: 15px 5% 75px;}
.tip-con h4{font-size:16px; font-weight:bold; text-align:center}
.tip-con p{ line-height: 20px; text-align:center;word-wrap:break-word;word-break:break-all; color:#d0f0ff}
.tip-con p.tip_h{font-weight:bold; font-size:18px; line-height:34px}
.btns_false{position:absolute; left:50%; bottom:35px; margin-left:-51px;}
