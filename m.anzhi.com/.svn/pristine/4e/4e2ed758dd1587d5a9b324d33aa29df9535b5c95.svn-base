/* CSS Document */
html,body,div,span,img,ul,dl,li,dd,dt,p,h1,h2,h3,h4,form, input, textarea, p, th, td, menu,a{ border:0; margin:0; padding:0;  font-family: Arial,Helvetica,sans-serif;font-size: 14px;}
.m{ margin:0 5px; }
.left{ float:left;}
.right{ float:right;}
.clear{ clear:both;}
.top{ height:28px;background: -moz-linear-gradient(center top , #6495c1 0%,  #5283ae 100%) repeat scroll 0 0 transparent; background-color:#5786b5; position:relative;}
.top span,.top span a{ color:#FFFFFF; font-size:18px;  line-height:28px; padding-left:8px; text-shadow:1px 1px 1px #5786b5; text-decoration:none;}
.top span#return{ position:absolute; right:5px; top:0;}
.header{ height:75px; position:relative;border-bottom:1px solid #94bf63;background:url(../image/bg06.png) repeat-x scroll transparent;margin-bottom:10px; }
.logo{padding:8px 0 0 0;}
.logo img{}

.bigbox{height:35px;background: -moz-linear-gradient(lfet top , #6fc414 0%, #549e00 84%, #519800 100%) repeat scroll 0 0 transparent;   background:url(../image/bg07.png) repeat-x scroll transparent;padding:0 5px 10px 10px;}
nav{ position:relative;padding-top:12px;}
nav a{width:20%;height:32px; font-size:14px; position:relative; display:inline-block; text-align:center;  text-decoration:none;font-weight:bold; position:relative;}
nav a em{ position:absolute;width:14px; height:7px; background:url(../image/bg09.png) no-repeat scroll transparent; overflow:hidden; bottom:0; left:42%; display:none; }
nav a  span{height:26px; line-height:26px; display:inline-block;color:#569f08;}
nav a.selected em{display:block;}
nav a.selected  span{ display:block;  border-radius:5px;background:url(../image/bg08.png) repeat-x scroll transparent; height:26px; color:#FFFFFF;line-height:26px;}
.header .downimg{ width:68px; height:28px; line-height:28px; position:absolute; right:10px; top:25px;  font-size:17px;background:url(../image/d.png) no-repeat scroll;}
.free{ width:68px; height:28px; line-height:28px; position:absolute; right:10px; top:25px;  font-size:17px;background:url(../image/down_s.png) no-repeat scroll;}
/*.header .downimg:hover,.free:hover{ background:url(../image/down_s.png) no-repeat scroll;}*/
/*.header .downimg{ font-weight:bold;}*/

.hot{ position:absolute;top:-6px; right:4%;}
.sbox{position:relative;height:30px;margin-right:5px; margin-bottom:10px;}
.sch{height:100%;margin-right:50px; margin-left:10px; box-shadow:inset 0 2px 2px rgba(47, 0, 0, 0.4) ;border-color: #75c51b ;border-style: solid; border-width: 1px 0 1px 1px;border-radius:5px 0 0 5px; }
#bbs{background:none;}

.search_text{ padding:4px 0; color:#adadad;font-size:16px;font-family:"宋体";  border-radius:5px 5px 5px 5px;  width:100%; background-color:transparent;}
.s_btn{  width:53px; height:29px; line-height:28px; background:url(../image/ss.png) no-repeat scroll transparent;  cursor:pointer;font-size:14px;color:#323333; position:absolute; right:0; z-index:2;top:0; border:1px solid #75c51b;border-radius:5px;font-size:14px; text-align:center;font-family:"黑体";}

.imgbox{ margin:0 auto; text-align:center;}
.imgbox img{width:225px; height:315px; margin-right:5px;}
.tj{ height:40px; overflow:hidden; border-style:solid; border-width:1px 0;border-color:#ededed;border-radius:5px 5px 0 0;}
.t_center{ height:69px; width:100%; position:relative; overflow:hidden;}
#dotimg{ position:absolute; top:17px; left:10px;}
.t_center h2{ position:absolute; height:22px; line-height:22px; color:#349800; font-size:16px;left:26px; top:10px; font-weight:normal;}
.list ul{ list-style:none; margin:0; padding:0; }
.list li{border-bottom:1px solid #ededed; height:80px; position:relative; padding-left:80px;}
.list li .icon{position:absolute; top:10px; left:10px; height:60px; line-height:60px; text-align:center; vertical-align:middle;}

.list .stars{position:absolute; top:50px; right:12px;width:70px; height:12px;}
.downbox{position:absolute; right:12px; top:20px;width:70px; height:22px; background: -moz-linear-gradient(center top , #95e039 0%,  #6dcf00 100%) repeat scroll 0 0 transparent; background-color:#89db29;border:1px solid #52a318;  border-radius:5px;}
.down_item,.header .downimg,.free{ display:block; color:#FFFFFF;text-shadow: 1px 1px 1px #0963ae; text-decoration:none; text-align:center;}
.down_item{ line-height:22px;}

.list ul li h3{font-size:16px;color:#323232; height:20px; line-height:20px;overflow:hidden; font-weight:normal; white-space: nowrap;text-overflow:ellipsis; padding:10px 90px 5px 0; }
.list ul li h3 a{ text-decoration:none;color:#323232;}
.list ul li .info{ padding:0 100px 0 65px; position:relative; line-height:20px;}
#type{left:0; top:0; overflow:hidden; white-space:nowrap; position:absolute; }
#date{left:0; top:20px; overflow:hidden;white-space:nowrap;position:absolute;  }
.os,.mb{ overflow: hidden; white-space: nowrap;text-overflow:ellipsis;margin-left: -10px;}
#type,#date,.os,.mb{ font-size:12px; color:#999999; height: 18px; line-height:18px;}
.footer{  border:none;height:114px; padding-top:8px;  } 
.pageline{height:32px; margin:0 auto 6px;background-color:#e9ecf1; border-top:2px solid #dde1e4; border-radius:5px; width:98%;}
.p_center{width:100%; height:30px;  position:relative;text-align:center; line-height:30px; color:#666867; font-size:16px;}
.pre{ display:block; position:absolute; width:70px; height:22px; background:url(../image/pre.png) no-repeat scroll;top:3px;left:5px; text-indent:-9999px;}
.pre:hover{background:url(../image/pre_s.png) no-repeat scroll;}
.next{ display:block; position:absolute; width:70px; height:22px; background:url(../image/next.png) no-repeat scroll;top:3px;right:5px;text-indent:-9999px;}
.next:hover{ background:url(../image/next_s.png) no-repeat scroll;}

.footerinfo{ text-align:center; color:#717171; font-size:14px; height:20px; line-height:20px;}
.btnline{ height:30px; line-height:30px; position:relative; width:98%; margin:0 auto;}
.btnline a{ text-decoration:none; color:#4d7ad7; font-size:20px; }
.suggest{ position:absolute; left:0;}
.returntop{ position:absolute;right:0;}

.titlebar{ height:40px;border-color:#d9d9d9;border-width:1px 0;border-style:solid; background-color:#eeeeee; border-radius:5px 5px 0 0; overflow:hidden; position:relative;}
.titlebar div{width:33%; text-align:center;font-size:22px; color:#329900; line-height:40px;  height:40px; }
.titlebar div a{color:#909092;text-decoration:none; display:block; width:100%; height:40px;font-size:16px;}
.titlebar div a:hover{background-color:#ffffff; color:#329900;}
.titlebar .selected a{ background-color:#ffffff;color:#329900;} 
.bright{background-color:#d9d9d9;height:40px;width:1px; position:absolute; top:0; margin:0; padding:0;}
#first{left:33%;}
#second{left:66%;}
.titlebar div.w_34{width:34%;}


.subject{ height:auto; overflow:hidden;}
.subject ul{ list-style:disc; list-style-position:inside; color:#707070;}
.subject ul li{ height:50px; border-style:solid; border-width:0px 0 1px; border-color:#ededed; line-height:50px;font-size:22px; position:relative; padding-left:15px;}
.subject ul li a{ text-decoration:none; color:#707070;width:100%; height:50px;display:block; }
.subject ul li span{ position:absolute;  top:0;left:35px; cursor:pointer; display:block;}
.subject .more{ display:block; position:absolute; width:22px; height:22px; right:23px; top:15px; background:url(../image/more.jpg) no-repeat scroll transparent;cursor:pointer;}
.subject ul li span:hover{text-decoration:underline;}
/*.subject .more:hover{ background:url(../image/more_s.gif) no-repeat scroll transparent;}*/
.subject .border_l{ width:5px; height:69px; background:url(../image/border07_l.gif) no-repeat scroll; position:absolute;top:-1px; left:-1px;}
.subject .border_r{width:5px; height:69px; background:url(../image/border07_r.gif) no-repeat scroll; position:absolute;top:-1px; right:-1px;}
.subject li.bordertop{border-top:1px solid #ededed; font-size:16px;}
.bordertop{border-top:1px solid #ededed; }
.head{height:85px; padding-top:14px;overflow:hidden; position:relative;}
.headimg{ height:70px; width:70px; padding-right:8px; padding-left:10px;}
.title{height:24px;line-height:24px; color:#333333;  font-size:16px; position:absolute; top:14px;left:90px; }
.address{top:43px;left:90px;position:absolute;}
.star{ width:116px; height:16px; position:absolute;top:68px; left:90px;}
.head .number{ width:150px;font-size:14px; color:#747474; position:absolute; top:68px;left:170px; line-height:14px; height:14px; padding-right:90px; overflow:hidden; white-space:nowrap; text-overflow:ellipsis;}
.free{ right:0px;top:19px;font-size:14px;}

.goindex,.softinfo{ background-color:#999999; color:#FFFFFF;font-size:14px;}
.goindex{text-indent:10px;height:30px; line-height:30px; }
.goindex a{ text-decoration:none; color:#ffffff;}
.softinfo{ text-align:center;height:25px; line-height:25px; }
.des{ overflow:hidden; height:auto; white-space:normal;}
.des p{ font-size:16px; color:#676767; line-height:150%; text-align:left; }
.des p.alert{ color:#ff0000;}
.btn{ width:100%; text-align:center; line-height:16px; font-size:16px; height:16px; display:block; margin:10px 0;}
.btn a{color:#99cd00; text-decoration:none;}
.btn a:hover{ text-decoration:underline;}
.noborder{ border-left:none; border-right:none; border-top:1px solid #ededed;}
.js{ height: 45px; padding:4px; width: 80%; border:1px solid #000000;}
.des p.ts{padding:10px 0;}
.inp{width:80%;border:1px solid #000000; height:25px; line-height:25px;}
.des form{ text-align:left;}
.sb{ width:70px;  border: 1px solid #c2c2c2;border-radius: 3px 3px 3px 3px; margin:10px 0; height:25px; background-color:#f3f3f3; display:block;}
.notop{border:none;}