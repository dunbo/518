/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto;font-size:14px; font-family:Microsoft Yahei,"微软雅黑"; background-color: #ffffff; color: #5b5b5b;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#730b91; font-size: 14px; outline: none;}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
::-webkit-input-placeholder { color:#666666;}
:-moz-placeholder {color:#666666;}
::-moz-placeholder {color:#666666;}
:-ms-input-placeholder {color:#666666;}


.banner{width: 100%; margin: 0 auto;}
.banner img{ width: 100%;}

.step{padding: 0px 10px;height: 50px;line-height: 50px;font-size: 16px;border-bottom: 1px solid #e6e6e6}
.step.b_none{border-bottom: 0;}
.step span{padding:0px 6px 1px 6px;background-color: #b0e651;color: #fff;border-radius: 50%;}
.app{}
.app li{float: left;padding:20px 0px;position:relative;width: 50%;border-bottom:1px solid #e6e6e6 }
.app li .app_icon{float: left;padding-right: 5px;padding-left: 10px;}
.app li:nth-of-type(even) .app_icon{padding-left: 0px;}
.app li .app_icon img{width: 50px;height: 50px;}
.app li .app_info{}
.app li .app_info h4{font-size: 17px;height:22px;overflow: hidden;}
.app li .app_info p{margin-top: 10px;}
.app li .app_info p span{color: #a5a5a5;}
.app li .app_info p a{position:absolute;right:10px;bottom:20px;display:block;padding: 1px 6px;border: 1px solid #19d5fd;color: #19d5fd;border-radius: 2px;}
.app li:nth-of-type(odd) p a{right:2%;}

.input_box{margin:0px 10px;padding:10px;border:1px solid #e6e6e6;}
.input_box input{width:100%;border:0;background-color:#fff;font-family: Microsoft Yahei, "微软雅黑";font-size: 14px;}
.prompt{padding: 10px 10px 0px 10px;color:#f07a27}

.btns{margin: 15px 10px;}
.btns a{display: block;width: 100%;height: 35px;line-height: 35px;background-color:#19d5fd;color: #fff; text-align: center;border-radius: 3px;}
.btns a.btn_top{width: 50%;margin: 0 auto;}
.btns a.btn_gray{cursor: default;background-color: #c8c8c8}

.rule{margin: 10px;padding: 10px;border:1px solid #f07a27;border-radius: 7px}
.rule h4{color: #f07a27;font-size: 16px;}
.rule p{margin-top:10px;line-height: 22px;color: #a5a5a5;}

.loading{padding: 10px 0px;}
.loading p{margin-top:10px;color: #a5a5a5;text-align: center;font-size: 16px;}
.loading p img{width: 45px;height: 45px;margin:0 auto; -moz-animation:rotate 1s ease-in-out infinite;-webkit-animation:rotate 1s ease-in-out infinite;animation:rotate 1s ease-in-out infinite; }
@-moz-keyframes rotate{
    0%{ -moz-transform:rotate(0deg);}
    100%{-moz-transform:rotate(-360deg);}
}
@-webkit-keyframes rotate{
    0%{ -webkit-transform:rotate(0deg);}
    100%{-webkit-transform:rotate(-360deg);}
}
@keyframes rotate{
    0%{ transform:rotate(0deg);}
    100%{transform:rotate(-360deg);}
}
.success{margin: 20px 0px;}
.success h4{text-align:center;font-size:24px;color: #19d5fd}
.success p{margin-top:10px;font-size:16px;text-align:center;}

.end{margin:50px 0px 70px 0px;text-align: center;font-size: 16px;}