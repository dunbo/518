    <script type="text/javascript">
        function save(){
            var status = $("#status").val();
            if($("#softname").val().length==0)
            {
                $("#s1").html('<em>*</em><font color="red">产品名称不能为空<font>');
                return;
            }else{
                $("#s1").html('<em>*</em>');
            }


            if($("#size").val().length==0)
            {
                $("#s3").html('<em>*</em><font color="red">包源大小不能为空<font>');
                return;
            }else{
                $("#s3").html('<em>*</em>');
            }

            if($("#companyname").val().length==0)
            {
                $("#s4").html('<em>*</em><font color="red">公司名称不能为空<font>');
                return;
            }else{
                $("#s4").html('<em>*</em>');
            }     
            if($("#com_tj_tel").val().length==0)
            {
                $("#slx").html('<em>*</em><font color="red">联系方式不能为空<font>');
                return;
            }else{
                $("#slx").html('<em>*</em>');
            } 

            if($("#hztype").val().length==0)
            {
                $("#s5").html('<em>*</em><font color="red">合作方式不能为空<font>');
                return;
            }else{
                $("#s5").html('<em>*</em>');
            }

            if($("#jianjie").val().length==0)
            {
                $("#s6").html('<em>*</em><font color="red">简介不能为空<font>');
                return;
            }else{
                $("#s6").html('<em>*</em>');
            }       
			
			if($("#customer_tel").val()!==''){
				var customer_tel_len = $('#customer_tel').val().length;
				if(customer_tel_len > 30){
					$('#customer_tel_error').html('<img src="./images/error_icon.png" alt="" /><dd style="color:#CE0421;float:left;margin-left:10px;margin-top:4px;">限30个字符以内，请重新输入</dd>');
					return;
				}else{
					$('#customer_tel_error').html('');
				}
				if((/[\u4e00-\u9fa5]+/).test($("#customer_tel").val())){
					$('#customer_tel_error').html('<img src="./images/error_icon.png" alt="" /><dd style="color:#CE0421;float:left;margin-left:10px;margin-top:4px;">字符格式错误，请重新输入</dd>');
					return;
				}else{
					$('#customer_tel_error').html('');
				}
			}
			if($("#customer_qq").val()!==''){
				var customer_qq_len = $('#customer_qq').val().length;
				if(customer_qq_len > 30){
					$('#customer_qq_error').html('<img src="./images/error_icon.png" alt="" /><dd style="color:#CE0421;float:left;margin-left:10px;margin-top:4px;">限30个字符以内，请重新输入</dd>');
					return;
				}else{
					$('#customer_qq_error').html('');
				}
				if(isNaN($("#customer_qq").val())){
					$('#customer_qq_error').html('<img src="./images/error_icon.png" alt="" /><dd style="color:#CE0421;float:left;margin-left:10px;margin-top:4px;">字符格式错误，请重新输入</dd>');
					return;
				}else{
					$('#customer_qq_error').html('');
				}
			}
			
            if(status==1)
            {
                if($("#reason").val().length==0)
                {
                    $("#s7").html('<em>*</em><font color="red">拒绝评测原因不能为空<font>');
                    return;
                }else{
                    $("#s7").html('<em>*</em>');
                }             
            }

			var param = {
				softname_new: $("#softname").val(),
				soft_id: $("#soft_id").val()
			};
			$.ajax({
				url:'__URL__/add',
				type:'POST',
				data:param,
				secureuri:false,
				dataType:'json',
				success:function(data){
				   if(data==2)
				   {
						$("#s1").html('<em>*</em><font color="red">该产品名称已存在<font>');
					}else if(data==1)
					{
						$('#add').submit();
					}
				}
			});
        };

        function check_softname() {
            var param = {
                softname_before : softname_before,
                softname : $("#softname").val()
            };
            $.ajax({
                url:'__URL__/check_softname',
                type:'POST',
                data:param,
                secureuri:false,
                dataType:'json',
                success:function(data){
                if(data.status != 0) {
                        $("#s1").html('<em>*</em><font color="red">产品名称已存在<font>');
                    } else {
                        $("#s1").html('<em>*</em>');
                    }
                }
            });
        };

        // 网页加载时执行的函数
        // 获得产品名称
        var softname_before;
        $(function() {
            softname_before = $("#softname").val();
        });
