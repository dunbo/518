*{outline:0 !important; -webkit-text-size-adjust: none; -webkit-tap-highlight-color: rgba(0, 0, 0, 0); position:relative;-ms-touch-action:none;background-size:100% auto;background-position:center center;background-repeat:no-repeat;}
html,body,div{padding: 0;margin: 0;}
html, body {width:100%;height: 100%;overflow:hidden;}
#show {width:100%;height:100%;overflow:hidden;max-width:640px;margin:auto;}
#show > * {width:100%;height:100%;}
.page {position:absolute;width:100%;height:100%;overflow:hidden;display:none;-webkit-transition:all 1.1s;transition:all 1.1s;}
.page > * {position:absolute;width:100%;height:100%;}
#p1,#p2,#p3,#p4,#p5,#p6,#p7,#p8,#p9 {background-image:url(../image/p1.jpg);background-size:100% 100%;}
#p1 .p1_icon {background-image:url(../image/p1_icon.png);}
#p2 .p2_icon {background-image:url(../image/p2_icon.png);}
#p3 .p3_icon {background-image:url(../image/p3_icon.png);}
#p4 .p4_icon {background-image:url(../image/p4_icon.png);}
#p5 .p5_icon {background-image:url(../image/p5_icon.png);}
#p6 .p6_icon {background-image:url(../image/p6_icon.png);}
#p7 .p7_icon {background-image:url(../image/p7_icon.png);}
#p8 .p8_icon {background-image:url(../image/p8_icon.png);}
#p9 .p9_icon {background-image:url(../image/p9_icon.png);}
#p1.current .p1_icon,#p2.current .p2_icon,#p3.current .p3_icon,#p4.current .p4_icon,#p5.current .p5_icon,#p6.current .p6_icon,#p7.current .p7_icon,#p8.current .p8_icon,#p9.current .p9_icon{animation:fadeIn 1.5s 0s linear 1 normal forwards;-webkit-animation:fadeIn 1.5s 0s linear 1 normal forwards}
#bgm{height:0;width:0;display:none;}
#bgm_btn {position:absolute;width:9%;height:9%;right:5%;top:5%;display: none;}
#bgm_btn.off{background-image:url(../image/off.png);}
#bgm_btn.on{background-image:url(../image/on.png);}
#next {position:absolute;width:7%;height:7%;bottom:3%;left: 50%;margin-left: -3%;background-image:url(../image/next.png);animation:next 0.7s 0s linear alternate infinite;-webkit-animation:next 0.7s 0s linear alternate infinite;display: none;}
.btns{position: absolute;width:80%;height:6%;bottom: 9%;left: 10%;z-index: 2;}
#btn_again,#btn_share{width:40%;animation:btn_scale 0.7s 0s linear alternate infinite;-webkit-animation:btn_scale 0.7s 0s linear alternate infinite}
.btns img{width: 100%;}
.fl{float: left;}
.fr{float: right;}
#logo{position: absolute;width:18%;height:4%;bottom: 2%;right: 2%;z-index: 1;display: none;}
#logo img{width: 100%;}
#share_mark{width:100%;height:100%;background:rgba(0,0,0,.6); display:none;z-index: 9999}
#share_mark img{width: 100%;}
#loading {position:absolute;width:100%;height:100%;background-color:#fff;top:0;left: 0;}
.center {width:0;height:0;left:50%;top:50%;}
#loading .loading_icon {width:50px;height:50px;left:-25px;top:-48px;background-image:url(../image/loading.png);-webkit-animation:Rotate 7s cubic-bezier(0,0,0,0) infinite;animation:Rotate 7s cubic-bezier(0,0,0,0) infinite;opacity:.35;}
#loading .loading_text {width:50px;height:20px;left:-25px;background-image:url(../image/loading_text.png);top:-43px;}

@-webkit-keyframes Rotate{
    0% {-webkit-transform:rotate3d(0,0,1,0deg);}
    100%{-webkit-transform:rotate3d(0,0,1,360deg);}
}
@keyframes Rotate{
    0% {transform:rotate3d(0,0,1,0deg);}
    100%{transform:rotate3d(0,0,1,360deg);}
}
@keyframes btn_scale{
    0%{transform:scale(0.9);}
    100%{transform:scale(1.0);}
}
@-webkit-keyframes btn_scale{
    0%{-webkit-transform:scale(0.9);}
    100%{-webkit-transform:scale(1.0);}
}
@keyframes fadeIn{
    0%{opacity: 0;transform:scale(0.9);}
    100%{opacity: 1;transform:scale(1.0);}
    }
@-webkit-keyframes fadeIn{
    0%{opacity: 0;-webkit-transform:scale(0.9);}
    100%{opacity: 1;-webkit-transform:scale(1.0);}
}
@keyframes next{
    0%{transform:translate(0,0);}
    100%{transform:translate(0,5px);}
    }
@-webkit-keyframes next{
    0%{-webkit-transform:translate(0,0);}
    100%{-webkit-transform:translate(0,5px);}
}