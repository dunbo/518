/* CSS Document */
body,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
fieldset,img {border: 0;}
a{text-decoration:none; color:#252525;}
.clear{clear:both;}
ul,li{list-style:none;}
body{ font-family:'微软雅黑'; color:#252525; font-size:14px; line-height:20px;}
#header{ height:38px; padding-top: 2px; background: url(../images/nav_bg.png) repeat-x; border-radius: 5px 5px 0 0;}
#logo h1{ width:150px; margin:0 auto; padding-left:15px; background:url(../images/logo2.png) no-repeat; font-size:24px; text-align:center; font-weight:normal; text-shadow:0 2px 0 #7fbf1c; line-height:38px;}

.app_header_detail{padding:10px 10px; color:#555555;}
.app_info_detail{width:100%; position:relative;}
.app_info_detail .app_img{float:left; width:57px; height:57px; margin-right:10px; display:inline;}
.app_btn{display:block; width:75px; height:26px; background:#71b32b; border-radius:3px; color:#ffffff; text-align:center; line-height:26px; position:absolute; right:8px; top:5px; }
.app_txt h3{padding-top:5px; font-size:22px; font-weight:normal;}
.app_txt ul{margin-top:10px; margin-left:67px;}
.app_txt ul li{float:left; width:50%; margin-bottom:4px; line-height:22px; overflow:hidden}
.app_txt ul li img{float:left; width:17px; height:18px; padding:0 1px;}

.app_cnt{border-top:3px solid #8cb340; color:#555555}
.appcnt_bg{height:30px; background:#f4f7f6; color:#669900; line-height:30px; padding:0 10px;}
.appcntlist{ padding:8px 10px; font-size:16px;}

#jietu{padding:10px 0; height:246px; background:#cccccc; border-top:1px solid #a3a3a3;}
#jietu ul{width:320px; margin:0 auto; overflow:hidden}
#jietu ul li{float:left; width:144px; height:242px; border:3px solid #669900; margin-left:8px; display:inline;}
#jietu ul li img{width:144px; height:242px;}
.down_btn_detail a{ display:block; width:194px; height:45px; margin:30px auto; background:#71b32b; font-size:24px; border-radius:3px; color:#ffffff; text-align:center; line-height:45px; text-shadow:0 1px 0 rgba(0, 0, 0,0.4)}

.an_mark{height:100px; background:#6fd800 url(../images/icon_01.png) repeat; position:relative}
.an_mark h2{padding:25px 25px 15px 0; text-align:right; color:#fff; font-size:30px; font-weight:normal; text-shadow:0 4px 1px rgba(0, 0, 0,0.4)}
.an_mark h3{font-size:18px; text-align:right; padding-right:5px; color:#fff; font-weight:normal; text-shadow:0 1px 1px rgba(0, 0, 0,0.4)}
.an_mark span{display:block; width:55px; height:55px; background:url(../images/logo.png) no-repeat; position:absolute; left:5px; top:15px;}

#footer{background:#e0ebcc; padding:8px 0px; border-top:1px slild #c4df9b; text-align:center; color:#336600; line-height:23px;}
#footer p a,.topbtn{padding:0 9px;}
.topbtn{cursor:pointer;}
#footer a{color:#336600}
