/* CSS Document */
body,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
fieldset,img {border: 0;}
a{text-decoration:none; color:#252525;}
.clear{clear:both;}
ul,li{list-style:none;}
body{ font-family:'微软雅黑'; color:#252525; font-size:16px; line-height:22px;}

#header{width:320px; margin:0 auto;}
#header h1{height:28px; padding-left:30px; background:url(../images/logo.png) no-repeat; font-weight:normal; font-size:12px;}
.header_01{height:71px; margin-top:7px; background:url(../images/header_01.jpg) no-repeat center center;}
.header_02{height:58px; background:url(../images/header_02.jpg) no-repeat center center;}

#main{width:320px; margin:0 auto;}
#main_01_wrap{ background:url(../images/repeat_bg1.png) repeat-y center center; overflow:hidden}
#main_01{background:url(../images/main1_bg1.png) no-repeat center top;}
#main_01_inner{ background:url(../images/main1_bg2.png) no-repeat center bottom; padding:15px 20px 20px 30px; line-height:22px;}
#main_01_inner p span{color:#fc6633;}
.down_btn{float:right; width:122px; height:36px; margin-top:5px; margin-right:5px; background:url(../images/btn_bg.png) no-repeat 0 0; line-height:34px; text-align:center; font-size:18px; overflow:hidden; color:#fff; display:inline}
.down_btn:hover{ background:url(../images/btn_bg.png) no-repeat 0 -36px; text-decoration:none;}

.main_infor{ width:290px; padding:20px 10px 10px 10px; margin:30px auto 0; background:#f0f0f0; line-height:24px; position:relative;}
.main_infor h3{width:140px; height:39px; padding-left:11px; background:url(../images/title_bg.png) no-repeat; font-weight:normal; color:#fff; line-height:30px; position:absolute; left:-9px; top:-17px;}
.main_infor p span{color:#539e2c;}
.main_03_list p{padding-left:18px;}