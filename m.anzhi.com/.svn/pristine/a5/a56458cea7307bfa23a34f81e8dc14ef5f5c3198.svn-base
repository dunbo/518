/* CSS Document */
/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{max-width:640px;min-width:320px; margin:0 auto;font-size:14px; font-family:Microsoft Yahei,"微软雅黑"; background: #eaeaea; color: #414141;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#414141; font-size: 14px;}
.a,input,button,div{-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{ float: left;}
.fr{ float: right}
.cnt{text-align: center}
.wordwrap{word-wrap:break-word;word-break:break-all;}
.ptop10{ padding-top: 10px;}
.btns{display:block; width:76px; height:30px; margin:10px auto; background:#23b4ff; border-radius:4px; color:#fff; font-size:16px; text-align:center; line-height:30px;}
#slotMachineButton1{width:132px; height:37px; line-height:37px; font-size:18px; background:#ffb83b }
.banner{margin:9px 9px 0;}
.banner img{ width: 100%;vertical-align: bottom;}
.main{margin:0 9px 0; padding-bottom:50px; background:#fff; position:relative}
.app{ min-height:73px; position:relative}
.app_img{margin:0 9px;width:78px; height:78px; border:4px solid #eaeaea; border-radius:25px; position:absolute; left:0; top:-18px; overflow:hidden}
.app img{display:block; width:78px; height:78px; margin:0 auto; border-radius:17px;}
.app_txt{margin:0 0 0 96px; padding-top:9px; font-size:15px}
.app_txt h3{font-size:18px; color:#626262;width:100%; }
.app_txt h3 em{display:block; min-height:30px; margin-right:10px; line-height:30px; overflow:hidden; white-space:nowrap; text-overflow:ellipsis}
.app_txt h3 span{ position:absolute; right:10px; top:11px; height:22px; background:#fe6869; padding:1px 8px; border-radius:12px;  color:#fff; font-size:15px; line-height:23px}
.app_txt p{padding:5px 0 0; line-height:24px}
.app_txt p span{padding:0 6px 0 0; color:#23b4ff}


.title h3{display:inline-block; padding:2px 15px 2px 10px; border-radius:0 15px 15px 0; background:#ffb83b; font-size:16px; color:#fff;}
.gift_wrap,.lottery_wrap,.infor_wrap,.game_imgs,.news_wrap{padding-bottom:8px}
.gift_wrap ul{margin:0 12px}
.gift_wrap ul li{min-height:25px; margin:12px 0 0; line-height:25px; position:relative}
.gift_wrap ul li span{display:block; margin:0 70px 0 0; font-size:15px; white-space:nowrap; text-overflow:ellipsis; overflow:hidden}
.gift_wrap ul li a{display:block; width:60px; height:25px; background:#23b4ff; border-radius:2px; text-align:center; line-height:25px; color:#fff; position:absolute; right:0; top:0; font-size:15px}
.gift_wrap ul li a.red{background:#ff6263}
.infor{ padding:10px 10px 0 13px; line-height:24px; font-size:15px; word-break: break-all; word-wrap: break-word;}
.infor h4{margin-bottom:2px; font-size:18px}
.infor p{}
.infor img{max-width:100%; margin-top:5px}
.lottery_info{ padding:0 0 8px;}
.lottery_info p{float:left; padding:17px 0 0 12px; font-size:15px}
.lottery_info a{margin-right:8px; display:inline}
.swiper-container {width: 96%; margin:10px auto 0;}
.swiper-slide {width:33.3%;}
.swiper-slide img{width:100%;display:block}

.news_wrap ul{margin:0 12px}
.news_wrap ul li{min-height:22px; margin:10px 0 0; line-height:22px; position:relative}
.news_wrap ul li a{display:block; margin:0 0 0 50px; font-size:15px; white-space:nowrap; text-overflow:ellipsis; overflow:hidden}
.news_wrap ul li span{display:block; width:40px; height:22px; background:#ff75a4;  text-align:center; line-height:22px; color:#fff; position:absolute; left:0; top:0; font-size:15px}

.prize_list{padding:12px}
.btns2{width:314px; margin:0 auto}
.btns2 a{float:left; width:137px; height:36px; margin:10px; text-align:center; line-height:36px; font-size:16px; border-radius:2px; color:#fff}
//.btns2 a.gray{background:#a6a6a6}
.form-items {margin:0 12px 12px}
.form-items li{ margin-top: 30px; padding:0 10px; border:1px solid #7d7d7d; clear: both; overflow: hidden;}
.form-items li label{ float: left;width:90px;padding-top: 10px; color:#482608;}
.form-items li span{display:block;margin-left: 90px;}
.form-items li input{ width:90%;padding:10px 6px;border:none;color: #000;  font-family: Microsoft Yahei,"微软雅黑"; background-color:#fff; border-radius:2px}
.form-items li textarea{width:90%; height:48px; padding:10px 6px; border:none; font-family: Microsoft Yahei,"微软雅黑"; background-color:#fff;}
.form_error{display:block; padding:8px 0 0 90px; color:red}

//.btns_wrap{margin-top:5px; padding:25px 12px 15px; background:url(../images/title_bg.png) repeat-x; position:relative}
//.btns_wrap a{height:30px; line-height:30px; border-radius:2px; text-align:center; line-height:30px; color:#fff; font-size:16px}
//.btns_wrap a.disabled{background:rgba(35,180,255,.5)}
.app_btn{ display:block; margin:0 80px 0 0; height:30px; background:#23b4ff}
.share_btn{ width:60px; height:30px; background:#ffb83b; position:absolute; right:10px; top:10px}
.return_top{width:36px; height:36px; background:url(../images/return_icon.png) no-repeat; background-size:100% 100%; position:fixed; bottom:70px; right:5%; cursor:pointer; z-index:99990}
/*老虎机抽奖*/
.lottery{width:96%; margin:0 auto; min-width:270px; padding-bottom:1px; border-radius:4px; background:url(../images/lottery_bg01.png) no-repeat; background-size:100% 100%}
.lottery_inner {width:270px; margin:0 auto; padding:65px 0 26px 0;}
.lottery_inner2{padding:2px; background:#d43434 }
.slotMachine{width:80px; height: 100px; margin:0 2px; overflow: hidden; display: inline-block;}
.noBorder{border:none !important; background: transparent !important;}
.slot{width:80px; height:100px; border-radius:5px;}
.slot1{background-image: url("../images/lottery_sl1.png");}
.slot2{background-image: url("../images/lottery_sl2.png");}
.slot3{background-image: url("../images/lottery_sl3.png");}
.slot4{background-image: url("../images/lottery_sl4.png");}
.slot5{background-image: url("../images/lottery_sl5.png");}

/*九宫格抽奖*/
.lottery2{width:279px; padding:13px; background:url(../images/lottery_bg02.png) no-repeat #ff6666; background-size:100% 100%; margin:0 auto; position:relative;}
.lottery2 img{display:block; width:81px; height:81px;}
.lottery2 table{border-collapse: separate; width:100%;}
.lottery2 td{width:81px;height:81px; padding:4px 6px 6px 5px; background:#d43434}
#my_click {width:91px;height:91px; cursor:pointer;}
.lottery2 p{position:absolute; width:91px;height:91px; left:106px; top:106px; display:block;}
.lottery2 p a{display:block; width:91px; height:91px;}



/*转盘抽奖*/
.rotate_tips{padding:0 0 8px; text-align:center;}
.rotate {width: 300px; margin:0 auto;position: relative;}
.rotate-pointer {position: absolute;top: 50%;left: 50%;width: 46%;height: 46%;margin-top: -23%;margin-left: -23%;background: url(../images/pointer.png) no-repeat 0 0;background-size: 100% 100%; cursor: pointer;-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.pointer-disabled{background: url(../images/pointer_disabled.png) no-repeat 0 0;background-size: 100% 100%; z-index:11}
.rotate_list{width:60px; height:78px; position:absolute; left:0; top:0; left:50%; top:50%; z-index:10}

.wrap{min-height:200px; margin:10px; padding:10px; border:1px solid #7d7d7d; overflow:hidden;}
.wrap p{ padding:2px 0 10px}
.end_title{color:#b30000; text-align:center; font-size:24px}
.end_btn{display:block; width:137px; height:36px; margin:5px auto 10px; background:#23b4ff; border-radius:2px; text-align:center; line-height:37px; color:#fff; font-size:16px}
.yellow{color:#ffb83b}

.tip_hd2 img{width: 107px; height:134px; margin:15px auto 0;}
.tip-mid2{padding-bottom:4px;border-radius: 0px 0px 15px 15px;}
.tip-con2{padding:15px 0 25px;margin:0px 4px}
.tip-con2 p{line-height: 25px; text-align: center;font-size: 16px;}

.clo{ position: absolute; right: -5px; top: -6px;}
.clo a{ display: block; width:38px; height:38px;background:url("../images/tip_clo.png") 0 0 no-repeat;background-size:100% auto;text-indent: -9999px;}
.ft_module{ width:100%; position:fixed; bottom:0;left:0px; z-index:999999; background: url(../images/fudong_bg.png) repeat;}
.ft_btnA {color: #FFFFFF; padding:5px 2px 5px 4px;}
.ft_btnA, .ft_btnA dl {float:left; margin-top:3px;}
.ft_btnA dl.ft_margin15 {margin:0px 0 0 5px;}
.ft_btnA dl p.font18 {font-size:16px; margin-bottom:2px;}
.ft_btnA dl p.font14 {font-size:12px;}
.ft_btnB { float:right; margin-right:5px; margin-top:8px;}
.ft_btnB .down_button { margin-top:5px; padding:5px; text-align:center; cursor:pointer; color:#FFFFFF; background-color:#7FC31E;}
.ft_btnB .down_button a {font-size: 14px;}
.ft_close{float:right;/* border-left:1px solid #898989;*/ height:100%;}
.ft_close a{display:inline-block; background:url(../images/close_menu.png) no-repeat 0 0px;width:20px; height:20px; cursor:pointer}

/*提示框*/
#body-bg{position:absolute; top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-webkit-opacity: 0.5;opacity: 0.5;z-index: 99998;}
#body-bg2{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;z-index: 99998;display:none;}
.tip-box{ position:absolute;width: 90%; left:50%;top:50%; background:#fff; border-radius:5px; margin-left:-45%; z-index: 99999; font-size:15px; color:#a5a5a5}
.tip-title{height:38px; padding:0 0 0 15px; border-radius:15px 15px 0 0; background: #f4f7f8; line-height:36px; position:relative}
.tip-title h6{padding-left:8px;font-size:16px; color:#545454}
.tip-con{ margin: 15px 0; padding: 10px 0 25px; border-bottom:1px solid #e6e6e6; text-align:center;}
.tip-con h4{font-size:16px; font-weight:bold; text-align:center}
.tip-title span{position:absolute; right:10px; top:6px; width:28px; height:28px; background:url(../images/tip_clo3.png) no-repeat; background-size:100% 100%; cursor:pointer}
.btns_close{display:block; height:28px; margin:0 0 15px; color:#19d5fd; line-height:28px; text-align:center; font-size:16px}
#tip-box5{background:none}
#tip-box5 img{width:300px; margin:10px auto;}
.tip-con2{ position:relative}
.tip-con2 p{text-align:center; color:#fff}
.btns_login{display:block; width:152px; height:36px; margin:20px auto 0; background:url(../images/login_btn.png) no-repeat; background-size:100% 100%; line-height:36px; text-align:center; color:#f10d2d; font-size:16px}
.tip-con2 span{display:block; width:30px; height:30px; background:url(../images/tip_clo2.png) no-repeat; background-size:100% 100%; position:absolute; right:-10px; top:-5px; cursor:pointer}
#tip-box4 .tip-title h6{background:none; padding-left:12px; font-size:18px}
#gift_name{display: inline-block; color:#ffb926; font-size:18px}
