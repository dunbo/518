
.check_prev{width:100%; height:100%;}
.check_prev_newbox{display:none;}
.check_prev_shortbox{cursor:pointer;}
.check_prev{width:100%; position:relative;}
.check_prev_newbox{width:300px; min-height:150px; padding:10px; background:#fff; border:1px solid #dcdcdc; position:absolute; bottom:0; left:-5px; z-index:100}
.check_prev_arrow{width:20px; height:11px; background:url(../images/icon_01.png) no-repeat; position:absolute; left:145px; top:-11px;}
.check_prev_close{width:32px; height:32px; background:url(../images/close_btn.png) no-repeat; position:absolute; right:4px; top:0; cursor:pointer;}

