.comment_info{height:24px;}
.phone_verson{float:left; width:40%; color:#a5a5a5; white-space:nowrap; overflow:hidden; text-overflow:ellipsis; line-height:24px; font-size:13px}
.comment_infor{float:right; position:relative; margin-top:4px}
.comment_infor span{float:left; margin-left:8px; display:inline; color:#959595}
.comment_infor span.comment_num{}
.comment_num i{float:left; width:22px; height:18px; background:url(http://img3.anzhi.com/static/feature/images/icon_01.png) no-repeat 0 0; background-size:16px 15px; cursor:pointer; -webkit-tap-highlight-color:rgba(255,0,0,0);}
.comment_num font{display:block; width:10px; height:20px; position:absolute; right:0; top:10px; color:#f00; line-height:20px; opacity:0}
.comment_num font.comment_num_act{animation:addAction 0.7s ease-in-out;-webkit-animation:addAction 0.7s ease-in-out;}
.comment_infor span.comment_infor2{background:url(http://img3.anzhi.com/static/feature/images/icon_02.png) no-repeat 0 1px; background-size:16px 15px; padding:0 0 0 22px; }
.comment_infor em{display:block; position:absolute; left:8px; top:17px; width:18px; height:10px;background:url(http://img3.anzhi.com/static/feature/images/icon_03.png) no-repeat 0 4px; background-size:18px 10px; z-index:3}
.comment_cnt{/*height:28px;*/ color:#5b5b5b; font-size:17px; word-break:break-all; word-wrap:break-word; /*white-space:nowrap; overflow:hidden; text-overflow:ellipsis*/}
.replay_list{width:94%; margin:6px auto 4px; border:1px solid #e6e6e6; border-bottom:none; background:#f4f9fa; position:relative}
.replay_list li{float:left; width:100%;border-bottom:1px solid #e6e6e6;}
.replay_list .comment_img{width:30px; height:30px; left:10px}
.replay_list .comment_r{margin:0 0 0 42px}
.comment_infor i.comment_act{animation:scale 0.5s ease-in-out;-webkit-animation:scale 0.5s ease-in-out; background:url(http://img3.anzhi.com/static/feature/images/icon_04.png) no-repeat 0 2px; background-size:18px 16px;}

/*轮播图片*/
.focus{margin:0; position:relative}
.focus_bd{width:100%; overflow:hidden}
.focus_bd ul{width:100%; float:left;}
.focus_bd ul li{float:left; width:49%; margin-left:1%; -webkit-tap-highlight-color:rgba(255,0,0,0);}
.focus_bd li img{width:100%; display:block}
.focus_dotted{ left:50%; height:40px; bottom:-2px; position:absolute;}
.focus_dotted ul{left:-50%; padding-top:16px; position:relative;}   
.focus_dotted li{display:inline-block; width:18px; height:4px; background:#000; margin:0 5px; overflow:hidden; opacity:0.5; text-indent:9999px; cursor:pointer; text-align:center
    }
.focus_dotted .on{ background:#fff; opacity:1}
.focus_bd2{width:100%; overflow:hidden}
.focus_bd2 ul li{float:100%; -webkit-tap-highlight-color:rgba(255,0,0,0);}
.focus_bd2 li img{width:100%; display:block}
.banner_txt{position:absolute; left:0; top:30px; min-width:58px; height:22px; padding:0 15px 0 10px; color:#fff; font-size:16px; line-height:22px}
.banner_txt a{display:block; width:100%; height:100%}
.share_btn{position:absolute; right:0; top:30px; padding:4px 10px 4px 15px; color:#fff; font-size:16px; cursor:pointer}
@keyframes scale{
    0%{transform: scale(1.5);}
    100%{transform: scale(1.0);}
}
@-webkit-keyframes scale{
    0%{-webkit-transform: scale(1.5);}
    100%{-webkit-transform: scale(1.0);}
}
@keyframes addAction{
    0%{opacity:0; top:10px}
    50%{opacity:1; top:0px}
    100%{opacity:0; top:-16px}
}
@-webkit-keyframes addAction{
    0%{opacity:0; top:10px}
    50%{opacity:1; top:0px}
    100%{opacity:0; top:-16px}
}
