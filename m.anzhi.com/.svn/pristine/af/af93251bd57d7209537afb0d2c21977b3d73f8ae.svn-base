//验证手机号码   验证规则：11位数字，以13|14开头。	
function  checkMobile() {
	var str = $('#mobile_num').val();
	//if(!str){
		//alert("请输入手机号");
		//return false;	
	//}
	if(str.length == 11){
		var  re = /^(13|14|15|17|18)\d{9}$/
		if (!re.test(str)){
			$('#sub_but').attr('class','sign_btn gray');
			$('#sub_but').attr('onclick','');
			return false;
		}else{
			$('#sub_but').attr('class','sign_btn');
			$('#sub_but').attr('onclick','sign();');
		}
	}else{
		$('#sub_but').attr('class','sign_btn gray');
		$('#sub_but').attr('onclick','');
	}
	return true;
}
$('#mobile_num').bind('input keyup', function(e){
	checkMobile();
});
function save(){
	var lxname = $("#lxname").val();
	if(lxname == "") {
		$('#name_err').html('联系人姓名不能为空');
		return false;
	}
	var name_len = lxname.length;
	if(name_len > 10){
		$('#name_err').html('联系人姓名不能超过10个字');
		return false;	
	}
	$('#name_err').html('');
	var mobile_phone = $("#mobile_phone").val();
	if(mobile_phone == "") {
		$('#phone_err').html('联系人手机号码不能为空');
		return false;
	 }
	var  re = /^(13|14|15|17|18)\d{9}$/
	if (!re.test(mobile_phone)){
		$('#phone_err').html('请输入正确手机号');
		return false;
	}
	$('#phone_err').html('');

	var address = $("#address").val();
	if(address == "") {
		$('#address_err').html('邮寄地址不能为空');
		return false;
	 }
	var address_len = address.length;
	if(address_len > 30){
		$('#address_err').html('邮寄地址不能超过30个字');
		return false;	
	}	 
	$('#address_err').html('');
	$.ajax({
		url: '/lottery/october_sign/userinfo.php',
		data:"aid="+aid+"&sid="+sid+"&lxname="+lxname+"&mobile_phone="+mobile_phone+"&address="+address,
		type:"post",
		dataType: 'json',
		success:function(data) {
			if(data.code == 1){
				cloBox('#tip-box');
				var str =  "提交成功";
				$("#notice").html(str);					
				showOpenBox('#tip-box3');
			}else if(data.code == 2){
				location.href=data.url;
				return false;
			}
		},
	});
}
function sign(){
<!--{if $out['is_login'] == 1}-->
	if(open_num == 1){
		sign_do();
	}else{
		showOpenBox('#tip-box2');
	}
<!--{else}-->
	location.href="<!--{$out.login_url}-->";
<!--{/if}-->	
}

function sign_do(){
	var is_sing = "<!--{$out.is_sing}-->";
	var mobile_num = $('#mobile_num').val();
	$.ajax({
		url: '/lottery/october_sign/sign.php',
		data: 'sid='+sid+'&aid='+aid+'&mobile_num='+mobile_num,
		type: 'post',
		dataType: 'json',				
		success: function(data){
			if(data.code == 1){
				if(is_sing != 1){
					location.reload();//加载页面
				}else{
					var str =  "签到成功";
					$("#notice").html(str);				
					showOpenBox('#tip-box3');
				}
				return false;
			}else if(data.code == 2){
				location.href=data.url;
				return false;
			}				
		}
	});
}

