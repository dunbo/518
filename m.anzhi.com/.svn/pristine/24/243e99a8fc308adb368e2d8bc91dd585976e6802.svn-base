/* CSS Document */
/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width:320px;max-width:640px;margin:0 auto;font-size:14px; font-family:Microsoft Yahei,"微软雅黑";}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{vertical-align: bottom;}
a{text-decoration:none; font-size: 14px; outline: none;}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.cl{overflow:hidden;}
.clear{clear:both;overflow:hidden;}
.fl{float: left;}
.fr{float: right;}

.mtb{ margin: 20px 0px;}
.mtop{margin-top: 25px;}
.pbtom{padding-bottom: 30px;}
.wth{width: 90%; margin:0 auto;}

.banner{position: relative; width: 100%;min-width: 320px; margin:0 auto; text-align: center;}
.banner img{ width: 100%;vertical-align: bottom;}

.soft_list{min-height:64px; padding:10px 8px; background-color:rgba(0,0,0,.2); position:relative;}
.soft_icon{float:left; margin-right:8px; display:inline}
.soft_icon img{display:block; width:64px; height:64px;}
.soft_info h4{padding: 1px 85px 5px 0; line-height: 20px; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; color:#fff; font-size:18px;}
.soft_info p{ color:#fff; line-height:18px}
.soft_info p em{ display:block; margin-right:120px; font-style:normal;}
.soft_info p .form_phone{margin-right:30px}
.form_phone input{max-width:115px; height:20px; padding:1px 3px; margin:0 0 2px 0; border:none; border-radius:4px; color:#585858; font-family:微软雅黑; letter-spacing:1px; line-height:21px; overflow:hidden;}
.soft_info  p .form_phone{color:#ffcc00;}
.download{display: block; min-width:60px;  height:30px; padding:0 10px; background:#ffba00; border-radius:30px; position: absolute; right: 5px; top: 27px; line-height:31px; color: #402811; text-align: center;}
.download:hover{background:#ffd800}

.like_txt p{padding:22px 0 15px; color:#ffcc00; font-size:16px; text-align:center}
.like_btn{width:210px; height:50px; margin:0 auto 18px; background:url(../images/btns_bg01.png) no-repeat; background-size:210px 50px}
.like_btn:hover{background:url(../images/btns_bg01hs.png) no-repeat; background-size:210px 50px}
.like_btn a{display:block; width:146px; height:50px; padding:0 0 0 74px; background:url(../images/like_bg01s.png) no-repeat 34px center; background-size:26px 26px;  color:#ffffff; line-height:50px; font-size:24px; font-weight:bold; letter-spacing:2px}

.like-plus{display:block; width:60px; height:40px; position:absolute; left:50%; top:50%; margin-left:-7px; z-index:3}
.like-plus span{display:block; position:absolute; left:10px; top:10px; font-family:arial; font-size:22px; color:#fff; opacity:0}
.like-plus span.bounceinup{ animation:1.2s bounceInUp forwards; -webkit-animation:1.2s bounceInUp forwards}

.like-plus em{display:block; width:13px; height:11px; background:url(../images/like_01.png) no-repeat; background-size:13px 11px; position:absolute; left:35px; top:10px; opacity:0}
.like-plus em.zoomin{animation:0.8s zoomIn 0.5s; -webkit-animation:0.8s zoomIn 0.5s}
.like-plus i{display:block; width:8px; height:7px; background:url(../images/like_01.png) no-repeat; background-size:8px 7px; position:absolute; left:48px; top:5px; opacity:0}
.like-plus i.zoomin{animation:0.8s zoomIn 0.7s; -webkit-animation:0.8s zoomIn 0.7s}
.disabled{background:url(../images/btns_disabled.png) no-repeat; background-size:210px 50px}
.disabled a{background:url(../images/like_bg01hs.png) no-repeat 34px center; background-size:26px 26px; color:#b6b1ab}
.disabled:hover{background:url(../images/btns_disabled.png) no-repeat; background-size:210px 50px}
.line{width:100%; height:2px; margin:0 0 20px 0; background:url(../images/line_bg01.png) no-repeat center top; overflow:hidden}
.like_time{ float:left; position:relative; left:50%;}
.like_time_inner{position:relative; right:50%; margin-bottom:20px}
.like_time p,.like_time em{ padding:12px 0 0 0; font-size:16px; color:#fec72e;}
.like_time p{float:left;}
.like_time span{float:left; width:32px; height:44px; background:#e33c30; border-radius:6px; line-height:45px; text-align:center; color:#fff; font-size:22px; font-family:Arial; font-weight:bold}
.like_time em{float:left; margin:0 2px; display:inline; font-style:normal;}

.share img{width:100%; }
.share_btn{display:block; width:172px; height:38px;  background:none; border:1px solid #fff; border-radius:30px; margin:20px auto; text-align:center; line-height:39px; font-size:18px; color:#fff; cursor:pointer}
.share_btn:hover{background-color:rgba(0,0,0,.4)}

.rules h3{ padding:0 0 10px; text-align:center; font-size:20px}
.rules_l{position:absolute; left:-10px; top:-16px; display:block; width:64px; height:58px; background:url(../images/rules_l.png) no-repeat; background-size:64px 58px;}
.rules_r{position:absolute; right:-10px; bottom:-16px; display:block; width:64px; height:58px; background:url(../images/rules_r.png) no-repeat; background-size:64px 58px;}

.scale_wrap{width:100%; height:150px; position:relative;}
.like_mark{width:16px; height:114px; background:url(../images/mark_04.png) no-repeat; background-size:16px 114px; position:absolute; left:50%; margin-left:-8px; bottom:9px; z-index:3}
.bar_hand{width:92px; height:56px;  background:url(../images/like_bg02.png) no-repeat; background-size:92px 56px; position:absolute; left:50%; margin-left:-46px; bottom:-10px; z-index:2}
.bar_level_01{width:128px; height:110px; background:url(../images/like_bg05hs.png) no-repeat; background-size:128px 110px; position:absolute; left:50%; margin-left:-64px; bottom:10px; z-index:1}

.prize_box{position:absolute; left:50%; bottom:0; margin-left:-165px}
.prize_box span{display:block; color:#fff; text-align:center; font-size:13px;}
.prize_box span.prize{padding:1px 2px; background:#ffc000; border-radius:2px; color:#633838}
#prize_box2{margin-left:72px; bottom:30px;}
.prize_current span.prize{background:#fa405e; color:#fff;}
.prize_over span{color:#fff}
.prize_over span.prize{background:#fff; color:#a398a4;}
#prize_box3{ bottom:65px;}
#prize_box4{margin-left:72px; bottom:90px;}
#like_bar3 .like_mark{background:url(../images/mark_03.png) no-repeat; background-size:16px 114px;}
#like_bar3  .prize_box{bottom:10px}
#like_bar3 #prize_box2{ bottom:50px;}
#like_bar3 #prize_box3{ bottom:85px;}
#prize_box4{margin-left:72px; bottom:90px;}
#like_bar5 .like_mark{background:url(../images/mark_05.png) no-repeat; background-size:16px 114px;}
#like_bar5 .prize_box{bottom:-2px}
#like_bar5  #prize_box2{ bottom:20px;}
#like_bar5  #prize_box3{ bottom:45px;}
#like_bar5  #prize_box4{ bottom:70px;}
#like_bar5   #prize_box5{ bottom:95px;}


.like_end{ margin-top:-15px; text-align:center}
.prize_list{display:block; font-size:20px; padding:5px 0 0;}

/*提示框*/
#body-bg,#body-bg2{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{position:absolute;width: 86%;left:50%;top:50%;margin-left:-43%; z-index: 99999;}
.tip-box-inner{border:6px solid #ffdf3f; border-radius:8px; box-shadow:0 6px 0 #e99502; background:#fef3df;}
.tip_hd{width: 122px; height:125px; margin:0 auto 5px;}
.tip_hd img{width: 122px; height:110px; margin:15px auto 0;}
.tip-mid{padding-bottom:4px;border-radius: 0px 0px 15px 15px;}
.tip-con{padding:0px 0 10px 0}
.tip-con p{line-height: 25px; text-align: center;font-size: 16px; color:#472d14}
.clo{ position: absolute;right: -17px;top: -17px;}
.clo a{ display: block; width:38px; height:38px;background:url("../images/tip_clo.png") 0 0 no-repeat;background-size:100% auto;text-indent: -9999px;}
a.btns{display:block; width:212px; height:48px; margin:10px auto; background:url("../images/btns_bg01s.png") 0 0 no-repeat; background-size:212px 48px;  line-height:49px;  color:#fff; text-align:center; font-size:22px;}
a.btns:hover{background:url("../images/btns_bg01h.png") 0 0 no-repeat; background-size:212px 48px;}

.tip-top{position:fixed; right:10px; top:10px; width:180px; padding:2px 5px; background:#fef3df; border-radius:4px; z-index:100000; line-height:22px}
.tip-top span{display:block; width:16px; height:8px; background:url("../images/tips_03.png") 0 0 no-repeat; position:absolute; top:-8px; right:20px;}
/*尾部*/
.footer{width:100%;margin-top: 15px;}
.footer img{width: 100%;}

@keyframes bounceInUp {
	0%,100%,60%,75%,90% {
		-webkit-transition-timing-function: cubic-bezier(0.215,.61,.355,1);
		transition-timing-function: cubic-bezier(0.215,.61,.355,1)
	}

	0% {opacity: 0;-webkit-transform: translate3d(0,3000px,0);-ms-transform: translate3d(0,3000px,0);transform: translate3d(0,3000px,0)}
	40% {opacity: 1;-webkit-transform: translate3d(0,-20px,0);-ms-transform: translate3d(0,-20px,0);transform: translate3d(0,-20px,0)}
	60% {opacity: 1;-webkit-transform: translate3d(0,10px,0);-ms-transform: translate3d(0,10px,0);transform: translate3d(0,10px,0)}
	75% {-webkit-transform: translate3d(0,-5px,0);-ms-transform: translate3d(0,-5px,0);transform: translate3d(0,-5px,0);opacity: 1;}
	80% {-webkit-transform: translate3d(0,-0,0);-ms-transform: translate3d(0,-0,0);transform: translate3d(0,-0,0);opacity: 1;}
	100% {-webkit-transform: translate3d(0,0,0);-ms-transform: translate3d(0,0,0);transform: translate3d(0,0,0);opacity: 1;}
}
@-webkit-keyframes bounceInUp {
	0%,100%,60%,75%,90% {
		-webkit-transition-timing-function: cubic-bezier(0.215,.61,.355,1);
		transition-timing-function: cubic-bezier(0.215,.61,.355,1)
	}
	0% {opacity: 0;-webkit-transform: translate3d(0,3000px,0);transform: translate3d(0,3000px,0)}
	40% {opacity: 1;-webkit-transform: translate3d(0,-20px,0);transform: translate3d(0,-20px,0)}
	60% {-webkit-transform: translate3d(0,10px,0);transform: translate3d(0,10px,0);opacity: 1;}
	75% {-webkit-transform: translate3d(0,-5px,0);transform: translate3d(0,-5px,0);opacity: 1;}
	80% {-webkit-transform: translate3d(0,-0,0);transform: translate3d(0,-0,0);opacity: 1;}
	100% {-webkit-transform: translate3d(0,0,0);transform: translate3d(0,0,0);opacity: 1;}
}
@keyframes zoomIn {
	0% {opacity: 0;-webkit-transform: scale3d(.3,.3,.3);-ms-transform: scale3d(.3,.3,.3);transform: scale3d(.3,.3,.3)}
	50% {opacity: 1}
	100% {opacity: 1}
}
@-webkit-keyframes zoomIn {
	0% {opacity: 0;-webkit-transform: scale3d(.3,.3,.3);-ms-transform: scale3d(.3,.3,.3);transform: scale3d(.3,.3,.3)}
	50% {opacity: 1}
	100% {opacity: 1}
}