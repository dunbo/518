/* CSS Document */
/*applist*/
.app_list ul{width:572px; margin-left:5px; overflow:hidden}
.app_list ul li{float:left; width:568px; height:65px; padding-top:14px; border-bottom:1px solid #ebebeb; font-size:12px; color:#999999; overflow:hidden}
.app_icon{float:left; width:48px; height:48px; margin:0 12px; display:inline}
.app_icon img{display:block; width:48px; height:48px;}
.app_info{float:left; width:400px; height:65px;}
.app_top{width:100%; height:28px; line-height:24px;}
.app_name{width:120px; font-size:16px; color:#669900}
.app_name a{color:#669900}
.app_version{width:90px;}
.app_downnum{width:125px;}
.app_list ul li p{width:95%; height:24px; white-space:nowrap; overflow:hidden; font-size:13px;}
.app_down{float:left; width:62px; height:20px; margin-top:10px;}

.search_hot{margin-bottom:20px;}
.search_hot #bj_recommend2{height:488px; overflow:hidden;}
.search_no{width:300px; min-height:433px; _height:433px; padding-top:55px; margin:0 auto; text-align:center;}
.search_no img{  width:104px; height:122px; margin-bottom:25px;}
.search_no p{color:#555555; line-height:24px;}
.search_no p a{color:#337b10;}