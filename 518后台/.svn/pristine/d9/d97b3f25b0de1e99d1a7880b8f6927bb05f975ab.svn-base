{// 加载头部公共文件 }
<include file="../Public/header" />
<div class="main" >
<div class="content">
<center>
<form id="upload" method='get' action="__URL__/userlog" >
<table width="100%" height="89" border="1">
  <tr>
    <td colspan="10">查询会员id：(不添为查看全部)</td>
  </tr>
  <tr>
    <td colspan="3">UID：</td>
    <td colspan="4"><input type="text" name="user_id" id="user_id" /></td>
    <td colspan="3"><input type="submit"  value="查询" /></td>
  </tr>
</table>
</form>
<table width="100%" border="1">

  <tr>
    <td>LID</td>
    <td>用户名</td>
    <td>行为</td>
    <td>节点名称</td>
    <td>节点作用</td>
    <td>时间</td>
  </tr>
  <tr>
  <volist name='userloglist' id='userlogvo'>
    <tr>
    <td>{$userlogvo.user_log_id|default='&nbsp;'}</td>
    <td>{$userlogvo.truename|default='&nbsp;'}</td>
    <td>{$userlogvo.actionexp|default='&nbsp;'}</td>
    <td>{$userlogvo.nodename|default='&nbsp;'}</td>
    <td>{$userlogvo.postil|default='&nbsp;'}</td>
    <td>{$userlogvo.logtime|date='Y-m-d H:i:s',###|default='&nbsp;'}</td>
  </volist>
  </tr>
  <tr>
    <td colspan="6" ><input type="button" value='返回' onclick=" window.history.go(-1); "/></td>
  </tr>
  <tr>
</table>
{$page}
</center>
</div>
</div>
<!-- 主页面结束 -->
{// 加载尾部公共文件 }
 