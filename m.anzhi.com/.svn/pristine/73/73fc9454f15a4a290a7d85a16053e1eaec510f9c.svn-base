/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto;font-size:14px; font-family:Microsoft Yahei,"微软雅黑"; background-color: #110926; color: #6c8bac;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#730b91; font-size: 14px; outline: none;}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
::-webkit-input-placeholder { color:#4065af;}
:-moz-placeholder {color:#4065af;}
::-moz-placeholder {color:#4065af;}
:-ms-input-placeholder {color:#4065af;}

.cnt{text-align: center;}

.btns a{background: url("../images/common_icon.png") 0 0 no-repeat;background-size: 160px 240px;}

.banner{ position:relative;width: 100%; margin: 0 auto;}
.banner img{ width: 100%;}
/**/
.main{position:relative;margin-top:-20%;z-index: 2}
.mod{width: 100%;margin: 0 auto 15px auto;}
.mod_t img,.mod_b img{width: 100%}
.mod_m{padding:0px 10%;background:url(../images/mod_m.png) 0 0 repeat-y; background-size:100% auto;}
.m_con{position:relative;margin:-9% 0px -10%; z-index: 3}
.mod p{line-height: 22px;}
.mod_t{position:relative;}
.mod_hd{position: absolute;top:-17%;left:0;}
.sub_hd{margin-bottom: 15px;}
.mod_hd img,.sub_hd img{width: 100%;}
/*签到*/
.btns{margin: 15px 0px}
.btns a{ display: block;width:158px; height:45px;margin:0 auto;background-position: 0px -48px;text-indent: -9999px;}
.btns a.btn_gray{background-position: 0px -96px;cursor: default}
.btns a.btn_download{background-position: 0px 0px;}
.btns a.btn_install{background-position: 0px -146px;}
.btns a.btn_open{background-position: 0px -194px;}
.input_box{position:relative;}
.input_box span{position: absolute;top:0px;left: 0px; display: block;width:66px; height:30px;background: url("../images/input_l.png") -1px -1px no-repeat;background-size: 66px auto;z-index: 2}
.input_box input{width:80%;padding:12px 15px;border: 1px solid #4065af;background-color:#111f56;color: #4065af;font-size:16px;font-family: "Microsoft Yahei"}
.input_box span.r{float:left;width:8px;background: url("../images/input_r.png") 0 0 repeat-x;background-size:8px 48px; margin-left:-8px}
/*活动规则*/
.rule{margin-top:-5%;color:#6c8bac;}
.rule h4,.rule p.b_hd,.lot_rule h4,.awards_list dt{ margin-bottom:5px;font-size: 16px; font-weight:bold;color:#d2deea;/*background-image: -webkit-gradient(linear, 0 0, 0 bottom, from(rgba(255, 255, 255,1)), to(rgba(163, 188, 214, 1)));-webkit-background-clip: text;-webkit-text-fill-color: transparent;*/}
.rule h4{text-align: center;}
.rule p.b_hd{margin-top: 5px;}
.rule p{font-size: 14px;line-height: 22px;}
.rule p img{width: 100%}
/*抽奖规则*/
.lot_rule{}
.lot_rule h4{margin-top: 10px;}
.lot_rule img{width: 100%;margin: 0 auto;}
.r_text{margin: 5px 0px 10px}
.split{padding: 5px 10px 10px 10px}
.split img{width: 100%;}
/*奖品设置*/
.slideBox{ position:relative;width: 100%;margin:0 auto;background: url("../images/prize_bg.png") 0 0 no-repeat;background-size: 100% auto;}
.slideBox .bd{margin:0px auto;}
.slideBox .bd li{position:relative;text-align:center;padding: 1.5% 0px 2% 0px}
.slideBox .bd li h4{position: absolute;top:0px;left: 50%;margin-left: -50px;width: 100px;height: 22px; line-height:22px;background: url("../images/prize_tt.png") 0 0 no-repeat;background-size: 100px auto;color: #fff;}
.slideBox .bd li .prize_name{position:absolute;bottom:4%;left:50%;margin-left:-50%;width:100%;height: 22px;line-height:22px;background-color:rgba(0, 0, 0,0.4);color: #4e92ce;}
.slideBox .bd li img{ width:46%;margin:0 auto;}

/**/
.app{width: 92%; margin: 0 auto;}
.app_l{float: left;width: 30%;}
.app_icon{}
.app_icon img{width:100%;max-width:80px;margin: 0 auto;}
.app_r{float: right;}

/*获奖名单*/
.awards_list{line-height: 22px;color: #6c8bac}
.slide_up{overflow:hidden;height:110px;position: relative;text-align: center}
/*尾部*/
.footer{width: 100%; margin-top:-28%; position:relative;z-index: 1}
.footer img{width: 100%;}

.prize_level{width: 100%;background:url(../images/rule_bg.png) 0 0 #0e0c2c no-repeat;background-size:100% 100%;border-top:1px solid #3e456d;border-left:1px solid #3e456d;text-align: center}
.prize_level thead{background-color: #0e0c2b}
.prize_level th,.prize_level td{border-right:1px solid #3e456d;border-bottom:1px solid #3e456d;}
.prize_level td{font-size: 12px;height: 38px;}
.prize_level th{color: #8fb9e6;padding: 5px 0px;font-size: 13px;}