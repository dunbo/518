<div class="newbox" id="copy_managers">
	<h6>复制权限：</h6>
	<div class="newbox_cnt">
		<form name='copyAdmin' method="post" action="__URL__/toAdmin/" class="copy_managers_form">
			<div class="copy_managers_list">
				<label>复制人:</label>
				<input type="hidden" value="{$search}" name="search" />
				<select name='aid'>
					<option value="0">请选择复制人</option>
					<volist id='list' name='admin'>
					<option value='{$list.aid}'>{$list.aname}</option>
					</volist>
				</select>
			</div>
			<input name='fromid' value='{$fromId}' type='hidden' />
		</form>
		<div class="open_close_btn">
			<a href="javascript:document.copyAdmin.submit();">确认</a>
			<a href="javascript:;" onclick="tb_remove();">取消</a>
			<div class="clear"></div>
		</div>
	</div>
</div>