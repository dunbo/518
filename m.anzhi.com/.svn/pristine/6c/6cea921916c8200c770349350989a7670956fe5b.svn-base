/* CSS Document */
body,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
fieldset,img {border: 0;}
a{text-decoration:none; color:#000000;}
.clear{clear:both;}
ul,li{list-style:none;}
body{ font-family:'微软雅黑'; font-size:14px; line-height:20px;}

#main{width:320px; margin:0 auto;}
.banner{width:320px; }
.banner img,.banner_02 img,.banner_03 img{display:block; width:320px;}
.banner_03{width:320px; height:172px;}

.active_intro{background:#fff;}
.btns{width:167px; height:45px; margin:16px auto;}
.btns_01{ display:block; width:167px; height:45px; background:url(../images/btn_01.jpg) no-repeat; cursor:pointer}
.active_txt{width:300px; margin:5px auto; color:#cc374b;}
.active_txt h6{font-size:14px; font-weight:bold}
.active_txt p{font-size:12px; line-height:18px;}

.pages_content{width:310px; padding:10px 0 10px 10px; background:#ffedb5; color:#cc374b; font-size:13px;}
.pages_content p{padding-bottom:8px;}
.footer{width:320px; height:119px; background:url(../images/footer_bg.jpg) no-repeat; overflow:hidden}
.btns_02{display:block; width:167px; height:45px; background:url(../images/btn_02.png) no-repeat;}
.btns_03{display:block; width:167px; height:45px; background:url(../images/btn_03.jpg) no-repeat;}

