/* CSS Document */
.about_us{padding:25px 22px 30px 30px;}
.about_us p{line-height:24px; margin:0 0 22px 0; text-indent:2em;}
.contact_hd{padding:5px 20px; line-height: 35px; text-indent: 2em;}
.contact{padding-bottom:20px;margin:0px 20px;  line-height:24px; border-bottom: 1px dashed #d6d6d6}
.contact_cnt{ width: 960px; margin:0 auto; padding: 5px 0px 30px 0px;}
.contact_left{float: left; color: #fff; display:inline;width: 175px;height:357px;padding:20px 10px 0px 10px;background-color: #80be44;font-size: 16px;}
.contact_left h4{ font-size: 26px; }
.contact_left p{ margin-top:15px;font-size: 14px; }
.contact_left a{ color: #fff; font-size: 16px;}
.contact_left .message,.contact_left .qq,.contact_left .r_email,.contact_left .telephone{ background: url("../images/anzhi_icon.png") no-repeat; padding-left: 50px;  margin:20px 0px;}
.contact_left .message{background-position:0 -907px; height: 41px; line-height: 41px;}
.contact_left .qq{background-position:0 -955px;height: 35px; padding-top: 10px;}
.contact_left .r_email{background-position:0 -1011px;height: 27px; line-height: 27px;}
.contact_left .telephone{background-position:0 -1051px;height: 45px; line-height: 45px;}
.contact_right{ float: left; width: 765px;}
.contact_list{ float:left;display:inline;width:225px; margin-left: 10px; margin-bottom: 10px; height:164px; padding: 10px; background-color: #ededed;}
.contact_list h6{font-size:22px; padding-bottom:4px; color: #80BE44;}
.contact_list h6 span{ font-size: 16px;}
.contact_list p{ color: #9c9c9c; height: 58px;}
.email{height:24px;color: #717171; padding-left:40px; font-size:16px;background: url("../images/anzhi_icon.png") no-repeat 0 -1103px; display:block;margin-top: 10px;}
.contact_info{ margin-top: 10px;}
.contact_info span{float:left; display:inline;}
.contact_info span.contact_tpl{float:left; height: 25px; color: #717171; line-height: 25px; font-size: 16px;padding-left:10px; /*padding-left: 25px; background: url("../images/anzhi_icon.png") no-repeat 0 -1138px;*/}

.register_box{padding:50px 30px 50px 70px;}
.register_box_l{float:left; width:460px; min-height:380px; padding-right:30px; border-right:1px solid #ebebeb;}
.register_list{height:33px; margin:10px 0 8px; line-height:33px;}
.register_list label{float:left; width:70px; padding-right:25px; text-align:right}
.register_list input.inputtext{float:left; width:250px; height:31px; padding:0 5px; background:#f5f5f5; border:1px solid #d8d8d8; line-height:31px;}
.register_list span{float:left; margin:5px 0 0 8px; diaplay:inline}
.register_list input.checkinput{background:url(../images/login_bg.gif) repeat-x 0 -66px; border:1px solid #bfbfbf}
.form_check{ color:#CE0421; padding-left:95px; font-size:12px; display:none;}
.verification{margin-top:10px;}
.verification img{float:left; width:150px; height:49px; margin:0 10px 0 95px; display:inline;}
.verification span{float:left; padding-top:30px; color:#255ca7; text-decoration:underline; cursor:pointer}
.agree{padding:20px 0 40px 95px; font-size:12px;}
.agree a{color:#255ca7; text-decoration:underline}
.register_btn{padding-left:95px;}
.register1{float:left; width:182px; height:42px; background:url(../images/bg2.png) no-repeat 0 -25px; border:none; font-size:18px; color:#fff; line-height:40px; text-align:center; cursor:pointer}
.register_btn p{float:right; padding:20px 15px 0 0;}
.register_btn p a{color:#255ca7; font-weight:bold}
.login_cookies{padding:0 0 15px 95px; font-size:12px;}
.login_check{padding:0 0 10px 95px; }
.login_check span{font-size:12px; color:#ce0421;}
#login_btn{padding-top:20px;}
.register_box_r{float:right;}
.register_box_r ul{width:260px; height:320px; padding-left:82px; background:url(../images/register_bg.png) no-repeat 0 7px;}
.register_box_r ul li{float:left; width:260px; height:75px; margin:10px 0 12px;}
.register_box_r ul li h6{padding-bottom:8px; font-size:16px; color:#336600;}
.register_box_r ul li p{color:#555555; font-size:13px; line-height:18px;}

.hr_list{padding:10px 0; float: left; width: 430px; display: inline;}
.hr_mr{ margin-right: 55px;}
.hr_list h6{font-size:16px; padding:5px 0px 5px 25px;background:url("../images/anzhi_icon.png") no-repeat -39px -812px;;}
.hr_list p{ color: #808080; line-height: 30px;}
.hr_list p strong{ color: #4d4d4d;}

.links_box{padding:0px 18px 20px 18px;}
.links_title{margin:10px 0}
.links_title h6{ width:85px; height:24px;  font-size:16px; line-height:24px;}
.links_list{float:left; padding-bottom:10px; border-bottom:1px solid #bfbfbf;}
.links_list li{float:left; width:112px; height:22px; padding-left:2px; margin-right:1px; line-height:22px; color:#252525; display:inline; font-size:12px;}
.links_list li a{color:#252525;}
.links_box p.links_request{padding:8px 0 15px; line-height:22px; color:#5d5d5d;}
#links_form p{height:29px; margin-bottom:12px; line-height:29px;}
#links_form label{ float:left; width:80px;}
#links_form .links_inputtext{width:270px; height:27px; padding:0 4px; border:1px solid #d6d6d6;}
.links_btn{padding-left:80px;}

.links_copy{padding:5px 0 8px;}
.links_copy h6{font-size:16px; color:#669801}
.links_copy p{height:32px; line-height:32px;}
.links_copy p label{float:left;}
.copy_txt{ float:left;display:inline; width:705px; height:30px; padding:0 4px; margin:0 10px; border:1px solid #d6d6d6; display:inline; line-height:30px; background-color: #fff;}
.copy_btn{float:left; display:inline; width:85px; height:32px; line-height: 32px; background-color: #7fc31e;  overflow:hidden; color: #fff; text-align: center; cursor: pointer}
#links_copy2{padding-bottom:15px;}

#getpassword form p{height:31px; margin:10px 0 20px; line-height:31px;}
#getpassword form p label{float:left; width:80px; padding-right:25px; text-align:right}
#getpassword form p input.inputtext{width:250px; height:31px; padding:0 5px; background:#cccccc; border:none; line-height:31px;}
#getpassword .open_btn{padding:10px 0 10px 35px; text-align:left}
