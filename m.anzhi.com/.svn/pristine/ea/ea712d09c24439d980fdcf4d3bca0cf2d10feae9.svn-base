/* CSS Document */
/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{max-width:640px;min-width:320px; margin:0 auto;font-size:14px; font-family:Microsoft Yahei,"微软雅黑"; }
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#001d0b; font-size: 14px;}
.a,input,button,div{-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{ float: left;}
.fr{ float: right}
.cnt{text-align: center}
.wordwrap{word-wrap:break-word;word-break:break-all;}

::-webkit-input-placeholder { color:#becaca;}
:-moz-placeholder {color:#becaca;}
::-moz-placeholder {color:#becaca;}
:-ms-input-placeholder {color:#becaca;}

.mtop{ margin-top: 35px;}
.mtop1{ margin-top: 20px;}
.mtop2{margin-top:10px;}
.mbot{margin-bottom: 8px;}
.ptop{ padding-top: 10px;}
.ptop1{padding-top: 45px;}
.pbot{padding-bottom: 25px;}
.share_btn,.btns_app{width:132px; height:37px; line-height:37px; font-size:18px;}
#slotMachineButton1{width:132px; height:37px; line-height:37px; font-size:18px; background-image: -webkit-linear-gradient(top, rgba(253, 76, 68, 1) 0%, rgba(233, 30, 60, 1) 100%); background-image: -moz-linear-gradient(top, rgba(253, 76, 68, 1) 0%, rgba(233, 30, 60, 1) 100%); background-image: -o-linear-gradient(top, rgba(253, 76, 68, 1) 0%, rgba(233, 30, 60, 1) 100%); background-image: -linear-gradient(top, rgba(253, 76, 68, 1) 0%, rgba(233, 30, 60, 1) 100%); }
.banner{position: relative; width: 100%;min-width: 320px; margin:0 auto; text-align: center;}
.banner img{ width: 100%;vertical-align: bottom;}
.banner a.detail{ display: block;width:88px; height:31px;line-height:30px;position: absolute;bottom:6px;right:2%; background-position: 0 -32px;background-size:136px 170px; font-size: 16px; color: #f0436e;}
/*公告*/
.notice{width: 100%;height: 26px; margin-top:5px;line-height: 26px;overflow: hidden; position:relative}
.notice-icon{width: 25px;height:21px;margin:2px 0px 0px 5px; background:url(../images/icon_01.png) no-repeat; background-size:25px 21px; position:absolute; left:0; top:0}
.notice-list{margin:0 10px 0 35px}
.notice-list span{color: #fff;padding-right: 20px;}
.notice-list span em{ padding-left: 10px;}
.prompt{ padding:0 0 8px; font-size:16px; color:#fffffe;}

.wth{width: 94%; margin:0 auto;}
.white-wrap{width: 94%; margin:10px auto 30px auto; background-color: #fff;}

.main{margin:8px 6px;}
.wrap{border:0px solid #fff; border-radius:8px;}

/*软件*/
.soft-p{padding:8px 0; font-size:16px;}
.soft-list li{ float: left; display: inline; width: 25%;margin:7px 0px;text-align: center;}
.soft-list li .soft-icon{text-align: center;}
.soft-list li .soft-icon img{ width: 60px;height: 60px; margin: 0 auto;}
.soft-list li .soft-name{margin-top: 6px;height: 20px; line-height: 20px; overflow: hidden;}
.soft-list li .soft-size{height: 20px; line-height: 20px;overflow: hidden;}
.soft-list li a.downlode{ display: block; width:64px;height: 25px; line-height: 26px; margin: 6px auto 8px auto;  border-radius: 3px; cursor: pointer;}

.soft-list2 li{position: relative;padding:15px 0px;clear: both; overflow: hidden;}
.soft-list2 li .soft_icon{position:relative;float: left; padding:0 10px;}
.soft-list2 li .soft_icon img{width: 60px;height: 60px;}
.soft-list2 li .soft_info h4{padding:2px 85px 4px 0px;line-height: 20px;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}
.soft_info .star{height: 13px;}
.soft_info .star img{float:left;width: 13px; height: 13px;}
.soft_info .size{ padding-top: 3px;}
.soft-list2 .downlode{position: absolute; display:block;width:64px;height:25px;line-height:26px;right: 10px;top:25px;  border-radius: 3px; cursor: pointer; text-align:center;}
.click_bar{position: absolute; display: block; width:100%; height:100%; top:0px; left: 0px; z-index: 3}

.prizemy_title{margin:20px 15px 0; font-size:18px; color:#fff}
.prizemy_title img{display:block; width:260px; z-index:2; position:relative}
.prize-title{padding:20px 10px 0; font-size:24px;}
.prize-text{padding:5px 10px 0; font-size:18px; line-height:28px}
.prize_btns{display:block; padding:4px 10px; background:#ef4854; border-radius:15px; position:absolute; right:15px; top:10px; color:#fff; font-size:16px}
.prize_btns2{position:static; width:80px; margin:10px auto 0; border-radius:10px; padding:8px; text-align:center}
.btns_tip{ border-radius:10px; color:#fff; border:none}
.tips_p{padding:20px 0 10px}
.tips_p img{display:block; width:107px; height:134px; margin:15px auto 0;}

/*detail 活动规则*/
.title{width:143px; height:53px; margin:10px auto; background:url(../images/title_bg.png) no-repeat; text-align:center; line-height:53px; color:#fff; font-size:18px; background-size:100% 100%}
.detail_wrap{margin:20px 15px; background:#febd45; padding:15px; border-radius:8px; color:#fff; position:relative;}
.detail_list h4{font-size: 18px; font-weight: bold; padding-bottom: 6px;}
.detail_list  p{font-size: 16px; line-height: 25px;}

/*活动结束*/
.end-title{width:260px; height:40px; margin:10px auto; padding:8px 0; background:url(../images/title_bg2.png) no-repeat; background-size:100% 100%; line-height:40px; font-size:24px;text-align:center}
.end{padding:10px 10px 8px; font-size:18px; line-height:28px; font-weight:normal}
.end a{ display:block; padding:15px 0 10px; font-size:18px; text-decoration:underline; color:#fff}
.end em{font-size:16px;}

/*邮寄地址*/
.form1{width:85%; margin:0 auto;}
.form_title{margin:15px auto 0;}
.form-item{ margin:8px 0 0;  font-size:16px;}
.form-item label{ float: left;width:55px; padding:8px 0;}
.form-item .textbox input{float:left; width:68%; padding:8px 12px; background:#fff; border-radius:2px; border:none; font-family: "Microsoft Yahei"; color:#898787;}
.prompt2{padding:5px 0 0;}
.prompt3{padding-left:55px}
.footer img{width:100%}

/*提示框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;z-index: 99998;display:none;}
.tip_hd img{width: 107px; height:134px; margin:15px auto 0;}
.tip-mid{padding-bottom:4px;border-radius: 0px 0px 15px 15px;}
.tip-con{padding:15px 25px 25px 25px;margin:0px 4px}
.tip-con p{line-height: 25px; text-align: center;font-size: 16px;}
.clo{ position: absolute; right: -5px; top: -6px;}
.clo a{ display: block; width:38px; height:38px;background:url("../images/tip_clo.png") 0 0 no-repeat;background-size:100% auto;text-indent: -9999px;}
.prize_okl{float:left; width:70px}
.prize_okl img{display:block; width:70px; height:70px}
.prize_okl span{display:block; line-height:22px}
.prize_okr{padding:0 0 5px 90px; font-size:16px; line-height:24px}
.btns2{width:216px; margin:0 auto}
.btns2 a{float:left; margin:10px;}
.btns3{width:288px}
.prize_ok{margin:10px 18px}

/*老虎机抽奖*/
.lottery_inner {width:270px; margin:0 auto; padding:65px 0 26px 0;}
.lottery_inner2{padding:2px; background:#d43434 }
.slotMachine{width:80px; height: 100px; margin:0 2px; overflow: hidden; display: inline-block;}
.noBorder{border:none !important; background: transparent !important;}
.slot{width:80px; height:100px; border-radius:5px;}
.slot1{background-image: url("../images/lottery_sl1.png");}
.slot2{background-image: url("../images/lottery_sl2.png");}
.slot3{background-image: url("../images/lottery_sl3.png");}
.slot4{background-image: url("../images/lottery_sl4.png");}
.slot5{background-image: url("../images/lottery_sl5.png");}

.tips_sim{ margin-bottom:8px; padding:5px; text-align:center; font-size:16px;}

/*九宫格抽奖*/
.lottery2 img{display:block; width:81px; height:81px;}
.lottery2 table{border-collapse: separate; width:100%;}
.lottery2 td{width:81px;height:81px; padding:4px 6px 6px 5px; background:#d43434}
#my_click {width:91px;height:91px; cursor:pointer;}
.lottery2 p{position:absolute; width:91px;height:91px; left:106px; top:106px; display:block;}
.lottery2 p a{display:block; width:91px; height:91px;}



/*转盘抽奖*/
.rotate_tips{padding:0 0 8px; text-align:center;}
.rotate {width: 300px; margin:0 auto;position: relative;}
.rotate-pointer {position: absolute;top: 50%;left: 50%;width: 46%;height: 46%;margin-top: -23%;margin-left: -23%;background: url(../images/pointer.png) no-repeat 0 0;background-size: 100% 100%; cursor: pointer;-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.pointer-disabled{background: url(../images/pointer_disabled.png) no-repeat 0 0;background-size: 100% 100%; z-index:11}
.rotate_list{width:60px; height:78px; position:absolute; left:0; top:0; left:50%; top:50%; z-index:10}

/*开宝箱*/
.lottery_box{margin:5px 0; height:324px; background:url(../images/lotter_bg.png) no-repeat; background-size:100% 100%; position:relative}
.gift_btn{height:31px; position:relative}
.gift_btn a{display:block; width:90px; height:29px; border:1px solid #a92c00; background-image: -webkit-linear-gradient(top, rgba(255, 243, 50, 1) 0%, rgba(253, 216, 55, 1) 100%); background-image: -moz-linear-gradient(top, rgba(255, 243, 50, 1) 0%, rgba(253, 216, 55, 1) 100%); background-image: -o-linear-gradient(top, rgba(253, 216, 55, 1) 0%, rgba(255, 243, 50, 1) 100%); background-image: -linear-gradient(top, rgba(255, 243, 50, 1) 0%, rgba(253, 216, 55, 1) 100%); line-height:29px; text-align:center; position:absolute; top:-7px; border-radius:15px;}
.gift_btn a.l{left:-3px; color:#fff}
.gift_btn a.r{right:-3px; color:#c33430}
.gift{height:240px; position:relative;}
.txt1{width:200px; height:26px; color:#fff; font-size:17px; position:absolute; left:50%; bottom:-5px; margin-left:-100px; text-align:center; line-height:26px}
.txt1 span{color:#fddf33}
.gift_box{width:160px; height:186px; background:url(../images/gift_box.png) no-repeat; background-size:100% 100%; position:absolute; left:50%; margin-left:-80px; bottom:8px; cursor:pointer; z-index:4; -webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.gift_box_disabled{ background:url(../images/gift_box3.png) no-repeat; background-size:100% 100%;}
.gift_box2{width:316px; height:180px; background:url(../images/gift_box2.png) no-repeat; background-size:100% 100%; position:absolute; left:50%; margin-left:-158px; bottom:8px; z-index:2; opacity:0}
.light{width:200px; height:136px; background:url(../images/light.png) no-repeat; background-size:100% 100%; position:absolute; left:50%; margin-left:-100px; top:-5px; z-index:3; opacity:0}
.light_open{-webkit-animation:light .6s .1s linear forwards; animation:light .6s .1s linear forwards}
.open{-webkit-animation:open .5s .0s linear forwards; animation:open .5s .0s linear forwards}

/*尾部*/
.footer{max-width:640px;}
.footer img{width: 100%;}

.blue{color:#0072bb;}
.redcolor{ color:#f6323a;}

@keyframes open {
    0%{opacity:0;}
    100%{opacity:1;}
}
@-webkit-keyframes open {
      0%{opacity:0;}
    100%{opacity:1;}
}
@keyframes light{
    0%{transform:scale(0.1,0.1);opacity:0;}
	70%{transform:scale(1,1); opacity:1;}
	90%{transform:scale(1.2,1.2); opacity:1;}
    100%{transform:scale(1,1);opacity:0;}
}
@-webkit-keyframes light{
    0%{-webkit-transform:scale(0.1,0.1);opacity:0;}
	70%{-webkit-transform:scale(1,1); opacity:1;}
	90%{-webkit-transform:scale(1.2,1.2); opacity:1;}
    100%{-webkit-transform:scale(1,1);opacity:0;}
}

/*提示框*/
#body-bg{position:absolute; top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-webkit-opacity: 0.5;opacity: 0.5;z-index: 99998;}
#body-bg2{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;z-index: 99998;display:none;}
.tip-box6{ position:absolute;width: 90%; left:50%;top:50%; background:#fff; border-radius:5px; margin-left:-45%; z-index: 99999; font-size:15px; color:#a5a5a5}
.tip-title{height:38px; padding:0 0 0 15px; border-radius:15px 15px 0 0; background: #f4f7f8; line-height:36px; position:relative}
.tip-title h6{padding-left:8px;font-size:16px; color:#545454}
.tip-con{ margin: 15px 0; padding: 10px 0 25px; border-bottom:1px solid #e6e6e6; text-align:center;}
.tip-con h4{font-size:16px; font-weight:bold; text-align:center}
.tip-title span{position:absolute; right:10px; top:6px; width:28px; height:28px; background:url('http://img3.anzhi.com/static/activity/appointment/images/tip_clo3.png') no-repeat; background-size:100% 100%; cursor:pointer}
.btns_close{display:block; height:28px; margin:0 0 15px; color:#19d5fd; line-height:28px; text-align:center; font-size:16px}
#tip-box5{background:none}
#tip-box5 img{width:300px; margin:10px auto;}
.tip-con2{ position:relative}
.tip-con2 p{text-align:center; color:#fff}
.btns_login{display:block; width:152px; height:36px; margin:20px auto 0; background:url(../images/login_btn.png) no-repeat; background-size:100% 100%; line-height:36px; text-align:center; color:#f10d2d; font-size:16px}
.tip-con2 span{display:block; width:30px; height:30px; background:url(../images/tip_clo2.png) no-repeat; background-size:100% 100%; position:absolute; right:-10px; top:-5px; cursor:pointer}
#tip-box4 .tip-title h6{background:none; padding-left:12px; font-size:18px}
#gift_name{display: inline-block; color:#ffb926; font-size:18px}
