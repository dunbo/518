/* CSS Document */
/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{max-width:640px;min-width:320px; margin:0 auto;font-size:0.14rem; font-family:Microsoft Yahei,"微软雅黑"; background: #fef0e8; color: #4d4d4d;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#4d4d4d; font-size: .14rem;}
.a,input,button,div{-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{ float: left;}
.fr{ float: right}
.banner{width:100%; position: relative;}
.banner img{ width: 100%; display:block;}
.part1{padding:0.2rem 0.2rem 1.77rem; background:url(../images/flow_bg.jpg) no-repeat; background-size:100% 100%}
.btn_nav{margin-top:-3px; position: relative; z-index: 2;}
.btn_nav_l{float:left; width:1.66rem; height:0.55rem; background:#04757c; border-radius: 25px; }
.part1 a{font-size: 0.3rem; color:#fff; text-align: center;  line-height: 0.58rem;}
.btn_nav_r{float: right; width:2.07rem; height:0.55rem; background:#04757c; border-radius: 25px;}
.part1 a.btn_log{float:left; width:1.03rem;}
.part1 a.btn_share{float:left; width:1.02rem; border-left:1px solid #5bbbc1}
.flow_info p{padding-top:0.68rem; font-size: 0.3rem; line-height: 0.23rem; color:#fff; text-align: center;}
.flow_info p span{display: block; padding:0.34rem 0 0.25rem; font-size:0.77rem; line-height: 0.6rem;}
.flow_info a{display: block; width:2.16rem; height: 0.66rem; margin: 0 auto; background:#fef0e8; border-radius: 30px; text-align: center; line-height: 0.69rem; color:#f67a7d; font-size: 0.3rem;}
.soft_wrap{padding-top:0.66rem; background:url(../images/part2_bg.png) no-repeat; background-size:100% auto}
.soft_wrap .title{padding:0 0 0 0.2rem; font-size: 0.3rem;}
.soft-list{width: 96%;margin: -0.2rem auto 0;}
.soft-list li{float: left;display:inline;width: 23%;margin: 0.5rem 1% 0;}
.soft-list li p{text-align: center;}
.soft-list li .soft-icon img{width: 92%; margin:0 auto; border-radius: 15%;}
.soft-list li .soft-name{width:100%; height:0.24rem; margin: 0.15rem 0 0.13rem; line-height:0.24rem; font-size:0.24rem; text-overflow:ellipsis; white-space:nowrap; overflow: hidden}
.soft-list li a.downlode{display:block; width:1.23rem; height:0.49rem; background:#f48f43; margin:5px auto 0; border-radius: 25px; text-align:center; line-height:0.53rem; font-size: 0.24rem; color:#fff}
.btn1{display: block; width:3.64rem; height:0.69rem; margin: 0 auto; background:#5bbbc1; border-radius: 30px; line-height: 0.72rem; font-size: 0.26rem; color: #fff; text-align: center;}

.main{min-height:6.92rem; background: url(../images/bg1.png) no-repeat; background-size: 100% auto; overflow: hidden;}
.wrap{margin: 0.35rem; border:2px solid #ff9fa1; border-radius: 10px;}
.tips{padding-bottom:1rem; text-align: center;}
.tips p{color:#ff9fa1; font-size: 0.3rem; line-height: 0.3rem;}
.end_wrap img{width:2.2rem; margin: 1rem auto 0.45rem;}
.update_wrap p{padding:0.3rem 0 0.68rem}
.update_wrap img{width:2.46rem; margin: 0 auto;}
.rule_b{display:block; padding-bottom: 0.7rem; color:#f48f43; font-size: 0.26rem; text-align: center;}
.main2{background:url(../images/bg2.png) no-repeat; background-size: 100%  auto; overflow: hidden;}
.result_wrap .tips{padding-bottom:0.25rem;}
.result_wrap img{width:1.92rem; margin: 0.78rem auto 0.3rem;}
.result_txt span,.result_txt em,.result_txt i{display: block;}
.result_txt span{font-size: 0.3rem;}
.result_txt em{padding:0.22rem 0 0.5rem; font-size: 0.64rem; color:#ff9fa1; line-height: 0.46rem;}
.result_txt i{font-size: 0.24rem; font-style:normal;}
.rule_wrap{margin-top:0.7rem; border:1px solid #ff9fa1; background:#fff; position: relative;}
.rule{padding:0.5rem 0.2rem; color:#666666; font-size: 0.24rem; line-height: 0.36rem;}
.rule h3{width:2.48rem; height:0.55rem; background:#f67a7d; border-radius: 6px; text-align: center; line-height: 0.55rem; font-size: 0.3rem; letter-spacing: 2px; color:#fff; position: absolute; left:50%; margin-left:-1.24rem; top:-0.225rem}
.log_infor{padding-top:0.1rem}
.log_infor li{margin-bottom:0.15rem; padding:0.06rem 0.15rem 0 0; font-size: 0.3rem; line-height: 0.3rem; text-align: right;}
.log_list{width:100%; font-size: 0.3rem;}
.log_list th{height:0.6rem; background:#ff9fa1; text-align: left; color:#fff; font-weight: normal;}
.log_list th,.log_list td{padding-left: 0.3rem;}
.log_list td{height:0.9rem; border-bottom: 1px solid #ff9fa1;}
.log_list td.right{padding-right:0.3rem; text-align: right;}
.footer{height:0.7rem; background:#ff9fa1; line-height: 0.7rem; text-align: center; font-size: 0.24rem; color:#fff}
.main3{background:url(../images/bg3.png) no-repeat; background-size: 100%  auto; overflow: hidden;}
.flow_num{margin:0.1rem 0.3rem 0.5rem 0; border-top:1px solid #cccccc}
.flow p{padding:0.18rem 0 0.3rem}
.flow_num{position: relative;}
.flow_num li{float:left; width:20%; padding-top:0.24rem; font-size: 0.24rem;}
.flow_num li.flow_num3{text-align: center;}
.flow_num li.flow_num4,.flow_num li.flow_num5{text-align: right;}
.flow_num span{position:absolute; top:-0.075rem; width:0.15rem; height:0.15rem; margin-left:-0.075rem; background:#cccccc; border-radius: 10px; z-index: 3; cursor: pointer;}
.flow_num span.flow1{left:0;}
.flow_num span.flow2{left:25%;}
.flow_num span.flow3{left:50%;}
.flow_num span.flow4{left:75%;}
.flow_num span.flow5{right:0}
.flow_num span.current{background:#ff9fa1}
.line{width:30%; height:1px; background:#ff9fa1; position: absolute; left:0; top:-1px; z-index: 2; transition: 0.3s all;}
.flow_tip{text-align: center; font-size: 0.24rem;}

.part2{margin-top:-0.25rem; padding:0 0.2rem;}
.part2 p{font-size: 0.3rem;}
.process_wrap{height:0.55rem; margin-bottom: 0.4rem; position: relative; overflow: hidden;}
.process{width:3.4rem; height: 0.27rem; margin-top:0.14rem; background:#fff; border-radius: 15px; border:0.04rem solid #5bbbc1; position:relative}
.process span{display: block; width:0.9rem; height:0.27rem; background:#ffe000; border-radius: 15px; position: absolute; left:0; top:0}
.process em{display: block; width:0.8rem; height:0.27rem; position: absolute; left:3.5rem; top:0rem; font-size: 0.3rem; color:#5bbbc1; line-height: 0.27rem;}
.process_wrap a{display: block; width:1.56rem; height:0.55rem; background:#f48f43; border-radius: 25px; text-align: center; line-height: 0.57rem; font-size: 0.3rem; color:#fff; position: absolute; right:0; top:0}
.process_wrap a img{display: block; width:0.56rem; height:0.35rem; margin: 0.1rem auto;}
.bg4{padding:0.7rem 0 1.1rem; background:url(../images/bg4.png) no-repeat; background-size:auto auto;}
.gray{background:#cecece}
/*提示框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{position:absolute; width:90%; height:auto; left:50%; top:50%; margin-left:-45%; z-index: 99999;}
.close{display: block; position: absolute; width:30px; height:30px; top:5px; right:5px; background:url(../images/close.png) no-repeat; background-size: 28px 28px; cursor: pointer;}
.tips_wrap{padding:0.2rem; background:#fef0e8; font-size: 0.24rem;}
.tips_form p{padding:0.2rem 0 0.4rem; text-align: center; color:#ff6e72}
.form_item{margin-bottom:0.1rem; padding:0 0.1rem}
.form_item label{display: inline-block; height:0.36rem; padding-right: 0.1rem; line-height: 0.36rem; letter-spacing: 0.02rem;}
.inputtext{display:inline-block; width:2.95rem; height:0.36rem; line-height:0.36rem; background:#fff; border:1px solid #ff9fa0; border-radius: 4px;}
.shortinput{width:1.15rem}
.code{display: inline-block; width:1.56rem; height:0.36rem; margin-left:0.15rem; background:#f1c60e; border-radius: 20px; line-height: 0.36rem; text-align: center; color:#fff; font-size: 0.18rem;}
.tips_form .btns,.btns_yellow{display: block; width:2.06rem; height:0.52rem; margin: 0.2rem auto 0.2rem; background:#5bbbc1; border-radius: 20px; text-align: center; line-height: 0.55rem; color:#fff; font-size: 0.3rem;}
#tip-box2 .wrap{background:#fef0e8 url(../images/stars.png) no-repeat; background-size:100% auto}
.tip_img img{display: block; width:2.63rem; position: relative; left:50%; margin-left:-1rem}
.tip_txt p{margin-top:0.2rem; text-align: center;}
.tip_txt p span{display:block; color:#ff9fa1; line-height: 0.46rem;}
.btns_yellow{background:#f1c60e}
#tip-box3{padding-top:0.4rem}
#tip-box3 .tip_txt{padding-top:0.3rem}
#tip-box3 .tip_txt p em{color:#ff9fa1;}
.btns2{padding:0.8rem 0.2rem 0.2rem;}
.btns2 a{display: block; width:1.56rem; height:0.69rem; background:#5bbbc1; border-radius: 25px; text-align: center; line-height: 0.69rem; color:#fff; font-size: 0.26rem;}
.btns2 a.fl{background:#a1a1a1}
#tip-box4 .btns{margin-top:0.2rem}
#tip-box5 .tip_img img{width:1.69rem}
#tip-box6 .tip_img img{width:2.18rem}
#tip-box7 .tip_img img{width:3.51rem; margin-left:-1.76rem}

.tip-loading{position:absolute; width:2.37rem; height:1.52rem; left:50%; top:50%; margin-left:-1.185rem; margin-top:-0.76rem; background:rgba(0,0,0,0.5); border-radius: 5px; z-index: 99999;}
.tip-loading img{width:0.62rem;height:0.62;margin:0.24rem auto 0.1rem; }
.tip-loading p{padding:0rem 0 0 0.5rem;color:#ffffff;font-size: 0.22rem;}