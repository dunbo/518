/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto; background:#2a0c4a; font-size:15px; font-family:Microsoft Yahei,"微软雅黑"; line-height:20px}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none; font-size: 14px; outline: none;}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{float:left;}
.fr{float:right;}


.main{position:relative; background:url(../jan_sign_images/main_bg1.png) repeat-y; background-size:100% 100%}
.maininner{padding:0 10% 6px; background:url(../jan_sign_images/bg1.png) no-repeat; background-size:100% auto; position:relative}
.mainbom img{width:100%}
/*有通用的地方*/
.wrap{padding:32px 18px 10px 5px; background:url(../jan_sign_images/bg2.png) no-repeat top right; background-size:100% 100%; position:relative}
.rounddot1{position:absolute; left:14px; top:14px; width:8px; height:8px; background:#d6a4ff; border-radius:50%}
.rounddot2{position:absolute; right:24px; top:14px; width:8px; height:8px; background:#d6a4ff; border-radius:50%}
.main2{padding: 60px 0 0; background: rgba(0, 0, 0, 0) url("../jan_sign_images/bg3.png") no-repeat scroll 0 0 / 100% auto;overflow: hidden;}
.wrap2{margin:0 15px; padding:10px 10px 0; background:#783bbc; position:relative}
.wrap2_bg{display:block; width:100%; height:5px; background:url(../jan_sign_images/main_bg3.png) no-repeat; background-size:100% 100%; position:absolute; left:0; top:-5px}
.titles{width:96px; height:28px; margin:3px 0 10px; background:url(../jan_sign_images/title_01.png) no-repeat; background-size:100% 100%}
.rule_title2{ background:url(../jan_sign_images/title_02.png) no-repeat; background-size:100% 100%}
.rule_title3{background:url(../jan_sign_images/title_06.png) no-repeat; background-size:100% 100%}
.end_title{background:url(../jan_sign_images/title_07.png) no-repeat; background-size:100% 100%}
.btns{background:url(../jan_sign_images/btns_01.png) no-repeat; background-size:100% 100%; color:#671808; font-size:16px; font-weight:bold; text-align:center;}
.title{padding:10px 0 8px 0; position:relative}
.title h3{font-weight:bold;color:#2a0c4a}

.banner{width: 100%; margin: 0 auto; background:url(../jan_sign_images/); position:relative; }
.banner img{ width: 100%;}
/*index*/
.tiger{width:134px; height:72px; background:url(../jan_sign_images/tiger.png) no-repeat; background-size:100% 100%; position:absolute; left:31%; top:-6px}
.rule{padding:50px 0 0 0; color:#c99afa;}
.rule p{line-height: 22px;}
.login_wrap{margin:12px 0 0; padding:20px 5px 10px; background:#fece50; position:relative; color:#da5d1a}
.logined{ padding:0 8px; position:relative}
.logined_r{width:80px; height:26px; position:absolute; right:5px; top:2px;}
.logined_r a,.my_prize_list li a{display:block; width:80px; height:30px; background:url(../jan_sign_images/btns.png) no-repeat; background-size:100% 100%; text-align:center; line-height:31px; color:#fff; font-weight:bold}
.logined_l p{ padding:2px 0;}
.login{padding-top:15px; padding-bottom:15px}
.login .logined_r{top:13px;}
.star_list{margin: 10px 0px;}
.star_list span{float: left;width:32px;height:32px;line-height:32px;background: url("../jan_sign_images/stars.png") 0 0 no-repeat;background-size: 48px 144px;text-align: center}
.star_list span.star_gold{background-position:0 0; color:#915825}
.star_list span.star_gray{background-position:0 -40px;color: #606060}
.star_list span.star_lightgray{background-position:0 -78px;color: #d0d0d0}
.star_list em{float: left;width:6px;height:32px;background: url("../jan_sign_images/stars.png") 0 0 no-repeat;background-size: 48px 144px;}
.star_list em.spot_gold{background-position:-40px 0px;}
.star_list em.spot_gray{background-position:-40px -40px;}
.star_list em.spot_lightgray{background-position:-40px -76px;}
.active_ctn{ margin-bottom:10px;}
.active_ctn li{position:relative; padding:0 0 0 10px; line-height:22px; color:#ffebc0; font-weight:bold}
.active_ctn span{width:20px; position:absolute; top:1px; color:#f6b527}
.active_ctn p{margin-left:20px;}
.notice{ margin:18px 0; padding:2px; background:url(../jan_sign_images/notice_bg.png) no-repeat; background-size:100% 100%; color:#fff1ad;}
.notice_list{height:28px; padding:0 25px 0 15px; line-height:28px;}
.btns1 a{display:block; width:105px; height:36px; margin:10px auto; line-height:36px}
.soft{padding:14px 0 10px; position:relative}
.soft .titles{width:125px; background:url(../jan_sign_images/title_03.png) no-repeat; background-size:100% 100%}
.soft_title a{display:block; width:80px; height:24px; background:url(../jan_sign_images/sfot_btn.png) no-repeat; background-size:100% 100%; text-align:center; line-height:25px; font-weight:bold; position:absolute; right:3.5%; top:18px; color:#671808}
.soft_list{padding:10px 0;}
.soft_list li{ float: left; display: inline; width: 25%;margin-top:10px;text-align: center;}
.soft_list li .soft-icon{text-align: center;}
.soft_list li .soft-icon img{ width: 66px;height: 66px; margin: 0 auto;}
.soft_list li .soft-name{margin-top: 10px;height: 20px; line-height: 20px; overflow: hidden; color:#fdb902; white-space:nowrap; overflow:hidden; text-overflow:ellipsis}
.soft_list li .soft-name a{ color: #fdb902;}
.soft_list li a.downlode{ display: block; width:64px;height: 24px; line-height: 24px; margin: 10px auto; background:#c792fc; background-clip: padding-box; border-radius:15px; border:1px solid #b453ff; line-height:24px; color:#6126a0;cursor: pointer; text-align: center; font-weight:bold}

.lottery_wrap{position:relative}
.lottery_wrap .titles{width:168px; background:url(../jan_sign_images/title_04.png) no-repeat; background-size:100% 100%}
.lottery_bg{width:214px;height:224px;background:url(../jan_sign_images/lottery_bg.png) no-repeat; background-size:100% 100%; position:absolute; left:-11%; top:60px}
.active_title{background:url(../jan_sign_images/title_05.png) no-repeat; background-size:100% 100%}
.lotter_wrap{position: relative; margin:0 10px 0;overflow:hidden}
.lottery_info{margin:10px 10px 0; color:#fff;}
.lottery_info p{ font-size:14px}
.lottery_info p span{font-size:16px; color:#ff0700}
.lottery{position:relative; margin-top:50px; z-index:2}
.lottery_wrap  a{display:block; position:absolute; right:15px; top:65px; width:102px; height:33px; background:url(../jan_sign_images/lottery_btn.png) no-repeat; background-size:100% 100%; text-align:center; line-height:34px; font-weight:bold; color:#7b1307}
.rotate {width:92%;  margin:5px auto 0; position: relative; overflow}
.rotate-con{width:100%;}
.rotate-pointer {position: absolute;top: 50%;left: 50%;width: 30%;height: 30%;margin-top: -15.4%;margin-left: -15.6%; background: url(../jan_sign_images/pointer.png) no-repeat 0 0;background-size: 100% 100%; cursor: pointer;-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.pointer-disabled{background: url(../jan_sign_images/pointer_disabled.png) no-repeat 0 0;background-size: 100% 100%; z-index:11}

/*我的奖品*/
.my_prize{padding:15px; position:relative; color:#c99afa}
.my_prize_list li{line-height:25px; font-size:16px; position:relative;}
.my_prize_list li a{width:100px; height:30px; margin:5px auto 0; padding:0; background:#d1a3ff; background-clip: padding-box; border-radius:15px; border:1px solid #b453ff; line-height:31px; font-weight:bold; color:#6126a0}
.my_prize_list2{padding:5px 0;}
.my_prize_list2 li{ position:relative; font-size:16px}
.my_prize_list2 p{ padding:6px 0;}
.form-items li{ margin-top: 20px; clear: both; overflow: hidden;}
.form-items li label{ float: left;width:90px;padding-top: 10px; color:#c99afa}
.form-items li span{display:block;margin-left: 90px;}
.form-items li input{ width:90%;padding:10px 6px;border:none;color: #000;  font-family: Microsoft Yahei,"微软雅黑"; background-color:#fff; border-radius:2px}
.form_error{display:block; padding:8px 0 0 90px; color:red}
.split{height:7px; background:url(../jan_sign_images/line_bg.png) no-repeat; background-size:100% 100%; margin: 8px 0px 15px; overflow:hidden;}

/*结束页*/
.awards{width:80%; margin:0 auto; padding:10px 20px; position:relative; color:#ffe6b1}
.awards_list{line-height: 22px; text-align:center}
.slide_up{overflow:hidden;height:110px;position: relative;}
/*提示框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-webkit-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{ position:absolute; width:90%; left:50%; top:50%; padding:0 0 30px 0; margin-left:-45%; z-index: 99999; background:url(../jan_sign_images/dialog_bg.png) no-repeat; background-size:100% 100%; font-size:15px;}
.tip-con{ margin: 15px 15px 0 15px;padding: 50px 9% 10px 13%; color:#783bbc}
.tip_title h6{ padding:6px; min-height:22px;  color:#4a1a7f; font-size:15px; font-weight:bold; line-height:18px;text-align:center}
.tip-con p{padding:0 0 0 20px; line-height: 22px; word-wrap:break-word;word-break:break-all; font-size:14px;}
.tips_btns a{display:block; width:82px; height:28px; margin:0 auto; line-height:28px; background:url(../jan_sign_images/btns.png) no-repeat; background-size:100% 100%; color:#fff; text-align:center; font-size:16px; font-weight:bold;}

.footer{background:#34006e; text-align:center; position:relative; color:#a351ff}
.footer p{padding:12px 10px 10px; line-height:22px}
.bold{font-weight:bold;}
.red{color:#ab0500}
.prompt_01{color:#ffebc0}
.mtop10{margin-top: 10px;}

@keyframes pound{
    from{transform: none;}
    50% {transform: scale(1.2);}
    to {transform: none;}
}
@-webkit-keyframes pound{
    from{-webkit-transform: none;}
    50% {-webkit-transform: scale(1.2);}
    to {-webkit-transform: none;}
}
.heart {animation: pound 1.2s infinite; -webkit-animation: pound 1.2s infinite;}
