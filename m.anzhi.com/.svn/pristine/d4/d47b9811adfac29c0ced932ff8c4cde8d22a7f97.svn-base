/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width:320px; max-width:640px;  margin:0 auto; background:#ed63ad; font-size:16px; font-family:Microsoft Yahei,"微软雅黑"; line-height:20px}
html{overflow:hidden}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none; font-size: 14px; outline: none;}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{float:left;}
.fr{float:right;}

.loading_progress{width:70%; height:28px; background:white; border:2px solid #ffbed0; border-radius:10px; position:absolute; left:50%; margin-left:-35%; bottom:25px; overflow:hidden; z-index:1; opacity:0}
.progress_bar{width:0%; height:100%; border-radius:10px; background:url(../images/loading_bg.png); background-size:1px 43px;}
#main{width:100%; height:100%; position:relative}
#loadding_page{width:100%; height:100%; background:#ed63ad; position:absolute; left:0; top:0; z-index:11}
.loadding_txt{width:291px; height:64px; background:url(../images/txt_01.png) no-repeat; background-size:100% 100%; margin:20px auto}
.loadding_img{width:88%; height:64%; margin-left:4%; background:url(../images/person_01.png) no-repeat; background-size:100% 100%; position:relative}
.loadding_txt2{width:161px; height:110px; background:url(../images/txt_02.png) no-repeat; background-size:100% 100%; position:absolute; right:-20px; top:-20px; z-index:1}
.loadding_img_inner{width:100%; height:100%; background:url(../images/person_02.png) no-repeat; background-size:100% 100%; position:absolute; left:0; top:0;animation:personMove .8s infinite linear; -webkit-animation:personMove .8s infinite linear;}
.loadding_img2{width:567px; height:191px; margin-top:-1px; background:url(../images/person_03.png) no-repeat;}

#page1{width:100%; height:100%; position:absolute; left:0; top:0; z-index:10; opacity:0;}
.night{width:100%; height:100%; background:url(../images/night_sky_01.png) no-repeat; background-size:100% 100%; position:absolute; left:0; top:0}
.night2{width:100%; height:100%; background:url(../images/night_sky_02.png) no-repeat; background-size:100% 100%; position:absolute; left:0; top:0; animation:personMove .8s linear infinite; -webkit-animation:personMove .8s linear infinite;}
.person3{width:100%; height:91%; background:url(../images/person_04.png) no-repeat; background-size:100% 100%; position:absolute; left:0; bottom:0;}
.txt_03{width:172px; height:62px; background:url(../images/txt_03.png) no-repeat; background-size:100% 100%; position:absolute; left:25px; top:30px; opacity:0}
.arrow{width:56px; height:36px; background:url(../images/arrow.png) no-repeat; background-size:100% 100%; position:absolute; left:50%; margin-left:-30px; bottom:20px; animation: arr_ac 1s infinite ease-in-out; -webkit-animation: arr_ac 1s infinite ease-in-out; z-index:10}

#problem{width:100%; height:100%; position:absolute; left:0; top:0; background:#ed63ad; z-index:9; opacity:0;}
.problem_list{display:none}
.problem_list p{margin:50px 0 25px; color:#fff; text-align:center; font-size:20px}
.question{width:100%;}
.question img{display:block; width:100%;}
.answer{margin-top:20px;}
.answer ul li{float:left; width:21.5%; margin-left:2%; border:2px solid #fff; border-radius:15px; display:inline; position:relative}
.answer ul li.select:before,.answer ul li.right:after,.answer ul li.wrong:after,.answer ul li.miss:after{position:absolute; left:0; top:0; width:100%; height:100%; content:" "}
.answer ul li.right:after,.answer ul li.wrong:after,.answer ul li.miss:after{width:46px; height:34px; left:50%; margin-left:-23px; top:64%;}
.answer ul li img{display:block; width:70px; height:70px; margin:13px auto 8px;}
.answer ul li span{display:block; height:22px; margin-bottom:10px; line-height:22px; text-align:center;}
.answer ul li.select:before{background:rgba(99,99,99,.4); border-radius:15px;}
.answer ul li.right:after{background:url(../images/right2.png) no-repeat; background-size:100% 100%;}
.answer ul li.wrong:after{background:url(../images/error.png) no-repeat; background-size:100% 100%;}
.answer ul li.miss:after{background:url(../images/right1.png) no-repeat; background-size:100% 100%;}
.answer ul li.lengthli img{margin-top:8px; margin-bottom:6px;}
.answer ul li.lengthli span{line-height:18px; margin-bottom:17px}
.answer2{margin-top:50px;}
.answer2 ul li{width:46%}
.answer2 ul li img{width:90%; height:26px;}
.answer2 ul li.right:after,.answer2 ul li.wrong:after,.answer2 ul li.miss:after{width:46px; height:34px; left:50%; margin-left:-23px; top:15%;}
.btns{display:block; width:105px; height:42px; background:url(../images/btns.png) no-repeat; background-size:100% 100%; position:absolute; left:50%; margin-left:-53px; bottom:60px; text-indent:-9999px}
.btns2{background:url(../images/btns2.png) no-repeat; background-size:100% 100%;}

.person4{width:100%; height:100%; background:url(../images/person_05.png) no-repeat; background-size:100% 100%; position:absolute; left:0; bottom:0;}
.txt_04{width:143px; height:97px; background:url(../images/txt_04.png) no-repeat; background-size:100% 100%; position:absolute; right:6px; top:28px}

#applist{width:100%; height:100%; position:absolute; left:0; top:0; z-index:8; opacity:0;}
