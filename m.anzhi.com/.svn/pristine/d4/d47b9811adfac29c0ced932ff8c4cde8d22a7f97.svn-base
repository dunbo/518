.app li:nth-of-type(3) em{background:url(../images/app/app_10.png) no-repeat; background-size:100% 100%}
.app li:nth-of-type(4){left:83px; top:104px}
.app li:nth-of-type(4) span{background:url(../images/app/app_04.png) no-repeat; background-size:100% 100%}
.app li:nth-of-type(4) em{background:url(../images/app/app_11.png) no-repeat; background-size:100% 100%}
.app li:nth-of-type(5){left:160px; top:70px}
.app li:nth-of-type(5) span{background:url(../images/app/app_05.png) no-repeat; background-size:100% 100%}
.app li:nth-of-type(5) em{background:url(../images/app/app_12.png) no-repeat; background-size:100% 100%}
.app li:nth-of-type(6){left:240px; top:27px}
.app li:nth-of-type(6) span{background:url(../images/app/app_06.png) no-repeat; background-size:100% 100%}
.app li:nth-of-type(6) em{background:url(../images/app/app_13.png) no-repeat; background-size:100% 100%}
.app li:nth-of-type(7){left:234px; top:104px}
.app li:nth-of-type(7) span{background:url(../images/app/app_07.png) no-repeat; background-size:100% 100%}
.app li:nth-of-type(7) em{background:url(../images/app/app_14.png) no-repeat; background-size:100% 100%}
#share{width:100%; height:100%; position:absolute; left:0; top:0; z-index:7; opacity:0;}
.person6{width:100%; height:80%; background:url(../images/person_06.png) no-repeat; background-size:100% 100%; position:absolute; left:0; bottom:18%;}
.txt_06{width:185px; height:72px; background:url(../images/txt_06.png) no-repeat; background-size:100% 100%; position:absolute; left:10%; top:5px; opacity:0}
.share_btn{width:74%; height:29px; position:absolute; left:50%; margin-left:-37%; bottom:41px;}
.share_btn a{height:29px; display:block; text-indent:-9999px}
.share_btn a:nth-of-type(1){float:left; width:72px; background:url(../images/share_btn1.png) no-repeat; background-size:100% 100%}
.share_btn a:nth-of-type(2){float:right; width:114px; background:url(../images/share_btn2.png) no-repeat; background-size:100% 100%}
.logo{width:68px; height:30px; background:url(../images/logo.png) no-repeat; background-size:100% 100%; position:absolute; right:5px; bottom:5px;}
.share_mark{width:100%; height:100%; position:absolute; left:0; top:0; background:rgba(0,0,0,.6); display:none;}
.share_mark img{width:75%; position:absolute; right:10px; top:30px;}
.txt_ac{animation:fadeIn 0.7s ease-in-out forwards; -webkit-animation:fadeIn 0.7s ease-in-out forwards;}
/* 右侧滑入 */
.right-in {animation:right_in_animate 0.7s ease-in-out 0s 1 normal forwards; -webkit-animation:right_in_animate 0.7s ease-in-out 0s 1 normal forwards;}
@keyframes right_in_animate{
	from{opacity:0; right:0}
	to{opacity:1; right:160px}
}
@-webkit-keyframes right_in_animate{
	from{opacity:0; right:0}
	to{opacity:1; right:160px}
}
/* 左侧滑入 */
.left-in {animation:left_in_animate 0.7s ease-in-out 0s 1 normal forwards; -webkit-animation:left_in_animate 0.7s ease-in-out 0s 1 normal forwards;}
@keyframes left_in_animate{
	from{opacity:0; left:0}
	to{opacity:1; left:155}
}
@-webkit-keyframes left_in_animate{
	from{opacity:0; left:0}
	to{opacity:1; left:155}
}
@keyframes fadeIn {
    0%{opacity:0}
	100%{opacity:1}
}
@-webkit-keyframes fadeIn {
    0%{opacity:0}
	100%{opacity:1}
}
@keyframes diminish {
    0%{transform: scale(2.3); opacity:1}
	20%{transform: scale(2.0); opacity:1}
	40%{transform: scale(1.6); opacity:1}
	60%{transform: scale(1.3) rotateY(-90deg);}
	90%{transform: scale(0.9) rotateY(-180deg); opacity:0}
	100%{transform: scale(1.0);}
}
@-webkit-keyframes diminish {
    0%{-webkit-transform: scale(2.3); opacity:1}
	20%{-webkit-transform: scale(2.0); opacity:1}
	40%{-webkit-transform: scale(1.6); opacity:1}
	60%{-webkit-transform: scale(1.3) rotateY(-90deg);}
	90%{-webkit-transform: scale(0.9) rotateY(-180deg); opacity:0}
	100%{-webkit-transform: scale(1.0);}
}
.largen {animation:largen 0.7s ease-in-out 0s 1 normal forwards; -webkit-animation:largen 0.7s ease-in-out 0s 1 normal forwards;}
@keyframes largen {
    0%{transform: scale(0.0);}
	20%{transform: scale(0.3);}
	40%{transform: scale(0.6);}
	60%{transform: scale(0.9);}
	70%{transform: scale(1.2);}
	100%{transform: scale(1);}
}
@-webkit-keyframes largen {
    0%{-webkit-transform: scale(0.0);}
	20%{-webkit-transform: scale(0.3);}
	40%{-webkit-transform: scale(0.6);}
	60%{-webkit-transform: scale(0.9);}
	70%{-webkit-transform: scale(1.2);}
	100%{-webkit-transform: scale(1);}
}
@keyframes arr_ac{
	0%,30%{opacity:0; transform:translate(0, 10px);}
	60%{opacity:1; transform:translate(0,0);}
	100%{opacity:0; transform:translate(0,-8px);}
}
@-webkit-keyframes arr_ac{
	0%,30%{opacity:0; -webkit-transform:translate(0, 10px);}
	60%{opacity:1; -webkit-transform:translate(0,0);}
	100%{opacity:0; -webkit-transform:translate(0,-8px);}
}
@keyframes personMove{
	0%{opacity:0}
	100%{opacity:1}
}
@-webkit-keyframes personMove{
	0%{opacity:0}
	100%{opacity:1}
}
@keyframes loadingMove{
	0%{background-position:1px 0}
	100%{background-position:0 0}
}
@-webkit-keyframes loadingMove{
	0%{background-position:43px 0}
	100%{background-position:0 0}
}
@keyframes pound{
    from{transform: none;}
    50% {transform: scale(1.2);}
    to {transform: none;}
}
@-webkit-keyframes pound{
    from{-webkit-transform: none;}
    50% {-webkit-transform: scale(1.2);}
    to {-webkit-transform: none;}
}
.heart {animation: pound 1.2s infinite; -webkit-animation: pound 1.2s infinite;}
