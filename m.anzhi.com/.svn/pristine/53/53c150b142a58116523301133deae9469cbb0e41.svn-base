function onLoginComplete(sid,tip){
	var is_register = 0;
	if(tip == "注册成功"){
		var is_register = 1;
	}
	sid_str = sid.replace("PHPSESSID=", "");
	location.href="/lottery/"+prefix+"/index_2017.php?sid="+sid_str+"&aid="+aid+"&is_register="+is_register;
}
function my_prize(){
	<!--{if $out['is_login'] == 1}-->
		location.href="/lottery/"+prefix+"/userinfo_2017.php?sid="+sid+"&aid="+aid;
	<!--{else}-->
		login('<!--{$out.login_url}-->','<!--{$out.version_code}-->');
	<!--{/if}-->
}

function my_userinfo(){
	location.href="/lottery/"+prefix+"/userinfo_2017.php?types=3&sid="+sid+"&aid="+aid;
}
function recode_last(is_drop,from_type){
	$.ajax({
		url: '/lottery/'+prefix+'/index_2017.php',
		data: 'sid='+sid+'&aid='+aid+"&login_check=1&is_drop="+is_drop+"&from_type="+from_type,
		type: 'get',
		async:false,
		success:function(data){

		}
	});
}
//抽奖
function lottery_click(from_type,azb_mount){
	cloBox('#tip-box');
	<!--{if $out['is_login'] == 2}-->
		recode_last(1,from_type);
		login('<!--{$out.login_url}-->','<!--{$out.version_code}-->');
		return false;
	<!--{/if}-->
	if(from_type>1 && (az_money < azb_mount)){
		recode_last(0,from_type);
		$("#tip_but").attr('onclick', "cloBox('#tip-box')").attr('class',"r").html("确 定");
		pop_tips("","您的安智币余额不足，请在电脑端安智用户中心或联运游戏中进行充值；","",1);			
		return false;		
	}
	if(from_type>1 && isHasPayPwd == 1){
		$("#sub_pwd").attr('onclick', 'lottery_do('+from_type+','+azb_mount+')');
		pop_tips("","","2",1);		
		return false;		
	}else{
		lottery_do(from_type,azb_mount);
	}	
}
function lottery_do(from_type,azb_mount){
	if(isHasPayPwd == 1){
		cloBox('#tip-box2');
		var pwd = $("#azb_pwd").val();		
	}
	$('#lottery_'+from_type).attr('onclick', '');
	$.ajax({
		url: '/lottery/'+prefix+'/lottery_2017.php',
		data: 'sid='+sid+'&aid='+aid+"&pwd="+pwd+"&from_type="+from_type+"&azb_mount="+azb_mount,
		type: 'post',
		dataType: 'json',				
		success: function(data){
			if(data.code == 1){
				az_money = az_money-azb_mount;
				eggs();//砸蛋效果
				$('#az_money').html(az_money);
				if(from_type == 3){
					$(".tip-con").attr('style', "height:150px; overflow:auto");	
				}
				if(data.is_kind == 1){
					$("#tip_but").attr('onclick', "my_userinfo();").html("完善个人信息").attr('class',"r");
				}else{
					$("#tip_but").attr('onclick', "my_prize();").html("查看获奖记录").attr('class',"r");
				}	
				if(from_type == 1){
					$('#lottery_'+from_type).html('明天再来哦').attr('class','egg_btn egg_no');
				}else{
					$('#lottery_'+from_type).attr('onclick', 'lottery_click('+from_type+','+azb_mount+')');
				}
				setTimeout(function(){
					pop_tips('',data.msg,"",1);
				}, 2000);
			}else if(data.code == 2){
				location.href=data.url+"?sid="+sid+"&aid="+aid;
			}else{
				$('#lottery_'+from_type).attr('onclick', 'lottery_click('+from_type+','+azb_mount+')');
				pop_tips("",data.msg,"",1);		
					
			}			
		}
	});
}


