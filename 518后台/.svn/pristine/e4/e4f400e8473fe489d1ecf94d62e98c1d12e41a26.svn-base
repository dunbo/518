<center>
<script>
function checkinput(){
	var pkg = document.getElementById('pkg').value;
	var level = document.getElementById('lvl').value;
	if(pkg.length == 0){
		alert('包名不能为空');
		return false;
	}
	if(level.length == 0){
		alert('优先级不能为空');
		return false;
	}
	return true;
}
</script>
<form action="__URL__/addreadysoft" method="POST">
<table>
<tr><td>软件包名:</td><td><input type="text" name="package" id="pkg"/></td></tr>
<tr><td>优先级:</td><td><input type="text" name="level" id="lvl"/></td></tr>
<tr><td></td><td><input type="submit" value="提交" onclick="return checkinput()"></td></tr>
</table>
</form>
</center>