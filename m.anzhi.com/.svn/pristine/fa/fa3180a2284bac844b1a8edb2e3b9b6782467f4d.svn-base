.comment_title{width:82px; height:24px; background:url(http://img3.anzhi.com/static/feature/images/bg3.png) no-repeat; background-size:100% 100%; color:#fff; font-size:16px; text-indent:6px; line-height:24px; position:relative}

.comment_list{margin-bottom:10px;overflow:hidden}
.comment_list li{position:relative; margin:10px 0 0; padding-bottom:4px; border-bottom:1px solid #e6e6e6;}
.comment_list .comment_img{display:block; width:42px; height:42px; position:absolute; left:18px; top:0; border-radius:50%}
.comment_r{margin:0 12px 0 66px}
.comment_name{height:24px;line-height:24px}
