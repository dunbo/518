/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto;font-size:15px; font-family:Microsoft Yahei,"微软雅黑"; background: #ad1c3b; color: #000000;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#000; font-size: 14px; outline: none;}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.cl{overflow:hidden;}
.clear{clear:both;overflow:hidden;}
.l{float:left}
.r{float:right}

.banner{width: 100%; margin: 0 auto;}
.banner img{ width: 100%;}
.btns{display:block; width:110px; height:34px; margin:20px auto 0;  border:2px solid #740000; border-radius:18px; background:url(../images/btns_04.png) no-repeat; background-size:100% 100%; color:#00455e; text-align:center; line-height:30px; font-size:18px; font-weight:bold; color:#8c2700}
/*活动规则*/
.title1{height:56px; background:url(../images/title_bg.png) no-repeat; background-size:100% 100%; position:relative; overflow:hidden}
.title1 h3{width:108px; height:25px; margin:25px auto 0; background:url(../images/txt_01.png) no-repeat; background-size:100% 100%; text-indent:-9999px}
.rule{  color:#803b02; font-size:15px;}
.wrap_inner{padding:6% 10% 13% 12%; background:url(../images/bg1.png) no-repeat; background-size:100% 100%;}
.rule p{line-height: 24px;}
.rule p.rule_title{ font-size:16px; font-weight:bold}
.rule a{float:right;margin-right:10px; padding:2px 10px; display:inline; border:1px solid #803b02; border-radius:10px; color:#530200; text-align:center;}
/*公告*/
.notice{height:32px; margin:20px 4%; padding:0 20px; background:#ffa803; border-radius:15px; line-height:32px; color:#803b02; font-size:16px}
.main{margin-top:-30px; padding:0 0 12px; position:relative;}

.prize_list{width:100%; color:#a00b12; height:280px; position:relative}
.prize_list li{position:absolute; width:92px; height:138px; margin:1% 0 0;}
.prize_list li.prize_list1{left:1.5%; top:0}
.prize_list li.prize_list2{right:0; top:0}
.prize_list li.prize_list3{left:1.5%; top:138px}
.prize_list li.prize_list4{right:0; top:138px} 
.prize_list li.pointer{width:74px; height:74px; background:url(../images/pointer.png) no-repeat; background-size:100% 100%; position:absolute; left:50%; top:50%; margin-left:-37px; margin-top:-37px; cursor:pointer}
.prize_list li.disabled{background:url(../images/pointer_disabled.png) no-repeat; background-size:100% 100%;}
.prize_list li.prize_tips{width:70px; min-height:24px; height:auto; position:absolute; left:50%; bottom:16%; margin-left:-35px; color:#cc0000; font-weight:bold; font-size:15px; text-align:center}
.prize_info{width:72px; position:relative; padding:7px 10px 3px;}
.prize_info span{width:100%; margin:2px 0 0; font-size:12px}
.prize_img{width:72px; height:72px; border-radius:4px; background:#ffa803}
.prize_action{animation:0.5s flashlight 3; -webkit-animation:0.5s flashlight 3}
.prize_info img{display:block; width:72px; height:72px; margin:2px auto 0;}
.prize_info h4{padding:6px 0; font-size:12px; color:#803b02; white-space:nowrap; text-overflow:ellipsis; overflow:hidden; text-align:center}
.prize_info span.prize_current{width:72px; height:72px; background:rgba(255,240,0,.65); position:absolute; left:10px; top:7px}
.prize_shapes{width:72px; height:10px; background:rgba(0,0,0,.43); border-radius:5px; margin:0 auto; position:relative; font-size:12px}
.prize_shapes i{display:block; width:60%; height:10px; border-radius:4px; background:url(../images/bg6.png) no-repeat; background-size:100% 100%}
.prize_shapes span{float:left;}

/*软件*/
.title{padding:4px 0 0 0; position:relative}
.title h3{color:#f4c100; font-weight:bold;}
.title img{display:block; width:110px;}
.title p{padding:5px 0; color:#fff; font-size:16px}
.title2 h3{width:110px; height:30px; margin:0 auto 15px; background:#a60401; border-radius:20px; font-weight:bold; color:#fde1e1; text-align:center; font-size:18px; line-height:26px;}
.list_mod .title h3{width:106px; height:22px; background:url(../images/txt_01.png) no-repeat; background-size:100% 100%}
.prize_btn{position:absolute; right:2%; top:10px; display:block;width:118px; height:42px; background:url(../images/btns_01.png) no-repeat; background-size:100% 100%; text-indent:-9999px}
.prize_btnh{background:url(../images/btns_01h.png) no-repeat; background-size:100% 100%;}
.btn_change{position:absolute; right:0; top:0; width:84px;height: 26px; line-height:26px; margin: 3px auto 10px auto; background:#ffc000; border-radius:5px; color:#9c0d2b; font-weight:bold; text-align:center; font-size:15px}
#soft_mod{padding:15px 0 30px; background:url(../images/bg2.png) no-repeat; background-size:100% 100%}
.soft_wrap{margin:0 5%; padding:10px; background:#9c0d2b; border:1px solid #eb3057; border-radius:15px;}
.soft-list{padding:5px 0 0; margin:10px 0 0;}
.soft-list li{ float: left; display: inline; width: 25%;margin:7px 0px;text-align: center;}
.soft-list li .soft-icon{text-align: center;}
.soft-list li .soft-icon img{ width: 60px;height: 60px; margin: 0 auto; border:2px solid #fff; border-radius:8px}
.soft-list li .soft-name{margin-top: 10px;height: 20px; color:#fff; line-height: 20px; overflow: hidden;}
.soft-list li .soft-name a{ color: #fff;}
.soft-list li a.downlode{ display: block; width:63px;height: 24px; line-height: 24px; margin: 13px auto 0 auto; background:#ffa803; border-radius:4px; color: #8c2700;cursor: pointer; font-weight:bold}
.share_btn{display:block; width:150px; height:44px; margin:0 auto 20px; background:url(../images/btns_02.png) no-repeat; background-size:100% 100%; line-height:44px; text-align:center; color:#7e341f; font-weight:bold; font-size:18px}

/*登录以后*/
.line_left{width:12px; height:66px; background:url(../images/line_01.png) no-repeat; background-size:100% 100%; position:absolute; left:50px; top:-25px; z-index:2}
.line_right{width:12px; height:66px; background:url(../images/line_01.png) no-repeat; background-size:100% 100%; position:absolute; right:50px; top:-25px; z-index:2}
.line_left2,.line_right2{height:120px; top:-91px; background:url(../images/line_02.png) no-repeat; background-size:100% 100%;}
.line_left2{left:50px;}
.line_right2{right:50px}

.logined_wrap{padding:7px 0 18px; background:url(../images/stars_03.png) no-repeat; background-size:100% 100%;}
.logined{ margin:0 5%; position:relative;min-height:35px; background:#ffa803; padding:18px 20px 12px 20px; border-radius:15px}
.logined ul{ line-height: 26px; color: #803b02;}
.logined ul li{float:left;}
.soft_icon{position: absolute;top:50%;right:10px;margin-top: -30px;}
.soft_icon img{width: 60px; height: 60px;}
.user_info{position: absolute;top:8px;right:12px;}
.user_info a,.user_info2 a{float:right; display: block;width: 84px;height: 28px;background:url(../images/btns_01.png) no-repeat; background-size:100% 100%; border: 1px solid #ffc99d; border-radius: 15px; line-height: 28px;text-align: center; color: #fff; font-size:15px; font-weight:bold;}
.user_info2{position: absolute;bottom:8px;right:12px;}
.list_title{padding:5px 10% 0}
.list_title li{float:left; width:30%; height:34px; margin-left:3.3%; background:url(../images/btns_04.png) no-repeat; background-size:100% 100%; text-align:center; line-height:34px; color:#fff; font-size:16px; text-shadow:0 1px 1px #818181; font-weight:bold}
.list_title li.current{background:url(../images/btns_03.png) no-repeat; background-size:100% 100%; text-shadow:0 2px 2px #f9550a;}

/*我的奖品*/
.my_prize_list{height:400px; overflow:auto}
.my_prize_list li{line-height:25px; padding:14px 0; font-size:16px; position:relative; border-bottom:1px solid #bd9568; color:#803b02}
.my_prize_list li a,.tip-con p a{display:block; width:80px; height:32px; margin:5px auto 0; padding:0; background:#fa9505; border-radius:5px; line-height:33px; font-weight:bold; color:#fff; text-align:center; letter-spacing:1px; font-size:16px; color:#fff; text-shadow:0 0 1px rgba(95,16,0,.75)}
.tip-con p a{margin-top:15px}
.my_prize_list li a{ width:80px; height:32px; margin-top:8px; background:#fa9505; border-radius:5px; line-height:32px;}
.my_prize_list li i{font-style:normal; color:#c28858}
.my_prize_list li span{color:#be0000}
.prize_wrap .wrap_inner{padding-right:10%}

/*联系信息*/
.form-items li{ margin-top: 15px; clear: both; overflow: hidden;}
.form-items li label{display:block; width:90px; height:20px; padding:10px 0; font-size:16px; font-weight:bold; color:#803b02; line-height:20px}
.form-items li span{display:block}
.form-items li input{ width:94%; padding:10px 6px;border:none;color: #000;  font-family: Microsoft Yahei,"微软雅黑"; background-color:#fff; border-radius:2px}
.form-items2 li{position:relative}
.form-items2 li label{position:absolute; left:0; top:0; padding:0;}
.form-items2 li i{display:block; margin-left:90px; margin-right:5px; font-style:normal; color:#803b02}
.form_error{display:block; padding:8px 0 0 90px; color:red}
.infor_wrap .title1 h3{background:url(../images/txt_03.png) no-repeat; background-size:100% 100%;}
.end .title1 h3{width:128px; background:url(../images/txt_04.png) no-repeat; background-size:100% 100%;}
.prize_wrap .title1 h3{background:url(../images/txt_05.png) no-repeat; background-size:100% 100%;}
.tip-box .title1 h3{width:130px; height:28px; background:url(../images/txt_06.png) no-repeat; background-size:100% 100%;}

/*获奖名单页*/
#main_end .mod{margin-left:0; margin-right:0}
.awards{padding:20px 5% 30px; margin:20px 5%; position:relative; background:#ffa803; border-radius:15px}
.awards .title{padding:0 0 5px; margin-bottom:10px; }
.awards .title h3{color:#803b02}
.awards-list{line-height: 22px; color:#803b02}
.slide_up{overflow:hidden;height:220px;position: relative;}
.slide_up dd{width:100%; height:22px; overflow:hidden}
.slide_up span{float:left}
.slide_up em{float:right}
.btns_02{margin-top:25px; padding:0 5% 10px; background:url(../images/bg4.png) no-repeat; background-size:100% 100%;}
.btns_02 a{display:block;width:142px; height:42px; background:url(../images/btns_02.png) no-repeat; background-size:100% 100%; color:#7e341f; font-weight:bold; font-size:18px; text-align:center; line-height:42px;}
.stars_01{position:absolute; left:-5%; top:5%; width:110%; height:230px; background:url(../images/stars_01.png) no-repeat; background-size:100% 100%}


/*提示框*/
#body-bg{position:absolute; top:0px;left:0px;width: 100%; height: 100%; background: #000 url(../images/stars_02.png) no-repeat center center; background-size:60% auto; opacity: 0.8;z-index: 99998;}
.tip-box{ position:absolute;width: 300px; height:296px; left:50%;top:45%; margin-left:-150px; z-index: 99999; font-size:15px; color:#803b02}
.tip-con{ margin:0 12px 15px 14px;padding: 25px 7% 35px; background:url(../images/tips_bg1.png) no-repeat; background-size:100% 100%}
.tip-con p{ line-height: 25px; word-wrap:break-word;word-break:break-all;}
.tip-con p.coin_tip{padding:4px 2.1% 0; font-size:16px; color:#803b02; font-weight:bold}
.tip-con p span{color:#ae0000}
.tip-con p input{display:block; width:90%; padding:8px 2%; margin:30px auto 20px; border:none; box-shadow:2px 2px 5px #dbdbdb inset}
.tip-box .btns_02{background:none}
.tip-box  .btns_02 a{width:113px; height:34px; line-height:34px; font-size:15px}
.tip-box .btns_02 .spec{line-height:22px; position:relative}
.tip-box .btns_02 i{position:absolute; width:100%; height:18px; bottom:2px; left:0; font-style:normal; font-size:12px; font-weight:normal; color:#ae0000}
.yellow{color:#ffd500;}
.footer{ background:#7b0300; text-align:center; color:#fff;}
.footer p{padding:12px; line-height:22px}
.prompt_05{padding-top:20px; color:#8b0000}
.prompt_01{padding-top:10px; color:#803b02}
.prompt_02{color:red}
@keyframes flashlight{
	0%{background:#fff}
	10%{background:#ff00ff}
	20%{background:#00ffff}
	30{background:#eb212e}
	40%{background:#d2ff00}
	50%{background:#ff9000}
	60%{background:#00ffae}
	70%{background:#ffe400}
	100%{background:#ff7403}
}
@-webkit-keyframes flashlight{
	0%{background:#fff}
	10%{background:#ff00ff}
	20%{background:#00ffff}
	30{background:#eb212e}
	40%{background:#d2ff00}
	50%{background:#ff9000}
	60%{background:#00ffae}
	70%{background:#ffe400}
	100%{background:#ff7403}
}
