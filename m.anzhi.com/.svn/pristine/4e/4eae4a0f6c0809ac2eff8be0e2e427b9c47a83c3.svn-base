/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto; background:#fff; font-size:14px; font-family:Microsoft Yahei,"微软雅黑"; line-height:20px; color:#000000}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none; font-size: 14px; outline: none; color:#000000}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{float:left;}
.fr{float:right;}
.main{position:relative; background:url(../images/bg1.jpg) no-repeat; background-size:100% auto}
.main_inner{padding-bottom:10px; background:url(../images/bg2.png) no-repeat left bottom; background-size:100% auto}
.main2{background:url(../images/bg4.jpg) no-repeat; background-size:100% 100%; overflow:hidden}
/*有通用的地方*/
.wrap{background-image: -webkit-linear-gradient(top, rgba(220, 190, 68, 1) 0%, rgba(201, 143, 60, 1) 100%); background-image: -moz-linear-gradient(top, rgba(220, 190, 68, 1) 0%, rgba(201, 143, 60, 1) 100%); background-image: -o-linear-gradient(top, rgba(220, 190, 68, 1) 0%, rgba(201, 143, 60, 1) 100%); background-image: -linear-gradient(top, rgba(220, 190, 68, 1) 0%, rgba(201, 143, 60, 1) 100%); margin:45px 6.5% 0; position:relative; z-index:1; color:#482608}
.wrap_inner{padding:20px 0 20px; background:url(../images/bg3.png) repeat-y; background-size:100% auto}
.decorate_01{width:100%}

.titles{width:82px; height:30px; margin:2px 0 0; background:url(../images/title_01.png) no-repeat; background-size:100% 100%}
.rule_title2{ background:url(../images/title_02.png) no-repeat; background-size:100% 100%}
.active_title{width:68px; margin:0 0 0 25px; background:url(../images/title_03.png) no-repeat; background-size:100% 100%}
.awards_title{padding:16px 10px 0; color:#fff; font-size:16px; font-weight:bold; text-align:center}
#end_wrap .titles{width:106px; margin-top:30px; background:url(../images/title_05.png) no-repeat; background-size:100% 100%}
.btns{display:block; width:113px; height:34px; background:url(../images/btns.png) no-repeat; background-size:100% 100%; text-align:center; line-height:32px; color:#fff; font-weight:bold; font-size:15px}
.title{padding:10px 0 15px 0; position:relative}
.title h3{font-weight:bold; color:#482608;}

.banner{width: 100%; margin: 0 auto; position:relative;  z-index:2}
.banner img{ width: 100%;}
/*index*/
.rule_wrap{ margin:0 25px 0; background-image: -webkit-linear-gradient(top, rgba(220, 190, 68, 1) 0%, rgba(201, 143, 60, 1) 100%); background-image: -moz-linear-gradient(top, rgba(220, 190, 68, 1) 0%, rgba(201, 143, 60, 1) 100%); background-image: -o-linear-gradient(top, rgba(220, 190, 68, 1) 0%, rgba(201, 143, 60, 1) 100%); background-image: -linear-gradient(top, rgba(220, 190, 68, 1) 0%, rgba(201, 143, 60, 1) 100%); position:relative;}
.rule{ padding:13px 15px 66px; background:url(../images/bg3.png) repeat-y; background-size:100% auto}
.rule p{ padding:3px 8px; line-height: 22px; position:relative; z-index:2; color:#6c3738}
.rule p span{color:#c80000;}
.active_time{display:inline-block}
.notice{ margin:15px 25px; padding:2px; background:#220f09; color:#fff}
.notice_list{height:28px; padding:0 15px; line-height:28px;}
#login_wrap,.awards_wrap{margin:12px 25px 0; padding:8px 0 5px; border:1px solid rgba(255,255,255,.3); background:#444444;}
.logined{ padding:0 10px; position:relative}
.logined_r{width:78px; height:26px; position:absolute; right:5px; top:16px;}
.logined_r a{display:block; width:78px; height:26px; padding:0 0 0 8px; background:url(../images/btns_02.png) no-repeat; background-size:80px 26px; text-align:center; line-height:27px; color:#fff; font-weight:bold; font-size:13px}
.logined_l p{ padding:3px 0 2px; color:#fff}
.login{padding-top:15px; padding-bottom:20px}
.login .logined_r a{width:80px; height:26px; padding:0; background:url(../images/btns_01.png) no-repeat; background-size:80px 26px;}
.star_list{margin: 6px 0px 10px;}
.star_list span{float: left;width:22px;height:22px;line-height:24px;background: url("../images/stars.png") 0 0 no-repeat;background-size: 32px 96px;text-align: center; font-size:12px}
.star_list span.star_gold{background-position:0 1px; color:#915825}
.star_list span.star_gray{background-position:0 -26px;color: #606060}
.star_list span.star_lightgray{background-position:0 -51px;color: #d0d0d0}
.star_list em{float: left;width:6px;height:22px;background: url("../images/stars.png") 0 0 no-repeat;background-size: 32px 96px;}
.star_list em.spot_gold{background-position:-26px 5px;}
.star_list em.spot_gray{background-position:-26px -22px;}
.star_list em.spot_lightgray{background-position:-26px -46px;}
.lottery_wrap{margin:30px 25px 0; padding:0 0 10px}
.prize_index{position:relative;}
.prize_index p{color:#c35214}
.prize_index p span{padding:0 1px; color:#de0000; font-size:16px}
.prize_index a{display:block; position:absolute; right:0; top:42%; width:83px; height:25px; background:url(../images/btns_03.png) no-repeat; background-size:100% 100%; text-align:center; text-indent:-9999px}
.prize_index p.prize_tips{color:#ffea00}
/*转盘抽奖*/
.rotate {width:90%; margin:15px auto 0; position: relative; overflow}
.rotate-con{width:100%;}
.rotate img{width:100%;}
.rotate-pointer {position: absolute;top: 50%;left: 50%;width: 46%;height: 46%;margin-top: -22.1%;margin-left: -23.1%;background: url(../images/pointer.png) no-repeat 0 0;background-size: 100% 100%; cursor: pointer;-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.pointer-disabled{background: url(../images/pointer_disabled.png) no-repeat 0 0;background-size: 100% 100%; z-index:11}
.lottery_txt{ padding:10px 0 5px; color:#fff; text-align:center}
.active_ctn{margin:0 0 0 25px}
.active_ctn li{position:relative; line-height:22px; color:#21335b}
.active_ctn span{width:20px; position:absolute; top:1px;}
.active_ctn p{margin-left:20px;}
.btns1 a{display:block; margin:10px auto;}
.soft{padding:14px 10px 10px; margin:12px 25px 0; position:relative;border:1px solid #ffb96d; background-image: -webkit-linear-gradient(left, rgba(246, 166, 79, 1) 0%, rgba(185, 109, 34, 1) 100%); background-image: -moz-linear-gradient(left, rgba(246, 166, 79, 1) 0%, rgba(185, 109, 34, 1) 100%); background-image: -o-linear-gradient(left, rgba(246, 166, 79, 1) 0%, rgba(185, 109, 34, 1) 100%); background-image: -linear-gradient(left, rgba(246, 166, 79, 1) 0%, rgba(185, 109, 34, 1) 100%);}
.soft .titles{width:114px; height:17px; background:url(../images/title_04.png) no-repeat; background-size:100% 100%}

.soft_list{padding:5px 0;}
.soft_list li{ float: left; display: inline; width: 95%; margin:10px 0 0 5%;}
.soft_list li .soft_icon{ width: 56px;height: 56px;}
.soft_list li .soft_icon img{ width: 56px;height: 56px;}
.soft_list li .soft_name{ margin-top: 10px;height: 20px; line-height: 20px; overflow: hidden; color:#fff; white-space:nowrap; overflow:hidden; text-overflow:ellipsis}
.soft_list li .soft_name a{ color: #fff;}
.soft_list li a.downlode{position:absolute; right:10px; bottom:54px; display: block; width:62px;height: 24px; line-height: 24px; margin: 10px auto; background:url(../images/btns.png) no-repeat; background-size:100%100%; background-clip: padding-box; border-radius:25px; border:1px solid #ffea00; line-height:24px; color:#fff;cursor: pointer; text-align: center; font-weight:bold}

#active_ctn li{margin-top:20px; color:#cb3731; font-weight:bold}

/*我的奖品*/
.my_prize{padding:15px 0 8%; margin:0 6%; position:relative; color:#482608}
.my_prize_list li{line-height:25px; font-size:16px; position:relative;}
.my_prize_list li a {display:block; width: 120px; height: 31px; margin:8px auto 0; background: url(../images/btns_04.png) no-repeat; background-size:100% 100%; text-indent:-9999px}
.my_prize_list2{padding:5px 0;}
.my_prize_list2 li{ position:relative; font-size:16px}
.my_prize_list2 p{ padding:6px 0;}
.form-items li{ margin-top: 30px; clear: both; overflow: hidden;}
.form-items li label{ float: left;width:90px;padding-top: 10px; color:#482608;}
.form-items li span{display:block;margin-left: 90px;}
.form-items li input{ width:90%;padding:10px 6px;border:none;color: #000;  font-family: Microsoft Yahei,"微软雅黑"; background-color:#fff; border-radius:2px}
.form_error{display:block; padding:0px 0 0 90px; color:red}
.split{height:11px; background:url(../images/line_bg.png) no-repeat; background-size:100% 100%;  margin: 8px 0px 15px; overflow:hidden;}
.my_prize .btns{background:url(../images/btns_01.png) no-repeat; background-size:100% 100%; margin-top:40px}
/*结束页*/
#end_wrap {padding-bottom:30px}
.awards_wrap{margin-top:30px}
.awards{padding:5% 20px 4%; position:relative; color:#fff}
.awards_list{line-height: 26px; text-align:center}
.slide_up{overflow:hidden;height:130px;position: relative;}

/*提示框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-webkit-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{ position:absolute; width:295px; height:295px; left:50%; top:50%; margin-left:-147px; z-index: 99999; background:url(../images/dialog_bg.png) no-repeat; background-size:100% 100%; font-size:15px;}
.tip-con{ margin: 15px 15px 0 15px; padding:55px 30px 10px 20px; color:#440e0e;}
.tip_title h6{ padding:2px 0 20px; min-height:20px; color:#4b2711; font-size:18px; font-weight:bold; line-height:18px;text-align:center}
.tip-con p{padding:0 0 20px 20px; line-height: 22px; word-wrap:break-word;word-break:break-all; font-size:15px; color:#4b2711}
.tips_btns a{display:block; width:98px; height:34px; background:url(../images/btns_01.png) no-repeat; background-size:100% 100%; margin:16px auto 0; line-height:35px; text-align:center; color:#fff}
/*#tip-box3 .tip-con,#tip-box4 .tip-con{padding-top:85px}
#tip-box3  .tips_btns a,#tip-box4  .tips_btns a{margin-top:16px;}*/

.footer{width:100%; height:43px; background:#333333;  text-align:center; position:relative; color:#fff; line-height:43px}
.bold{font-weight:bold;}
.red{color:#ab0500}
.prompt_01{color:#482608}
.prompt_02{padding:7px 0 0 90px; color:#ff0000}
.mtop10{margin-top: 10px;}
