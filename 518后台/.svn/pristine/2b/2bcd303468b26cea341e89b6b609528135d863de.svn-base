<script type="text/javascript">
	function checkPost(){
		var r = /^[0-9]$/     
		var b = /^[1-9][0-9]*$/       
		if(r.test($('#fast_entrance_slide_interval').val())||b.test($('#fast_entrance_slide_interval').val())){
			return true;
		}else{
			$('#fast_entrance_slide_interval').val('');
			alert('快速入口(648滑动)时间间隔-请输入大于等于0的整数');
			return false;
		}
	}
</script>
