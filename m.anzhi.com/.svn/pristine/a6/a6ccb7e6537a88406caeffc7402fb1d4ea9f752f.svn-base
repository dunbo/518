/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto; background:#280850; font-size:15px; font-family:Microsoft Yahei,"微软雅黑"; line-height:20px; color:#fefefe}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#fefefe; font-size: 14px; outline: none;}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{float:left}
.fr{float:right}
.wordwrap{word-wrap:break-word;word-break:break-all;}

.mtop10{margin-top: 10px;}
.space{height:36px;}
.main{position:relative; margin:0 8px; padding:0 0 10px 0}
.banner{width: 100%; margin: 0 auto; position:relative; z-index:2}
.banner img{ width: 100%;}

/*index*/
.notice{height:32px; margin:2px; background:#d630bc; line-height:33px; position:relative;}
.notice-icon{ display:block; width: 25px;height:21px; margin:0 0px 0px 3px; background:url(../images/icon_01.png) no-repeat; background-size:25px 21px; position:absolute; left:5px; top:5px;}
.notice-list{margin-right:15px; margin-left:42px}
.notice-list span{padding-right: 20px;}
.notice-list span em{ padding-left: 10px;}
.introduce{padding:50px 10px 20px 20px; background:url(../images/bg1.png) no-repeat; background-size:100% 100%;}
.app_select li{float:left; width:23%; margin:0 1%; padding:15px 0 0;}
.app_select li .select_inner{ height:86px; border:1px solid #ba00ef; padding:5% 0 0; border-radius:8px; box-shadow: inset 0px 0px 8px rgba(154,24,133,.75); background:url(../images/select_bg.png) no-repeat center center; background-size:22px 46px; position:relative}
.app_select li img{width:60px; height:60px; margin:0 auto}
.app_select li span{display:block; height:26px; text-align:center; line-height:26px}
.app_select li em{display:block; width:20px; height:20px; background:url(../images/delete_btn.png) no-repeat; background-size:100% 100%; text-align:center; line-height:26px; position:absolute; right:-5px; top:-5px; cursor:pointer}
.wrap{background:rgba(41,1,98,.35); min-height:180px; margin:10px 0 0; border:1px solid #ba00ef; border-radius:8px; box-shadow: inset 0px 0px 8px rgba(154,24,133,.75); position:relative}
#soft_wrap{margin-left:15px; margin-right:15px; border-radius:8px 0 8px 8px}
.soft_p{padding:8px 0; color:#ffffff; font-size:16px;}
.title{width:100%; height:33px; background:#ba00ef; border-radius:6px 6px 0 0; line-height:33px; position:absolute; left:0; top:0; font-size:16px}
.soft_title p{padding:0 0 0 3%}
.soft_title span{width:23px; height:34px; background:url(../images/title_bg4.png) no-repeat center center; background-size:23px 34px; position:absolute; top:-1px; right:-18px; z-index:2}
.soft_list {margin-top:32px}
.soft_list li{ float: left; display: inline; width: 50%; position:relative;}
.soft_list li .soft_icon{position:absolute; left:0; top:8px; width:80px; height:80px;}
.soft_list li .soft_icon img{ width: 60px;height: 60px; margin:0 auto}
.soft_list li .soft_icon span{display:block; padding:4px 0 0; font-size:12px; color:#ff0000; text-align:center}
.soft_txt{margin:0 0 0 70px;}
.soft_list li .soft_name{margin:11px 8px 5px; height: 20px; line-height: 20px; color:#fff; overflow: hidden; white-space:nowrap; text-overflow:ellipsis}
.soft_list li a.soft_btns{display:block; width:45px;height: 24px; line-height: 25px; margin: 0 8px 5px; background: #ba00ef; border-radius: 3px; cursor: pointer; text-align:center; font-size:14px}
.soft_list li a.download{background:#00a7b6}
.lottery_title,.rules_title {position:relative; height:30px;}
.lottery_title span{display:block; width:96%; height:20px; position:absolute; left:50%; bottom:-20px; margin-left:-48%; background:url(../images/title_bg.png) no-repeat; background-size:100% 100%;}
.lottery_title em{display:block; width:20px; height:5px; position:absolute; top:13px;  background:url(../images/dotted_bg.png) no-repeat; background-size:100% 100%;}
.lottery_title em:nth-child(2){right:10px;}
.rules_title span{position:absolute; left:0; top:-20px; width:130px; height:20px; background:url(../images/title_bg2.png) no-repeat; background-size:100% 100%; text-align:center; line-height:26px}
/*九宫格抽奖*/
.lottery{width:294px; margin:30px auto 10px; position:relative;}
.lottery img{display:block; width:80px; height:85px; border:3px solid #ff7deb; border-radius:2px}
.lottery table{border-collapse: separate; width:100%;}
.lottery td{width:80px;height:85px; padding:5px 6px 5px 6px;}
#my_click {width:86px;height:91px; cursor:pointer;}
.lottery p{position:absolute; width:86px;height:91px; left:104px; top:106px; display:block;}
.lottery p a{display:block; width:86px; height:91px;}
.waitGame{background:url(../images/lottery_disabled.png) no-repeat; background-size:100% 100%}

/*排行*/
#rank_wrap{width:90%; margin:-18px auto 0;}
#rank_wrap .wrap{ margin-top:12px; padding-bottom:20px}
.title_bg3{width:90%; height:5px; margin:20px auto 0; background:url(../images/title_bg3.png) no-repeat; background-size:100% 100%;}
.nav{width:90%; height:35px; margin:0 auto; position:relative; z-index:2}
.nav li{float:left; width:50%; height:35px; line-height:35px; text-indent:-9999px}
.nav li a{display:block; width:100%; height:100%; background:url(../images/btn_04.png) no-repeat; background-size:100% 100%;}
.nav li .app{background:url(../images/btn_03.png) no-repeat; background-size:100% 100%;}
.nav li.current a{background:url(../images/btn_02.png) no-repeat; background-size:100% 100%;}
.nav li.current2 a{background:url(../images/btn_05.png) no-repeat; background-size:100% 100%;}

.soft_list2{margin-top:10px}
.soft_list2 .soft_rank{padding:0 0 0 5px;}
.soft_list2 li{width:100%; margin-bottom:20px}
.soft_list2 li .soft_name{width:68%; margin-top:2px; margin-bottom:0; margin-left:5px;}
.soft_list2 .soft_txt span{padding:0 0 0 5px;color:#f00; font-size:12px}
.soft_list2 .soft_btns{position:absolute; right:5px; top:35%}

.rules{padding:12px 10px 12px 15px}
.btns{display:block; width:150px; height:34px; margin:10px auto 0; background-image: -webkit-linear-gradient(top, rgba(254, 214, 98, 1) 0%, rgba(255, 146, 0, 1) 100%); background-image: -moz-linear-gradient(top, rgba(254, 214, 98, 1) 0%, rgba(255, 146, 0, 1) 100%); background-image: -o-linear-gradient(top, rgba(254, 214, 98, 1) 0%, rgba(255, 146, 0, 1) 100%); background-image: -linear-gradient(top, rgba(254, 214, 98, 1) 0%, rgba(255, 146, 0, 1) 100%); border-radius:12px; box-shadow: inset 0px 0px 10px rgba(255,243,76,.75); color:#c1221c; text-align:center; line-height:34px; font-size:16px;}
.lottery_info{ margin:10px}
.lottery_info a,.lottery_info p{display:block; width:135px; height:30px; background-image: -webkit-linear-gradient(top, rgba(16, 142, 189, 1) 0%, rgba(8, 88, 161, 1) 100%); background-image: -moz-linear-gradient(top, rgba(16, 142, 189, 1) 0%, rgba(8, 88, 161, 1) 100%); background-image: -o-linear-gradient(top, rgba(16, 142, 189, 1) 0%, rgba(8, 88, 161, 1) 100%); background-image: -linear-gradient(top, rgba(16, 142, 189, 1) 0%, rgba(8, 88, 161, 1) 100%); border-radius:12px; box-shadow: inset 0px 0px 0 rgba(255,243,76,.75); text-align:center; line-height:30px; color:#01fcff; font-size:16px}
#share_btns,#soft_btns_bom a{background-image: -webkit-linear-gradient(top, rgba(245, 55, 89, 1) 0%, rgba(197, 27, 72, 1) 100%); background-image: -moz-linear-gradient(top, rgba(245, 55, 89, 1) 0%, rgba(197, 27, 72, 1) 100%); background-image: -o-linear-gradient(top, rgba(245, 55, 89, 1) 0%, rgba(197, 27, 72, 1) 100%); background-image: -linear-gradient(top, rgba(245, 55, 89, 1) 0%, rgba(197, 27, 72, 1) 100%); border-radius:12px; box-shadow: inset 0px 0px 0 rgba(255,243,76,.75); color:#fff;}
#soft_btns_bom a.disabled{background-image: -webkit-linear-gradient(top, rgba(199, 199, 199, 1) 0%, rgba(91, 91, 91, 1) 100%); background-image: -moz-linear-gradient(top, rgba(199, 199, 199, 1) 0%, rgba(91, 91, 91, 1) 100%); background-image: -o-linear-gradient(top, rgba(199, 199, 199, 1) 0%, rgba(91, 91, 91, 1) 100%); background-image: -linear-gradient(top, rgba(199, 199, 199, 1) 0%, rgba(91, 91, 91, 1) 100%);}
#soft_btns_bom a{width:120px; height:30px}
#soft_btns_bom a:nth-child(2){background-image: -webkit-linear-gradient(top, rgba(154, 0, 255, 1) 0%, rgba(74, 0, 158, 1) 100%); background-image: -moz-linear-gradient(top, rgba(154, 0, 255, 1) 0%, rgba(74, 0, 158, 1) 100%); background-image: -o-linear-gradient(top, rgba(154, 0, 255, 1) 0%, rgba(74, 0, 158, 1) 100%); background-image: -linear-gradient(top, rgba(154, 0, 255, 1) 0%, rgba(74, 0, 158, 1) 100%);}
#downadd_btns{width:224px; height:44px; background:url(../images/btn_01.png) no-repeat; box-shadow: inset 0px 0px 0 rgba(255,243,76,.75); border-radius:0; background-size:100% 100%; color:#fff; line-height:44px}

.colorblock_01,.colorblock_04{position:absolute; left:-15px; bottom:5px; width:104%; height:100%; background: url(../images/colorblock_01.png) 0 0 no-repeat; background-size: 100% auto; z-index:-1}
.colorblock_02{position:absolute; left:0; top:-45px; width:100%; height:100%; background: url(../images/colorblock_02.png) 0 0 no-repeat; background-size: 100% auto; z-index:-1}
.colorblock_03{position:absolute; left:0; top:-15px; width:100%; height:100%; background: url(../images/colorblock_03.png) 0 0 no-repeat; background-size: 100% auto; z-index:-1}
#rank_wrap .colorblock_03{left:-5.5%; top:10px}
.colorblock_04{ height:250px; bottom:15%}
.tips_p{ width:70%; margin:0 auto; padding:20px 0 10px; text-align:center;}
.tips_p img{display:block; width:107px; height:134px; margin:10px auto 0;}

/*我的奖品页*/
.title_bg{width:100%;}
.title_bg h3 img{width:142px; height:36px; padding:0 0 5px 0; margin:20px auto 0;}
.prize_list{ margin:20px 0 0; padding:140px 0 60px; color:#fff; position:relative}
.prize_list_inner{width:92%; margin:35px auto 15px}
.prize-title{padding:20px 10px 0; font-size:18px;}
.prize-text{padding:5px 10px 0; line-height:28px}
.prize_btns{display:block; padding:4px 13px; background:#ba00ef; border-radius:15px; position:absolute; right:3%; top:20px; color:#fff; font-size:16px}
.bg2{position:absolute; left:0; top:-1px; width:108px; height:40px; background:url(../images/bg2.png); background-size:100% 100%;}

/*邮寄地址*/
.form1{width:85%; margin:0 auto; font-size:14px;}
.form-item{ margin:12px 0 0; }
.form-item label{ float: left;width:55px; padding:8px 0;}
.form-item .textbox input{float:left; width:68%; padding:8px 12px; background:#fff; border-radius:2px; border:none; font-family: "Microsoft Yahei"; color:#898787;}
.prompt2{padding:5px 0 0;}
.prompt3{padding-left:55px}
#end_wrap{min-height:140px; padding-top:30px; font-size:16px}
#end_wrap p a{display:block; margin-top:20px; text-decoration:underline; font-size:16px}
#end_btns{margin-top:25px; margin-bottom:15px}
/*提示框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{position:absolute; width:90%; padding:0 5px 20px; background:#2a023e; left:50%; top:47%; margin-left:-46%; z-index: 99999;}
.tip_hd img{width: 107px; height:134px; margin:48px auto 0;}
.tip-mid{padding-bottom:4px;border-radius: 0px 0px 15px 15px;}
.tip-con{padding:0 25px;margin:0px 4px}
.tip-con p{line-height: 22px; text-align: center;font-size: 16px;}
.clo{ position: absolute; right: -6px; top: -5px;}
.clo a{ display: block; width:38px; height:38px;background:url("../images/tip_clo.png") 0 0 no-repeat;background-size:100% auto;text-indent: -9999px;}
.prize_ok{width:85%; margin:10px auto; padding:42px 5px 0}
.prize_okl{float:left; width:76px; background:rgba(187,0,239,.2)}
.prize_okl img{display:block; width:70px; height:70px; margin:3px auto}
.prize_okl span{display:block; line-height:22px; color:#fff; text-align:center}
.prize_okr{padding:0 0 5px 82px; font-size:14px; line-height:24px}
.tip-box .btns{width:106px; height:36px; background-image: -webkit-linear-gradient(top, rgba(154, 0, 255, 1) 0%, rgba(74, 0, 158, 1) 100%); background-image: -moz-linear-gradient(top, rgba(154, 0, 255, 1) 0%, rgba(74, 0, 158, 1) 100%); background-image: -o-linear-gradient(top, rgba(154, 0, 255, 1) 0%, rgba(74, 0, 158, 1) 100%); background-image: -linear-gradient(top, rgba(154, 0, 255, 1) 0%, rgba(74, 0, 158, 1) 100%); box-shadow:0 0 0 #000; color:#fff}
.tip-box .btns2{width:160px; margin:0 auto}
.tip-box .btns2 a{float:left; margin:12px 10px 0; width:60px; height:36px;}
.tip-box .btns3{width:212px}
.tip-box .btns3 a{font-size:12px;}
.tip-box .btns3 a:nth-child(2){background-image: -webkit-linear-gradient(top, rgba(245, 55, 89, 1) 0%, rgba(197, 27, 72, 1) 100%); background-image: -moz-linear-gradient(top, rgba(245, 55, 89, 1) 0%, rgba(197, 27, 72, 1) 100%); background-image: -o-linear-gradient(top, rgba(245, 55, 89, 1) 0%, rgba(197, 27, 72, 1) 100%); background-image: -linear-gradient(top, rgba(245, 55, 89, 1) 0%, rgba(197, 27, 72, 1) 100%); border-radius:12px; box-shadow: inset 0px 0px 0 rgba(255,243,76,.75);}
#tip-box6 .tip_hd img{width: 225px; height:141px; margin:48px auto 10px;}

.footer{background:#ba00ef; text-align:center;}
.footer p{padding:12px 10px 10px; line-height:22px}
.bold{font-weight:bold;}
.paddtop20{padding-top:20px}
.prompt1{color:#c700ff}
.cnt{text-align:center}