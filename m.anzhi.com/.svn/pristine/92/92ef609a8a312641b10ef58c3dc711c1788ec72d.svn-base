/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto; background:#9a2323; font-size:15px; font-family:Microsoft Yahei,"微软雅黑"; line-height:20px}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#000; font-size: 14px; outline: none;}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.cl{overflow:hidden;}
.clear{clear:both;overflow:hidden;}
.wordwrap{word-wrap:break-word;word-break:break-all;}

.mtop10{margin-top: 10px;}
.space{height:36px;}

.main{position:relative;}
.main2{background:url(../images/snow2.png) no-repeat; background-size:cover; overflow:hidden}
.banner{width: 100%; margin: 0 auto; position:relative; z-index:2}
.banner img{ width: 100%;}

/*index*/
.person1{margin:20px 0 0; padding:110px 15% 37px 18%; background: url(../images/person1.png) 0 0 no-repeat; background-size: 100% 100%; color:#ff4e00}
.tree_wrap{ padding:50px 0 0; background: url(../images/snow.png) 0 0 no-repeat; background-size: 100% 100%;}
.tree{width:260px; height:368px; background:url(../images/tree.png) no-repeat; background-size:100% 100%; margin:0 auto; position:relative}
.tree a{position:absolute; left:93px; top:38px; width:73px; height:51px; background:url(../images/heart_btn.png) no-repeat; background-size:100% 100%;}
.tree span{ position:absolute; left:30px; top:35px; width:175px; height:140px; background:url(../images/light1.png) no-repeat; background-size:100% 100%; display:none}
.tree i{display:block; position:absolute; left:30px; top:35px; width:235px; height:240px; background:url(../images/light2.png) no-repeat; background-size:100% 100%; display:none}
.tree em{display:block; position:absolute; left:91px; top:-60px; width:80px; height:80px; background:url(../images/star_01.png) no-repeat; background-size:100% 100%; z-index:2}
.tree em.star_light{background:url(../images/star_02.png) no-repeat; background-size:100% 100%;}
.tree_wrap p{padding:5px 2px; color:#fff; text-align:center;}
.tree_btn{padding:0 10px;}
.tree_btn a{float:left; display:block; width:128px; height:51px; background:url(../images/btn_01.png) no-repeat; background-size:100% 100%; text-indent:-9999px; overflow:hidden}
.tree_btn a:nth-child(2){float:right; background:url(../images/btn_02.png) no-repeat; background-size:100% 100%;}
.tree_btn a:nth-child(2).disabled{background:url(../images/btn_04.png) no-repeat; background-size:100% 100%;}
.bom_btn a{display:block; width:98px; height:60px; margin:10px auto 20px; background:url(../images/btn_03.png) no-repeat; background-size:100% 100%; color:#fff; line-height:80px; text-align:center}
/*lottery*/
.main2{margin-top:-85px; padding:92px 0 0; background:url(../images/main_bg.png) repeat-y; background-size:100% 100%}
.awards{width:90%; margin:0 auto; padding:53px 0 20px; background:url(../images/bg1.png) no-repeat; background-size:100% 100%; position:relative; color:#fff; line-height: 22px; text-align:center}
.slide_up{width:70%; height:88px; margin-left:16%; overflow:hidden;position: relative;}
.lottery_info{margin:20px 10px 0;}
.lottery_info a,.lottery_info p{float:right; padding:10px 15px; background:url(../images/cloud_01.png) no-repeat; background-size:100% 100%; color:#ff4e00;}
.lottery_info p{float:left;}
.lotter_wrap{padding:55% 10px 18%; background:url(../images/bg2.png) no-repeat; background-size:100% 100%;}
.lottery{position:relative}
.rotate-pointer {position: absolute;top: 50%;left: 50%;width: 46%;height: 46%;margin-top: -23.8%;margin-left: -23.2%;background: url(../images/pointer.png) no-repeat 0 0;background-size: 100% 100%; cursor: pointer;-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.pointer-disabled{background: url(../images/pointer_disabled.png) no-repeat 0 0;background-size: 100% 100%; z-index:11}

.soft{background:url(../images/bg3.png) no-repeat; background-size:100% 100%; margin:40px 0 0; padding:180px 0 60px; position:relative}
.soft-list{ width:75%; margin: 5px auto 10px; padding:0px 5px 12px;}
.soft-list a{color:#fffefe;}
.soft-list li{ float: left; display: inline; width: 50%;margin:8px 0px 0; position:relative;}
.soft-list li .soft-icon{position:absolute; left:8px; top:8px; width:60px; height:60px;}
.soft-list li .soft-icon img{ width: 60px;height: 60px;}
.soft-txt{margin:0 0 0 70px;}
.soft-list li .soft-name{width:90%; margin:11px 6px 8px; height: 20px; line-height: 20px; color:#fff; overflow: hidden; white-space:nowrap; text-overflow:ellipsis}
.soft-list li .soft-name a{ color: #fff;}
.soft-list li a.downlode{float:left; width:48px;height: 22px; line-height: 23px; margin: 0 6px; background: #ffd800; border-radius: 3px; color: #ea5e00;cursor: pointer; text-align:center; font-size:14px}
.soft_p{background: url(../images/cloud_02.png) no-repeat 0 0;background-size: 100% 100%; position:absolute; left:25px; top:-45px; color:#ff4e00}
.soft_p p{padding:15px 20px 35px;}
.btns_app{width:52px; height:50px; padding:12px 0 0 3px; background:url(../images/btns_bg.png); background-size:100% 100%; margin-right:15px; color:#fff; line-height:52px; color:#ffd800; position:absolute; bottom:5px; right:50px; font-size:13px; font-weight:bold;}
.lottery_bom{position:relative; padding:20px 0 0; background:url(../images/snow3.png); background-size:100% 100%;}
.lottery_info2{background:url(../images/bg4.png); background-size:100% 100%;}
.lottery_info2 p{width:75%; margin:0 auto; padding:10px; color:#ec1c3a;}
.lottery_info2 p a{color:#ec1c3a;}
.return_btn{display:block; width:130px; height:44px; margin:15px auto 0; background:url(../images/btn_bg.png); background-size:100% 100%; text-align:center; line-height:40px; color:#ec1c3a}
.banner_bom img{width:100%;}
/*积分兑奖index*/
#gift_inner{margin-left:1px; background: url(../images/bg5.png) 0 0 no-repeat; background-size: 100% 100%;}
.prize_index_btn{right:120px; top:24px;}
.prize_index_btn2{right:23px; top:20px;}
.gift_list{margin:0 22px}
.gift_top{padding:64px 0 0;}
.gift_list li{float:left; width:24%; margin:5px 0.5%; position:relative}
.gift_list li img{width:100%}
.gift_list li span{display:block; text-align:center; color:#ff0000; font-size:12px}
.gift_list li a{display: block; width:64px;height: 26px; line-height: 26px; margin: 0 auto 5px; background:url(../images/btn_05.png) no-repeat; background-size:100% 100%; color:#ffffff;cursor: pointer; text-align: center; font-weight:bold}
.gift_list2 li{width:25%; margin:5px 0;}
.gift_list2 em{position:absolute; float:left; width:19%; height:30px; top:50%; margin-top:-15px; color:#ff0000; font-size:22px; text-align:center}
.gift_list2_img{width:81%; margin-left:19%}
.gift_list2 li a{margin:10px auto 0;}

.main_inner{background:url(../images/snow4.png); background-size:100% 100%;}
.tree2_wrap{ height:290px; position:relative;}
.tree2{width:185px; height:290px; margin:10px 0 0 10%; background:url(../images/tree2.png); background-size:100% 100%;} 
.tree2 i{position:absolute; left:40%; top:45%; width:108px; height:160px; background:url(../images/person2.png); background-size:100% 100%;}
.tree2 i#person3{background:url(../images/person3.png); background-size:100% 100%;}
.tree2 i#person4{background:url(../images/person4.png); background-size:100% 100%;}
.tree2_wrap p{position:absolute; top:8%; right:15%; width:170px; padding:15px 20px 28px; background:url(../images/cloud_03.png); background-size:100% 100%; text-align:center; color:#e53c10}

/*结束页*/
.tree3{width:100%; background:url(../images/tree3.png); background-size:100% 100%; color:#fff}
.tree3 p{padding:100px 0;text-align:center}
.tree3 a{display:block; margin-top:8px; color:#fff; text-decoration:underline;}

/*detail 活动规则*/
.detail_wrap{ margin:10px 10px 20px; background:url(../images/bg5.png); background-size:100% 100%; padding:35% 13%; border-radius:15px; color:#aa0f06; position:relative;}
.snow4{position:absolute; left:0; top:0; width:100%; height:100%; background:url(../images/snow4.png) repeat-y; background-size:100% auto;}
.detail_list h4{padding:5px 15px 6px; font-size: 18px; font-weight: bold;}
.detail_list  p{padding:0 15px; font-size: 16px; line-height: 25px;}
.light3{width:140px; height:140px; background:url(../images/light3.png) no-repeat; background-size:100% 100%; position:absolute; left:50%; margin-left:-70px; top:-20px;}


/*我的奖品页*/
.title_bg{width:100%;}
.title_bg h3 img{width:133px; height:41px; margin:20px auto 0;}
.prize_list{ margin:20px 0 0; color:#fff; position:relative}
.prize_list_inner{padding:20px 11% 60px 14%; background:url(../images/bg7.png) no-repeat; background-size:100% 100%; position:relative}
.prize_list img{width:100%}
.prize-title{padding:25px 10px 0; font-size:18px;}
.prize-text{padding:5px 10px 0; line-height:28px}
.prize_btns{display:block; padding:4px 13px; background:#ffc601; border-radius:15px; position:absolute; right:15%; top:14px; color:#5a0404; font-size:16px}
.prize_btns2{position:static; width:80px; margin:10px auto 0; border-radius:20px; padding:8px; text-align:center}

/*邮寄地址*/
.form1{width:85%; margin:0 auto; font-size:14px;}
.form_title{margin:5px auto 0;}
.form-item{ margin:6px 0 0; }
.form-item label{ float: left;width:55px; padding:7px 0;}
.form-item .textbox input{float:left; width:68%; padding:7px 12px; background:#fff; border-radius:2px; border:none; font-family: "Microsoft Yahei"; color:#898787;}
.prompt2{padding:5px 0 0;}
.prompt3{padding-left:55px}

/*提示框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{position:absolute; width:300px; height:300px; left:50%; top:50%; background:url(../images/dialog_bg.png) no-repeat; background-size:100% 100%; margin-left:-150px; z-index: 99999; color:#5a0404}
.btns{display:block; width:125px; height:42px; margin:5px auto; background:url(../images/btn_bg.png) no-repeat; background-size:100% 100%; border-radius:4px; color:#fff; font-size:16px; text-align:center; line-height:38px; color:#5a0404}
.tip_hd img{width: 107px; height:134px; margin:65px auto 0;}
.tip-mid{padding-bottom:4px;border-radius: 0px 0px 15px 15px;}
.tip-con{padding:0 25px;margin:0px 4px}
.tip-con p{line-height: 22px; text-align: center;font-size: 16px;}
.clo{ position: absolute; right: 5px; top: 6px;}
.clo a{ display: block; width:38px; height:38px;background:url("../images/tip_clo.png") 0 0 no-repeat;background-size:100% auto;text-indent: -9999px;}
.prize_ok{padding:70px 5px 0}
.prize_okl{float:left; width:76px; background:#b14b4b}
.prize_okl img{display:block; width:70px; height:70px; margin:3px auto}
.prize_okl span{display:block; line-height:22px; color:#fff; text-align:center}
.prize_okr{padding:0 0 5px 80px; font-size:14px; line-height:24px}
.btns2{width:132px; margin:0 auto}
.btns2 a{float:left; margin:5px 3px; width:60px; height:36px; background:url(../images/btn_bg2.png); background-size:100% 100%; line-height:32px; font-size:14px}
.btns3{width:185px}
.btns3 a{font-size:12px;}
.btns3 a:nth-child(2){background:url(../images/btn_bg.png) no-repeat; background-size:100% 100%;}

.prize_ok{width:85%; margin:10px auto 0; font-size:14px}
.tip-box1 img{width:91px; height:120px}
.tip-box .prize-title{padding-top:76px; text-align:center}
#tip-box3 .btns{width:100px; height:36px; line-height:32px}
#tip-box4 {width:90%; height:auto; left:50%; margin-left:-45%; padding:5px 0 10px}
#tip-box4 .form-item{ margin:12px 0 0;}

/*share*/
#tip-box-share{background:#ffffff; width:90%; margin-left:-45%}
#tip-box-share .tip_hd{ height:30px; margin:0 15px; padding:10px 0 7px; border-bottom:1px solid #e6e6e6;}
#tip-box-share .tip_hd h4{font-size:18px;}
#tip-box-share .tip-mid{padding-bottom:18px;border-radius: 0px 0px 15px 15px;}
#tip-box-share .clo{ position: absolute;right: 20px;top: 10px;}
#tip-box-share .clo a{display: block; width:30px; height:30px;background:url("../images/tip_clo2.png") 0 0 no-repeat;background-size:100% auto;text-indent: -9999px;}
.share_wrap img{display:block; width:100%;}
.share_wrap p{width:204px; height:24px; position:absolute; left:50%; margin-left:-102px; top:37%}
.share_wrap p.share_none{width:174px; height:79px; margin-left:-87px; top:29%}
.share_tips li{float:left; width:33.3%; height:40px; margin:25px 0 0;}
.share_tips li a{display:block; width:32px; height:32px; margin:0 auto;}
.share_tips .jiathis_style_32x32 .jiathis_txt{float:none}
.share_tips .jiathis_style_32x32 .jtico{padding-left:0!important} 

.footer{background:#cac8c8; text-align:center; position:relative; color:#0d5227}
.footer p{padding:12px 10px 10px; line-height:22px}
.bold{font-weight:bold;}
@keyframes pound{
    from{transform: none;}
    50% {transform: scale(1.2);}
    to {transform: none;}
}
@-webkit-keyframes pound{
    from{-webkit-transform: none;}
    50% {-webkit-transform: scale(1.2);}
    to {-webkit-transform: none;}
}
.paddtop20{padding-top:20px}