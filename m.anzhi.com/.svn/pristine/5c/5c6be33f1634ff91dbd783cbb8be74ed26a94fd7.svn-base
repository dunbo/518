html {
    font-size: 100%;
    -webkit-text-size-adjust: 100%;
	height:100%;
	width: 100%;
}
body {
    padding: 0;
    margin: 0;
    font-size:18px;
    line-height: 20px;
    color: #000;
	height:100%;
	width: 100%;
	-webkit-user-select: none;
	overflow:hidden;
}
a {
    color: #000;
    text-decoration: none
}

a:hover, a:active {
    outline: 0
}
h1,h2,h3,h4,h5,h6p,ul,li,form,dl,dt,dd{margin:0;padding:0;}
ul,li{list-style:none}

