/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto; background:#fce480; font-size:15px; font-family:Microsoft Yahei,"微软雅黑"; color: #8b133a; line-height:20px}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#000; font-size: 14px; outline: none;}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.cl{overflow:hidden;}
.clear{clear:both;overflow:hidden;}

.mtop10{margin-top: 10px;}
.space{height:36px;}

.main{padding:0 8px; background:url(../images/snow.png) no-repeat; background-size:cover; position:relative;}
.main2{background:url(../images/snow2.png) no-repeat; background-size:cover; overflow:hidden}
.banner{width: 100%; margin: 0 auto;}
.banner img{ width: 100%;}

/*index*/
.introduce{position: relative; margin:-30px auto 0; background: url("../images/bg1.png") 0 0 no-repeat; background-size: 100% 100%; overflow:hidden}
.introduce_top{width:80%; margin:20px auto 0; padding: 10px; border-radius:20px; background:#aa1415; border:1px solid #df3539;}

.rule{color:#fff;}
.rule p{line-height: 22px;}
.rule p.strong{font-weight:bold; color:#ddd; font-size:16px; line-height:24px;}
.rule p a{float:right; margin-right:20px; display:inline; text-decoration:underline; color:#5a1c80;}
.notice{ margin:18px 0 0; padding:4px; border-radius:4px; background:url(../images/notice_bg.jpg) no-repeat; color:#6d2d10; position:relative}
.notice_list{height:28px; padding:0 5px 0 25px; background:#f4d421; line-height:28px;}
.gift_decorate2{position:absolute; left:-10px; top:-18px; width:36px; height:40px; background:url(../images/gift_decorate2.png) no-repeat; background-size:100% 100%}

.logined{ width:80%; margin:6px auto 20px; padding:0 10px; position:relative}
.logined_r{width:95px; height:33px; position:absolute; right:0; top:19px;}
.logined_r a,.my_prize_list li a{display:block; width:75px; height:33px; padding:0 0 0 20px; background:url(../images/userinfo_btn.png) no-repeat; background-size:95px 66px; text-align:center; line-height:33px; color:#fff4d9}
.logined_l p{ padding:2px 0; text-shadow:#fff 1px 0 0,#fff 0 1px 0,#fff -1px 0 0,#fff 0 -1px 0; color:#aa1415}
.login{padding-top:5px; padding-bottom:5px}
.login .logined_r{top:3px;}
.gift_decorate1{position:absolute; left:0; top:-4px; width:64px; height:62px; background:url(../images/gift_decorate1.png) no-repeat; background-size:100% 100%; z-index:2}
.lotter_wrap{position: relative; margin:-10px 10px 0; background: url("../images/bg2.png") 0 0 no-repeat; background-size: 100% 100%; overflow:hidden}
.lotter_inner{position: relative; margin:-10px 0 0; padding:0 0 10px 0; background: url("../images/bg3.png") 0 0 no-repeat; background-size: 100% 100%;}
.lotter_inner h3{width:130px; height:36px; position:absolute; left:5%; top:2%;}
.lotter_inner h3.lotter_title1{background: url("../images/lotter_title1.png") 0 0 no-repeat; background-size: 100% 100%;}
.lotter_inner h3.lotter_title2{background: url("../images/lotter_title2.png") 0 0 no-repeat; background-size: 100% 100%;}
.prize_index{position:relative; margin:10px 10px 0}
.prize_index p{height:30px; padding:48px 0 0 9%; color:#fff; line-height:30px}
.prize_index p span{color:#ff1c00; font-size:24px; font-style:italic}
.prize_index  a,.prize_index_btn2{display:block; position:absolute; right:10px; top:10px; width:86px; height:38px; background:url(../images/btn_01.png) no-repeat; background-size:100% 100%; text-align:center; line-height:38px; font-weight:bold; color:#fff}
#gift_inner .prize_index_btn2{background:url(../images/btn_08.png) no-repeat; background-size:100% 100%;}
.lottery{position:relative}
.lottery  a,.prize_index_btn{display:block; position:absolute; right:20px; top:-5px; width:71px; height:26px; background:url(../images/btn_02.png) no-repeat; background-size:100% 100%; text-align:center; line-height:26px; font-weight:bold; color:#fff;z-index:9999}
.rotate {width:260px; height:260px;  margin:5px auto 0; position: relative; overflow}
.rotate-con{width:100%;}
.rotate-con img{position:absolute; left:0; top:0; z-index:2}
.rotate-pointer {position: absolute;top: 50%;left: 50%;width: 46%;height: 46%;margin-top: -23%;margin-left: -23%;background: url(../images/pointer.png) no-repeat 0 0;background-size: 100% 100%; cursor: pointer;-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none; z-index:9999}
.pointer-disabled{background: url(../images/pointer_disabled.png) no-repeat 0 0;background-size: 100% 100%; z-index:9999}
.rotate-pointer2 {position: absolute;top: 50%;left: 50%;width: 46%;height: 46%;margin-top: -23%;margin-left: -23%;background: url(../images/pointer2.png) no-repeat 0 0;background-size: 100% 100%; cursor: pointer;-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none; z-index:9999}
.pointer-disabled2{background: url(../images/pointer_disabled2.png) no-repeat 0 0;background-size: 100% 100%; z-index:9999}
.lottery_bg2{width:122px; height:102px; background:url(../images/lottery_bg2.png) no-repeat; background-size:100% 100%; position:absolute; left:-18%; bottom:-2%; z-index:3}
.lottery_bottom p{padding:6px 3px; text-align:center; color:#fff; line-height:24px}
.lottery_bottom a{display:block; width:95px; height:26px; margin:0 auto 10px; background:url(../images/btn_03.png) no-repeat; background-size:95px 26px;}
.lottery_bottom a.lottery_btn2{background:url(../images/btn_07.png) no-repeat; background-size:95px 26px;}
.soft{padding:54px 20px 10px;background:url(../images/bg4.png) no-repeat; background-size:100% 100%; position:relative}
.soft_title a{display:block; width:80px; height:27px; background:url(../images/btn_04.png) no-repeat; background-size:100% 100%; text-align:center; line-height:27px; color:#fff; font-weight:bold; position:absolute; right:11%; top:38px}
.soft_list{padding:10px;}
.soft_list li{ float: left; display: inline; width: 25%;margin-top:7px;text-align: center;}
.soft_list li p{}
.soft_list li .soft-icon{text-align: center;}
.soft_list li .soft-icon img{ width: 56px;height: 56px; margin: 0 auto;}
.soft_list li .soft-name{margin-top: 10px;height: 20px; line-height: 20px; overflow: hidden;}
.soft_list li .soft-name{ color: #326f00;}
.soft_list li:nth-child(2n) .soft-name{color:#ca5b49}
.soft_list li .soft_size{ color: #f46600;height: 22px; line-height: 22px;overflow: hidden;}
.soft_list li a.downlode{ display: block; width:64px;height: 26px; padding-top:6px; line-height: 26px; margin: 10px auto; background:url(../images/btn_06.png) no-repeat; background-size:100% 100%; color:#ffffff;cursor: pointer; text-align: center; font-weight:bold}
.active_bom{margin:0 15px; color:#6f1800}
.title{padding:10px 0 8px 0; position:relative}
.title h3{color:#5c001e; font-weight:bold}

#gift_inner{margin-left:1px; background: url(../images/bg5.png) 0 0 no-repeat; background-size: 100% 100%;}
.prize_index_btn{right:120px; top:24px;}
.prize_index_btn2{right:23px; top:20px;}
.gift_list{margin:0 22px}
.gift_top{padding:64px 0 0;}
.gift_list li{float:left; width:24%; margin:5px 0.5%; position:relative}
.gift_list_img{position:relative; width:100%}
.gift_list li img{width:100%}
.gift_list_img i,.gift_list2_img i{position:absolute; left:0; top:0; width:100%; height:100%; background:url(../images/mark_01.png) no-repeat; background-size:100% 100%;}
.gift_list li span{display:block; text-align:center; color:#ff0000; font-size:12px}
.gift_list li a{display: block; width:64px;height: 26px; line-height: 26px; margin: 0 auto 5px; background:url(../images/btn_05.png) no-repeat; background-size:100% 100%; color:#ffffff;cursor: pointer; text-align: center; font-weight:bold}
.gift_list li a.disabled2{background:url(../images/btn_09.png) no-repeat; background-size:100% 100%;}
.gift_list2 li{width:25%; margin:5px 0;}
.gift_list2 em{position:absolute; float:left; width:19%; height:30px; top:50%; margin-top:-15px; color:#ff0000; font-size:22px; text-align:center}
.gift_list2_img{width:81%; margin-left:19%; position:relative}
.gift_list2_img i{background:url(../images/mark_02.png) no-repeat; background-size:100% 100%;}
.gift_list2 li a{margin:10px auto 0;}

/*我的奖品*/
.gift_decorate3{margin:50px 0 0; width:100%; height:80px; background:url(../images/gift_decorate3.png) no-repeat; background-size:100% 100%; position:relative; z-index:-1;}
.my_prize{margin:-30px 10px 10px; padding:15px; background:url(../images/bg6.png) no-repeat; background-size:100% 100%; position:relative}
.my_prize_inner{padding:15px; background:#fff8ee; border-radius:20px}
.my_prize_list li{line-height:25px; font-size:16px; position:relative; color:#5c001e}
.my_prize_list li a {width:64px; height:26px; margin:5px auto 0; padding:0; background:url(../images/btn_05.png) no-repeat; background-size:100% 100%; line-height:26px; font-weight:bold; color:#fff}
.my_prize_list2{padding:5px 0; color:#5c001e}
.my_prize_list2 li{ position:relative}
.my_prize_list2 p{ padding:6px 0;}
.box_bottom{margin:0 10px}
.box_bottom img{width:100%}
.btns{margin-top:20px}
.btns a{display:block; width:120px; height:40px; margin:0 auto; background:url(../images/btn_04.png) no-repeat; background-size:100% 100%; text-align:center; line-height:40px; color:#fff; font-size:18px; font-weight:bold}
.form-items li{ margin-top: 20px; clear: both; overflow: hidden;}
.form-items li label{ float: left;width:90px;padding-top: 10px;}
.form-items li span{display:block;margin-left: 90px;}
.form-items li input{ width:100%;padding:10px 6px;border:none;color: #000;  font-family: Microsoft Yahei,"微软雅黑"; background-color:#fff; border-radius:2px}
.form_error{display:block; padding:8px 0 0 120px; color:red}
.split{height:3px; background:url(../images/line_bg.png) no-repeat; background-size:100% 100%; margin: 8px 0px 15px; overflow:hidden;}
.gift_decorate4{position:absolute; left:-15px; top:-28px; width:70px; height:74px; background:url(../images/gift_decorate4.png) no-repeat; background-size:100% 100%}
.border_bg{width:162px; height:39px; background:url(../images/border_bg.png) no-repeat; background-size:100% 100%; position:absolute; right:-3px; top:-13px;}

/*结束页*/
.line{width:80%; height:1px; margin:5px auto 0; background:#550f06; overflow:hidden}
.awards{width:80%; margin:0 auto; padding:10px 20px; position:relative; color:#270400}
.awards h4{padding:0; margin-bottom:10px; text-align:center;}
.awards_list{line-height: 22px; text-align:center}
.slide_up{overflow:hidden;height:110px;position: relative;}

/*提示框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-webkit-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{ position:absolute; width:90%; left:50%; top:50%; padding:0 0 20px 0; margin-left:-45%; z-index: 99999; background:url(../images/dialog_bg.png) no-repeat; background-size:100% 100%; font-size:15px;}
.tip-con{ margin: 15px 15px 0 15px;padding: 160px 12% 10px 5%; color:#2b2b2b}
.tip_title{margin-left:34%; margin-right:1%;background:url(../images/tips_bg1.png) no-repeat; background-size:100% 100%; position:relative }
.tip_title h6{ padding:6px; min-height:22px;  color:#81d418; font-size:15px; font-weight:bold; line-height:18px;text-align:center}
.tip-con p{padding:20px 0 10px; line-height: 22px; word-wrap:break-word;word-break:break-all;}
.tip-con p.tip_h{font-weight:bold; font-size:18px; line-height:34px}

.btns2{width:200px; margin:0 auto 15px}
.btns2 a{float:left; display:block; width:80px; height:26px; margin:0 10px; background:url(../images/btn_05.png) no-repeat; background-size:100% 100%; text-align:center; line-height:26px; color:#fff; font-weight:bold}

.footer{background:url(../images/footer_bg.png) repeat-x #e5c745; text-align:center; position:relative; color:#836e14}
.footer p{padding:25px 10px 10px; line-height:22px}
.bold{font-weight:bold;}
@keyframes pound{
    from{transform: none;}
    50% {transform: scale(1.2);}
    to {transform: none;}
}
@-webkit-keyframes pound{
    from{-webkit-transform: none;}
    50% {-webkit-transform: scale(1.2);}
    to {-webkit-transform: none;}
}
.heart {animation: pound 1.2s infinite; -webkit-animation: pound 1.2s infinite;}
