/* CSS Document */
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td,hr,button,article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section {margin:0;padding:0;}
fieldset,img {border: 0;}
a{text-decoration:none; color:#000000;}
input,input:hover,textarea,textarea:hover,select:hover{ border:1px solid #e6241a}
.clear{clear:both;}
ul,li{list-style:none;}
body{ font-family:MicroSoft YaHei,SimSun,sans-serif; background: url(../images/feedback_images/items_bg.png) repeat; font-size:14px; line-height:20px; color:#000000; -moz-user-select: none; -webkit-user-select: none}

#main{width:98%; margin:0 auto;}
.items_list_wrap{width:100%; margin:5px auto 0; border-top:1px solid #fff; background:#f6f6f7;/* border-radius:4px*/}
.items_sort_wrap{width:100%; /*margin:6.7px auto 0;*/}
.items_sort{width:100%;margin:0 auto;margin-top:0px; overflow:hidden;}
.items_sort li{text-align:center; margin-right:0.4%; float:left; width:49.8%;}
.items_sort li.blank_module{ float:right; margin-right:0px;}
.items_sort li dl{width:99%;display:block; /*padding:12px 0;height:28px; border-radius:4px*/ float:left;margin-top:5px; padding:2px 0 2px 1.5px;position:relative;border-top:1px solid #fff; background:#f6f6f7;}
.items_sort li dl dt.girls,.items_sort li dl dt.boy{/*padding:15px 12px;*/ margin-left:8px; float:left; background:url(../images/fm_1.png) no-repeat;}
.items_sort li dl dt.girls{ background-position:0 -25px;min-height:28px;max-height:34px; width:28px;padding-left:17%;}
.items_sort li dl dt.boy{ background-position:0 5px;/* height:34px;*/}
.items_sort li dl dd.girls_boy_btn{float:left; margin-left:6.7px; line-height:32px;}
.items_sort li a{ color:#2b2b2b; font-size:15px; width:99%; height:auto;}

.items_title{/*width:99%;*/ margin:0 auto; padding:3.7px 6.7px;}
.items_titleA{width:100%; margin:0 auto; padding:6.7px 0;}
.items_title h3{color:#2b2b2b;}
.items_titleA h3{ color:#575757;}
.items_title h3,.items_titleA h3{float:left; padding-left:;font-size:18px; font-weight:normal;/*line-height:30px*/}
/*height:36px; padding-left:50px; background:url(../images/feedback_images/items_bg1.png) no-repeat; */
.items_title span,.items_titleA span{float:right; margin:8px 0px 0 0;font-size:15px}
.items_title .font_size14{float:right; margin:5px 0px 0 0;font-size:15px;}
.items_title .font_size14 em,.items_titleA span a{display:block; padding-right:25px; font-style:normal;background:url(../images/feedback_images/items_bg_more.png) no-repeat right 3px; color:#929292;}
.items_list{/*width:99%; border-top:1px solid #e2e2e2; padding:0 0 6.7px; margin:0 6.7px;*/ padding:0; margin:0;}
.items_list li{width:25%; text-align:center; float:left; position:relative;}
.left_border{border-left:1px solid #d8d8d8;}
.right_border{border-right:1px solid #d8d8d8;}
.items_list li a{ display:block; padding:12px 0; border:1px solid #eeeeee;/* border-left:none; border-top:none; */margin-left:-1px; margin-top:-1px;color:#666; font-size:15px}
//.items_list li:nth-child(5n) a,.items_list li:first-child a{/*border-left:1px solid #d2d2d2;*/}
.items_list li:nth-child(4n) a{/*border-right:none;*/}
.items_list li span{position:absolute; right:0px; top:0; display:block;width:16px; height:20px; background:url(../images/feedback_images/items_bg_re.png) no-repeat; z-index:2 }


/*feedback_new*/
.title_top{height:46px; background:#e6241a; border-bottom:1px solid #0367a6;}
.title_back{float:left; margin-left:5px; font-size:20px; line-height:46px;}
.title_back a{ display:block;  height:46px; padding:0 10px 0 35px; background:url(../images/feedback_images/back.png) no-repeat 6px 12px; color:#fff; font-weight:bold}
.title_btns{float:right; height:34px; margin:7px 15px 0 0; display:inline}
.title_btns a{float:left; width:34px; height:34px; margin-left:25px; text-align:center; line-height:34px; display:inline}
.title_btns a#feedback_icon{background:url(../images/feedback_images/feedback_icon.png) no-repeat;}
.title_btns a#help_icon{background:url(../images/feedback_images/help_icon.png) no-repeat;}
.feedback_form_item{width:100%; margin-top:8px;}
.feedback_form_item select{width:100%; padding:5px 0 5px 5px; border:1px solid #bdbdbd; border-radius:4px; height:39px;font-size:14px;line-height:32px;font-family:MicroSoft YaHei,SimSun,sans-serif;letter-spacing:0.1em;}/*5px 0 5px 5px*/
.feedback_form_item select option{padding:5px 0 5px 10px; height:29px; line-height:32px;font-size:14px; letter-spacing:0.1em;}
.feedback_text_wrap{padding:0 22px 0 8px; position:relative}
.feedback_form_item .inputtext1{width:99.6%; padding:10px 0 10px 5px; background:#ffffff; border:1px solid #bdbdbd; border-radius:4px;/* color:#797979;*/ font-family:MicroSoft YaHei,SimSun,sans-serif;}
.prompt_infor{ width:25px; color:red; text-align:right; position:absolute; right:5px; top:5px;}
.feedback_form_item p{ padding:5px 0 8px 5px; color:#666666; font-size:14px;}
.feedback_form_item textarea{ width:99.6%;max-width:99.6%;height:80px; padding:5px 0 5px 5px; background:#ffffff; border:1px solid #bdbdbd; border-radius:4px;}
.feedback_form_btn input{ display:block; width:80%; height:42px; margin:10px auto; background:#008aea; border:none; border-radius:4px; color:#ffffff; font-size:21px; line-height:42px; font-family:MicroSoft YaHei,SimSun,sans-serif;}
.contact{padding:10px 0; text-align:center; color:#666666;}
#feedback_select{position:absolute;  left:8px; right:16px; background:#eff2f2; display:none;z-index:999999}
#feedback_select li{/*width:98%;*/ padding:10px 0 10px 15px; color:#797979; margin:0 auto; border-bottom:1px solid #d8d8d8;z-index:999999}

/*comment*/
.comment_cnt{padding:8px 10px; border-top:0px solid #d2d2d2;}
.comment_top{padding:5px 0;}
.comments_user{width:100%; margin-top:15px; position:relative;border-top:0px solid #d2d2d2;}
.comments_az{width:100%; margin-top:15px; position:relative;}
.comments_az_infor{padding:0 0 0 88px; min-height:100px;}
.comments_user .comments_az_infor{padding:0 88px 0 0;}
.comments_img{position:absolute; left:0; top:0; width:70px; height:88px;}
.comments_img_user{position:absolute; right:0; top:0; width:70px; height:88px;}
.comments_img img,.comments_img_user img{display:block; width:60px; height:60px; margin:0 auto;}
.comments_img span,.comments_img_user span{display:block; height:22px; line-height:22px;text-overflow:ellipsis; overflow:hidden; width:70px;text-align:center}
.comment_tips{padding-top:5px;}
.comment_tips span{float:left; width:40px; height:22px; background:#7cb552; line-height:22px; color:#fff; text-align:center}
.comment_tips em{float:left; font-style:normal; padding-left:20px; color:#666;}
.comments_user .comment_tips em{float:right; font-style:normal; padding-right:20px; color:#666;}
.comment_des{padding:10px; margin-top:7px; background:#eff2f2; border-top:1px solid #fff; border-bottom:1px solid #d8d8d8; position:relative}
.comment_des > p{ width:100%;word-wrap:break-word;}
.comment_icon{display:block; position:absolute; left:-16px; top:-1px; width:16px; height:12px; background:url(../images/feedback_images/comment_icon.png) no-repeat;}
.comment_icon_user{display:block; position:absolute; right:-16px; top:-1px; width:16px; height:12px; background:url(../images/feedback_images/comment_icon2.png) no-repeat;}
.comment_form{padding:10px 0; position:relative}
.comment_form_inner{height:30px; padding:0 80px 0 0;}
.comment_form .inputtext2{width:100%; height:28px; padding:0 0 0 8px; border:1px solid #d2d2d2; display:block;}
.comment_form .comment_submit{position:absolute; right:0; top:10px; width:56px; height:30px; background:#008aea; border:none; border-radius:4px; color:#fff; font-size:16px; font-family:MicroSoft YaHei,SimSun,sans-serif; line-height:26px; text-align:center; line-height:30px; cursor:pointer}

/*feedback_list*/
.feedback_title{height:36px; border-top:1px solid #fff; border-bottom:1px solid #d2d2d2; background:#eff2f2;}
.feedback_title li{float:left; width:50%; height:33px; font-size:16px;}
.feedback_title li a{display:block; height:33px; text-align:center; line-height:33px; color:#737373;}
.feedback_title li.current a{ border-bottom:3px solid #e6241a; color:#e6241a;}
.feedback_list{width:100%;}
.feedback_list ul li{min-height:48px; padding:5px 10px 5px 10px; background:#eff2f2; border-top:1px solid #fff; border-bottom:1px solid #d8d8d8; margin-top:10px; border-radius:4px; position:relative;}
.feedback_list ul li h4{height:24px; line-height:24px; font-size:16px; color:#2b2b2b;}
.feedback_tips{height:22px; line-height:22px; color:#666666;}
.feedback_tips_left{float:left;}
.feedback_tips span span.red{color:#f00;}
.feedback_tips .feedback_tips_date{float:right; padding-left:5px;font-style:normal; font-size:12px; color:#515151;}
.feedback_tips .feedback_tips_date span{color:#666; padding-left:8px;}
.feedback_list_delete{ position:absolute; right:8px; top:8px; width:35px; height:40px; line-height:35px; text-align:center; cursor:pointer;}
.feedback_list ul li a.clickarea{ display:block; position:absolute; left:0; top:0; right:0; bottom:0; z-index:100;}

/*help*/
.help_wrap{width:100%; }
.help_wrap ul li .btn_module{background: none repeat scroll 0% 0% rgb(239, 242, 242); border-radius: 4px; border-top: 1px solid rgb(255, 255, 255); border-bottom: 1px solid rgb(216, 216, 216); color: rgb(43, 43, 43); padding: 8px; margin-top: 6px; position: relative;}
.help_wrap ul li .btn_module .dt_question{line-height: 32px; font-size: 16px; font-weight: normal; width: 90%; text-overflow:ellipsis ;white-space:nowrap;overflow:hidden;}
.help_wrap ul li .btn_module .updown_module{float: left; width:5%;}
.help_wrap ul li .btn_module .updown_module span{position:absolute; right:15px; top:14px; display:block; width:16px; height:16px; background:url(../images/feedback_images/down.png) no-repeat;}

.help_wrap ul li h6{height:32px; padding:8px; margin-top:6px; background:#eff2f2; border-radius:4px; border-top:1px solid #fff; border-bottom:1px solid #d8d8d8; line-height:32px; font-size:18px; font-weight:normal; color:#2b2b2b; position:relative}
.help_wrap ul li h6 span{ position:absolute; right:15px; top:14px; display:block; width:16px; height:16px; background:url(../images/feedback_images/down.png) no-repeat;}
.help_wrap ul li .btn_module .updown_module span.up{background:url(../images/feedback_images/up.png) no-repeat;}
.help_answer{padding:8px 5px 8px 8px; line-height:22px; display:none; color:#666; font-size:16px;}

/*report*/
.report_wrap{width:100%;}
.titles_01{padding:10px 0;border-bottom:1px solid #d2d2d2;}
.titles_01 p{font-size:16px; color:#aeaeae}
.titles_02{padding:10px 0;}
.titles_02 p{font-size:16px; color:#aeaeae}
.required_must{padding:0 10px; color:red; font-size:12px;}
.report_items{border-top:1px solid #cccccc; border-bottom:1px solid #cccccc;}
.report_items ul li{width:99%; font-size:18px; position:relative}
.report_items ul li span{display:block; width:24px; height:24px; background:url(../images/feedback_images/check_false.png) no-repeat; position:absolute; left:9px; top:11px;}
.report_items ul li span.checkedbg{background:url(../images/feedback_images/check_true_red.png) no-repeat;}
.report_items ul li label{ display:block; padding:13px 0 13px 45px; border-bottom:1px solid #cccccc; color:#575757;}
.report_items ul li:last-child label{border-bottom:none;}
.report_items ul li input{ position:absolute; left:-10000px; top:0; clip:rect(0,0,0,0);}
.report_form textarea{width:97%; height:80px; margin:0 auto; padding:4px 1%; background:#fff; color:#797979; border:1px solid #bdbdbd; border-radius:5px; display:block}
.form_btn input{ display:block; width:99%; height:42px; margin:10px auto; border-radius:4px; background:#e6241a; border:none; font-size:21px; color:#fff; font-family:MicroSoft YaHei,SimSun,sans-serif;}

.c_app{margin:8px 0 -10px 0px;}
.c_app li{ clear: both; overflow: hidden; position:relative; padding: 10px;border-top:1px solid #ccc;}
.c_app li.on{ background-color: #cecece}
.c_app li .click_bar{position: absolute;top:0px; left: 0px; display: block; width: 100%; height: 100%;z-index: 1;-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.c_app_icon{ float: left;}
.c_app_icon img{ display:block;width:38px; height: 38px; }
.c_app_info{margin-left:45px;margin-right:55px;}
.c_app_info h4{line-height: 20px;font-weight: bold;line-height: 20px; white-space: nowrap; overflow: hidden; text-overflow: ellipsis}
.c_app_info .c_app_size{padding-top:2px;font-size: 12px; line-height:16px;color: #828282 }
.c_download{position: absolute;top:17px;right:10px;z-index: 2;}
.c_download a{ display: block; width:45px; height:22px; line-height: 22px; border: 1px solid #e6241a; border-radius: 3px;-webkit-border-radius:3px; background-color: #eee; color: #e6241a; text-align: center;-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.c_download a.on{ background-color: #e6241a; color: #ffffff}
