<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<meta name="viewport" content="width=device-width,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no">
<meta content="telephone=no" name="format-detection">
<meta name="keywords" content="" />
<meta name="description" content="" />
<title>假期玩不停，你敢下我就敢送</title>
<link type="text/css" rel="stylesheet" href="/css/vacation_second_common.css?v1"/>
<script type="text/javascript" src="/js/jquery-1.8.0.min.js"></script>
<script type="text/javascript">
    var scroltime=100;//间隔时间
    function slideUp(obj){
        $(obj).find("div:first").animate({
            marginTop:"-25px"
        },1000,function(){
            $(this).css({marginTop:"0px"}).find("dd:first").appendTo(this);
        });
    }
    $(document).ready(function(){
        setInterval('slideUp("#fristPrize")',scroltime);
        setInterval('slideUp("#secondPrize")',scroltime);
        setInterval('slideUp("#thirdPrize")',scroltime);
        setInterval('slideUp("#forthPrize")',scroltime);
        setInterval('slideUp("#fivthPrize")',scroltime);
    });

</script>
</head>
<body>
<section  class="pbot" style="background-color: #cde0e2">
    <div class="banner" id="top">
        <a href="javascript:;"><img src="/images/vacation_second_images/cam_banner.jpg"/></a>
    </div>
    <div class="notice clear">
        <div class="notice-list cnt">活动已结束</div>
    </div>
    <div class="white-wrap">
        <div class="awards">
            <h3 class="awards-hd">获奖名单</h3>
			<!--{if $out.first_award}-->
            <dl class="awards-list">
                <dt>幸福迎春大奖</dt>
				<!--{foreach from=$out.first_award item=vo}-->
                <dd><em><!--{$vo.the_telphone}--></em><span><!--{$vo.prize}--></span></dd>
				<!--{/foreach}-->
            </dl>
			<!--{/if}-->
			<!--{if $out.second_award}-->
            <dl class="awards-list">
                <dt>喜悦迎春大奖</dt>
