						if(!data){
							//$("#my_info").html('<a href="<!--{$out.login_url}-->">个人信息</a>');
							var str = "<div class='prompt'>请您填写如下信息，以便于我们将奖品邮寄给您!</div><ul class='form-items'><li><label>联系人姓名：</label><span><input type='text' id='name' name='name' value=''/></span></li><li class='name_error error'></li><li><label>联系人手机号码：</label><span><input type='text' id='mobile' name='mobile' value=''/></span></li><li class='mobile_error error'></li><li><label>邮寄地址：</label><span><input type='text' id='address' name='address' value=''/></span></li><li class='address_error error'></li></ul><div class='btn_submit'><a href='javascript:;' onclick='return saveMyInfo(1)'>提&nbsp;&nbsp;交</a></div>";
						}else{		
							var str = "<div class='prompt'>请您填写如下信息，以便于我们将奖品邮寄给您!</div><ul class='form-items'><li><label>联系人姓名：</label><span><input type='text' id='name' name='name' value='"+data.name+"'/></span></li><li class='name_error error'></li><li><label>联系人手机号码：</label><span><input type='text' id='mobile' name='mobile' value='"+data.mobile+"'/></span></li><li class='mobile_error error'></li><li><label>邮寄地址：</label><span><input type='text' id='address' name='address' value='"+data.address+"'/></span></li><li class='address_error error'></li></ul><div class='btn_submit'><a href='javascript:;' onclick='return saveMyInfo(1)'>提&nbsp;&nbsp;交</a></div>";
							
						}
						$("#tip-box4 .cont").html(str);
					}else{
						if(!data){
							var str = "<li><label>联系人姓名：</label><span><input type='text' id='name' name='name' value=''/></span></li><li class='name_error error'></li><li><label>联系人手机号码：</label><span><input type='text' id='mobile' name='mobile' value=''/></span></li><li class='mobile_error error'></li><li><label>邮寄地址：</label><span><input type='text' id='address' name='address' value=''/></span></li><li class='address_error error'></li>";
						}else{
							var str = "<li><label>联系人姓名：</label><span><input type='text' id='name' name='name' value='"+data.name+"'/></span></li><li class='name_error error'></li><li><label>联系人手机号码：</label><span><input type='text' id='mobile' name='mobile' value='"+data.mobile+"'/></span></li><li class='mobile_error error'></li><li><label>邮寄地址：</label><span><input type='text' id='address' name='address' value='"+data.address+"'/></span></li><li class='address_error error'></li>";
						}
						
						if(type == 5){
							$("#tip-box5 .form-items").html(str);
							if(is_end==1) $(".btn_submit").css('display','none');
						}else{
							$("#tip-box .form-items").html(str);
						}
						
					}
					
				}
		})
	}
	
	function saveMyInfo(type){
		var name = $("#name").val();
		var mobile = $("#mobile").val();
		var address = $("#address").val();
		if(!name){
			$(".name_error").html("姓名不能为空");
		}else{
			$(".name_error").html("");
		}
		var mobile_re = /^[0-9]{11}$/;
		if(!mobile){
			$(".mobile_error").html("手机号码不能为空");
		}else{
			$(".mobile_error").html("");
		}
		if(!mobile_re.test(mobile)){
			$(".mobile_error").html("手机号码不正确");
		}else{
			$(".mobile_error").html("");
		}
		if(!address){
			$(".address_error").html("地址不能为空");
		}else{
			$(".address_error").html("");
		}
		if(!name||!mobile||!address){
			return false;
		}

		$.ajax({                			
				url: '/lottery/flyback/flyback_userinfo.php',
				data: 'uid='+uid+'&type=save_info&sid='+sid+'&name='+name+'&mobile='+mobile+'&address='+address,
				type: 'post',
				dataType: "json",
				asyn:false,
				success: function(data){
					if(data==1){
						if(type==1){
							cloBox("#tip-box4");
						}else if(type==5){
							cloBox("#tip-box5");
						}else{
							cloBox("#tip-box");
						}
						showOpenBox("#tip-box8");
					}
				}
		})
	}
</script>