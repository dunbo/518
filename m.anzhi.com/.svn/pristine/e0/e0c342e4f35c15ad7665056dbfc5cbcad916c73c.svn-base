/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto;font-size:14px; font-family:Microsoft Yahei,"微软雅黑"; background-color: #572a01; color: #7e3c00;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#730b91; font-size: 14px; outline: none;}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
::-webkit-input-placeholder { color:#d9d2c4;}
:-moz-placeholder {color:#d9d2c4;}
::-moz-placeholder {color:#d9d2c4;}
:-ms-input-placeholder {color:#d9d2c4;}

.btns a,.soft-list li a.downlode,.tip-box .clo a,.tip-hd .chang_btn{background: url("../images/common_icon.png") 0 0 no-repeat;background-size: 150px 225px;}

.banner{position:relative;width: 100%; margin: 0 auto;}
.banner img{ width: 100%;}
.banner .person1{position: absolute;width: 45%;right: 0px;top:0px;z-index: 2}
.banner .person1 img{width: 100%}
.persons{position: absolute;left: 19%;bottom:-10%;width:42%;z-index: 2}
.persons img{width: 100%;}
/**/
.main{position:relative;width: 100%;padding-bottom:3%;background: url("../images/main_bg.jpg") 0 0 no-repeat;background-size:100% 100%;}
.mod{width:83%;margin: 0 auto 3px auto;padding:3%;background-color: #fae9cc}
.mod_inner{margin-bottom:10px;padding:10px;background-color: #dec79b;border-radius: 4px;}
.mod_inner_hd{margin-bottom: 10px;color: #bf0000}
.mod_box{padding:10px;background-color: #f4ebc4;border-radius: 4px;}
.mod p{line-height: 22px;}
.mod_hd{margin-bottom: 10px;}
.mod_hd span{padding:2px;background-color:#743701;color: #eee0c1;font-size: 16px;border-radius: 12px;}
.mod_ft{position: absolute;bottom:-1%;left: 0px;}
.mod_ft img{width: 100%;}
.table_list{width: 100%;text-align: center;font-size: 12px;}
.table_list th{padding: 5px 0px;color: #743701;border-right: 1px solid #e6dfc0;}
.table_list td{padding: 5px 0px;color: #ad6e36;border-right: 1px solid #e6dfc0;border-top: 1px solid #e6dfc0;}
.table_list th:last-child,.table_list td:last-child{border-right: 0;}
/*签到*/
.form_item{width: 100%;}
.info{float:right;}
.input_box{margin-right:90px;width:auto; line-height: 26px;}
.input_box input{width:96%;padding:0px 2%;height: 25px;border-radius:12px;border:none;background-color: #fff;font-family: "Microsoft Yahei","微软雅黑";font-size: 14px;}
.star_list{margin: 10px 0px;}
.star_list span{float: left;width:32px;height:32px;line-height:32px;background: url("../images/stars.png") 0 0 no-repeat;background-size: 48px 144px;text-align: center}
.star_list span.star_gold{background-position:0 0; }
.star_list span.star_gray{background-position:0 -40px;color: #606060}
.star_list span.star_lightgray{background-position:0 -78px;color: #d0d0d0}
.star_list em{float: left;width:6px;height:32px;background: url("../images/stars.png") 0 0 no-repeat;background-size: 48px 144px;}
.star_list em.spot_gold{background-position:-40px 0px;}
.star_list em.spot_gray{background-position:-40px -40px;}
.star_list em.spot_lightgray{background-position:-40px -76px;}
.prompt{}
.prompt span{color: #ca0101;}
/*按钮*/
.btns{}
.btns a{ display: block;width:72px; height:26px;line-height:26px;background-position: 0px 0px;text-align: center;color: #fff;font-weight:bold;}
.btns a.sign_btn{background-position: -80px 0px;color: #743701}
.btns a.sign_btn.gray{background-position: -80px -33px;color: #555;cursor: default }
.btns a.submit_btn{width:75px;height:30px;line-height:30px;margin: 0 auto;background-size: 130px 195px;background-position: 0 -60px;}
/*活动规则*/
.rule_wrap{background: url("../images/rule_bg.jpg") 0 0 no-repeat;background-size: 100% 100%;}
.rule{width:80%;margin: 0 auto;padding:6% 5% 3% 5%;}
.rule h4{margin: 10px 0px;}
.rule h4 span{padding:2px 6px 2px 3px;background-color:#703601;border-radius:12px;color:#eee0c1;font-size: 16px;}
.rule h4 span em{padding:0px 5px;border-radius: 50%;background-color: #fdf3e0;color: #703601;font-family: Arial}
.rule_list{position:relative;padding:3px 8px;background-color:#ebb64e;border-top:5px solid #f4d18d;line-height: 22px;border-radius:0px 0px 4px 4px;}
.person2{position: absolute;left: 100px;top:-40px;width: 50px;}
.person3{position: absolute;left: 100px;top:-45px;width: 45px;}
.person2 img,.person3 img{width: 100%;}
/*抽奖规则*/
.r_text{margin: 20px 0px 10px 0px;}
.r_text p{clear:both; overflow:hidden;color: #ad6e36}
.r_text p em{float: left;}
.r_text p span{display:block;margin-left: 13px;}
/*奖品设置*/
.slide{position:relative;padding:11% 0px;margin-top:-3%;background: url("../images/slider_bg.png") 0 0 no-repeat;background-size: 100% 100%;}
.person6{position: absolute;top:-16%;left: 35%;width: 15%;}
.person6 img{width: 100%;}
.slideBox{ position:relative;width: 90%;margin:0px auto;background-color: #fae9cc}
.slideBox .hd{position: absolute;left: 20px;bottom:10px;z-index: 5}
.slideBox .hd ul{ clear: both;overflow: hidden;}
.slideBox .hd ul li{display:inline;float: left;width: 18px;height: 8px;background-position: 0px -142px;margin-right: 5px;background-color: #c4c4c4}
.slideBox .hd ul li.on{background-color: #c05d00}
.slideBox .bd{margin:0px auto;}
.slideBox .bd li{position:relative;text-align:center;padding: 0px;}
.slideBox .bd li img{ width:100%;margin:0 auto;}
.slideBox .bd li .pop{position: absolute;right: 0px;bottom:0;width: 45%;}
.slideBox .bd li .pop img{width: 100%;}

/*软件*/
.soft-list{margin: 10px 15px 30px 15px;padding:15px 0px;background-color: #ebb64e;border-radius: 4px}
.soft-list li{ float: left; display: inline; width: 25%;margin-top:7px;text-align: center;}
.soft-list li p{}
.soft-list li .soft-icon{text-align: center;}
.soft-list li .soft-icon img{ width: 56px;height: 56px; margin: 0 auto;}
.soft-list li .soft-name{margin-top: 5px;height: 20px; line-height: 20px; overflow: hidden;}
.soft-list li .soft-name a{ color: #fff;}
.soft-list li .soft-size{ color: #f46600;height: 22px; line-height: 22px;overflow: hidden;}
.soft-list li a.downlode{ display: block; width:58px;height: 24px;line-height: 24px; margin: 5px auto; background-size:100px 150px;background-position:0px -45px ;color:#fff;cursor: pointer;font-size:12px;text-align: center;}
/*获奖名单*/
.awards-list{line-height: 22px;}
.slide_up{overflow:hidden;height:110px;position: relative;}

/*尾部*/
.footer{padding:30px 0px 20px 0px;background: url("../images/ft_bg.jpg") 0 0 no-repeat;background-size: 100% 100%;}
.copyright{text-align: center;color: #743701;}
/*提示框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: rgba(0,0,0,0.5);z-index: 99998;}
.tip-box{ position:absolute;width: 300px;left:50%;top:50%;margin-left:-150px;background:url("../images/tip_box.png") 0 0 no-repeat; background-size:100% 100%;z-index: 99999;}
.tip-hd{margin: 15px 15px 10px 15px;color: #fff;font-size: 14px;}
.tip-hd span{ float: left;margin-top: 4px;padding-left:10px;}
.tip-hd .chang_btn{ float: right;margin-right:20px;width: 66px;height: 26px;line-height: 26px;color:#fff;cursor: pointer;text-align: center;background-size:100px 150px;background-position:0px -73px }
.tip-con{margin: 10px 15px 30px 15px;padding:10px;background-color: #ebb64e;border-top:5px solid #f4d18d;}
.tip-con h4{padding-bottom:10px;margin-bottom:15px;}
.tip-box .clo{position: absolute;right: -8px;top:-15px; }
.tip-box .clo a{display: block;width: 45px;height: 45px;background-size: 100px 150px;background-position: 0px -104px;text-indent: -9999px;}
.tip-text{padding:20px 0px;text-align: center;}
.mbot{margin-bottom: 10px;}
/*联系信息*/
.form_items li{ margin-bottom: 20px;clear: both;overflow: hidden;}
.form_items li label{float: left;width:100px;padding:10px 0px;}
.form_items li span{display:block;width: auto;margin-left: 110px;}
.form_items li input{width:90%;padding:10px 10px;border:none; color: #666;  font-family: Microsoft Yahei,"微软雅黑";background-color: #fff;}
.prompt_submit{color: #fffc00;}


