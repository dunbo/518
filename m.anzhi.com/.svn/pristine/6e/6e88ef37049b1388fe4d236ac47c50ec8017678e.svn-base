/* CSS Document */
/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td,i{margin:0; padding:0;}
body{max-width:640px;min-width:320px; background:#fff; margin:0 auto;font-size:14px; font-family:Microsoft Yahei,"微软雅黑";}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;}
.a,input,button,div{-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{ float: left;}
.fr{ float: right}
.cnt{text-align: center}
.underline{text-decoration:underline;}
.main{padding:0 0 14px;}
.nav{width:100%; height:36px; border-bottom:1px solid #e0e0e0;}
.nav li{float:left; width:50%; height:35px; line-height:35px; text-align:center; color:#8f8f8f; font-size:16px}
.nav li a{display:block; width:100%; height:100%; color:#8f8f8f}
.nav li.current{border-bottom:2px solid #0394f1; color:#0394f1;}
.nav li.current a{color:#0394f1;}
.search_wrap{ height:34px; margin:12px 10px 0; position:relative}
.search{margin:0 78px 0 0; height:34px; border:1px solid #e0e0e0; background:url(../images/search_icon.png) no-repeat 6px 8px; background-size:18px 18px; border-radius:2px; position:relative}
.search input{display:block; width:70%; margin:0 0 0 28px; height:32px; border:none; font-size:16px; line-height:31px}
.search em{position:absolute; right:8px; top:7px; width:18px; height:18px; background:url(../images/search_icon2.png) no-repeat 0 0; background-size:18px 18px; cursor:pointer}
.search_btn{display:block; width:64px; height:34px; border:1px solid #0394f1; border-radius:2px; line-height:35px; text-align:center; position:absolute; right:0; top:0; color:#0394f1}
.search_bg_no{background:none}
.search_bg_no input{margin-left:7px}
.applist{width:100%;}
.applist li{padding:10px 10px 8px; min-height:62px; border-bottom:1px solid #e6e6e6; position:relative;color:#a5a5a5;}
.applist2 li{float:left; width:50%; min-height:62px; border-bottom:1px solid #e6e6e6; position:relative;color:#a5a5a5;}
.app_icon{width:56px; height:56px; position:absolute; left:8px; top:11px}
.app_icon img{display:block; width:56px; height:56px;}
.app_info{padding:0 0 0 62px;}
.app_info h4{width:69%; padding:2px 0;color:#545454; font-size:16px; text-overflow: ellipsis; white-space: nowrap; overflow: hidden;}
.app_code{padding:1px 68px 4px 0; color:#0394f1; word-wrap:break-word; word-break:break-all;}
.app_count{font-size:12px}
.app_count p{float:left; width:34%; height:4px; margin:7px 5px 0 0; background:#dddddd;}
.app_count p span{display:block; height:4px; width:40%; background:#0394f1;}
.app_count em i{color:#f34d4d; font-style:normal}
.app_btn{width:72px; height:28px; border:1px solid #0394f1; border-radius:2px; position:absolute; right:8px; top:26px; line-height:28px; text-align:center; font-size:14px; color:#0394f1; z-index:11}
.app_btn:hover{background:#0394f1; color:#fff;}
.applist2{ border-top:1px solid #e6e6e6}
.applist2 .app_info{padding:9px 0 10px 70px; border-right:1px solid #e6e6e6}
.applist2 li:nth-child(2n+0) .app_info{border-right:none}
.app_count3{padding-top:1px}
.app_form{height:22px; padding:3px 0 0; line-height:22px;}
.app_form em{float:left; font-style:normal;}
.app_form span{float:right;}
.click_area{position:absolute; left:0; top:0; bottom:0; right:0; z-index:10}

.app_no{width:100%;}
.app_no img{width:120px; margin:80px auto 0; display:block}
.app_no p{padding:20px 0 0; text-align:center; color:#666666; font-size:16px}
.result_title{margin:12px 0 0; font-size:17px; color:#2b2b2b}

.applist_more{width:38%; height:30px; margin:15px auto 5px; background:#fff; border:1px solid #0394f1; color:#0394f1; line-height:31px; text-align:center; position:relative}
.applist_more a{display:block; width:100%; height:100%; color:#0394f1; font-size:17px}
.app_loading i{position:absolute; left:40%; width:80px;  font-size:17px; margin-left:-40px; font-style:normal; color:#666;}
.app_loading img{position:absolute; left:68%; width:30px; height:30px;}

.list{margin:12px 0 0; border-top:1px solid #e6e6e6}
.list li{ padding:0 12px 8px; border-bottom:1px solid #e6e6e6; position:relative}
.list li h4{padding:6px 0 4px; font-size:18px; color:#333f4d;}
.list li p{color:#a5a5a5; font-size:15px}
/*detail*/
.description{width:98%; margin:0 auto 0;}
.description h1{padding:16px 0 0; color:#333f4d; font-size:18px; text-align:center; font-style:normal; font-weight:normal;}
.desc_tips{text-align:center; padding:5px 0 6px; color:#bfbfbf; font-size:14px;}
.desc_cnt{font-size: 14px; color:#a5a5a5; line-height:22px; padding:0 8px 16px;word-break:break-all; word-wrap:break-word}
.desc_cnt img{max-width:100%; margin:10px auto; display:block}

/*弹框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{position:absolute;width: 90%;left:50%;top:50%; padding-bottom:8px; margin-left:-45%; background:#ffffff;  border-radius:4px; z-index: 99999; color:#545454}
.tip_hd{ height:30px; background:#f4f7f8; padding:10px 15px 7px; border-radius:4px 4px 0 0; line-height:30px}
.tip_hd h4{font-size:18px;}
.tip-mid{padding:25px 15px; border-bottom:1px solid #e6e6e6;}
.tip-mid p{color:#545454;}
.active_code{display:block; margin:15px 0 8px; padding:4px; border:1px solid #0394f1; color:#0394f1; text-align:center; word-wrap:break-all; }
.tips_btn{display:block; width:100%; margin:10px 0 4px; text-align:center; color:#0394f1; font-size:18px}
.clo{ position: absolute;right: 20px;top: 10px;}
.clo a{ display: block; width:30px; height:30px;background:url("../images/tip_clo.png") 0 0 no-repeat;background-size:100% auto;text-indent: -9999px;}