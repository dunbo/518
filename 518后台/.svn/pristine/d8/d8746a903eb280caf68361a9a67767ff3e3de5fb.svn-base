<include file="../../Public/header"  />
<link type="text/css" rel="stylesheet" href="/Public/css/tablelist.css" />
<script type="text/javascript" src='/Public/js/thickbox-compressed.js' ></script>
<link type="text/css" rel="stylesheet" href="/Public/css/thickbox.css" />
<script type="text/javascript" src="/Public/js/My97DatePicker/WdatePicker.js"></script>
<script type="text/javascript" src="/Public/js/zhiyoo/common.js"></script>
<style type="text/css">
#submid{height:31px;}
h2{margin:10px;}
.fh{margin:12px 10px 10px 10px;float:left;}
.fh a{margin:10px;font-size:14px;}
</style>
<body style="overflow-x: hidden; overflow-y: auto; ">
<div style="float:left;">
<h2>当前位置：ROM管理列表选项</h2>
</div>
<div class="fh">
<a href="/index.php/Zhiyoo/RomBrands/brandslist/">返回ROM品牌链接</a>
</div>
<form method='post' action="__URL__/edit_manage_rank/" onsubmit="return clickConfirm('确定保存优先级更改吗？');">
<div style="float:right;margin: 5px;">
	<input class="search_btn" style="display:none;float:left;" type="submit" id="submid" value="保存优先级">
	<a class="search_btn" style="display:inline-block;" href="javascript:priority('P_id');" id="priorityid">编辑优先级</a>
	
	<span style="margin:10px;padding:3px;background-color:white"><a href="__URL__/add_manage/" class="thickbox search_btn" style="display:inline-block;">添加选项</a></span>
</div>
<table border="0" cellpadding="0" cellspacing="0" class="table_list">
	<thead>
		<tr>
			<th width="65">
				<a href="__URL__/brandsmanage/field/rank/order/{$order}">优先级↓↑</a>
			</th>
			<th >展示名称</th>
			<th >操作</th>
		</tr>
	</thead>
	<volist name="mdata" id="val">
	<tr>
		<td id="P_id">
			<a>{$val.rank|default=''}</a>
			<input type="text" name="level[{$val.id}]" onkeyup="this.value=replaceNum(this.value);" size="4" id="level" style="display:none;" value="{$val.rank|default=''}" /> 
		</td>
		<td>{$val.mname|default=' '}</td>
		<td>
			<a href="__URL__/edit_manage/id/{$val.id}" class="thickbox" style="color:blue">编辑</a>|
			<a href="__URL__/update_manage/id/{$val.id}/status/-1" onclick="return del();" style="color:blue">删除</a>
		</td>
	</tr>
	</volist>
</table>
</form>
<script>
function del(){
   if(confirm("确定要删除数据吗？")) return true;
   else return false
}
</script>
</body>
</html>