/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto; background:#6eba43; font-size:15px; font-family:Microsoft Yahei,"微软雅黑"; line-height:20px; color:#1d1f21}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none; font-size: 14px; outline: none; color:#1d1f21}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{float:left;}
.fr{float:right;}
.main{position:relative; padding:0 0 12px;}
/*有通用的地方*/
.wrap{min-height:20px; padding:15px 0; margin-bottom:20px; position:relative}
.btns{display: block; width:102px;height: 33px; margin-left:auto; margin-right:auto; line-height: 33px; background:#ffe83c; border-radius: 6px; color:#104b17;cursor: pointer; text-align: center;font-size:16px;}
.banner{width: 100%; margin: 0 auto; position:relative;}
.banner img{ width: 100%;}
/*index*/
.notice{padding:3px 0 3px; background:#78be50; color:#ffffff; position:relative}
.notice-icon{width:20px; height:20px; left:5px; top:7px;  background: url(../images/icon_01.png) no-repeat 0 0; position:absolute; background-size:20px; height:20px}
.notice_list{height:28px; padding:0 15px 0 34px; line-height:28px; font-size:16px}
.line1 img{width:100%;}
/*拼图*/
.puzzle_wrap{width:315px; height:297px; margin:12px auto; background:url(../images/bg1.png) no-repeat; background-size:100% 100%;}
#puzzle{ width: 285px; height: 261px; margin:0 auto; background: url(../images/puzzle1.png) no-repeat; background-size:100% 100%; font-size: 0; position: relative;  overflow:hidden}
#puzzle_list li{ width: 95px; height: 87px; display: inline-block; background-image: url(../images/puzzle1.png); background-position:0 0; position:absolute}
#process_wrap{width:282px; height:10px; margin:18px auto 0; background:url(../images/process_01.png) no-repeat; background-size:100% 100%; position:relative}
#process{width:0px; height:8px; background:url(../images/bg2.png) no-repeat; background-size:100% 100%; position:absolute; left:1px; top:1px; border-radius:5px}
#process span{display:block; width:34px; height:20px; background:url(../images/icon_03.png) no-repeat; background-size:100% 100%; position:absolute; left:0px; top:-15px; font-size:13px; text-align:center; line-height:18px; color:#538e29; display:none; z-index:3}
#process_wrap em{width:40px; height:20px; position:absolute; right:0; top:-19px; color:#538e29; line-height:20px}
.lottery_btn{width:82px; height:33px; background:url(../images/btns_01.png) no-repeat; background-size:100% 100%; position:absolute; left:-5px; bottom:-19px; text-align:center; line-height:30px; color:#fff; font-size:16px; z-index:2}
.lottery_btn2{width:38px; height:36px; background:url(../images/btns_02.png) no-repeat; background-size:100% 100%; position:absolute; right:-5px; bottom:-19px; text-align:center; line-height:30px; color:#fff; font-size:16px; text-indent:-9999px; z-index:2}
.puzzle_wrap p{padding:50px 0 0;}
#begin_btn{width:315px;}
.puzzle_disabled{background-color:#cdcdcd;}

.lottery_btn3{width:230px; height:50px; margin:15px auto 0; background:#1fa1e9; border-radius:10px; display:block; line-height:50px; text-align:center; font-size:20px; color:#fff; }
.lottery_disabled{background:url(../images/btns_03.png) no-repeat; background-size:100% 100%;}
.txt1{color:#fff; font-size:17px; text-align: center;}
.txt1 span{color:#fddf33}

.soft_wrap{background:#87d9ff; padding:10px 6px 0;}
.soft-list{padding:5px 0 0; margin:10px 0 0;}
.soft-list li{ float: left; display: inline; width: 25%;margin:7px 0px;text-align: center;}
.soft-list li .soft-icon{text-align: center;}
.soft-list li .soft-icon img{ width: 60px;height: 60px; border:4px solid #fff; margin: 0 auto; border-radius:15px}
.soft-list li .soft-name{margin-top: 10px;height: 20px; color:#fff; line-height: 20px; overflow: hidden; white-space: nowrap; text-overflow: ellipsis;}
.soft-list li .soft-name a{ color: #fff;}
.soft-list li a.download{ display: block; width:70px;height: 29px; border-radius: 5px; line-height: 26px; margin: 13px auto 0 auto; background:#1fa1e9; color: #fff;cursor: pointer;}
.soft-list li a.open_btn{}
.soft-list li a.install_btn{}
.soft2 .soft-list li{ width:50%}
.soft2 .soft-list li .soft-icon img{ width: 146px; height:66px; margin: 0 auto; border:none}
.soft2 .soft-list li a.download{margin-top:6px}
.soft_bg{display: block; width:100%}
/*抽奖*/
.lottery{margin-bottom: 20px;}
.rotate {width: 300px; margin:0 auto;position: relative;}
.rotate-pointer {position: absolute;top: 50%;left: 50%;width: 46%;height: 46%;margin-top: -23%;margin-left: -23%;background: url(../images/pointer.png) no-repeat 0 0;background-size: 100% 100%; cursor: pointer;-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.pointer-disabled{background: url(../images/pointer_disabled.png) no-repeat 0 0;background-size: 100% 100%; z-index:11}
.lottery_txt{padding:18px 0 5px; text-align:center; color:#fff; font-size:18px}
.lottery_txt span{color:#fddf33}
.lottery_txt2{padding:0 0 10px}

/*我的奖品*/
.prize_list{ margin:20px 10px 0; padding:20px 0 30px; background:#9dc847; border:2px solid #538e29; border-radius:10px; color:#30600e; position:relative}
.prize_list_inner{width:90%; margin:0 auto;}
.prize-title{padding:10px 10px 0; font-size:16px; color:#124818}
.prize-text{padding:6px 10px 10px; line-height:28px; font-size: 15px;}
.prize-text li{padding:0 0 0 50px; min-height:30px; position:relative; line-height:30px; color:#fff}
.prize-text li span{position:absolute; left:0; top:0; width:50px; height:30px; color:#125c1b}
.prize_btns{display:block; width:62px; height:26px; background:#ffe83c; border-radius: 5px; position:absolute; right:5%; top:45px; color:#6eba43; font-size:16px; text-align:center; line-height:24px;}

/*detail 活动规则*/
.detail_wrap{ margin:0 0 20px; padding:15px 10px; position:relative; color:#30600e; font-size:16px}
.detail_list{margin:10px 0 0}
.detail_list h4{padding:5px 10px 6px; font-size: 18px; color:#0e2f12;}
.detail_list  p{padding:0 10px; line-height: 25px;}
.detail_ctn{ margin-bottom:10px;}
.detail_ctn li{position:relative; padding:0 0 0 10px; line-height:22px; color:#fff}
.detail_ctn span{width:14px; position:absolute; top:1px;}
.detail_ctn p{margin-left:14px;}
.detail_wrap .prize-text li{padding:0 0 0 70px;}
.prize-text li span{width:70px;}
.detail_wrap .line1{margin:12px 0 8px}
.tips_p img{display:block; width:136px; height:120px; margin:20px auto 0; padding:15px 0; overflow:hidden}
.tips_p p{text-align:center; color:#ffffff}
.tips_p a,.tip-box a{margin:20px auto; width:70%; height:36px; background:#ffe83c; border-radius: 10px; color:#104b17; line-height:34px}
.rule_btn{display:block; width:110px; height:36px;  margin:20px auto 0; border:1px solid #ffe83c; border-radius:10px; line-height:36px; text-align:center; color:#ffffff; font-size:16px; color:#ffe83c}
.btns2{padding:15px 12px 0;}
.btns2 a{display:inline-block; width:136px; height:35px; background:#ffd225; border-radius:10px; text-align:center; line-height:35px; color:#104b17}
.tip-box a.cancel_btn{margin-left:20px;width:126px;display:inline-block; background:#d4d4d4;}

.footer{padding:5px 0 20px; color:#fff; text-align:center}
/*提示框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{position:absolute; width:90%; height:250px; padding:0 0; background:#9dc847; border:2px solid #538e29; border-radius:10px; left:50%; top:50%; margin-left:-45%; z-index: 99999;}
.tip-mid{padding:15px 0 4px;}
.tip-con{width:80%; padding:8px 15px;margin:0px auto; background:url(../images/dialog_bg2.png) no-repeat; background-size:100% 100%;}
.tip-mid img{width:90px; margin:0 auto}
.tip-con p{line-height: 22px; text-align: center; color:#124818;}
.clo{ position: absolute; right: 10px; top: 10px;}
.clo a{ display: block; width:30px; height:30px;background:url("../images/tip_clo.png") 0 0 no-repeat; background-size:100% auto;text-indent: -9999px;}
.tip-box a{margin-top:0}
#tip-box3 a.btns{margin-top:20px}
#tip-box5,#tip-box2{height:235px}
#tip-box4{height:220px}

/*邮寄地址*/
.form1{width:90%; margin:12px auto 0; font-size:14px;}
.form-item{ margin:12px 0 0; }
.form-item label{ float: left;width:55px; padding:5px 0; color:#125c1b}
.form-item .textbox input{float:left; width:60%; padding:5px 12px; background:#69b53e; border:none; border-radius:3px;font-family: "Microsoft Yahei"; color:#fff;}
.prompt1{color:#124818; font-size:18px}
.prompt2{padding:5px 0 0;}
.prompt3{padding-top:6px; text-align:center; color:#ffe83c; font-size:14px}
