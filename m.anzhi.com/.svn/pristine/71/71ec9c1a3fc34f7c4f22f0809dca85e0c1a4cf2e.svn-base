/* CSS Document */
body,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0; font-size:14px; color:#333;}
fieldset,img {border: 0;}
a{text-decoration:none; color:#333;}
.clear{clear:both;}
ul,li{list-style:none;}
body{ font-family:Arial, Helvetica, sans-serif;}

#header{height:39px; padding:7px 10px 0 8px; background:url(../images/header_bg.png) repeat-x; position:relative;}
#logo{position:absolute; left:7px; top:5px; width:35px; height:35px; background:url(../images/logo_icon.png) no-repeat 0 0;}
#search_area{height:30px; margin:2px 0 0 40px; border-radius:5px; background:white; -webkit-box-shadow:0 2px 2px rgba(12, 2, 8, 0.1) inset; position:relative;}
.search_text{ margin-right:54px;}
.search_text input{width:100%; height:30px; padding:0 0 0 7px;  border:none; background:transparent; /*-webkit-box-shadow:0 -10px 5px #edeced inset;*/ line-height:31px; color:#A6A6A6; font-size:18px; }
.btn_out{width:54px; height:30px;  position:absolute; top:0; right:0;}
.search_line{position:absolute; top:4px; right:34px; width:1px; height:24px; background:#cccccc;}
.search_btn{ position:absolute; right:4px; top:3px; width:25px; height:25px;}

#nav{height:36px; background:url(../images/nav_bg.png) repeat-x;}
#nav ul li{float:left; width:20%; height:36px;  line-height:37px; text-align:center; color:#fff; font-size:16px; position:relative;}
#nav ul li a{float:left; width:100%; height:36px; color:#fff;}
#nav ul li span{ display:block; position:absolute; right:0; top:0; width:2px; height:36px; background:url(../images/line_bg1.png) no-repeat right top;}
#nav ul li.selected a{background:url(../images/nav_bg2.png) repeat-x;}
/*#nav ul li.selected span{ background:url(../images/line_bg2.png) no-repeat left top;}*/

.secnav{padding:6px 32px 7px; background:#f5f9f9; border-bottom:1px solid #fff;}
/*#secnav ul{ height:45px; border:1px solid #929ea9;}*/
.secnav ul li{float:left; width:50%; height:27px; background:-webkit-gradient(linear,left top,left bottom,from(#fafbfc),color-stop(70%, #edeff1),to(#d8dcdf)); line-height:27px; text-align:center;}
.secnav ul .secnav_in{ border:1px solid #929ea9; font-size:16px; font-family:"微软雅黑";}
.secnav ul li.secnav_01 .secnav_in{border-radius:5px 0px 0px 5px;}
.secnav ul li.secnav_02 .secnav_in{border-radius:0 5px 5px 0;}
.secnav ul .noborder .secnav_in{ border-left:none;}
.secnav ul li.selected{background:#e4e6e8; -webkit-box-shadow:1px 1px 4px #939faa inset; border-radius:5px 0 0 5px;}
.secnav ul li.selected2{background:#e4e6e8; -webkit-box-shadow:1px 1px 4px #939faa inset;}
.secnav ul li.selected3{background:#e4e6e8; -webkit-box-shadow:1px 1px 4px #939faa inset; border-radius:0 5px 5px 0;}
#secnav2{padding:6px 15px 7px; border-bottom:1px solid #c2ccd0;}
#secnav2 ul li{width:33%}
.secnav ul .secnav_in a{width:100%; height:27px; display:block;}


.listitems{height:56px; background:#f5f9f9; border-top:1px solid #fff; border-bottom:1px solid #c2ccd0; padding:6px 0px 6px 10px; position:relative;}
.items_img{float:left; width:48px; height:48px; padding:3px; margin-right:10px; border-radius:5px; border:1px solid #fff; background:#eaebeb;}
.items_img img{width:48px; height:48px;}
.items_txt{line-height:20px; color:#666; margin-top:-3px; display:inline-block; width:60%;}
.items_txt .sname{ padding-top:2px; font-size:16px; font-weight:bold; color:#1e395b; line-height:30px; white-space:nowrap; overflow:hidden;}
.score span{padding-left:5px; color:#a0b1b7;}
.download_box{width:42px; height:43px; background:url(../images/anzhi_iconbg.png) no-repeat -31px -93px; position:absolute; top:9px; right:10px;}
.download_box a{display:block; width:42px; height:20px; margin-top:23px; overflow:hidden; color:#339900; font-size:12px; text-align:center;}

/*图片广告位*/
.recommend-area{position:relative; height:85px; padding-top:8px; background:url(../images/recommed.png) repeat-x; -webkit-tap-highlight-color:rgba(0,0,0,0);}
.recommend{width:100%; height:85px; white-space:nowrap; overflow:hidden; text-align:center;}
.items{display:inline-block;}
.item{width:132px; height:76px; margin:0 5px; display:inline-block; }
.item img{width:132px; height:76px;}
.toLeft{width:72px; height:85px; position:absolute; left:0; top:0;}
.toLeftIcon{width:32px; height:32px; position:absolute; top:27px; left:15px; background:url(../images/anzhi_iconbg.png) no-repeat 0 -31px;}
.toRight{width:72px; height:85px; position:absolute; right:0; top:0;}
.toRightIcon{width:32px; height:32px; position:absolute; top:27px; right:15px; background:url(../images/anzhi_iconbg.png) no-repeat -32px -31px;}
#rec-nav{margin:5px 0; text-align:center;}
.cycle{display:inline-block; width:17px; height:3px; margin:0 5px; background:#B2B2B2;}
#rec-nav .selected{background:#44AF06;}
.swipe-trans {
    -webkit-transition: -webkit-transform .2s cubic-bezier(0,0,0.25,1);
}

/*详细页*/
.min_nav{height:30px; background:#68ba0e; padding:0 10px; line-height:30px; font-size:21px; color:#fff; position:relative;}
.min_nav a{color:#fff;}
.min_nav p{color:#fff; font-size:21px;}
.min_nav span{position:absolute; right:10px; top:0; height:30px; line-height:30px; font-size:21px;}
.soft_header{padding:10px 10px 0 10px;}
.soft_title{ padding-bottom:5px; font-size:22px; color:#323232;}
.soft_img{float:left; width:70px; height:74px; padding-right:12px; margin-right:10px; border-right:1px dashed #969797;}
.soft_img img{display:block; width:70px; height:70px; border-radius:5px;}
.soft_txt{line-height:23px; color:#666; margin-top:-3px; display:inline-block;}
.soft_btn a{float:left; width:75px; height:26px; background:#69bd0d; background: -webkit-gradient(linear, left top, left bottom, from(#59cd0a),to(#4aa408)); /*background: -moz-linear-gradient(-90deg,#59cd0a,#4aa408);*/ -webkit-box-shadow:0 1px 1px #c2c2c2,0 1px 1px #80dc36 inset; border:1px solid #6fc613; border-radius:4px; line-height:26px; text-align:center; color:#fff; font-size:16px; font-family:微软雅黑;}
.soft_btn a.soft_btn2{margin-left:5px; width:120px; background: -webkit-gradient(linear, left top, left bottom, from(#4aa408),to(#59cd0a)); background: -moz-linear-gradient(-90deg,#4aa408,#59cd0a);}
.soft_cnt{color:#666;}
.soft_cntbg{margin:8px 0 4px 0; background:#999; color:#fff; line-height:25px;}
.center{text-align:center;}
.soft_title_pad{padding-left:10px;}
.font16{font-size:16px; font-weight:bold;}
.soft_cntlist{padding:0 8px 0 10px; line-height:20px;}
.softcnt_img img{display:block; width:226px; height:308px; margin:4px auto;}
#soft_more{height:22px; padding:8px 0; font-size:16px; color:#66cc00; text-align:center; line-height:22px;}
#soft_more a{ color:#66cc00; text-decoration:underline;}
#mylove{width:99%; margin:0 auto;}
#mylove li{float:left; width:33%; height:100px; padding:11px 0 0 0; border-bottom:1px solid #d4dbde;}
#mylove li img{display:block; width:70px; height:70px; margin:0 auto;}
#mylove li span{display:block; height:24px; text-align:center; line-height:24px; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}

#version_old{padding-left:10px;}
#version_old li{line-height:22px; padding-top:4px;}
#version_old li a{text-decoration:underline;}

#footer{width:95%; margin:0 auto; padding:10px 0; text-align:center;}
#footer_top{height:28px; line-height:28px; color:#44a7e6;}
#footer_links{ color:#44a7e6;}
#footer_top a{ padding:0 9px;}
#footer a{color:#44a7e6;}
#footer img{display:none;}
#footer span.spec{padding:0 9px; color:#44a7e6;}


/*分页*/
.pagebar{ margin:0 auto; padding:15px 0 0 0; }
.pagebarl{float:left; width:56%;}
.pagebarl a,.pagebarl span{float:left; height:18px; padding:0 3px;}
.pagebar a.pre,.pagebar a.next{color:#44A7E6;}
.pagebarr{float:right; width:130px;}
.pagebarr span{float:left;}
.pagebarr input{float:left;}
.pagebarr .pagetext{width:40px; height:16px; background:#f8f8f8; border:1px solid #ccc;}
.pagebarr .pagebtn{width:27px; height:17px; border:none; }

/*subject_index*/
.subject_list{display:none;}
.subject_li{ position:relative;}
.subject_title{height:38px; background:#f5f9f9; border-top:1px solid #fff; border-bottom:1px solid #c2ccd0; padding:0 0px 0 10px; line-height:38px; color:#8c8c8c; position:relative;}
.subject_title a{color:#8c8c8c;}
.subject_items{ width:70%; background:url(../images/icon.png) no-repeat 0 15px; padding-left:15px; font-size:16px; display:block; height:38px; white-space:nowrap; overflow:hidden;}
.subject_items_spec{background:url(../images/icon.png) no-repeat 0 8px;}
.subject_items a{display:block; width:100%;}
.subject_title2{height:28px; line-height:28px;}
.subject_btn{width:33px; height:30px; background:url(../images/anzhi_iconbg.png) no-repeat -33px -63px; position:absolute; top:4px; right:15px;}
.subject_btn2{width:33px; height:30px; background:url(../images/anzhi_iconbg.png) no-repeat 0 -63px; position:absolute; top:4px; right:15px;}
.subject_more{background:#f5f9f9; padding:10px 0; border-top:1px solid #fff; border-bottom:1px solid #c2ccd0;}
.subject_more a{ display:block; width:150px; height:30px; margin:0 auto; /*background: -webkit-gradient(linear, left top, left bottom, from(#fafbfc),to(#dadee1));*/background: -webkit-gradient(linear, left top, left bottom, from(#fafbfc),to(#dadee1)) ;background: -moz-linear-gradient(-90deg,#fafbfc,#dadee1); border-radius:5px; border:1px solid #939ea9; line-height:30px; text-align:center; font-size:18px; color:#7c7c7c;}

/*意见*/
.sug_form{margin:0 3px 0 11px; padding-top:3px;}
.sug_form p{ font-size:16px; color:#676767; line-height:130%; text-align:left; }
.sug_form p.alert{ color:#ff0000;}
#message{ height: 45px; padding:4px; width:95%; border:1px solid #666666;}
.sug_form p.ts{padding:10px 0;}
.inp{width:95%; padding:4px; border:1px solid #666;}
.sug_form form{ text-align:left;}
.sb{ width:70px;  border: 1px solid #c2c2c2;border-radius: 3px 3px 3px 3px; margin:10px 0; height:25px; background-color:#f3f3f3; display:block;}

#banben{height:30px; background:#fef6bb; margin-bottom:1px; line-height:30px; text-align:center;}
#banben p{font-size:18px; color:#0071c0;}
#banben p a{color:#0071c0;}

.yanzheng{margin:20px 3px 20px 11px;}
.yanzheng p{ font-size:16px; color:#676767; line-height:130%; text-align:left; }
.yanzheng p.ts{padding:10px 0;}
.yanzheng .inp{padding:8px 4px;}
.yanzheng_img{margin:18px 0;}
.yanzheng_img img{display:block; width:140px; height:50px;}
.yanzheng .sb{margin:18px 0;}
.yanzheng p.alert{ color:#ff0000;}

/*搜索无结果*/
#search_reslut{width:100%;  padding:15px 10px 30px; font-family:"微软雅黑"; font-size:14px; line-height:22px;}
#search_reslut p{margin:5px 0;}
#search_reslut p span{color:#66CC00; font-weight:bold;}
