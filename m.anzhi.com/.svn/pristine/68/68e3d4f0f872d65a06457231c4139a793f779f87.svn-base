/* CSS Document */
/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{max-width:640px;min-width:320px; margin:0 auto;font-size:0.14rem; font-family:Microsoft Yahei,"微软雅黑"; background: #acf7fc; color: #4d4d4d;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#4d4d4d; font-size: .14rem;}
.a,input,button,div{-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{ float: left;}
.fr{ float: right}
.banner{width:100%; position: relative;}
.banner img{ width: 100%; display:block;}
.part1{padding:1.67rem 0 0.53rem; background:url(../images2/bg1.png) no-repeat; background-size:100% auto}
.btns{display: block; width:5.3rem; height:1.6rem; margin: 0 auto 0.55rem; text-indent: -9999rem;}
.btns_01{background:url(../images2/btn_01.png) no-repeat; background-size: 100% 100%;}
.btns_02{background:url(../images2/btn_02.png) no-repeat; background-size: 100% 100%;}
.btns_03{background:url(../images2/btn_03.png) no-repeat; background-size: 100% 100%;}
.btns_04{background:url(../images2/btn_04.png) no-repeat; background-size: 100% 100%;}
.wrap{margin: 0.35rem; background:#fff; border:2px solid #5cbbc1; border-radius: 10px;}
.tips{padding-top:0.36rem; padding-bottom:1rem; text-align: center;}
.tips p{color:#4d4d4d; font-size: 0.3rem; line-height: 0.3rem;}
.end_wrap img{width:2.33rem; margin: 0.8rem auto 0.2rem;}
.update_wrap p{padding:0 0 0.68rem}
.update_wrap img{width:1.7rem; margin: 0 auto;}
.update_btn{display: block; width:1.76rem; height:0.52rem; margin: 0.6rem auto 0; background:#5cbbc1; border-radius: 15px; text-align: center; line-height: 0.52rem; color:#fff; font-size: 0.26rem;}
.sim_wrap .tips{padding-bottom: 0.4rem;}
.sim_wrap p{line-height: 0.36rem;}
.sim_wrap img{display: block; width:2.23rem; margin: 0.5rem auto 0.3rem;}
.sim_wrap span{font-size: 0.3rem;}
.footer{height:0.7rem; background:#5bbbc1; line-height: 0.7rem; text-align: center; font-size: 0.24rem; color:#fff}