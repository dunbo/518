
function randomnum(smin, smax) {// 获取2个值之间的随机数
	var Range = smax - smin;
	var Rand = Math.random();
	return (smin + Math.round(Rand * Range));
}

function runzp(index) {
	var angle = 0;
	var message = "";
	var myreturn = new Object;
	var angle0 = 0;//
	var angle1 =60;
	var angle2 = 120;
	var angle3 = 180;
	var angle4 = 240;
	var angle5 = 300;
	switch (index) {
	case 0:// 一等奖
		angle = angle0;
		break;
	case 1:// 二等奖
		angle = angle1;
		break;
	case 2:// 三等奖
		angle = angle2;
		break;
	case 3:// 四等奖
		angle = angle3;
		break;
	case 4:// 五等奖
		angle = angle4;
		break;
	case 5:// 六等奖
		angle = angle5;
		break;
	}

	myreturn.angle = angle;
	myreturn.message = message;
	return myreturn;
}