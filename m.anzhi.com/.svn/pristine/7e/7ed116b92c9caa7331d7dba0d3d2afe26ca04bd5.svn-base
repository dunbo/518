//抽奖
function lottery_click(){
	<!--{if $out['is_login'] == 2}-->
		login('<!--{$out.login_url}-->','<!--{$out.version_code}-->');
		return false;
	<!--{/if}-->
	var pkg_str = open_pkg_arr.join(";");	
	if(parseInt(lottery_num) <= 0 ){
		$('#lottery_but').attr('class', 'btns fl disabled');
		pop_tips("","剩余抽奖次数不足！","",1);			
		return false;	
	}
	$('#lottery_but').attr('onclick', '');
	var static_url = "<!--{$out.static_url}-->";
	$.ajax({
		url: '/lottery/feb_sign/lottery.php',
		data: 'sid='+sid+'&aid='+aid+'&pkg='+pkg_str,
		type: 'post',
		dataType: 'json',				
		success: function(data){
			if(data.code == 1){
				$('.firecracker_02').addClass('fireMove');
				$('.firecracker_03').addClass('fireMove2');
				is_user_winning =1;
				lottery_num = lottery_num-1;
				$('#lottery_num').html(lottery_num);
				if(data.pid > 0){
					setTimeout(
						function(){		
							var str = data.prizename;
							var img_url = static_url+"/feb_sign/images/lottery_"+data.pid+".png"
							$("#lottery_img").attr('src', img_url);
							pop_tips("【恭喜中奖啦！】",str,"4",1);						
						},2000
					);	
				}else if(data.pid == -1){
					pop_tips(data.title,data.msg,"",1);		
				}else{
					setTimeout(
						function(){		
							pop_tips("","","3",1);	
						},2000
					);	
				}				
		
				if(lottery_num > 0){
					setTimeout(function(){
						$('#lottery_but').attr('class', 'btns fl');
						$('#lottery_but').attr('onclick', 'lottery_click();');
						$('.firecracker_02').removeClass('fireMove');
						$('.firecracker_03').removeClass('fireMove2');							
					}, 3000);					
				}else{
					setTimeout(function(){
						$('#lottery_but').attr('class', 'btns fl disabled');
						$('.firecracker_02').removeClass('fireMove');
						$('.firecracker_03').removeClass('fireMove2');							
					}, 3000);
				}				
			}else if(data.code == 2){
				location.href=data.url+"?sid="+sid+"&aid="+aid;
			}else{
				pop_tips("",data.msg,"",1);			
			}			
		}
	});
}
