{// 加载头部公共文件 }
<include file="../Public/header" />
<link rel="stylesheet" type="text/css" media="all"
	href="__PUBLIC__/js/calendar-win2k-cold-1.css" title="win2k-cold-1" />
<script type="text/javascript" src="__PUBLIC__/js/calendar_bak.js"></script>
<script type="text/javascript" src="__PUBLIC__/js/calendar-zh.js"></script>
<script type="text/javascript" src="__PUBLIC__/js/calendar-setup.js"></script>
<script type="text/javascript" src="__PUBLIC__/js/result.js"></script>
<script type="text/javascript" src="__PUBLIC__/js/action.js"></script>
<style type="text/css">
<!--
body,td,th {
	font-size: 12px;
}
.tb{ border-top:1px solid #666666;border-left:1px solid #666666;}
.tb th,.tb td{border-right:1px solid #666666; border-bottom:1px solid #666666;}
-->
</style>
<div class="main" >
<div class="content">
<center>
<form id="upload" method='post' action="__URL__/softReplaceLog" >
<table width="935" height="89" border="0" cellpadding="0" cellspacing="0" class="tb">
  <tr>
    <td colspan="9">操作日志管理：</td>
  </tr>
  <tr>
    <td width="99">管理员：</td>
    <td width="166">
    	<select name="admin_user" id="admin_user">
    	<option value="0" selected>无</option>
    	<volist name="admin_users" id='uservo'>
           <option value="{$uservo.admin_user_id}" <if condition="$uservo.admin_user_id eq $selected_user">selected</if>>{$uservo.admin_user_name}</option> 
        </volist>
        </select> 
    </td>
    <td width="125">起始时间:</td>
    <td width="150">
		<input id="fromdate" name="fromdate"
			style="cursor: hand; width: 100px; border: none 0px black;"
			value="{$from_value}" size="15" type="text"><img
			src="__PUBLIC__/js/calendar.gif"
			onclick="return showCalendar('fromdate', 'y-m-d');"
			style="margin: 1px; cursor: hand;" width="16px" align="absmiddle"
			height="15px">
	</td>
    <td width="125">结束时间：</td>
    <td width="150">
		<input id="todate" name="todate"
			style="border: 0px none black; width: 100px;" value="{$to_value}"
			size="15" type="text"><img src="__PUBLIC__/js/calendar.gif"
			onclick="return showCalendar('todate', 'y-m-d');"
			style="margin: 1px;" width="16px" align="absmiddle" height="15px">
	</td>
	  <td width="125">搜索id：</td>
  <td width="125"><input name='id' value='{$id}'></td>
    <td width="150" rowspan="2"><input type="submit" name="button" id="button" value="搜索" /></td>
  </tr>
</table>
</form>
<br />
<table width="100%" border="0" cellpadding="0" cellspacing="0" class="tb">
  <tr>
    <th>ID</th>
    <th>操作人员</th>
    <th>应用名称</th>
    <th>开发者</th>
    <th>替换时间</th>
    <th>操作</th>
  </tr>
  <tr>
  <volist name='display_ary' id='vo'>
    <tr>
    <td>{$vo.admin_log_id|default='&nbsp;'}</td>
    <td>{$vo.user|default='&nbsp;'}</td>
    <td><a target="_blank" href="http://www.goapk.com/intro.php?softid={$vo.softid}&auth=1">{$vo.softname}</a></td>
    <td>{$vo.dev_name|default='&nbsp;'}</td>
    <td>{$vo.logtime|date='Y-m-d H:i:s',###|default='&nbsp;'}</td>
    <td>{$vo.actionexp|default='&nbsp;'}</td>
  </tr>
  </volist>
</table>
{$page}
</center>
</div>
</div>
<!-- 主页面结束 -->
{// 加载尾部公共文件 }
 
 