/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto; background:#2bb1eb; font-size:15px; font-family:Microsoft Yahei,"微软雅黑"; line-height:20px; color:#1d1f21}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none; font-size: 15px; outline: none; color:#1d1f21}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{float:left;}
.fr{float:right;}
.main{ background:url(../images/main_bg.png) no-repeat; background-size:100% 100%}
.main2{padding-top:70px; background:url(../images/main_bg2.png) no-repeat; background-size:100% 100%}
.main_inner{padding:0 10px 80px; background:url(../images/bg_bom.png) no-repeat left bottom; background-size:100% auto}
.main_inner2{padding:0 10px 80px; background:url(../images/bg_bom2.png) no-repeat left bottom; background-size:100% auto}
/*有通用的地方*/
.btns{display: block; width:87px;height: 26px; margin-left:auto; margin-right:auto; line-height: 26px; background:url(../images/btns_01.png) no-repeat; background-size:100%100%; color:#fff;cursor: pointer; text-align: center; font-weight:bold; font-size:16px; text-shadow:-1px -1px 1px rgba(0,0,0,.5)}
.banner{width: 100%; margin: 0 auto; position:relative;}
.banner img{ width: 100%;}
.txt_ac img{position:absolute; bottom:0; left:0; animation:1.5s txt_ac infinite alternate; -webkit-animation:1.5s txt_ac infinite alternate}
/*index*/
/*活动规则*/
.rule{background:#006dcd; padding:10px; border-radius:8px; color:#ffffff; position:relative}
.titles{width:82px; height:28px; margin:3px 0; background:url(../images/btns_01.png) no-repeat; background-size:100% 100%; text-align:center; line-height:28px; color:#1277c7; font-size:16px; text-shadow:0 1px 0 rgba(0,58,104,.75)}
.rule p{padding-left:8px; line-height: 24px;}
.rule a{float:right; width:90px; height:28px; border:1px solid #eeff3e; border-radius:15px; color:#e1ff15; text-align:center; line-height:28px; position:relative; z-index:1000}
.rule .bg1{position:absolute; right:10%; top:-12px; width:214px; height:172px;}
.rule .bg2{position:absolute; left:-1%; bottom:-12px; width:100%;}

.login_wrap{background:#0747a8; margin-top:14px; padding: 5px 10px; border-radius:8px; color:#ffffff; position:relative}
.logined{padding:12px 10px; position:relative}
.logined_r{position:absolute; right:0; top:32%; margin-top:-19px}
.logined_l p{padding:3px 0 5px;}
.login{padding-top:5px; padding-bottom:20px}
.logined .btns{width:106px; height:38px; background: url(../images/btns_02.png) no-repeat; background-size:100% 100%; line-height:29px}

.notice_wrap{background:#006dcd; margin-top:14px; padding:5px 5%; border-radius:15px; color:#fff;}
.notice_list{height:28px; line-height:28px; text-shadow:0 2px 4px rgba(0,0,0,.4)}

/*转盘抽奖*/
.wrap2{background:#0a7fd4; margin-top:14px; padding:10px; border-radius:8px; color:#ffffff; position:relative}
.bg3{position:absolute; right:-2%; top:-12px; width:103%;}
.bg4{position:absolute; right:0; bottom:-12px; width:101%; z-index:2}
.bg5{position:absolute; right:10%; bottom:0; width:50%;}
.lottery_info{ padding:0 0 12px;}
.lottery_info p{float:left; padding:5px 0 0 12px; font-size:15px; color:#d0f0ff}
.lottery_info a{width:100px; height:34px; margin-right:8px; display:inline; background:url(../images/btns_03.png) no-repeat; background-size:100% 100%; color:#fff; line-height:34px; position:absolute; right:0; top:10px; z-index:2}
.rotate_tips{padding:0 0 8px; text-align:center;}
.rotate {width: 300px; margin:0 auto;position: relative;}
.rotate-pointer {position: absolute;top: 50%;left: 50%;width: 46%;height: 46%;margin-top: -23%;margin-left: -23%;background: url(../images/pointer.png) no-repeat 0 0;background-size: 100% 100%; cursor: pointer;-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.pointer-disabled{background: url(../images/pointer_disabled.png) no-repeat 0 0;background-size: 100% 100%; z-index:11}
.rotate_list{width:60px; height:78px; position:absolute; left:0; top:0; left:50%; top:50%; z-index:10}
.rotate-con{position:relative; z-index:2}
.rotate i img{display:block; width:90%; position:absolute; left:50%; margin-left:-45%; bottom:-10px;}

.btn_change{position:absolute; right:0; top:0; width:100px; height:34px; line-height:34px; margin: 0 auto 10px auto; background:url(../images/btns_03.png) no-repeat; background-size:100% 100%; text-align:center; color:#fff; font-weight:bold}
.soft_wrap{padding:15px 0; margin-top:10px; position:relative; z-index:3}
.soft_title{position:relative}
.soft_title h3{width:154px; height:32px; background:url(../images/title_01.png) no-repeat; background-size:100% 100%; text-indent:-9999px}
.soft-list{padding:5px 0 0; margin:10px 0 0;}
.soft-list li{ float: left; display: inline; width: 33.3%;margin:7px 0px;text-align: center;}
.soft-list li .soft-icon{text-align: center;}
.soft-list li .soft-icon img{ width: 60px;height: 60px; margin: 0 auto; border-radius:8px}
.soft-list li .soft-name{margin-top: 10px;height: 20px; color:#fff; line-height: 20px; overflow: hidden; white-space:nowrap; text-overflow:ellipsis;}
.soft-list li .soft-name a{ color: #fff;}
.soft-list li a.downlode{ display: block; width:63px;height: 24px; line-height: 24px; margin: 13px auto 0 auto; background:#f68506; border:1px solid #fff000; border-radius:15px; color: #fff;cursor: pointer; font-weight:bold}

.share_btn{display:block; width:156px; height:54px; background:url(../images/share_btn.png) no-repeat; background-size:100% 100%; margin:30px auto 0; text-indent:-99999px}


/*我的奖品*/
.my_prize{padding:15px 20px 8%; margin:0 10px; position:relative; background-image: -webkit-linear-gradient(top, rgba(0, 109, 205, 1) 0%, rgba(0, 129, 242, 1) 100%); background-image: -moz-linear-gradient(top, rgba(0, 109, 205, 1) 0%, rgba(0, 129, 242, 1) 100%); background-image: -o-linear-gradient(top, rgba(0, 109, 205, 1) 0%, rgba(0, 129, 242, 1) 100%); background-image: -linear-gradient(top, rgba(0, 109, 205, 1) 0%, rgba(0, 129, 242, 1) 100%); border-radius:15px; color:#fff; line-height:26px; font-size:16px}
.my_prize_inner h3{color:#fff; line-height:40px; position:relative; text-align:center; font-size:18px}

.my_prize_list2{padding:5px 0;}
.my_prize_list2 li{ position:relative;}
.my_prize_list2 p{ padding:6px 0;}
.my_prize_tips{font-size:15px; color:#ffa200}
.split{height:56px; background:url(../images/line_bg.png) repeat-x; background-size:contain;  margin: 35px 0px 15px; overflow:hidden;}
.my_prize .btns{width:156px; height:54px; margin:20px auto; background:url(../images/btns_04.png) no-repeat; background-size:100% 100%; text-indent:-99999px}
.my_prize2{font-size:18px; line-height:30px}
.my_prize2 .my_prize_inner h3{padding:8px 0 15px; font-size:24px}
.my_prize .btns2{width:190px; margin:0 auto}
.my_prize .btns2 .btns{float:left; width:87px; height:26px; margin:0 4px; line-height:26px; display:inline-block}
.my_prize .btns2 .btns_sub{background:url(../images/btns_01.png) no-repeat; background-size:100% 100%}
.info{width:80%; margin:0 auto; padding-top:30px}
.info li{padding:5px 0;}
/*rule*/
.rule_more{padding:5% 3% 6% 5%;  color:#fff; line-height:24px}
.rule_more h3{margin-bottom:8px; color:#fff; font-weight:bold; font-weight:bold; text-align:left}

/*结束页*/
.awards_wrap{padding:12px 15px 60px;}
.awards_wrap .titles{margin-top:-18px; background:none;font-size:18px; font-weight:bold; color:#ffffff}
.awards{padding:5% 20px 4%; position:relative; color:#ffffff;}
.awards_list{line-height: 26px; }
.slide_up{margin-top:20px; overflow:hidden;height:156px;position: relative;}
.awards_wrap .btns{position:absolute; right:25px; top:20px; background:url(../images/btns_02.png) no-repeat; background-size:100% 100%; font-size:15px}
.awards_wrap p{padding-top:20px; color:#eeff3d; text-align:center}

.footer{width:100%; height:50px; text-align:center; position:relative; color:#fff; line-height:40px; color:#9363bd}
.bold{font-weight:bold;}
.red{color:#ab0500}
.prompt_01{color:#52f4f6}
.prompt_02{padding:7px 0 0 90px; color:#ff0000}
.prompt_03{color:#001e33}

/*提示框*/
#body-bg{position:absolute; top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-webkit-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{ position:absolute;width: 90%; min-height:160px; background-image: -webkit-linear-gradient(top, rgba(0, 109, 205, 1) 0%, rgba(0, 129, 242, 1) 100%); background-image: -moz-linear-gradient(top, rgba(0, 109, 205, 1) 0%, rgba(0, 129, 242, 1) 100%); background-image: -o-linear-gradient(top, rgba(0, 109, 205, 1) 0%, rgba(0, 129, 242, 1) 100%); background-image: -linear-gradient(top, rgba(0, 109, 205, 1) 0%, rgba(0, 129, 242, 1) 100%); border-radius:15px; left:50%;top:50%; margin-left:-45%; z-index: 99999; font-size:15px; color:#fff}
.tip-con{ margin: 15px;padding:25px 6% 25px;}
.tip-con p{font-size:20px; font-weight:bold; line-height: 30px; text-align:center;word-wrap:break-word;word-break:break-all;}
.tip-con p.tip_h{font-weight:bold; font-size:18px; line-height:34px}
.tip-box .btns{width:156px; height:54px; margin:30px auto; background:url(../images/btns_04.png) no-repeat; background-size:100% 100%; text-indent:-99999px}

.yellow{color:#ffd500;}
.prompt_05{padding-top:10px; color:#4c0000}

@keyframes txt_ac{
	0%{transform:translate(0,-15px)}
	100%{transform:translate(0,0)}
}
@-webkit-keyframes txt_ac{
	0%{-webkit-transform:translate(0,-15px)}
	100%{-webkit-transform:translate(0,0)}
}
