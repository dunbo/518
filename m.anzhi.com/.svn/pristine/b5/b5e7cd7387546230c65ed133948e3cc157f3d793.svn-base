/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto; background:#00b26d; font-size:16px; font-family:Microsoft Yahei,"微软雅黑"; line-height:20px; color:#1d1f21}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none; font-size: 16px; outline: none; color:#1d1f21}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{float:left;}
.fr{float:right;}
.main{position:relative;}
/*有通用的地方*/
.wrap{min-height:20px; padding:15px 0; margin-bottom:20px; position:relative}
.btns{display: block; width:102px;height: 33px; margin-left:auto; margin-right:auto; line-height: 38px; background:url(../images/btns_01.png) no-repeat; background-size:100%100%; color:#fd538d;cursor: pointer; text-align: center; font-weight:bold; font-size:16px;}
.banner{width: 100%; margin: 0 auto; position:relative;}
.banner img{ width: 100%;}
/*index*/
.notice{padding:5px 0; background:#fff; color:#ff0000; position:relative}
.notice-icon{width:26px; height:27px; left:8px; top:7px;  background: url(../images/icon_01.png) no-repeat 0 0; position:absolute; background-size:100% 100%;}
.notice_list{height:28px; padding:0 30px 0 44px; line-height:28px; font-size:16px}
.notice_list span em{padding:0 2px;}
.line1{width:100%; height:4px; margin-bottom:20px; background:url(../images/icon_02.png) repeat-x; background-size:contain}
.icon_04{width:42%; margin:0 auto}
.icon_04 img{display:block; width:100%}
/*拼图*/
.puzzle_wrap{width:315px; height:auto; margin:0 auto;}
#puzzle{ width: 285px; height: 285px; margin:0 auto; background: url(../images/puzzle1.png) no-repeat; background-size:100% 100%; position: relative;  overflow:hidden}
#puzzle_list li{ width: 95px; height: 95px; display: inline-block; background-image: url(../images/puzzle1.png); background-position:0 0; position:absolute}
#process_wrap{width:300px; height:16px; margin:20px auto 0; background:#fff; border:1px solid #007346; border-radius: 10px; background-clip: padding-box; position:relative}
#process{/*width:120px;*/ height:16px; background:#fccd32; position:absolute; left:0; top:0; border-radius:9px}
#process span{display:block; width:34px; height:20px; background:url(../images/icon_03.png) no-repeat; background-size:100% 100%; position:absolute; left:0px; top:-15px; font-size:13px; text-align:center; line-height:18px; color:#f14d05; display:none; z-index:3}
#process_wrap em{width:40px; height:20px; position:absolute; right:0; top:-19px; color:#f14d05; line-height:20px}
.lottery_btn{width:82px; height:33px; background:url(../images/btns_01.png) no-repeat; background-size:100% 100%; position:absolute; left:-5px; bottom:-19px; text-align:center; line-height:30px; color:#fff; font-size:16px; z-index:2}
.lottery_btn2{width:38px; height:36px; background:url(../images/btns_02.png) no-repeat; background-size:100% 100%; position:absolute; right:-5px; bottom:-19px; text-align:center; line-height:30px; color:#fff; font-size:16px; text-indent:-9999px; z-index:2}
.puzzle_wrap p{padding:50px 0 0;}
.btns2{padding:20px 22px 0;}
.btns2 a{display:inline-block; width:126px; height:35px; background:#fff; border-radius:15px; text-align:center; line-height:35px; color:#f00}
.tip-box a.cancel_btn{margin-left:20px;width:126px;display:inline-block; background:#fff;}
/*转盘抽奖*/
.lottery_wrap{margin-bottom:0; background: url(../images/soft_bg4.png) no-repeat; background-size:100% auto}
.lottery_btn3{width:244px; height:46px; margin:20px auto 0; background:#fff232; border-radius: 20px; display:block; line-height:45px; text-align:center; font-size:18px; color:#f00; font-weight:600}
.lottery_disabled{width:244px; height:46px; margin:20px auto 0; background:#cdcdcd; border-radius: 20px; display:block; line-height:45px; text-align:center; font-size:18px; color:#929292; font-weight:600}

.txt2{padding:45px 0 15px; text-align: center; color:#fff}
.txt2 span{color:#fddf33}

.lottery_info img{width:58%; margin:15px auto 10px}
.lottery_wrap a{margin:15px auto; color:#ffffff}
.rotate_wrap{width:320px; margin: 0 auto; padding:3% 0;background:url(../images/lottery_bg.png) no-repeat center center; background-size:100% 100%}
.rotate_tips{padding:0 0 8px; text-align:center;}
.rotate {width: 280px; margin:0 auto;position: relative;}
.rotate-pointer {position: absolute;top: 50%;left: 50%;width: 36%;height: 36%;margin-top: -18%;margin-left: -18%;background: url(../images/pointer.png) no-repeat 0 0;background-size: 100% 100%; cursor: pointer;-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.pointer-disabled{background: url(../images/pointer_disabled.png) no-repeat 0 0;background-size: 100% 100%; z-index:11}
.rotate_list{width:60px; height:78px; position:absolute; left:0; top:0; left:50%; top:50%; z-index:10}

.soft_wrap{}
.txt1{padding-top:16%; padding-bottom:0; background:url(../images/soft_bg1.png) no-repeat; background-size: 100% auto;text-align: center;}
.soft1{padding-top:8px; padding-bottom:60px; background:url(../images/soft_bg3.png) no-repeat; background-size: 100% 100%;}
.soft-list{padding:10px 0 0;}
.soft-list li{ float: left; display: inline; width: 25%;margin:7px 0px;text-align: center;}
.soft-list li .soft-icon{text-align: center;}
.soft-list li .soft-icon img{ width: 60px;height: 60px; border:4px solid #ffd225; margin: 0 auto; border-radius:20px}
.soft-list li .soft-name{margin-top: 10px;height: 20px; color:#fff; line-height: 20px; overflow: hidden;}
.soft-list li .soft-name a{ color: #fff;}
.soft-list li a.download{ display: block; width:80px;height: 30px; background:#fff; border-radius:8px; line-height: 26px; margin: 13px auto 0 auto; color: #ff0000; cursor: pointer; font-weight:bold; line-height: 30px;}
.soft2{background:url(../images/soft_bg2.png) no-repeat; background-size: 100% 100%;}
.soft2 .soft-list li{ width:50%}
.soft2 .soft-list li .soft-icon img{ width: 146px; height:66px; margin: 0 auto; border:none}
.soft2 .soft-list li a.download{width:112px; height:38px; margin-top:6px; line-height: 38px;}

/*我的奖品*/
.main2{margin: 0 20px 20px;}
.prize_list{ margin:20px 0 0; padding:20px 0 30px; background:#00b26d; border:4px solid #fff232; border-radius:10px; color:#fff; position:relative}
.prize_list_inner{width:90%; margin:0 auto;}
.prize-title{padding:10px 10px 0; font-size:17px; color:#fff232}
.prize-text{padding:6px 10px 10px; line-height:28px; font-size: 16px;}
.prize-text li{padding:0 0 0 50px; min-height:30px; position:relative; line-height:30px}
.prize-text li span{position:absolute; left:0; top:0; width:50px; height:30px; color:#fff838}
.prize_btns{display:block; width:62px; height:26px; border-radius: 10px; background:#fff232; position:absolute; right:5%; top:45px; color:#f00; font-size:16px; text-align:center; line-height:26px;}

/*detail 活动规则*/
.detail_wrap{ margin:0 0 20px; padding:0 10px 15px; position:relative; color:#fff; font-size:16px}
.detail_list{margin:10px 0 0}
.detail_list h4{padding:5px 10px 6px; font-size: 18px; font-weight: bold; color:#fff232;}
.detail_list  p{padding:0 10px; line-height: 25px;}
.detail_ctn{ margin-bottom:10px;}
.detail_ctn li{position:relative; padding:0 0 0 10px; line-height:22px;}
.detail_ctn span{width:14px; position:absolute; top:1px;}
.detail_ctn p{margin-left:14px;}
.detail_wrap .prize-text li{padding:0 0 0 70px;}
.prize-text li span{width:70px;}
.detail_wrap .line1{margin:12px 0 8px}
.tips_p img{display:block; width:136px; height:120px; margin:25px auto 0; padding:15px 0; overflow:hidden}
.tips_p p{text-align:center; color:#fff}
.tips_p a,.tip-box a{margin:20px auto; width:148px; height:40px; background:#fff232; border-radius: 20px; display:block; line-height:40px; text-align:center; font-size:18px; color:#f00; font-weight:600}
.rule_btn{display:block; width:100px; height:38px; border:1px solid #fff; border-radius: 10px; margin:40px auto 10px; line-height:38px; text-align:center; color:#ffffff; font-size:16px; font-weight:bold;}
.footer{ padding:10px 0 20px; color:#fff; text-align:center}
/*提示框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{position:absolute; width:90%; /*height:220px;*/ padding:20px 0; background:#00b26d; border:2px solid #fff232; background-clip: padding-box; border-radius:10px; left:50%; top:50%; margin-left:-45%; z-index: 99999;}
.tip-mid{padding:0 0 4px;}
.tip-con{width:80%; padding:8px 15px;margin:0px auto; /*background:url(../images/dialog_bg2.png) no-repeat;*/ background-size:100% 100%;}
.tip-mid img{width:128px; margin:-5px auto 0}
.tip-con p{line-height: 20px; text-align: center; color:#fff;}
.clo{ position: absolute; right: 10px; top: 10px;}
.clo a{ display: block; width:30px; height:30px;background:url("../images/tip_clo.png") 0 0 no-repeat;background-size:100% auto;text-indent: -9999px;}
.tip-box a{margin-top:0}
#tip-box3 a.btns{margin-top:10px}

/*.flower{position:absolute; left:-20px; bottom:-30px; width:127px; height:99px; background:url(../images/flower.png) no-repeat; background-size:100% 100%}*/

/*邮寄地址*/
.form1{width:90%; margin:12px auto 0; font-size:14px;}
.form-item{ margin:12px 0 0; }
.form-item label{ float: left;width:55px; padding:5px 0; color:#fff838}
.form-item .textbox input{float:left; width:65%; padding:5px 12px; background:#fff; border:1px solid #e3dfb7; border-radius:3px;font-family: "Microsoft Yahei"; color:#f00;}
.prompt1{color:#fff232; font-size:18px}
.prompt2{padding:5px 0 0;}
.prompt3{padding-top:6px; text-align:center; color:#fff838; font-size:14px}