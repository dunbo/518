			<!--{/if}-->
			<!--{if $out.forth_award}-->
            <dl class="awards-list">
                <dt>一等奖</dt>
                <div <!--{if $out.forth_award_count > 3}-->id="fristPrize"<!--{/if}--> style="overflow:hidden;height:75px;">
                    <div>
						<!--{foreach from=$out.forth_award item=vo}-->
                        <dd><em><!--{$vo.the_telphone}--></em><span><!--{$vo.prize}--></span></dd>
						<!--{/foreach}-->
                    </div>
                </div>
            </dl>
			<!--{/if}-->
			<!--{if $out.fivth_award}-->
