/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto;font-size:14px; font-family:Microsoft Yahei,"微软雅黑"; background:url("../images/body_bg.png") repeat-x 0 0 #b61016;background-size: auto 100%; color: #b50805;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#730b91; font-size: 14px; outline: none;}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}

.cnt{text-align: center;}

.banner{ position:relative;width: 100%; margin: 0 auto;}
.banner img{ width: 100%;}
/**/
.main{position:relative;z-index: 2}
.intro{line-height: 30px;}
.mod{position:relative;margin:0px 5% 10px 5%;padding:7%;background-color: #fbdf99;border-radius: 10px;}
.mod_t{position: absolute;top:-15px;width: 70%;left:50%;margin-left:-35%;height: 30px;border-radius: 50px;background-color: #fbdf99}
.mod_b{position: absolute;bottom:-15px;width: 80%;left:50%;margin-left:-40%;height: 30px;border-radius: 50px;background-color: #fbdf99}
.mod p{line-height: 22px;}
.ribbon.ptop{padding-top: 15px;}
.ribbon img{width: 100%;}
.sub_hd{margin-bottom: 15px;text-align: center;font-size: 18px;}
.corner{position: absolute;background: url("../images/corner.png") 0 0 no-repeat;width:45px;height:45px;background-size: 90px 90px; z-index: 5}
.corner.top_l{left: 5px;top:5px;background-position:0 0 }
.corner.top_r{right: 5px;top:5px;background-position:-45px 0px }
.corner.bot_l{left: 5px;bottom:5px;background-position:0px -45px}
.corner.bot_r{right: 5px;bottom:5px;background-position:-45px -45px}
/*提交*/
.btns{margin: 10px 0px 20px 0px}
.btns a{ display: block;width:98px; height:45px;line-height:35px;margin:0 auto;background:url("../images/btns.png") 0 0 no-repeat;background-size: 100px 100px;color: #fff;text-align: center;font-size: 16px;}
/*升级*/
.egg{padding: 15px 0px;}
.egg img{width: 60%;margin: 0 auto;}
.btn_upgrade a{display: block;width: 130px;height:35px;line-height:35px;margin: 0 auto;background-color: #b61016;border-radius: 15px;color: #fff102;text-align: center;font-size: 16px;}
.propmt{padding:5px 30px 0px 30px }
.propmt1 p{text-align: center}
.input_box{width:80%;margin:0 auto;background:url("../images/input_box.png") 0 0 no-repeat;background-size: 100% 100%;}
.input_box input{width:90%;padding:0px 5%;height:40px;border: 0;background:none;color: #000;font-family: "Microsoft Yahei"}
/*活动规则*/
.rule{}
.rule_hd{width:100%;position: absolute;top: 0%;left: 0px;}
.rule_hd img{width: 30%;margin: 0 auto;}
.prize_hd{width:100%;position: absolute;top: -20px;left: 0px;}
.prize_hd img{width: 50%;margin: 0 auto;}
/*奖品设置*/
.prize{}
.prize li{width:44%;float: left;display: inline;margin: 3%;}
.prize li img{width: 100%;}
/**/
.app{width: 84%;padding:4% 8%; margin: 0 auto;background-color: #ffeec2;border-radius: 5px;}
.app li{padding:15px 0px;clear: both;overflow: hidden;}
.app_icon{float: left;margin-left:-100%;}
.app_icon img{width: 55px;height:55px;}
.info_wrap{width:100%;float:left;}
.app_info{width: auto;margin-left:60px; margin-right:69px;}
.app_info p{line-height: 22px;margin-top: 4px;white-space: nowrap;overflow: hidden;text-overflow: ellipsis}
.btn_download{float: right;margin-top: 15px;margin-left:-59px;}
.btn_download a{display: block;width: 59px;height: 35px;line-height:33px;background: url("../images/btns.png") 0 -46px no-repeat;background-size: 100px 100px;color: #fff;text-align: center}

/*获奖名单*/
.ptb{padding: 30px 0px}
.awards_list{margin:20px 0px;padding:30px 0px;background-color:#ffc94e;border-radius:10px;line-height: 30px;}
.awards_list span{padding-right: 15px;}
.slide_up{overflow:hidden;height:300px;position: relative;text-align: center}
/*尾部*/
.footer{width: 100%;margin-top:10px;padding:5% 0px;background-color: #ae1116}
.footer p{text-align: center;color: #fff;font-size:14px; }
