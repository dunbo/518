<include file="../../Public/header"  />
<link type="text/css" rel="stylesheet" href="/Public/css/tablelist.css" />
<script type="text/javascript" src='/Public/js/thickbox-compressed.js' ></script>
<link type="text/css" rel="stylesheet" href="/Public/css/thickbox.css" />
<script type="text/javascript" src="/Public/js/My97DatePicker/WdatePicker.js"></script>
<script type="text/javascript" src="/Public/js/zhiyoo/common.js"></script>
<style type="text/css">
#submid{height:31px;}
h2{margin:10px;}
</style>
<body style="overflow-x: hidden; overflow-y: auto; ">
<div style="float:left;">
<h2>当前位置：状态 </h2>
</div>
<div style="float:right;margin: 5px;">
	
	<span style="margin:10px;padding:3px;background-color:white"><a href="__URL__/add/type/wstatus/" class="thickbox search_btn" style="display:inline-block;">添加</a></span>
</div>
<table border="0" cellpadding="0" cellspacing="0" class="table_list">
	<thead>
		<tr>
			<th >序号</th>
			<th >分类名称</th>
			<th >产品数量</th>
			<th >操作</th>
		</tr>
	</thead>
	<volist name="wstatus" id="val">
	<tr>
		<td>{$val.wstatus|default=' '}</td>
		<td>{$val.wsname|default=' '}</td>
		<td>{$val.count|default=0}</td>
		<td>
			<a href="__URL__/edit_name/type/wstatus/id/{$val.wstatus}/" class="thickbox" style="color:blue">编辑</a>|
			<a href="__URL__/del/type/wstatus/id/{$val.wstatus}/" onclick="return clickConfirm('确定要删除此条数据吗？');" style="color:blue">删除</a>
		</td>
	</tr>
	</volist>
</table>

<div class="navgition">
	{$page|default='&nbsp;'}
</div>
</body>
</html>