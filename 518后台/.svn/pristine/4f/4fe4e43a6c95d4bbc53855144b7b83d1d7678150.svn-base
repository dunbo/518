<link rel="stylesheet" href="/Public/css/jquery.autocomplete.css" type="text/css" media="screen" />
<script type="text/javascript" src="/Public/js/jquery.autocomplete.js"></script>

<style type="text/css">
    input:disabled{
    border: 1px solid #DDD;
    background-color: #F5F5F5;
    /*color:#ACA899;*/
}
</style>
<div class="main" >
<div class="content">
    <form name="fml" action="__URL__/saveUser"  method="post">
    <div style="float:left;margin-left: 80px">
        用户名：<input type="text" style="width:140px" name="name" id="name" value="{$user.name}" class="search_item_text"> </div>    
    <div style="float:left;margin-top: 10px;margin-left: 80px">
        密&nbsp;&nbsp;&nbsp;码：<input type="text" style="width:140px" name="password" id="password" value='{$user.password}' class="search_item_text"></div>
    <div style="float:left;margin-top: 10px;margin-left: 80px">
        公司名：<input type="text" style="width:140px" name="company" id="company" value='{$user.company}' class="search_item_text">
    </div>

    <div style="float:left;margin-top:10px;" id="confirm_game">
    <input type='hidden' name='id' value='{$user.id}'>
        <div style="float:left;margin-left:70px"><input type="submit" value="确定" class="search_btn" onclick="return check()"></div>
        <div style="float:left;margin-left:20px"><input type="button" value="取消" class="search_btn " onclick="tb_remove();"></div>
    </div>

</form>
</div>
</div>
<script type="text/javascript">
    $(document).ready(function() {
        
    })
    
    function check(){
        var name = $.trim($("#name").val());
        var password = $.trim($("#password").val());
        var company = $.trim($("#company").val());
        if(name == ''){
            alert('用户名不能为空');
            return false;
        }
        if(password == ''){
            alert('密码不能为空');
            return false;
        }
        if(company == ''){
            alert('公司名不能为空');
            return false;
        }
        window.fml.submit();
    }
</script>

