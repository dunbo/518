<script type="text/javascript">
    var flags={canMove:true};
    function showOpenBox(obj){
        setTimeout(function(){
            var bg_h=$(document).height(),
                    top_h= $(obj).height()/ 2-$(document).scrollTop();
            //$(window).resize(function(){
            //    bg_h=$(document).height(),
            //    top_h= $(obj).height()/ 2-$(document).scrollTop();
            //})
            $(obj).css("margin-top",-top_h+"px").show();
            $("#body-bg").css("height",bg_h+"px").show();
            flags.canMove=false;
        },300)
    }
    function cloBox(obj){
        $(obj).hide();
        $("#body-bg").hide();
        flags.canMove=true;		
    }
    $(function(){
        //弹框
        $("body").bind('touchmove', function (e) {
            if(!flags.canMove){
                e.preventDefault();
            }
        });
    })
	function myAward(is_end){
		$.ajax({                			
				url: '/lottery/flyback/flyback_userinfo.php',
				data: 'uid='+uid+'&type=get_winner',
				type: 'post',
				dataType: "json",
				success: function(data){
					if(data){
						if(data.pid <= 4){
							$("#tip-box5 .my_prize_list").html('<li>恭喜您在'+data.create_tm+'时候获得'+data.prizename+'</li>');
							getMyInfo(5,is_end);
							showOpenBox("#tip-box5");
						}else if(data.pid > 4&&data.pid <= 7){
							$("#tip-box6 .my_prize_title").html('【我的奖品】');
							$("#tip-box6 .my_prize_list").html(' <li>恭喜您获得'+data.prizename+'</li>');
							showOpenBox("#tip-box6");
						}else{
							$("#tip-box2 .tip_prompt").html('<p>恭喜您在'+data.create_tm+'时候获得'+data.prizename.softname+'礼包一份，</p><p>礼包码：'+data.prizename.gift_number+'</p>');
							showOpenBox("#tip-box2");
						}	
					}else{
						showOpenBox("#tip-box1");
					}
					
				}
		})
	}
	function myInfo(){
