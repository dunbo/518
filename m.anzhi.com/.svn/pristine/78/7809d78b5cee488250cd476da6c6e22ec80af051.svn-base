/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto;font-size:15px; font-family:Microsoft Yahei,"微软雅黑"; background: #c7322e; color: #000000;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#000; font-size: 14px; outline: none;}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.cl{overflow:hidden;}
.clear{clear:both;overflow:hidden;}

.banner{width: 100%; margin: 0 auto;}
.banner img{ width: 100%;}

.mod{margin:20px 5% 0; position:relative}
.mod_t{height:25px; background:url(../images/mod_t.png) no-repeat; background-size:100% 100%}
.mod_m{padding:0 22px; background:url(../images/mod_m.png) repeat-y; background-size:100% auto}
.mod_b{height:25px; background:url(../images/mod_b.png) no-repeat; background-size:100% 100%}
.decration_01{position:absolute; right:-5px; top:-22px; width:112px; height:69px; background:url(../images/decration_01.png) no-repeat; background-size:100% 100%}
.btns{display:block; width:110px; height:34px; margin:20px auto 0;  border:2px solid #740000; border-radius:18px; background:url(../images/btns_04.png) no-repeat; background-size:100% 100%; color:#00455e; text-align:center; line-height:30px; font-size:18px; font-weight:bold; color:#8c2700}
/*活动规则*/
.rule{  color:#fde1e1; font-size:14px;}
.rule p{line-height: 24px;}
.rule p.rule_title{ width:100px; height:22px; margin-top:6px; background:#a60401; border-radius:10px; font-weight:bold; color:#fde1e1; text-align:center; font-size:15px}
.rule a{float:right; display:inline; color:#530200; text-align:center;  text-decoration:underline}
/*公告*/
.notice{height:32px; margin:20px 5%; padding:0 20px; background:#540200; border-radius:12px; line-height:32px; color:#fde1e1; font-size:16px}
/**/
.main{width:100%; padding:0 0 12px; position:relative; background: url("../images/bg1.png") no-repeat; background-size: 100% 100%; overflow:hidden}

.prize_list{color:#a00b12}
.prize_list li{float:left; width:44%; height:228px; margin:1% 0; background:#fde1e1; border:4px solid #920805; background-clip: padding-box; border-radius:10px; position:relative}
.prize_list li.right{float:right}
.prize_list li h4{background:#fcb5b3; padding:6px 5px; border-radius:6px 6px 0 0; font-size:15px; color:#540200; white-space:nowrap; text-overflow:ellipsis; overflow:hidden; text-align:center}
.total_num{padding:6px 0 0 15px}
.prize_over{position:absolute; left:0; top:0; width:100%; height:100%; background:rgba(0,0,0,.7) url(../images/prize_over.png) no-repeat center center; background-size:102px 82px; border-radius:6px}
.prize_info{position:relative; padding:7px 10px 3px;}
.prize_info span{float:left; width:40%; margin:5px 0 0; font-size:12px}
.prize_info img{display:block; width:78px; height:45px; margin:2px auto 0;}
.prize_time{padding:3px 10px 0}
.prize_time p{padding:0 0 6px; font-size:16px}
.prize_time span{display:inline-block; padding:3px; background:url(../images/bg5.png)repeat-x; background-size:auto 100%; border-radius:2px; color:#fff}
.pagebars{width:308px; margin:0 auto; padding:10px 0 5px;}
.pagebars a{float:left; display:block; width:130px; height:38px; margin:0 10px; border:2px solid #740000; border-radius:15px; background:url(../images/btns_03.png) no-repeat; background-size:100% 100%; color:#00455e; text-align:center; line-height:35px; font-size:18px; font-weight:bold}
.pagebars a.next{background:url(../images/btns_04.png) no-repeat; background-size:100% 100%; color:#8c2700}

.btns_05{float:right; width:110px; height:32px; background:url(../images/btns_02.png) no-repeat; background-size:100% 100%; line-height:30px; text-align:center; color:#fff; font-size:15px; font-weight:bold; letter-spacing:1px}
.prize_shapes{width:85%; height:10px; background:rgba(0,0,0,.43); border:1px solid #fff; border-radius:4px; margin:6px auto 0; position:relative; font-size:12px}
.prize_shapes i{display:block; width:60%; height:10px; border-radius:4px; background:url(../images/bg6.png) no-repeat; background-size:100% 100%}
.prize_shapes span{float:left;}
.prize_shapes em{float:right; margin-top:5px; text-align:right}
.prize_info_btn a{display:block; width:75px; height:28px; border-radius:8px; background:url(../images/bg3.png) no-repeat; background-size:100% 100%; margin:-2px auto 0; text-indent:-9999px}
/*软件*/
.title{padding:10px 0 0 0; position:relative}
.title h3{color:#f4c100; font-weight:bold;}
.title p{padding:5px 0; color:#fff; font-size:16px}
.title2 h3{width:110px; height:30px; margin:0 auto 15px; background:#a60401; border-radius:20px; font-weight:bold; color:#fde1e1; text-align:center; font-size:18px; line-height:26px;}
.list_mod .title h3{width:106px; height:22px; background:url(../images/txt_01.png) no-repeat; background-size:100% 100%}
.prize_btn{position:absolute; right:0; top:8px; display:block;width:98px; height:34px; background:url(../images/btns_01.png) no-repeat; background-size:100% 100%; text-indent:-9999px}
.prize_btnh{background:url(../images/btns_01h.png) no-repeat; background-size:100% 100%;}
.btn_change{position:absolute; right:0; top:0; width:104px;height: 40px; line-height:34px; margin: 3px auto 10px auto; background:url(../images/soft_btn2.png) no-repeat; background-size:100% 100%; text-indent:-9999px}
#soft_mod{padding:15px 20px; background:url(../images/bg2.png) no-repeat; background-size:100% 100%}
.soft-list{padding:5px 0 0; margin:10px 0 0;}
.soft-list li{ float: left; display: inline; width: 25%;margin:7px 0px;text-align: center;}
.soft-list li .soft-icon{text-align: center;}
.soft-list li .soft-icon img{ width: 60px;height: 60px; margin: 0 auto; border:2px solid #ffb000; border-radius:8px}
.soft-list li .soft-name{margin-top: 10px;height: 20px; color:#fff; line-height: 20px; overflow: hidden;}
.soft-list li .soft-name a{ color: #fff;}
.soft-list li a.downlode{ display: block; width:63px;height: 32px; line-height: 27px; margin: 13px auto 0 auto; background:url(../images/soft_btn.png) no-repeat; background-size:100% 100%; color: #8c2700;cursor: pointer; font-weight:bold}
.prompt2{ text-align: center; font-size: 18px;margin-top: 35px; color:#8b133a}
.prompt4{margin-bottom: 5px; text-align: center; font-size: 18px;}

/*抽奖*/
.lottery_wrap{margin:30px 25px 0; padding:0 0 10px}
.prize_index{position:relative;}
.prize_index p{color:#fde1e1}
.prize_index p span{padding:0 1px; color:#000000; font-size:18px}
.prize_index a{display:block; position:absolute; right:0; top:22%; width:110px; height:36px;  border:2px solid #740000; border-radius:15px; background:url(../images/btns_04.png) no-repeat; background-size:100% 100%; color:#00455e; text-align:center; line-height:33px; font-size:18px; font-weight:bold; color:#8c2700}
.prize_index p.prize_tips{color:#ffea00}
/*转盘抽奖*/
.rotate {width:90%; margin:15px auto 0; position: relative; overflow}
.rotate-con{width:100%;}
.rotate img{width:100%;}
.rotate-pointer {position: absolute;top: 50%;left: 50%;width: 36%;height: 36%;margin-top: -18.6%;margin-left: -18.2%;background: url(../images/pointer.png) no-repeat 0 0;background-size: 100% 100%; cursor: pointer;-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.pointer-disabled{background: url(../images/pointer_disabled.png) no-repeat 0 0;background-size: 100% 100%; z-index:11}
.lottery_txt img{display:block; width:85%; margin:20px auto 0}
/*登录以后*/
.line_left{width:12px; height:66px; background:url(../images/line_01.png) no-repeat; background-size:100% 100%; position:absolute; left:50px; top:-25px; z-index:2}
.line_right{width:12px; height:66px; background:url(../images/line_01.png) no-repeat; background-size:100% 100%; position:absolute; right:50px; top:-25px; z-index:2}
.line_left2,.line_right2{height:120px; top:-91px; background:url(../images/line_02.png) no-repeat; background-size:100% 100%;}
.line_left2{left:50px;}
.line_right2{right:50px}

.logined{ margin:22px 5% 0; position:relative;min-height:35px; padding:18px 20px 12px 20px; border:2px solid rgba(84,2,0,.5); border-radius:15px}
.logined ul{ line-height: 26px; color: #fcdd4f;}
.logined ul li{float:left;}
.soft_icon{position: absolute;top:50%;right:10px;margin-top: -30px;}
.soft_icon img{width: 60px; height: 60px;}
.user_info{position: absolute;top:12px;right:15px;}
.user_info a{float:right; display: block;width: 94px;height: 28px;background:url(../images/btns_03.png) no-repeat; background-size:100% 100%; border: 2px solid #740000;
border-radius: 13px; line-height: 28px;text-align: center; color: #fff; font-size:16px; font-weight:bold;color:#00455e;}

/*我的奖品*/
.my_prize{padding:15px 0 8%; position:relative; color:#fde1e1}
.my_prize_list li{line-height:25px; font-size:16px; position:relative;}
.my_prize_list li a,.soft_title a,.prize_okr a{display:block; width:140px; height:32px; margin:5px auto 0; padding:0; background:url(../images/btns_03.png) no-repeat; background-size:100% 100%; background-clip: padding-box; line-height:33px; font-weight:bold; color:#fff; text-align:center;}
.my_prize_list li a{ width:80px; height:38px; margin-top:8px; background:url(../images/soft_btn.png) no-repeat; background-size:100% 100%; line-height:32px; letter-spacing:1px; font-size:16px; color:#8c2700}
.my_prize_list2{padding:5px 0;}
.my_prize_list2 li{ position:relative; font-size:16px}
.my_prize_list2 p{ padding:6px 0;}
/*联系信息*/
.form-items li{ margin-top: 30px; clear: both; overflow: hidden;}
.form-items li label{ float: left;width:90px;padding-top: 10px; color:#4c0000;}
.form-items li span{display:block;margin-left: 90px;}
.form-items li input{ width:90%;padding:10px 6px;border:none;color: #000;  font-family: Microsoft Yahei,"微软雅黑"; background-color:#fff; border-radius:2px}
.form_error{display:block; padding:8px 0 0 90px; color:red}
.btn_submit{padding-top:10px; padding-bottom: 30px;}
.btn_submit a{ display: block;margin:0 auto;width: 159px; height: 48px; line-height: 48px; text-align: center; background:url(../images/btns_02.png) no-repeat; background-size:100% 100%; font-size:16px; color:#fff; text-align:center; font-weight:bold }
.split{height:20px; background:url(../images/line_bg.png) no-repeat; background-size:100% 100%; margin: 20px 0px 5px; overflow:hidden;}
.rule_more{padding:25px 0 15px; line-height:23px; font-size:15px; letter-spacing:1.5px}
.rule_more li{position:relative; margin-top:6px; line-height:22px; color:#fde1e1}
.rule_more span{width:20px; position:absolute; top:1px;}
.rule_more p{margin-left:20px;}


/*获奖名单页*/
#main_end .mod{margin-left:0; margin-right:0}
.awards{padding:40px 5% 30px; position:relative;}
.awards .title{padding:0 0 5px; margin-bottom:10px; }
.awards .title h3{color:#fff}
.awards-list{line-height: 22px; color:#fff}
.awards_btns{position:absolute; right:0; top:0; display:block; width:110px; height:38px; border-radius:20px; background:url(../images/bg4.png) no-repeat; background-size:100% 100%; text-indent:-9999px}
.slide_up{overflow:hidden;height:110px;position: relative;}

/*提示框*/
#body-bg{position:absolute; top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-webkit-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{ position:absolute;width: 300px; height:296px; left:50%;top:45%; margin-left:-150px; z-index: 99999;background:url(../images/tips_bg1.png) no-repeat; background-size:100% 100%; font-size:15px; color:#4c0000}
.tip-con{ margin: 15px;padding: 155px 5% 75px;}
.tip-con h4{font-size:16px; font-weight:bold; text-align:center}
.tip-con p{ line-height: 20px; text-align:center;word-wrap:break-word;word-break:break-all;}
.tip-con p.tip_h{font-weight:bold; font-size:18px; line-height:34px}

.btns_false{position:absolute; left:50%; bottom:35px; margin-left:-55px;}
.yellow{color:#ffd500;}
.footer{ background:#7b0300; text-align:center; color:#fff;}
.footer p{padding:12px; line-height:22px}
.prompt_05{padding-top:10px; color:#4c0000}

.win_sure{margin:60px 0 0 40px; color:#4c0000; font-size:14px;}
.win_sure h4{padding:0 0 5px 0; font-size:16px; font-weight:bold}
.win_num{padding:6px 0;}
.win_num p{float:left}
.win_num_l{float:left; width:40%; height:26px; background:#fff; position:relative}
.win_num_l input{width:40%; padding:0 30%; height:26px; border:0}
.win_num_l span{display:block; width:20px; height:20px; position:absolute; top:3px; background:#d2d2d2; color:#919191; font-weight:bold; text-align:center; line-height:20px; cursor:pointer}
.win_num_l span.win_reduce{left:3px;}
.win_num_l span.win_add{right:3px;}

#win_sure_btns{width:218px; margin:5px auto}
#win_sure_btns a{float:left; display:block; width:84px; height:22px; margin:2px 10px;  line-height:20px; font-size:14px}
#win_sure_btns a.blue{background:url(../images/btns_03.png) no-repeat; background-size:100% 100%; color:#00455e}
#tip-box5 .tip-con{padding: 70px 5% 75px;}
#tip-box5 .tip-con p,#tip-box8 .tip-con p{text-align:left}
#tip-box6 .tip-con{padding-top:138px}
#tip-box6 .btns{bottom:28px}
#tip-box7 .tip-con{padding: 120px 5% 75px;}
#tip-box7 .tip-con h4{padding-bottom:20px}
#tip-box8 .tip-con{padding: 120px 3% 75px;}
#tip-box8 .tip-con h4{padding-bottom:14px}
#tip-box8 .btns{bottom:20px}