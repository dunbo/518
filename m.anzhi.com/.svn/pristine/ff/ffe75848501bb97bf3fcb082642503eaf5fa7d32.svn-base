/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto; background:#303030; font-size:15px; font-family:Microsoft Yahei,"微软雅黑"; line-height:20px; color:#1d1f21}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none; font-size: 15px; outline: none; color:#1d1f21}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{float:left;}
.fr{float:right;}
.banner{position: relative; width: 100%;}
.banner img{ width: 100%;}
.description{margin:5px 0 0; padding:20% 9% 15% 10%; background:url(../images/bg1.png) no-repeat; background-size:100% 100%}
.description ul li{float:left; width:20%}
.description ul li img{width:96%; margin:0 auto}
.description ul li span{display:block; width:94%; height:30px; margin:8px auto 0;}
.description ul li.desc_01 span{background:url(../images/img_06.png) no-repeat; background-size:100% 100%}
.description ul li.desc_02 span{background:url(../images/img_07.png) no-repeat; background-size:100% 100%}
.description ul li.desc_03 span{background:url(../images/img_08.png) no-repeat; background-size:100% 100%}
.description ul li.desc_04 span{background:url(../images/img_09.png) no-repeat; background-size:100% 100%}
.description ul li.desc_05 span{background:url(../images/img_09.png) no-repeat; background-size:100% 100%}
.description ul li span.geted{background:#777; text-align:center; line-height:30px; color:#c6c6c6}
.description ul li.desc_01 span.current{background:url(../images/img_31.png) no-repeat; background-size:100% 100%}
.description ul li.desc_02 span.current{background:url(../images/img_32.png) no-repeat; background-size:100% 100%}
.description ul li.desc_03 span.current{background:url(../images/img_33.png) no-repeat; background-size:100% 100%}
.description ul li.desc_04 span.current{background:url(../images/img_34.png) no-repeat; background-size:100% 100%}
.description ul li.desc_05 span.current{background:url(../images/img_34.png) no-repeat; background-size:100% 100%}
.txt_01{display:block; width:85%; margin:10px 0 10px}
.desc_btn{display:block; float:right; padding:2px 6px; border:1px solid #ff0002; border-radius:15px; color:#ff0002; font-size:14px}

.sign_in{margin:0 10px; position:relative}
.sign_in p {padding:10px 0; font-size:22px; color:#ff001c;}
.sign_in p span{padding:0 5px; color:#ffc809; font-size:30px;}
.sign_in p.end_txt{width:68%; height:34px; padding:5px 0; color:#ff001c; font-size:16px}
.login_btn{ position:absolute; right:0; top:2.5%; width:92px; height:28px; background:url(../images/btn_01.png) no-repeat; background-size:100% 100%; text-indent:-9999px}
.logined_info{position:absolute; right:0; top:2.5%; width:100px; height:28px; color:#afafaf; font-size:14px; line-height:28px}
.sign_in ul{width:100%; margin:20px 0; }
.sign_in ul li{float:left; width:20%;}
.sign_list{ padding:6px; background:#1f1f1f; border:1px solid #343434; position:relative}
.sign_list img{display:block; width:80%; margin:0 auto}
.sign_list span{display:block; width:80%; height:100%; position:absolute; left:50%; top:0; margin-left:-40%; opacity:0}
.sign_list span.current{animation:fadeIn .8s infinite; -webkit-animation:fadeIn .8s infinite}
