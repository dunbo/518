/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px;max-width: 640px; margin:0 auto;background:#f6f1db; font-size:15px; font-family:Microsoft Yahei,"微软雅黑"; line-height:25px; color:#000}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none; font-size: 14px; outline: none; color:#000 }
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both}
.banner img{ width: 100%;}
.wrap{min-height:120px; margin:20px 8px 0; padding:20px 6%; background:url(../images/bg1.png) no-repeat; background-size:100% 100%; color:#fff; position:relative}
.wrap_icon{width:22px; height:14px; background:url(../images/icon_01.png) no-repeat; background-size:100% 100%; position:absolute; top:10px}
.wrap_icon_l{left:25px;}

.titles{width:176px; height:42px; background:url(../images/bg3.png) no-repeat; background-size:100% 100%; margin:0 auto 10px; text-align:center; line-height:35px; color:#fff; font-size:20px; font-weight:bold;}
.titles h3{font-weight:bold; text-shadow:#a26b0a 1px 0 0,#a26b0a 0 1px 0,#a26b0a -1px 0 0,#a26b0a 0 -1px 0}
.phone_form{width:80%; margin:0 auto}
.phone_form p{text-align:center; font-size:16px}
.form_item{width:100%; margin:10px 0 0; position:relative}
.form_item span{display:block; margin:6px 0px 0px 0; line-height: 18px;}
.input_text{padding:11px 4% 11px 5%; width:96%; background:url(../images/bg4.png) no-repeat; background-size:100% 100%;  border:none; color:#333333;background-clip: padding-box; font-size:16px; font-family:微软雅黑}
.form_btn a{display:block; width:76px; height:44px; margin:12px auto 0; background:url(../images/btns_02.png) no-repeat; background-size:100% 100%;  text-align:center; line-height:39px; color:#fff; font-size: 16px; text-shadow:#a26b0a 1px 0 0,#a26b0a 0 1px 0,#a26b0a -1px 0 0,#a26b0a 0 -1px 0; font-weight: bold;}
/*软件*/
.soft_list{width: 96%;margin: 15px auto 0;}
.soft_list li{float: left;display:inline;width: 31.3%;margin: 0px 1%; padding:2% 0;}
.soft_list li a{display: block;width: 70%; padding:15%;}
.soft_list li p{text-align: center;color: #fff}
.soft_list li .soft_icon img{width: 72px; margin:0 auto}
.soft_list li .soft_name{margin-top: 5px;}
.soft_list li .soft_name,.soft_list li .soft_size{width:100%;height:22px;line-height:22px;text-overflow:ellipsis;white-space:nowrap;overflow: hidden}
.soft_list li a.soft_btn{display:block; width:63px; height:36px; padding:0; border:none; background: url(../images/btns_02.png) 0 0 no-repeat;background-size: 100% 100%;  margin:5px auto 0; text-align:center; line-height:32px; color:#fff; font-size: 15px; text-shadow:#a26b0a 1px 0 0,#a26b0a 0 1px 0,#a26b0a -1px 0 0,#a26b0a 0 -1px 0; font-weight: bold;}
/*活动奖品*/
.prize_list{width: 99%;margin: 15px auto 0;}
.prize_list li{float: left; display:inline; width:29.3%; margin: 0px 2% 4% 2%; }
.prize_list li img{width: 100%;}
 
.rule p span{font-size:17px}
/*获奖名单*/
.awards_list{width:90%; margin: 5% auto; line-height: 22px; color: #fff; text-align:center}
.slide_up{overflow:hidden;height:220px;position: relative;}

/*升级*/
.upgrade_tip{padding: 30px 0px 15px; text-align:center}
.expression img{display:block; width:107px; height:132px; margin:0 auto 15px}
.upgrade_btn{display:block; width:108px; height:44px; margin:15px auto 20px; background:url(../images/btns_03.png) no-repeat; background-size:100% 100%; text-indent:-9999px}

.footer img{width:100%; margin-top:10px;}
.footer p{padding:13px 0; background:#1b90d3; text-align:center; color:#fff}