//图片验证	
function test(file_name)
{
	var result =/\.[^\.]+/.exec(file_name);
	return result;
}	
function onUploadImgChange(sender,img,obj){  
	if(!$("#"+img).val()){
		return false;
	}
    if( $("#"+img).val() && !sender.value.match( /.jpg|.gif|.png|.bmp|.jpeg/i ) )
	{        
        alert('图片格式无效！');   
		remove_file(img);	
        return false;       
    }   
	var ap_id = $('#ap_id').val();//图片名称后缀必须为小写，大些报错
	var b=test(sender.value);
	if( $("#"+img).val() &&  b!=".jpg"&&b!=".gif"&&b!=".png"&&b!=".bmp"&&b!=".jpeg")
	{
		alert('后缀只支持小写格式！');
		if(!ap_id)
		{
			remove_file(img);	
		}
		else
		{
			var file = $("#"+img);
			file.after(file.clone().val(""));
			file.remove();
		}
		return false;  
	}
	var pic_size = getPhotoSize(sender);
	if(!pic_size){
	    alert('图片大小请控制在200KB以内');
		remove_file(img);	
        return false;  
	}	
	var imgSrc = check_img(sender);
    var img_url = imgSrc;
    var newimg = new Image();
    newimg.src = img_url;
    // 加载完成执行
    newimg.onload = function () {
		// 打印
		var w = newimg.width;
		var h = newimg.height;
		if((img == 'ap_imgurl' || img == 'bg_img' || img == 'button_pic') && w != 640){
			alert('宽必须640');  
			remove_file(img);			
			return false;    			
		}
		if(img == 'share_weixin_pic'  && (w != 70 || h != 70)){
			alert('分享微信图请上传70*70的图片');  
			remove_file(img);			
			return false;   		
		}
		var objPreview = document.getElementById(obj); 
		$('#'+img+'_del').remove();
		str = '<button id="'+img+'_del" type="button" onclick="del_img(\''+img+'\');" >&nbsp;&nbsp;删&nbsp;&nbsp;除&nbsp;&nbsp;</button>';
		$('#'+obj).closest('img').after(str);		
		var imgs=document.getElementById(img);
		if( sender.files &&  sender.files[0] ){ 
			objPreview.style.display = 'block';        
			objPreview.style.width = 'auto';        
			objPreview.style.height = 'auto';
			objPreview.src = window.URL.createObjectURL(imgs.files[0]);
		}       
	}	
} 	
function del_img(img){
	if(confirm('是否确认删除？')) {
		remove_file(img);
	}
}
function remove_file(file){
	$('#'+file+'_div').removeAttr('src').css('display','none');
	$('#'+file+'_del').remove();	
	var file = $("#"+file);
	file.after(file.clone().val(""));
	file.remove();	
	return false;   	
}
function check_img(obj){
   var imgSrc = '';
    if (obj.files && obj.files[0]) {
        if (getOs() == 'Chrome') {
            imgSrc = window.webkitURL.createObjectURL(obj.files[0]);
        } else {
            imgSrc = window.URL.createObjectURL(obj.files[0]);
        }
    } else {
        //IE下，使用滤镜
        obj.select();
        //解决IE9下document.selection拒绝访问的错误
        if (navigator.appName == "Microsoft Internet Explorer" && navigator.appVersion.match(/9./i) == "9.") {
            obj.blur();
        }
        var imgSrc = document.selection.createRange().text;
        document.selection.empty();
    }
	return imgSrc;
}
function getOs() 
{ 
   var OsObject = ""; 
   if(isIE = navigator.userAgent.indexOf("MSIE")!=-1) { 
        return "MSIE"; 
   }
   if(isFirefox=navigator.userAgent.indexOf("Firefox")!=-1){ 
        return "Firefox"; 
   }
   if(isChrome=navigator.userAgent.indexOf("Chrome")!=-1){ 
        return "Chrome"; 
   }
   if(isSafari=navigator.userAgent.indexOf("Safari")!=-1) { 
        return "Safari"; 
   }
   if(isOpera=navigator.userAgent.indexOf("Opera")!=-1){ 
        return "Opera"; 
   } 
} 
function checkinput(){
	var ap_name = $.trim($('#ap_name').val());
	if (!ap_name){
		alert('活动名称不可为空');
		return false;
	}
	var lottery_num_limit = $.trim($('#lottery_num_limit').val());
	if (lottery_num_limit && isNaN(lottery_num_limit)){
		alert('用户每日获得抽奖次数限制【只能输入数字】');
		return false;
	}
	var version_code = $.trim($('#version_code').val());
	if (version_code && isNaN(version_code)){
		alert('限制参加活动版本号【只能输入数字】');
		return false;
	}

	var ap_id = $('#ap_id').val();
	var ap_type = $('#ap_type').val();
	var like_limit = $.trim($('#like_limit').val());
	var is_telephone =0;
	$("[name='is_telephone']").each(function(){
		if($(this).is(':checked')) {
			is_telephone = $(this).val();
		}
	});		
	if(!ap_id){
		if (!$('#ap_imgurl').val()){ 
			alert('请上传活动banner图'); 
			return false;
		}
	
		if (ap_type==2){	
			if (!like_limit){
				alert('签到天数不可为空');
				return false;
			}	
			if (like_limit && isNaN(like_limit)){
				alert('签到天数【只能输入数字】');
				return false;
			}
			var share_add =0;
			$("[name='share_add']").each(function(){
				if($(this).is(':checked')) {
					share_add = $(this).val();
				}
			});	
			if(share_add == 1){	
				if (!$('#lottery_pic').val()){
					alert('请上传抽奖区背景图片');
					return false;
				}
				if (!$('#click_lottery_pic').val()){
					alert('请上传点击抽奖图');
					return false;
				}	
				if (!$('#unclick_lottery_pic').val()){
					alert('请上传禁止抽奖图');
					return false;
				}	
			}
			var share_switch =0;
			$("[name='share_switch']").each(function(){
				if($(this).is(':checked')) {
					share_switch = $(this).val();
				}
			});	
			if(share_switch == 1){
				if (!$('#share_weixin_pic').val()){
					alert('请上传分享微信图');
					return false;
				}
				if (!$('#share_other_pic').val()){
					alert('请上传其他分享图');
					return false;
				}			
			}	
		}			
	}else{
		var share_add =0;
		$("[name='share_add']").each(function(){
			if($(this).is(':checked')) {
				share_add = $(this).val();
			}
		});	
		if(share_add == 1){	
			var lottery_pic = "<?php echo $list['lottery_pic']; ?>";
			if (!$('#lottery_pic').val() && !lottery_pic){
				alert('请上传抽奖区背景图片');
				return false;
			}
			var click_lottery_pic = "<?php echo $list['click_lottery_pic']; ?>";
			if (!$('#click_lottery_pic').val() && !click_lottery_pic){
				alert('请上传点击抽奖图');
				return false;
			}	
			var unclick_lottery_pic = "<?php echo $list['unclick_lottery_pic']; ?>";
			if (!$('#unclick_lottery_pic').val() && !unclick_lottery_pic){
				alert('请上传禁止抽奖图');
				return false;
			}	
		}
		var share_switch =0;
		$("[name='share_switch']").each(function(){
			if($(this).is(':checked')) {
				share_switch = $(this).val();
			}
		});	
		if(share_switch == 1){
			var share_weixin_pic = "<?php echo $list['share_weixin_pic']; ?>";
			if (!$('#share_weixin_pic').val() && !share_weixin_pic){
				alert('请上传分享微信图');
				return false;
			}
			var share_other_pic = "<?php echo $list['share_other_pic']; ?>";
			if (!$('#share_other_pic').val() && !share_other_pic){
				alert('请上传其他分享图');
				return false;
			}			
		}		
	}
	var download_config = $('#download_config').val();
	var soft_style = $('#soft_style').val();
	if(ap_type==2 && download_config == 1 && soft_style == 0){
		alert('指定游戏性质【错误】');
		return false;	
	}
	if(is_telephone == 3){
		if(like_limit<14){
			alert('icon大小选择【前台展示天数】签到天数必须选大于8天');
			return false;	
		}
	}else{
		if(like_limit>8){
			alert('icon大小选择【大icon或小icon】签到天数必须选8天以内');
			return false;			
		}
	}
	var dep_type =0;
	$("[name='dep_type']").each(function(){
		if($(this).is(':checked')) {
			dep_type = $(this).val();
		}
	});		
	if(dep_type == 1){
		var share_add_all =  $.trim($('#share_add_all').val());
		var rank_lottery_desc_pic =  $.trim($('#rank_lottery_desc_pic').val());
		if (share_add_all && isNaN(share_add_all)){
			alert('免费补签次数【只能输入数字】');
			return false;
		}		
		if (rank_lottery_desc_pic && isNaN(rank_lottery_desc_pic)){
			alert('安智币补签次数【只能输入数字】');
			return false;
		}		
		if ((!share_add_all || share_add_all == 0) && (!rank_lottery_desc_pic && rank_lottery_desc_pic == 0)){
			alert('补签次数不可为空');
			return false;
		}
		var rank_lottery_desc_text =  $.trim($('#rank_lottery_desc_text').val());
		if (rank_lottery_desc_text && isNaN(rank_lottery_desc_text)){
			alert('单次安智币补签扣除安智币个数【只能输入数字】');
			return false;
		}			
		if(rank_lottery_desc_pic > 0 ){
			if(rank_lottery_desc_text%10 != 0){
				alert('【单次安智币补签扣除安智币个数】请填写10的整数倍');
				return false;
			}
		}	
	}
}
