/* CSS Document */
/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{max-width:640px;min-width:320px; margin:0 auto;font-size:14px; font-family:Microsoft Yahei,"微软雅黑"; background-color: #fff; color: #000;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#001d0b; font-size: 14px;}
a,input,button,div{-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}

html,body{height: 100%;}
section{background: url("../images/vacation_third_images/end_bg.jpg") 0 0 no-repeat #d1ab70; background-size: 100% 100%;}
::-webkit-input-placeholder { color:#becaca;}
:-moz-placeholder {color:#becaca;}
::-moz-placeholder {color:#becaca;}
:-ms-input-placeholder {color:#becaca;}

a.btnAni.cur{-webkit-transition:all 0.1s;-moz-transition:all 0.1s;-o-transition:all 0.1s;transition:all 0.1s;-webkit-transform:scale(1.1);-moz-transform:scale(1.1);-o-transform:scale(1.1);transform:scale(1.1);}

.ptb{ padding-top: 15px;padding-bottom: 15px;}
.ptop{padding-top: 15px;}
.pbot{padding-bottom: 50px;}

.topbar{width:100%;height:51px; background-color: #0294f0; border-bottom:1px solid #0367a6;}
.topbar a{float:left; width:22px; height:22px;margin:15px 0px 0px 8px; background: url("../images/vacation_third_images/btn_back.png") 0 0 no-repeat; background-size: contain;}
.topbar h2{float:left; height:26px; padding:15px 0px 10px 8px;font-size:20px; color:#fff;}

.banner{position: relative; width: 100%;min-width: 320px; margin:0 auto; text-align: center;}
.banner img{ width: 100%;vertical-align: bottom;}
/*公告*/
.notice{position:absolute;left:50%;bottom:0px;width:90%;height:20px;margin-left:-45%;padding-top:16px;color:#202020;background: url("../images/vacation_third_images/notice_bg.png") 0 0 no-repeat; background-size: 100% 100%;font-size: 12px;}
.notice-icon{ float: left;height:16px;line-height:17px;margin-top:1px;display: inline;margin-left:8%;padding-left: 15px;background:url("../images/vacation_third_images/horn.png") 0 0px no-repeat;background-size: 12px 16px;}
.notice-list{padding:0px 8%;margin-top:2px;}
.notice-list span{color: ＃202020;padding-right: 20px;}
.notice-list span em{ padding-left: 10px;}

.wrap_hd img{width:100%;}
.wrap{width: 100%;margin: 0 auto;background: url("../images/vacation_third_images/con_mid.jpg") top left no-repeat; background-size:100% 100%}
.con{width: 80%;margin:0 auto;padding-top: 1px;}
.con_title{width:182px;height:42px;margin: 15px auto; background: url("../images/vacation_third_images/title_icon.png") 0 0 no-repeat;background-size:228px 171px;text-indent: -9999px;text-align: center; }
.con_title.rule_hd{ background-position:0px -90px; }
.con_title.rob_hd{width:228px;height:38px; background-position:0px -135px; }
.con_title.lottery_hd{background-position:0px -45px;}
.rule_bd p{line-height: 25px; color: #000;}
.rule_bd p.last{ line-height: 22px;}
.rule_bd p.last span{ color: #a10000}

.prize_list{}
.prize_list li{float: left;display: inline;width:32%;margin-right: 2%;}
.prize_list li img{width: 100%;}
.prize_list li.last{margin-right: 0;}

/**/
.prompt{padding:0px 15px 10px 15px;}
.prompt2{color: #1e1e1e;padding-left:14px;padding-top:5px;margin-bottom: -10px;}
.prompt3{color: #810000;padding: 10px 0px 0px 14px;margin-bottom: -15px}
.prompt4{color: #810000;padding: 3px 0px 0px 14px;margin-bottom: -5px}
.input_box{padding:5px 15px;background: url("../images/vacation_third_images/input_bg.png") 0 0 no-repeat;background-size: 100% 100%;}
.input_box input{width: 100%;padding:10px 0px 12px; background: none; border:none;font-family: "Microsoft Yahei"; color: #fff; font-size: 14px;}
/*软件*/
.soft-list{width: 100%; margin: 0 auto;}
.soft-list li{ float: left; display: inline; width: 25%;margin:7px 0px;text-align: center;}
.soft-list li p{}
.soft-list li .soft-icon{text-align: center;}
.soft-list li .soft-icon img{ width: 58px;height: 58px; margin: 0 auto;}
.soft-list li .soft-name{margin-top: 10px;height: 20px; line-height: 20px; overflow: hidden;}
.soft-list li .soft-name a{ color: #372221;}
.soft-list li .soft-size{ color: #f46600;height: 22px; line-height: 22px;overflow: hidden;}
.soft-list li a.downlode{ display: block; width:62px;padding:6px 0px 5px; margin: 10px auto; background:url("../images/vacation_third_images/btn_icon.png") 0 -86px  no-repeat;text-shadow:0px 1px 0px #000;background-size:148px 148px;color:#ffface;cursor: pointer;font-size:13px;text-align: center}

/*结束*/
.end-wrap{padding:10px 0px 40px;}
.end_hd{height:30px; line-height: 30px; background-color:#603921;-webkit-opacity:0.6;-moz-opacity:0.6; opacity:0.6;text-align: center; color: #ffe3c6 }
.awards{padding:15px;}
.awards-hd{padding-bottom:15px;font-size:16px;text-align: center;color: #482c1b}
.awards-list{padding: 10px;border-top: 1px solid #997142;line-height: 25px;}
.awards-list dt{}
.awards-list dd{ color: #482c1b; clear: both;overflow:hidden}
.awards-list dd em{ float: left}
.awards-list dd span{ float: right}
.slide_up{overflow:hidden;height:75px;position: relative;}
/*详情*/
.wth{padding: 0px 15px;}
.part{margin-top: 20px;}
.part h4{font-size: 16px;padding-bottom: 6px;}
.part p{line-height:25px;font-size: 14px; color: #92a6a7}
.part p span{ padding-right: 20px;}
/*邮寄地址*/
.form-item{padding:20px 15px 15px 15px;background: url("../images/vacation_third_images/formitem_bg.png") 0 0 no-repeat; background-size: 100% 100%;}
.form-item label{ float: left;width:70px;color: #622f0f}
.form-item .textbox{margin-left: 70px;}
.form-item .textbox input{width: 98%;font-family: "Microsoft Yahei"; color:#622f0f;background: none; border:none;font-size: 14px;}
.formitem-btn {margin: 25px 15px}
.formitem-btn.cur {margin: 25px 10px}
.formitem-btn a{ display: block;}
.formitem-btn a img{width:100%;}
/*获奖查询*/
.prize_q{padding: 0px 15px 60px 15px;}
.prize_q li{padding:15px 5px 10px 5px;border-bottom: 1px solid #997142}
.prize_q li .hd{padding-bottom:5px;font-size:16px;color: #1e1e1e}
.prize_q li .bd{ position: relative;}
.prize_q li .bd p{color: #9f3100;padding-top: 5px;}
.query_bar{text-align: right;padding-right: 10px;margin-top: -10px;}
.query_bar a{color:#c70000;text-decoration: underline;}
.no_prize{width:194px;height:206px;position: absolute;top:50%; left: 50%;margin-left:-97px;margin-top:-113px; }
.no_prize p{padding-top: 20px; text-align: center;font-size: 16px;color: #383838;}
.no_prize p img{width: 150px;margin: 0 auto;}
/*按钮*/
.btns{ width: 100%;}
.btns a{ display: block;height: 40px;line-height: 40px; margin:0 auto;background:url("../images/vacation_third_images/btn_icon.png") 0 0 no-repeat;background-size:148px 148px; text-align: center; text-indent: -9999px;}
.btns a.change_btn{width:150px;background-position: 0px 0px;}
.btns a.submit_btn{width:150px;height:31px;background-position: 0px -48px;}
/*提示框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{position:absolute;width: 300px;left:50%;top:50%;margin-left:-150px; z-index: 99999;}
.tip_hd img,.tip_ft img{width: 100%;}
.tip_hd h4{position: absolute;top:10px;left: 100px;text-align: center}
.tip_hd h4 img{width:100px;}
.tip-mid{padding-bottom:4px;background-color: #b6864b;border-radius: 0px 0px 15px 15px;}
.tip-con{padding:0px 25px 50px 25px;margin:0px 4px;background-color:#45301b;border:3px solid #614221;border-top:none; border-radius:0px 0px 15px 15px;color: #fff}
.tip-con p{margin-top:-15px;line-height: 25px; text-align: center;font-size: 18px;}
.clo{ position: absolute;right: 7px;top: 8px;}
.clo a{ display: block; width:35px; height:35px;background:url("../images/vacation_third_images/tip_clo.png") 0 0 no-repeat;background-size:100% auto;text-indent: -9999px;}

/*尾部*/
.footer{max-width:640px;}
.footer img{width: 100%;margin-top: -1px;}
