<script>

function checkit(){
    var choise = confirm("你确定你的操作吗？");
    if(!choise){
    return false;
    }else{
     return true;
    }
}

function showAreaName(id, name)
{
	if (name == ',,' || name == '' ) {
		return false;
	}
	var arr = name.split(';');
	var html = '';
	$(arr).each(function(){
		var t = this.split(' ');
		if (typeof(t[0]) != 'undefined') {
			var p = t[0].split(':');
			var n = area_list[p[0]];
			var m = n['name'].split("\t");
			html += m[0] + ',';
			
			if (typeof(t[1]) != 'undefined') {
				var ps = t[1].split(':');
				var n = area_list[p[0]]['sub'][ps[0]];
				var m = n.split("\t");
				html += m[0];
			}
			html += '<br>';
			$('#_div_' + id).html(html);
		}
	});
}
</script>
