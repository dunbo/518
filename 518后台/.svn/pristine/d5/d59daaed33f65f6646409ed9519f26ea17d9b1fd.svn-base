<div style="margin-top:14px">
	 
	<table width="99%" cellpadding="0" cellspacing="0" class="table_list" >
  		<thead>
		  <tr>
			 <th width="6%" height="28" bgcolor="#E3E2FE"><strong>序 号</strong></th>
			<th width="17%" bgcolor="#E3E2FE"><strong>采集关键词</strong></th>
			<th width="17%" bgcolor="#E3E2FE"><strong>包名</strong></td>
			<th width="19%" bgcolor="#E3E2FE"><strong>扫描站点</strong></th>
			<th width="12%" bgcolor="#E3E2FE"><strong>添加时间</strong></th>
            <th width="10%" bgcolor="#E3E2FE"><strong>软件类型</strong></td>
			<th width="8%" bgcolor="#E3E2FE"><strong>状态</strong></th>
			<th width="12%" bgcolor="#E3E2FE"><strong>操 作</strong></th>
		  </tr>
		</thead>
		<tbody>
            <volist name="userconfig" id="userconfig_list">
                <tr>
                    <td height="20"  align="center">{$userconfig_list.cid}</td>
                    <td align="center" >{$userconfig_list.searchname}</td>
                    <td align="center" >{$userconfig_list.apkname}</td>
                    <td align="center" >{$userconfig_list.scansite_name}</td>
                    <td align="center" ><div style="width:70px; margin:0 auto">{$userconfig_list.addtime|date='Y-m-d H:i:s',###|default='&nbsp;'}</div></td>
                    <td align="center" >{$userconfig_list.category_name}</td>
                    <td align="center" >
                        <if condition="$userconfig_list.status eq 1">
                        <font color="#00CC00">运行中...</font>
                        </if>
                        <if condition="$userconfig_list.status eq 0">
                        <font color="#999999">停用</font> 
                        </if>
                    </td>
                    <td  align="center"><a href="__URL__/userconfig_modify/cid/{$userconfig_list.cid}">修改</a> |
                        <if condition="$userconfig_list.status eq 1">
                        <a href="__URL__/userconfig_action/action/update/type/0/cid/{$userconfig_list.cid}">停用</a>
                        </if>
                        <if condition="$userconfig_list.status eq 0">
                        <a href="__URL__/userconfig_action/action/update/type/1/cid/{$userconfig_list.cid}">启用</a>
                        </if>
                    </td>
                </tr>
            </volist>
	</tbody>
</table>
  

