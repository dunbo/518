/*约战分享页*/
.wrap{margin-top: 60px;}
.d_logo{text-align: center}
.d_logo img{width: 82px; height: 82px;}
.desc{text-align: center;font-size: 17px;color: #ababab}
.desc a{color: #ababab}
.version{width: 140px;margin: 15px auto;}
.version li{line-height:25px;font-size: 16px;color: #ababab}
.d_download{}
.d_download a{display:block;margin:0 auto;width:200px;height: 45px;line-height:45px;text-align:center;background-color:#84c031;color: #fff;font-size: 22px;}
.view_h{text-align:center;margin-top:15px;}
.view_h a{font-size: 16px;color: #ababab}