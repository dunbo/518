/* CSS Document */
body,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td,figure{margin:0; padding:0;}
fieldset,img {border: 0;}
a{text-decoration:none; color:#333333;}
.clear{clear:both;}
ul,li{list-style:none;}
body{ font-family:'微软雅黑'; background:#fff; font-size:14px; line-height:20px; color:#333333;}
figure{border:0;}
#main{margin:0}
.inlineblock{display:inline-block;}
.fr{float:right;}
.clear{clear:both}

/*新反馈*/
.title_top{height:50px; background:#19d5fd;}
.title_back{float:left; margin-left:5px; font-size:20px; line-height:51px;}
.title_back a{ display:block;  height:46px; padding:0 10px 0 40px; background:url(../images/fb_v6/back.png) no-repeat 6px 14px; color:#fff; font-weight:bold}
.title_btns{float:right; height:34px; margin:7px 15px 0 0; display:inline}
.title_btns a{float:left; width:34px; height:34px; margin-left:25px; text-align:center; line-height:34px; display:inline}
.title_btns a#feedback_icon{background:url(../images/fb_v6/feedback_icon.png) no-repeat; background-size:34px 34px;}
.title_btns a#help_icon{background:url(../images/fb_v6/help_icon.png) no-repeat; background-size:34px 34px;}
.feedback_form{margin:0 10px;}
.feedback_form_item{width:100%; margin-top:10px; position:relative; color:#494949;}
.form_arrow{display:block; width:38px; height:38px; position:absolute; right:0; top:1px;}
.arrow_down{background:url(../images/fb_v6/arrow_down.png) no-repeat;}
.arrow_up{background:url(../images/fb_v6/arrow_up.png) no-repeat;}
.feedback_form_item .inputtext1{ width:96%; padding:12px 2%; background:#ffffff; border:1px solid #d9d9d9; border-radius:4px;  font-family:"微软雅黑"; font-size:14px; -webkit-tap-highlight-color:rgba(255,0,0,0); color:#494949;}
.feedback_form_item p{ padding:5px 2%; color:#666666; font-size:16px;}
.feedback_form_item textarea{float:left; width:96%; height:100px; padding:5px 2%; background:#ffffff; border:1px solid #d9d9d9; border-radius:4px; font-size:14px; -webkit-tap-highlight-color:rgba(255,0,0,0); font-family:"微软雅黑"; color:#494949;}
.feedback_form_btn input{ display:block; width:100%; padding:7px 0; margin:20px auto 10px; background:#19d5fd; border:none; border-radius:4px; color:#ffffff; font-size:18px; font-family:"微软雅黑";}
.bbs_form_btn input{background:#3ec3d5}
#feedback_select{position:absolute; left:0; z-index:1000; right:0; width:100%;  border:1px solid #d9d9d9; border-bottom:none; border-top:none; background:#fff; display:none}
#feedback_select li{padding:13px 2%; margin:0 auto; border-bottom:1px solid #d8d8d8; color:#a6a6a6;}
#feedback_select li:last-child{}
.contact{padding:20px 0; text-align:center; color:#a5a5a5;}

/*comment*/
.titles_01{padding:10px 0;}
.titles_01 p{font-size:16px;}
.comment_cnt{padding:0 10px 10px;}
.comment_top{padding:5px 0;}
.comments_az,.comments_user{width:100%; margin-top:15px; position:relative}
.comments_az_infor{padding:0 0 0 68px; min-height:100px;}
.comments_user .comments_az_infor{padding:0 68px 0 0;}
.comments_img{position:absolute; left:0; top:0; width:58px; height:48px;}
.comments_img_user{position:absolute; right:0; top:0; width:58px; height:48px;}
.comments_img img,.comments_img_user img{float:left; display:block; width:48px; height:48px; border-radius:30px}
.comments_img_user img{float:right;}
.comments_img span,.comments_img_user span{width:48px; display:block; height:22px; line-height:22px; text-align:center; overflow: hidden; text-overflow: ellipsis; white-space: nowrap;  clear:both; color:#4abd4d}
.comments_img span{padding-left:2px;}
.comments_img_user span{text-align:right;}
.comment_tips{padding-top:5px; color:#959595}
.comment_tips em{font-style:normal; text-align:center;}
.comments_user .comment_tips em{float:right; font-style:normal; padding-right:20px;}
.comment_des{padding:10px; margin-top:8px; background:#d5e7ed; border-radius:4px; position:relative; color:#5b5b5b; font-size:15px; line-height:25px; word-break: break-all; word-wrap: break-word;}
.comment_des p img{float:right; margin:0; border:2px solid #fff; max-width:100%;}
.comment_des_img{padding:0;  background:none;}
.comment_des_imgl p img{float:left;}
.comment_icon{display:block; position:absolute; left:-10px; top:10px; width:10px; height:14px; background:url(../images/fb_v6/comment_icon.png) no-repeat; background-size:10px 14px}
.comment_icon_user{display:block; position:absolute; right:-10px; top:10px; width:10px; height:14px; background:url(../images/fb_v6/comment_icon2.png) no-repeat; background-size:10px 14px}
.comments_az .comment_des{background:#dbf4df}
.comments_az .comments_img img{border-radius:0;}
/*feedback_list*/
.feedback_list{width:100%;}
.feedback_list ul li{min-height:52px; padding:10px 70px 6px 10px; border-radius:4px; border-bottom:1px solid #e6e6e6; position:relative;}
.feedback_list ul li h4{width:90%; min-height:24px; line-height:24px; font-size:16px; font-weight:normal; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; color:#5b5b5b }
.feedback_tips{height:25px; line-height:22px;}
.feedback_tips em{display:block; height:22px; margin-top:3px; line-height:22px; font-style:normal; font-size:13px; color:#a6a6a6; overflow: hidden; text-overflow: ellipsis; white-space: nowrap;}
.feedback_mark{ position:absolute; right:8px; top:4px; width:60px; height:40px; line-height:35px; text-align:right; cursor:pointer; color:#959595; font-size:13px}
.feedback_new{ position:absolute; right:8px; bottom:17px; width:8px; height:8px; background:url(../images/fb_v6/points.png) no-repeat; background-size:8px 8px; cursor:pointer;}
.colorgreen{color:#2ba83f;}
.clickarea{display:block; width:100%; height:100%; position:absolute; left:0; top:0; z-index:2}


.c_app{margin:8px 0 -10px 0px;}
.c_app li{ clear: both; overflow: hidden; position:relative; border-top:1px solid #b2ccb6;}
.c_app li:acitve{background-color: #cecece}
.c_app li.on{ background-color: #cecece}
.c_app_inner{ padding: 10px;}
.c_app li .click_bar{position: absolute;top:0px; left: 0px; display: block; width: 100%; height: 100%;z-index: 1;-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.c_app_icon{ float: left;}
.c_app_icon img{ display:block;width:38px; height: 38px; }
.c_app_info{margin-left:45px;margin-right:55px;}
.c_app_info h4{line-height: 20px;font-weight: bold;line-height: 20px; white-space: nowrap; overflow: hidden; text-overflow: ellipsis}
.c_app_info .c_app_size{padding-top:2px;font-size: 12px; line-height:16px;color: #828282 }
.c_download{position: absolute;top:17px;right:10px;z-index: 2;}
.c_download a{ display: block; min-width:48px; height:22px; line-height: 22px; border: 1px solid #19d4fd; border-radius: 3px;-webkit-border-radius:3px; background-color: #fff; color: #19d4fd; text-align: center;-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.c_download a:acitve{background-color #19d5fd; color: #ffffff}
.c_download a.on{ background-color: #19d5fd; color: #ffffff}

/*help*/
.help_wrap{width:100%;}
.help_wrap ul li {border-bottom:1px solid #e8e8e8;}
.help_wrap ul li .btn_module{padding:15px 28px 15px 12px; background:#fff; font-size:16px; font-weight:normal; color:#5b5b5b; position:relative}
.help_wrap ul li .btn_module span{ position:absolute; right:0; top:6px; display:block; width:42px; height:42px; background:url(../images/fb_v6//arrow_down.png) no-repeat;}
.help_wrap ul li .btn_module span.up{background:url(../images/fb_v6/arrow_up.png) no-repeat;}
.help_answer{margin-top:-3px; padding:0 5px 10px 8px; line-height:22px; display:none; color:#a6a6a6; font-size:16px;}

/*#main{width:100%; margin:0 auto;}*/
/*report*/
.report_wrap{width:100%;}
.titles_01{padding:10px;border-bottom:1px solid #e6e6e6;}
.titles_01 p{font-size:16px; color:#a5a5a5}
.titles_02{padding:10px;}
.titles_02 p{font-size:16px; color:#aeaeae}
.required_must{padding:0 10px; color:red; font-size:12px;}
.report_items{}
.report_items ul li{font-size:16px; position:relative}
.report_items ul li span{display:block; width:20px; height:20px; background:url(../images/check_no.png) no-repeat; position:absolute; left:10px; top:12px; background-size:20px 20px}
.report_items ul li span.checkedbg{background:url(../images/check_ok.png) no-repeat;background-size:20px 20px}
.report_items ul li label{ display:block; padding:12px 0 12px 45px; border-bottom:1px solid #e6e6e6; color:#575757;}
.report_items ul li input{ position:absolute; left:-10000px; top:0; clip:rect(0,0,0,0);}
.report_form .text_area{margin: 0px 10px;}
.report_form textarea{width:98%; height:70px; margin:0 auto; padding:4px 1%; background:#fff; color:#797979; border:1px solid #e6e6e6; display:block;font-family: "Microsoft YaHei", "微软雅黑"}
.form_btn{margin:10px;}
.form_btn input{ display:block; width:100%; height:37px;  border-radius:4px; background:#19d5fd; border:none; font-size:16px; color:#fff; font-family:MicroSoft YaHei,SimSun,sans-serif;}


.uninstall_des{min-height:118px; margin:8px 3px 0 0; padding-left:115px; background:url(../images/fb_v6/cry_bg.png) no-repeat; background-size:144px 118px; color:#545454}
.uninstall_des p{padding:5px 0 0 0; font-size:16px; line-height:25px;}
.uninstall_down a{display:block; width:168px; margin:5px auto 10px; padding:8px 0 7px; border:1px solid #19d5fd; border-radius:20px; font-weight:normal; text-align:center; font-size:17px; color:#19d5fd;-webkit-tap-highlight-color:rgba(255,255,255,0)}
.uninstall_down a.on{background:#19d5fd;background-clip: padding-box; border-radius:20px; color:#fff}
.uninstall_form{border-top:1px solid #e6e6e6}
.uninstall_form table{width:100%}
.uninstall_form table td{ width:50%; border-bottom:1px solid #e6e6e6;}
.uninstall_form table td:nth-of-type(odd){border-right:1px solid #e6e6e6}
.uninstall_list{width:100%; min-height:60px; position:relative;  color:#545454}
.uninstall_list input{position:absolute; left:-1000px; clip:rect(0,0,0,0); z-index:-1;}
.uninstall_form .uninstall_list_checkbox{ display:block; padding:8px 8px 8px 38px; background:url(../images/check_no.png) no-repeat 10px center; background-size:20px 20px; color:#575757; font-size:14px; font-family:"微软雅黑"; line-height:22px; cursor:pointer; -webkit-tap-highlight-color:rgba(255,255,255,0)}
.uninstall_list_checkbox.checked{background:url(../images/check_ok.png) no-repeat 10px center; background-size:20px 20px}
.uninstall_list2 .uninstall_list_checkbox{padding-top:18px; padding-bottom:20px;}
.uninstall_form p{ padding:5px 0 8px 5px; color:#666666; font-size:14px;}
.uninstall_form textarea{ display:block; width:97%; height:50px; padding:5px 1%; margin:0 auto; border:1px solid #bdbdbd; border-radius:5px; background:#ffffff;}
.uninstall_form_btn{ margin:10px 10px;}
.uninstall_form_btn input{ display:block; width:100%; padding:7px; border:none; border-radius:5px; background:#19d5fd; color:#ffffff; font-size:18px; font-family:"微软雅黑"}
.blue{color:#19d5fd}
