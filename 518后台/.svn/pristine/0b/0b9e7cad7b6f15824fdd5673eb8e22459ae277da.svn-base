<body>

<div class="demo">


<div align="right" style="margin-right:100px" >
<form action="__APP__/Sj/Statistical/sjtop/" method="post" enctype="multipart/form-data" id="csv_form">
<table width="100%" border="1">
<tr>
<td colspan="6">
选择查看日期(双击日期确定)： <span
			id="WebCalendar3"
			style="border: 1px solid #7F9DB9; align: absmiddle; cursor: hand; display: inline-block; width: 124px; padding: 1px">
		<input id="date0" name="date0"
			style="cursor: hand; width: 100px; border: none 0px black;"
			value="{$ptime}" size="15" type="text"><img
			src="__PUBLIC__/js/calendar.gif"
			class='Wdate' onClick='WdatePicker({startDate:"%y-%M-%d 00:00:00",el:"date0",dateFmt:"yyyy-MM-dd HH:mm:ss"})' 
			style="margin: 1px; cursor: hand;" width="16px" align="absmiddle"
			height="15px"></span> <span id="WebCalendar3"
			style="border: 1px solid rgb(127, 157, 185); padding: 1px; display: inline-block; width: 124px;">
		<input id="date1" name="date1"
			style="border: 0px none black; width: 100px;" value="{$ptime1}"
			size="15" type="text"><img src="__PUBLIC__/js/calendar.gif"
			class='Wdate' onClick='WdatePicker({startDate:"%y-%M-%d 23:59:59",el:"date1",dateFmt:"yyyy-MM-dd HH:mm:ss"})' 
			style="margin: 1px;" width="16px" align="absmiddle" height="15px">
		</span> 
</td>
<td>软件类别</td>
<td>{$conflist}<!-- <select name="categoryid" id="categoryid">
      <option value="0" selected>全部</option>
      <volist name='categorylist' id='categoryvo'>
      
		   <option value="{$categoryvo.category_id}" <if condition="$post_categoryid eq $categoryvo.category_id "> selected="selected" </if> >--{$categoryvo.name}--</option> 
		
        <volist name="categoryvo['child']" id='childvo'>
           <option value="{$childvo.category_id}" <if condition="$post_categoryid eq $childvo.category_id "> selected="selected" </if> >{$childvo.name}</option> 
        </volist>
      </volist>
      </select> --></td>
</tr>
<tr> 
<td>web下载量：<input id="web" name="web" type="checkbox" value="1" <if condition="$chekweb eq '1'">checked="checked"</if>/></td>
<td>手机下载量：<input id="sj" name="sj" type="checkbox" value="1" <if condition="$cheksj eq '1'"> checked="checked"</if>/>
</td>
<td>手机更新量：<input id="sjg" name="sjg" type="checkbox" value="1" <if condition="$cheksjg eq '1'"> checked="checked"</if>/>
</td>
<td>合作下载量：<input id="hz" name="hz" type="checkbox" value="1" <if condition="$chekhz eq '1'"> checked="checked"</if>/></td>
<td>wap下载量：<input id="wap" name="wap" type="checkbox" value="1" <if condition="$chekwap eq '1'"> checked="checked"</if>/></td>
<td>显示数量：<input id="count" name="count" type="text" value="{$count}"></td>
<td><input name="check" type="button" value="查看" onclick="checkwin()" ></td>
<td><input name="check1" type="button" value="导出" onclick="openwin()"></td>
</tr>
</table>
</form>
</div>

</div>
<!-- End demo -->



<div class="demo-description">

<table width="100%" border="1">
  <tr>
    <th scope="col" width="4%">序号</th>
    <th scope="col" width="10%">应用名称</th>
    <th scope="col" width="26%">包名</th>
	<th scope="col" width="10%">web下载量</th>
	<th scope="col" width="10%">手机下载量</th>
	<th scope="col" width="10%">手机更新量</th>
	<th scope="col" width="10%">合作下载量</th>
	<th scope="col" width="10%">wap下载量</th>
    <th scope="col" width="10%">总下载量</th>
  </tr>
  <volist name='list' id='usersvo'>
  <tr>
    <td width="4%">{$usersvo.num}</td>
    <td width="10%">{$usersvo.softname}</td>
    <td width="26%">{$usersvo.package|default='&nbsp;'}</td>
    <td width="10%">{$usersvo.web_dl_cnt|default='&nbsp;'}</td>
	<td width="10%">{$usersvo.mob_dl_cnt|default='&nbsp;'}</td>
	<td width="10%">{$usersvo.mob_up_cnt|default='&nbsp;'}</td>
	<td width="10%">{$usersvo.ptn_dl_cnt|default='&nbsp;'}</td>
	<td width="10%">{$usersvo.wap_dl_cnt|default='&nbsp;'}</td>
	<td width="10%">{$usersvo.s|default='&nbsp;'}</td>
  </tr>
  </volist>
</table>

</div>
<!-- End demo-description -->

{$page}
