/* CSS Document */
/*税率弹窗*/
.tax_rate_form,.money_form{width:280px; height:30px; margin:0 auto; padding:20px 0 25px; line-height:30px;}
.tax_rate_list label{float:left; width:80px;}
.inputtext{float:left; width:140px; height:28px; padding:0 2px; border:1px solid #dcdcdc; margin-right:5px; display:inline;}


/*审核弹窗*/
.checked_newbox_form{width:400px; margin-left:80px; padding-bottom:20px;}
.checked_newbox_form table tr{height:45px}
.checked_newbox_form table .inputtext{width:270px; padding:0 10px;}

/*备注弹窗*/
.add_remark_form{width:392px; margin-left:50px;}
.add_remark_list{float:left; width:392px; margin-bottom:15px; clear:both}
#add_remark_list2{width:390px; height:280px; margin-left:50px; border:1px solid #dcdcdc; overflow-y:auto; overflow-x:hidden;}
#add_remark_list2 ul{float:left; width:360px; padding:10px 15px;}
#add_remark_list2 ul li{float:left; width:360px; margin:10px 0; line-height:20px;}

/*编辑弹窗*/
#accout_edit_newbox{width:980px;}
.accout_edit_form{width:880px; margin:0 auto;}
.accout_edit_list{float:left; margin-bottom:8px; font-size:12px; line-height:30px;}
.accout_edit_list label{float:left; width:100px; padding-right:10px}
.accout_edit_list input.inputtext,.accout_edit_list select{float:left; width:198px;}
.accout_edit_list .accout_edit_tips{float:left; color:#ff0000;}

.tab_title{width:100%; height:30px;}
.tab_title li{float:left; width:98px; height:28px; margin-right:1px; background:#fff; border:1px solid #bdbdbd; text-align:center; line-height:28px; display:inline; cursor:pointer}
.tab_title li.current{ background:url(../images/btn_bg_01.png) repeat-x;}
.tab_cnt1 tbody td{text-align:left;}
.tab_cnt{display:none;}
#tab_cnt2 ul{width:360px; height:200px; padding:10 15px; margin-top:10px; border:1px solid #dcdcdc; overflow:hidden; overflow-y:auto;}
#tab_cnt2 ul li{float:left; width:340px; margin:10px 0 10px 20px; line-height:20px; display:inline;}