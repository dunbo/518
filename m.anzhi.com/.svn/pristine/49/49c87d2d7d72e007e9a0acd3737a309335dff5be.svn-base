                <dt>二等奖</dt>
                <div <!--{if $out.fivth_award_count > 3}-->id="secondPrize"<!--{/if}--> style="overflow:hidden;height:75px;">
                    <div>
						<!--{foreach from=$out.fivth_award item=vo}-->
                        <dd><em><!--{$vo.the_telphone}--></em><span><!--{$vo.prize}--></span></dd>
						<!--{/foreach}-->
                    </div>
                </div>
            </dl>
			<!--{/if}-->
			<!--{if $out.sixth_award}-->
