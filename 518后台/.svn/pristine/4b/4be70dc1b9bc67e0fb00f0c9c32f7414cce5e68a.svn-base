<div style="height:50px;margin:5px;">
	<div style="position:relative;float:left;margin: 5px;"><h2>|热门标签管理</h2></div>
</div>
<div >
<form method="post" id="form" action="__URL__/doedit/action/rename/id/{$_GET.id|default=''}">
<table border="0" cellpadding="0" cellspacing="0" class="table_list">
	<tr>
		<td>名称</td>
		<td><input type="text" id="rename" name="rename" value="{$result.rename|default=''}"/></td>
	</tr>
	<tr>
		<td colspan="2"><input type="submit" name="submit" value="提交" class="search_btn" style="display:inline-block;"onclick="return ckf()" > <button type="button" onclick="window.location='__URL__/index';" class="search_btn" style="display: inline;">返回</button></td>
	</tr>
</table></form>
</div>
<script type="text/javascript">
function ckf(){
    c = document.getElementById('rename');
    if(c.value == ''){
        alert('名称未填写');
        return false;
    }
    return true;
}
</script>