/* CSS Document */
/*公共样式*/
body,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width: 320px; font-size:14px; font-family:Microsoft Yahei,"微软雅黑"; background-color: #ffffff; color: #a6a6a6;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
a{text-decoration:none;}
.clear{clear:both;overflow:hidden;}
.pad_top{padding-top:70px;}
.banner{width: 100%;}
.banner img{ width: 100%;}
.az_info{width:320px; margin:0 auto; padding:24px 0; text-align:center;}
.az_info h1{width:165px; height:120px; margin:0 auto; background:url(../images/wifi_logo.png) no-repeat 0 0; background-size:165px 120px;}
.az_info a{display:block; width:200px; height:48px; margin:13px auto 8px; background:url(../images/down_btn.png) no-repeat 0 0; background-size:200px 48px;}
.az_moret{width:75%; height:20px; margin:10px auto 5px; border-top:1px solid #bababa; position:relative;}
.az_moret h2{width:40%; background:#fff; position:absolute; left:30%; top:-11px; text-align:center; font-size:18px;}
.az_more ul {width:94%; margin:0 auto;}
.az_more ul li{float:left; width:25%; height:90px; text-align:center;}
.az_more ul li img{display:block; width:48px; height:48px; margin:0 auto;}
.az_more ul li span{display:block; padding-top:5px; font-size:13px}
.footer_app{width:100%; left:0; bottom:0; background:url(../images/footer_bg.png) repeat; position:fixed; line-height:21px}
.footer_l{float:left; margin-top:3px; padding:5px 2px 5px 4px; color:#fff;}
.footer_l img{float:left; margin-top:3px;}
.footer_l p{float:left; margin-left:5px; display:inline; font-size:12px;}
.footer_l p span{font-size:16px; margin-bottom:2px;}
.ft_close{float:right; display:block; width:25px; height:20px; background:url(../images/close_btn.png) no-repeat right top;}
.ft_btn{float:right; margin-top:13px; padding:5px; background:#7fc31e; text-align:center; color:#fff; display:inline}
.wifi_btn{display:block; width:200px; height:48px; margin:10px auto 20px; background:url(../images/btn_01.png) no-repeat; background-size:200px 48px;}

