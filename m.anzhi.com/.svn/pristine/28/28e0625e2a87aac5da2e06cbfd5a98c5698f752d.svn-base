/* CSS Document */
/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{max-width:6.4rem;min-width:3.2rem; margin:0 auto;font-size:0.14rem; font-family:Microsoft Yahei,"微软雅黑"; background: #e6e6e6; color: #414141;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{ display: block;}
a{text-decoration:none;color:#414141; font-size: .14rem;}
.a,input,button,div{-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.clear{clear:both;overflow:hidden;}
.fl{ float: left;}
.fr{ float: right}
.banner img{ width: 100%;}
.main{ padding-bottom:0.1rem; position:relative}
.title_top{height:1.05rem; background:#fff; position: relative; line-height: 1.05rem;}
.title_top h3{text-align: center; font-size: 0.34rem;}
.title_top a{position: absolute; right:0.24rem; top:0; font-size: 0.24rem; color:#a5a5a5;}
.app{ background:#fff; min-height:1.6rem; padding-bottom: 0.2rem; position:relative}
.app_img{width:1.28rem; height:1.28rem; position:absolute; left:0.2rem; top:50%; margin-top:-0.64rem;}
.app img{display:block; width:1.28rem; height:1.28rem;  border-radius:0.23rem;}
.app_txt{margin:0.01rem 0.1rem 0 1.7rem; padding-top:0.35rem; font-size:0.2rem}
.app_txt h3{max-height:0.8rem; font-size:0.3rem; color:#545454;width:100%; overflow: hidden; line-height: 0.4rem}
.app_txt p{padding:0.1rem 0 0; line-height:0.24rem; font-size:0.2rem;color:#d5d5d5;}
.app_txt p span span{padding:0 0.06rem 0 0; color:#ff3a56}
.app_txt h2{padding:0.1rem 0 0; font-size:0.24rem; color:#818181;width:100%; }
.infor_wrap{ background:#fff;margin:0.1rem auto;}
.title h3{display:inline-block; padding:0.2rem 0.15rem 0.1rem 0.24rem; font-size:0.3rem; color:#545454;font-weight: bold;}

.infor{ padding:0.2rem 0.2rem 0; word-break: break-all; word-wrap: break-word;}
/* .infor p{color:#626262;font-size:0.24rem;padding:0.1rem 0.24rem 1rem 0;line-height:0.42rem;} */
.infor img{width:100%;}

.btns_wrap{width:100%;  margin:0 auto; padding:0.2rem 0; background:#fff; border-top:0.02rem solid #eeeeee;box-shadow: 0.1rem 0.01rem 0.1rem #888888; position:fixed; bottom:0; left:0; z-index:99996}
.btns_wrap a{height:0.5rem; line-height:0.5rem; border-radius:0.02rem; text-align:center; color:#fff; font-size:0.3rem}
.btns_wrap a.disabled{background:rgba(35,180,255,.5)}
.btns_wrap a.app_btn{ display:block; height:0.69rem; margin:0 0.25rem 0 1.26rem;  background:#19d5fd; line-height: 0.69rem; border-radius: 6px;}
.btns_wrap a.share_btn{ width:0.46rem; height:0.52rem; background:url(../images/share_btn.png) no-repeat; background-size: 100% 100%; position:absolute; left:0.4rem; top:0.27rem; text-indent: -9999px; line-height: 0.52rem;}
.category{margin:0 0 0.1rem 0;}
.category_title{  background:#fff;  text-align:center;padding:0.35rem 0 0.11rem;}
.category_title h3{display:inline-block;  line-height:0.34rem; font-size:0.34rem; color:#545454;}
.category_title span{ padding:0 0.2rem 0 0; font-size:0.24rem; color:#a5a5a5;float:right;}

.list{ background:#fff; padding-bottom: 0.2rem;border-bottom:1px solid #e6e6e6;}
.list_title{ padding:0.15rem 0.24rem 0; height:0.88rem; position: relative;}
.list_title h3{font-size: 0.3rem; line-height: 0.44rem; text-align: justify;}
.list_title span{height:0.3rem; position: absolute; right:0.24rem; bottom:0.05rem; color:#a5a5a5; font-size: 0.2rem; line-height: 0.3rem;}
.list_img img{display: block; width:100%; margin: 0 auto;}
.list_img{float:left; width:2.72rem; height:1.52rem; padding:0.24rem 0 0.24rem 0.24rem;}
.list_img img{display:block; width:2.72rem; height:1.52rem; }
.list_img3{margin:0.15rem 0.24rem 0.2rem}
.list_img3 li{float:left; width:31.3%; margin-right: 3%;}
.list_img3 li img{display: block; width:100%}
.list_img3 li:nth-of-type(3){margin-right: 0;}
.list_txt{float:right; margin:0 0.1rem 0 3.2rem; padding-top:0.27rem; font-size:0.2rem;position:absolute;}
.list_txt h3{max-height:1.2rem; font-size:0.3rem; color:#545454;width:100%; overflow: hidden; line-height: 0.4rem}
.list_txt p{padding:0.1rem 0 0; line-height:0.24rem; font-size:0.2rem;color:#a5a5a5;}

.list_app{ position: relative;}
.list_app_img{width:0.52rem; height:0.52rem; position: absolute; left:0.24rem; top: 0.03rem;}
.list_app_img img{display:block; width:0.52rem; height:0.52rem;}
.list_app_txt{margin:0 1.5rem 0 0.9rem; font-size:0.2rem;}
.list_app_txt h3{font-size:0.24rem; color:#545454;width:100%; }
.list_app_txt p{height:0.3rem;line-height:0.3rem;font-size:0.2rem;color:#a5a5a5; white-space:nowrap; text-overflow:ellipsis; overflow:hidden}
.list_app a{display: block; width: 1.18rem;height:0.46rem; background: #19d5fd; border-radius:0.02rem; text-align: center; line-height:0.51rem; color: #fff; position: absolute; right:0.24rem; top:0.3rem; margin-top:-0.23rem; font-size: 0.22rem;}
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.share_01{width:258px; height:162px; position: absolute; left:50%; top:1.6rem; margin-left:-120px;  z-index:99999; display: none;}
.share_01 img{display: block; width:258px; height: 162px;}
.end{margin-top:2rem; text-align: center; font-size: 0.3rem; line-height: 0.44rem;}
.end img{display: block; width:2.08rem; height:1.48rem; margin: 0.3rem auto;}