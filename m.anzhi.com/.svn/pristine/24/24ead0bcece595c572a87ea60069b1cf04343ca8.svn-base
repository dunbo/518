/* CSS Document */
/*公共样式*/
body,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{font-size:14px; font-family:Microsoft Yahei,"微软雅黑"; background-color: #ffffff; color: #372221;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{vertical-align: bottom;}
a{text-decoration:none;color:#001d0b; font-size: 14px;}
.cl{overflow:hidden;}
.clear{clear:both;overflow:hidden;}

html,body{width: 100%; height: 100%;}

::-webkit-input-placeholder { color:#979b98;}
:-moz-placeholder {color:#979b98;}
::-moz-placeholder {color:#979b98;}
:-ms-input-placeholder {color:#979b98;}

.mtop{ margin-top: 15px;}
.ptop{ padding-top: 15px;}
.ptb{padding:10px 0px; }
.pbottom{ padding-bottom: 80px;}
.bor-top{ border-top:1px solid #ffcd98;}

.tip-box .clo,#my_button a.gray-btn,.az-logo span.az-version,#gameContent table td.current{ background: url("../images/common_icon.png") no-repeat; }

.topbar{width:100%;height:51px; background-color: #e83328; border-bottom:1px solid #b4140a;}
.topbar a{float:left; width:22px; height:22px;margin:15px 0px 0px 8px; background: url("../images/btn_back.png") 0 0 no-repeat; background-size: contain;}
.topbar h2{float:left; height:26px; padding:15px 0px 11px 8px;font-size:20px; color:#fff;}
.topbar h2.login-hd{ width: 100%;padding-left:0px;text-align: center;}

.banner{position: relative; width: 100%;min-width: 320px; margin:0 auto; text-align: center;}
.banner img{ width: 100%;vertical-align: bottom;}
.banner a{}
.banner a.rule{ display: block;width:60px; height:18px;line-height:18px;position: absolute;bottom:6px;right:3%; border-radius: 10px; font-size: 12px;background-color: #fff300; color: #fa2d20; box-shadow: 0 3px 0px #ffa200;}
.banner a.join{ display: block;width:60px; height:18px;line-height:18px;position: absolute;bottom:6px;right:3%; border-radius: 10px; font-size: 12px;background-color: #ffee31; color: #ff0090;box-shadow: 0 3px 0px #ff0090;}

/*公告*/
.notice{width: 100%;height: 24px; margin-top:5px;line-height: 24px;background-color:#e83328; color: #fff; overflow: hidden; }
.notice-icon{ float: left; display: inline; width: 20px;height:20px;margin:2px 0px 0px 5px;background: url("../images/notice_icon.png") 0 0 no-repeat; background-size: contain;}
.notice-list{}
.notice-list span{color: #fff;padding-right: 20px;}
.notice-list span em{ padding-left: 10px;}

.blue-wrap{width: 100%; background: url("../images/lottery_bg_bg.png") 0 0 repeat-x;}
.white-wrap{ width: 94%; margin: 0 auto;}
.gray-wrap{width: 100%;padding:15px 0px; background-color: #f2f0f0}
.pink-wrap{width: 100%;padding:15px 0px; background-color: #ffeccd}
.red-wrap{width: 100%;padding:5px 0px; background-color: #f00202; margin: 5px 0px;}
.prompt-col{padding:10px 0px;color: #239ee2;}
.prompt-col a.my-prize{ float: right; color: #fff300;}
.prompt-col span.lott-times{ color: #ffffff;}
.prompt-col a.my-prize2{ float: right; color: #fc8200;}
.prompt-col span.lott-times2{ color: #ff344d;}
.prompt-mid{ padding:10px 0px;text-align: center;color: #ff344d;}
.act-end{}
.act-end p{line-height: 22px; text-align: center;color: #ffffff;font-size: 16px; }
.trees-bg{min-height:166px;padding:10px 0px;background: url("../images/trees_bg.png") center bottom no-repeat #ffeccd; background-size: 100% auto; }
.awards-list{padding-bottom: 125px;}
.awards-list dt{padding-bottom: 10px; text-align: center; font-size: 18px; color: #ff1d1d}
.awards-list dd{ padding:6px 0px;border-top:1px solid #ffcd98;font-size: 16px; color: #fc8200}
.awards-list dd p{ line-height: 25px;}

/*抽奖*/
#gameContent{ position:relative;width:296px; height:296px; margin:10px auto 0px auto;background:url(../images/lottery_bg.png) no-repeat scroll;border:none;font-size: 16px;color:#fff;}
#gameContent table{width:302px;height: 303px;}
#gameContent table td{width:88px;height:93px;padding-right:8px;padding-bottom:6px;text-align:center;vertical-align:middle;}
#gameContent table td.current{background-position:-56px -40px; color: #ffe66f;}
#my_button {position:absolute;left: 97px;top:98px;z-index:999;}
#my_button a{display:block;width:102px;height:102px;}
#my_button a.gray-btn{background-position:-48px -152px; cursor: default;}
.snow{position: absolute;width:310px;height:55px;top: -12px; left:-5px;background: url("../images/snow.png") 0 0 no-repeat; background-size:contain;z-index:999;}

#act-end{}
#SIM{ width: 99%;margin:0 auto;height:30px;line-height:30px;text-align: center; border:1px solid #00b13d;}
.prize-hd{width: 100%;padding-bottom:12px;border-bottom:1px solid #ffcd98;font-size:16px;text-align: center;color:#fc8200;}
.contact-info{width: 90%; margin: 0 auto;}
.contact-info .prompt-02{text-align: center; color:#e83328; font-size: 20px; padding: 12px 0px 20px 0px;}
.contact-info .prompt-03{color:#fc8200;padding-left:6px;}
.form-items{}
.form-items li{ margin-top: 10px;}
.form-items li input{ width: 98%;padding:16px 6px;border:none;border-radius:5px; color: #666;  font-family: Microsoft Yahei,"微软雅黑"; background-color:#fff;}
.prompt-submit{ color: #e83328; margin-top: 10px;padding-left: 6px;}

.act-intro_hd,.act-intro_bd{ color: #9b9b9b; line-height: 20px;}
.act-intro_hd{ margin-top: 10px;}
.act-intro_hd a{color: #9b9b9b; text-decoration: underline}
/*渠道包更新提示*/
.update-tip{width:282px;padding: 10px; margin:0px auto 10px auto; background-color: #fff;border-radius: 5px; }
/*软件*/
.soft-list{width: 95%; margin: 0 auto;}
.soft-list li{ float: left; display: inline; width: 25%;margin-bottom:15px;text-align: center;}
.soft-list li p{}
.soft-list li .soft-icon{}
.soft-list li .soft-icon img{ width: 64px; vertical-align: bottom;}
.soft-list li .soft-name{margin-top: 5px;height: 20px; line-height: 20px; overflow: hidden;}
.soft-list li .soft-name a{ color: #372221;}
.soft-list li .soft-size{ color: #f46600;height: 22px; line-height: 22px;overflow: hidden;}
.soft-list li a.downlode{ display: block; width:60px;height: 30px; background:url("../images/btn_download.png") 0 0 no-repeat;background-size:contain;  margin: 5px auto 0px auto;text-indent:-9999px;cursor: pointer;}

.mod-title{width:86px; height: 35px;line-height: 33px;margin: 15px 0px 10px 0px;padding-left:50px; background:url("../images/menu_bg.png") 0 0 no-repeat; background-size:contain; color: #fff; font-size: 16px;}
.mod-con{width: 95%;margin: 0 auto;}
.diamond-icon{ float: left;}
.diamond-txt{line-height: 19px;padding-left: 6px; font-size: 16px;}

.btns{ width: 100%;}
.btns a.short-btn{ display: block;width: 141px;height: 40px;margin:0 auto; background-position:0 0; background-repeat:no-repeat;background-size:contain; text-align: center; text-indent: -9999px;}
.btns a.long-btn{ display: block;width: 166px;height:40px; margin:0 auto; background-position:0 0; background-repeat:no-repeat;background-size:contain; text-align: center;text-indent: -9999px;}
.btns a.btn-change{background-image:url("../images/btn_change.png");}
.btns a.btn-top{background-image:url("../images/btn_top.png");}
.btns a.btn-share{background-image:url("../images/btn_share.png");cursor: pointer}
.btns a.btn-share-gray{background-image:url("../images/btn_share_gray.png");cursor: default;}
.btns a.btn-shared{background-image:url("../images/btn_shared.png"); cursor: pointer}
.btns a.btn-shared-gray{background-image:url("../images/btn_shared_gray.png"); cursor: default;}
.btns a.btn-submit{background-image:url("../images/btn_submit.png");}
.btns a.btn_upgrade{background-image:url("../images/btn_upgrade.png");}
.btns a.btn_agin{background-image:url("../images/btn_agin.png");}
.btns a.btn_godown{background-image:url("../images/btn_godown.png");}

/*我的奖品*/
.prize-list{}
.prize-con{ padding: 10px;}
.prize-con h4{ color: #e83328; font-size: 16px;}
.prize-con h4 span{ padding-right: 10px;}
.prize-con p{margin-top: 10px; font-size: 14px;}
.prize-date{margin-top: 10px;}
.prize-date span{ float: right; color: #a19797}

/*活动规则*/
.rule-list{margin-top: 15px;color: #9b9b9b}
.rule-list h4{}
.rule-list ul{}
.rule-list ul li{ line-height: 22px;}
.rule-list ul li span{padding-right: 10px;}

/*提示框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{ position:absolute;width: 230px;left:50%;top:50%;margin-left:-115px; background-color: #ffeccd; border-radius: 5px; color: #68483f; box-shadow:0 2px 0px #b7aa94;z-index: 99999;}
.tip-box h4{height: 18px;padding:0px 0px 7px 8px; border-bottom:1px solid #d9b57a;}
.tip-con{padding: 10px 10px 40px 10px;}
.tip-con p{ line-height: 20px; text-align: center;}
.tip-con p img{ width: 108px;height: 105px;}
.tip-con p.gift-icon img{ width: 147px;height: 100px;}
.tip-box .clo{ position: absolute;width:32px;height:32px;background-position:-1px -71px;right: -16px;top:-16px; text-indent: -9999px;}

/*slider*/
.slideBox{position:relative; width: 310px; margin: 0 auto 20px auto;}
.slideBox .bd{}
.slideBox .bd ul{ overflow:hidden; zoom:1; }
.slideBox .bd ul li{ position: relative; float:left; display:inline; margin-right:6px;padding: 15px; background-color:#fff; border-radius:5px;overflow:hidden; text-align:center;  }

.slideBox .bd ul li .pic{ text-align:center; }
.slideBox .bd ul li .pic img{  display:block;width:117px; height:117px;}
.slideBox .bd ul li .title{text-align:center;  margin-top: 15px; font-size:16px;line-height:24px; color: #737373;}
.slideBox .bd ul li .seal{ position: absolute;width: 68px; height:75px; bottom:12px;right: -1px; background:url("../images/seal.png") 0 0 no-repeat; background-size: contain;z-index: 80;}

.slideBox .prev{ position:absolute;display:block; width:38px; height:38px; left:-8px; top:53px; background:url("../images/arrow_left.png") 0 0 no-repeat;background-size:contain;z-index:10 }
.slideBox .next{ position:absolute;display:block; width:38px; height:38px;right:2px;top:53px; background:url("../images/arrow_right.png") 0 0 no-repeat;background-size:contain;z-index:10 }

/*圣诞节游戏*/
.game-hd{margin-top: 10px; color: #000;}
.game-hd h4{ font-size: 16px;}
.game-hd p{margin-top: 5px;}
.game-hd p span{padding-right:15px; color: #c3c3c3; font-size: 14px;}
.game-bd{color: #000;padding-bottom: 30px;}
.game-bd p{ text-indent: 2em;margin-top: 10px; line-height: 22px;}
.game-bd p.spe{margin-top: 0px;}
.game-bd p em{ color: #e83328;}
.game-bd p.p-img{margin:5px 0px; text-indent: 0;text-align: center;}
.game-bd p.p-img img{width: 100%;}
.game-award{margin-top: 1px;}
.game-award label{ float: left;padding-top:14px; text-indent: 2em;}
.game_table{ float:left;border: 0; background-color: #000;}
.game_table th{padding:2px 5px; background-color: #6bcf00; text-align: center}
.game_table td{padding:2px 5px;background-color: #f85200;text-align: center}
/*尾部*/
.footer{width:100%;height:75px;margin-top:30px;background-image:url("../images/foot_bg.png");background-position:0 0; background-repeat:no-repeat;background-size:100% 100%;}
