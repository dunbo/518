#page7a4{ position: absolute; right:0px; top: 76px; opacity: 0; }
.page8 .text{ text-align: center; }
.page8 .text img{ opacity: 0; }

#page8a1{ opacity: 0; position: absolute; left: 30px; top: 10px; }
#page8a2{ opacity: 0; position: absolute; right: 30px; top: 10px; }
.page10 .text{ width: 100%; text-align: center; }
.page10 .text img{ opacity: 0; }

#page11a{ opacity: 0; position: absolute; left: 57px; top: 25px; }
.page11 .text img{ opacity: 0; }

#page12a1{ position: absolute; right: 0px; opacity: 0; }

#page12a2{ position: absolute; left: 0; top: 75px; opacity: 0; }
#page12a3{ position: absolute; top: -160px; left: 363px; opacity: 0; }

.page12 .text{ height: 160px; text-align: center; top: 78px; }
.page12 .text img{ opacity: 0; }

#page13a1{ position: absolute; right: 333px; top: 264px; opacity: 0; }
#page13a2{ position: absolute; right: 290px; top: 209px; opacity: 0; }
#page13a3{ position: absolute; right: 10px; top: -10px; opacity: 0; }

.page13 .text{ height: 160px; text-align: center; top: 78px; }
.page13 .text img{ opacity: 0; }


.page14 ,.page15,.page16{ display: block; }
.page15 .step1,.page15 .step2{ height: 50%; }
.page14 .step1,.page14 .step2{ /* width: 100%; */
  height: 90px; display: block; text-align: center; /* display: inline-flex; */
  position: relative; /* margin: auto; */
  width: 640px; margin-top: 81px; }

.page14 .step2{ /* top: 150px; */}
.page15 img{ opacity: 0; }
.page14 .step1 img,.page14 .step2 img,.page15 .step1 img,.page15 .step2 img{ display: block; opacity: 0; }
.page16 img{ opacity: 0; }

.page17{ display: block; width: 100%; height: 100%; background: url(../images/page17_bg.png) no-repeat; background-size: 100% 100%; }
.page17 p{ text-align: center; opacity: 0; }
.page17 p img{ }
.page17 .earth{ position: absolute; width: 418px; height: 407px;left: 170px;bottom: 60px;}

.page18 .page18_btn img{ opacity: 0}
.page18_btn{ position: absolute; bottom: 70px; width: 100%; text-align: center; left: 0; }
.btnLine{ bottom: 10%; position: absolute; height: 100px; line-height: 100px; width: 100%; }
.btnLine .btnwrap{ display: inline-block; width: 48%; text-align: center; }

.page19{ display: block; }

.logo{ position: absolute; left: 10px; bottom: 20px; z-index: 999; width: 160px; height: 70px; background:url(../images/bg_logo.png) no-repeat; background-size: 100%; }

#share_box{ display: none; position: absolute; width: 100%; height: 100%; z-index: 9999; left: 0; top: 0; text-align: right; background: url(../images/share_bg.png) repeat; }

