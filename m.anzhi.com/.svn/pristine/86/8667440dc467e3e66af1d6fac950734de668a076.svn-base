<title>败家不够 礼物来凑</title>
<link type="text/css" rel="stylesheet" href="<!--{$out.static_url}-->/double11/css/common2.css?v1"/>
<script type="text/javascript" src="<!--{$out.static_url}-->/double11/js/jquery-1.8.0.min.js"></script>
<script type="text/javascript" src="/js/anzhi.js"></script>
<script type="text/javascript">
var actsid='<!--{$out.actsid}-->';
var aid='<!--{$out.aid}-->';

$(document).ready(function(){
	share_downloads();
	$('.gift_box').click(function(){
		$(this).css('display','none');
		$(this).siblings('.gift_box2').addClass('open');
		$(this).siblings('.light').addClass('light_open');
		$('.gift_txt').css('display','none');
		
		$.ajax({
			url:'/lottery/double11_promotion_get_redpacket.php?actsid='+actsid,
			type:'get',
			success:function(data){
				setTimeout(function(){
					window.location.href='/lottery/double11_promotion_share.php?actsid=' + actsid;
				},2000);
			}
		});
	})
})

function share_downloads() {
	var js_param = {type:'launchact', id:aid, actsid:actsid};
	var php_param = {type:'launchact', aid:aid, actsid:actsid, disable_auto:1};
	share_download(js_param,php_param);
}

</script>
</head>
<body>
<section>
	<div class="banner" id="top">
        <a href="#"><img src="<!--{$out.static_url}-->/double11/images/cam_banner.png"/></a>
    </div>
    <div class="main">
		<div class="wrap2 gift">
			<div class="gift_txt"><img src="<!--{$out.static_url}-->/double11/images/gift_txt.png"/></div>
			<div class="gift_img">
				<div class="gift_box"></div>
				<div class="gift_box2"></div>
				<div class="light"></div>
			</div>
		</div>
		<div class="prompt clear">
			<a href="/lottery/double11_promotion_lottery_rules.php?actsid=<!--{$out.actsid}-->" class="marauto prompt_btn" style="width:94px;">
				<i></i><span>活动规则</span><em></em>
				<div class="clear"></div>
			</a>
		</div>
		<div id="az_spirit"></div>
	</div>
	<div class="footer">
		<p>*本次活动最终解释权归安智所有</p>
	</div>
</section>

</body>
</html>
