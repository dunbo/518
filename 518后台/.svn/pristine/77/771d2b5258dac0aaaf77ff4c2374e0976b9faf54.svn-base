/* CSS Document */

.margin_left5{margin-left:5px;}
/*soft_upload*/
.process_soft{width:710px; height:35px; margin-top:10px; background:url(../images/process_soft_01.gif) no-repeat;}
#process_soft2{ background:url(../images/process_soft_02.gif) no-repeat;}
#process_soft3{ background:url(../images/process_soft_03.gif) no-repeat;}
.upload_apk h6{padding:40px 0 6px; font-size:18px;}
.inputtext{float:left; width:194px; height:28px; padding:0 2px; margin-top:2px; background:#ffffff; border:1px solid #dcdcdc; color:#535353}
.upload_apk form .upload_apk_div{ height:90px; padding-top:70px; position:relative;}
.upload_apk_btn{float:left; width:710px; height:56px; display:inline;}
.upload_apk_btn img{ display:block; width:194px; height:56px;}
.inputfile{ height:32px; font-size:16px; position:absolute; top:0; left:0; opacity:0; filter:alpha(opacity=0); _width:310px;}
.upload_apk form div span.upload_apk_check{ display:block; padding:15px 0;  color:#626262; font-size:12px; line-height:22px}
.upload_apk_infor{padding-top:40px; color:#535353; font-size:12px;}
.upload_apk_icon{float:left; width:72px; height:72px; margin-right:30px; display:inline;}
.upload_apk_icon img{display:block; width:72px; height:72px;}
.upload_apk_txt{float:left; width:608px;}
.upload_apk_inforb{min-height:28px; _height:28px; margin-top:2px; line-height:28px;}
.upload_apk_inforb .upload_apk_inforl,.upload_apk_inforr{float:left; width:304px;  word-break:break-all;}
.upload_apk_inforb span em{float:left; font-style:normal; font-size:14px; white-space:nowrap}
.upload_apk_inforb span em small{font-size:10px; font-family:Arial, Helvetica, sans-serif; -webkit-text-size-adjust:none;}
.upload_apk_inforb .upload_apk_inforl span{float:left;}
.upload_apk_inforl em{width:135px;}
.upload_apk_inforl span{float:left; width:165px;}
.upload_apk_inforr em{width:44px;}
.upload_apk_inforr span{float:left; width:256px;}
#upload_apk_infor2 .upload_apk_icon{margin:0 32px 0 20px;}
#upload_apk_infor2 .upload_apk_txt{width:584px;}
#upload_apk_infor2 .upload_apk_inforl,#upload_apk_infor2 .upload_apk_inforr{width:292px;}
#upload_apk_infor2 .upload_apk_inforl span{float:left; width:156px;}
#upload_apk_infor2 .upload_apk_inforr span{float:left; width:242px;}

.process_btn {padding:30px 0 0 125px;}
.process_btn a{float:left; width:132px; height:42px; margin-right:60px; background:url(../images/btns.png) no-repeat 0 -142px; text-align:center; line-height:42px; font-size:16px; text-decoration:none; display:inline}
.process_btn a:hover{ box-shadow: 0 0 4px rgba(128, 190, 68, 0.75);}
.process_btn2{padding-left:0; padding-top:50px}
.process_btn2 a,.process_btn3 a{width:192px; background:url(../images/btns.png) no-repeat 0 -32px; float:none; display:block;}
.process_btn3{padding:40px 0 0 10px;}


/*soft_upload2*/
.soft_list{min-height:31px; _height:31px; margin:10px 0; line-height:26px; position:relative;}
.soft_list label{float:left; width:100px; padding-right:25px; text-align:right;}
.soft_list .inputtext{margin-top:0;}
.soft_list select{float:left; width:158px; color:#535353;}
.soft_list span.prompt_infor{ padding-left:5px; font-size:12px; color:#959595; line-height:30px;}
.soft_list span.error_infor{float:left}
.soft_list span.error_infor img{float:left; margin:5px 5px 0 0;}
.soft_list span.right_infor img{float:left; margin:5px 5px 0 6px; display:inline}
.soft_list span em{color:#CE0421; padding:0 8px 0 0px;}
.soft_list span.register_check{padding-left:125px; font-size:12px; color:#959595;}
#soft_list_detail{min-height:130px; _height:130px}
#soft_list_detail_2{min-height:130px; _height:130px}
#soft_list_detail textarea{float:left; width:350px; height:124px; padding:2px 5px; background:#ebebeb; border:1px solid #d8d8d8; font-size:14px}
#soft_list_detail_2 textarea{float:left; width:350px; height:124px; padding:2px 5px; background:#ffffff; border:1px solid #d8d8d8; font-size:14px; color:#535353}
#soft_keywords input.inputtext{float:left; width:90px; margin-right:14px; display:inline;}
#soft_jietu{height:215px;}
.soft_jietu ul{float:left;}
.soft_jietu ul li{float:left; width:96px; height:185px; margin-right:14px; display:inline; position:relative; overflow:hidden;}
#soft_jietu span.jietu_img{float:left; width:94px; height:158px; padding:0; background:#eeeeee url(../images/jietu_bg.png) no-repeat center center; border:1px solid #dcdcdc;}
#soft_jietu #soft_icon_modify span.jietu_img{background:url(../images/jietu_bg2.png) no-repeat;}
#soft_jietu span.jietu_img img{display:block; width:94px; height:158px;}
#soft_jietu span.jietu_delete{float:left; width:94px; height:24px; padding:0;}
.jietu_delete a{ width:56px; height:22px; margin:2px auto 0; text-align:center; line-height:22px; color:#0041B9; font-size:12px;}
.jietu_delete a:hover{text-decoration:none;}
.jietu_file{width:96px; height:160px; font-size:130px; position:absolute; left:0; top:0; opacity:0; filter:alpha(opacity=0); overflow:hidden}

#copyright_jietu{height:165px;}
#copyright_jietu ul{float:left;}
#copyright_jietu ul li{float:left; width:182px; height:135px; margin-right:8px; display:inline; position:relative; overflow:hidden;}
#copyright_jietu span.copyright_img{float:left; width:180px; height:108px; padding:0; background:#eee; border:1px solid #dcdcdc;}
#copyright_jietu span.copyright_img img{ display:block; width:180px; height:108px;}
#copyright_jietu em.copyright_name{ display:block; width:180px; height:22px; padding-top:40px; font-style:normal; color:#b5b5b5; text-align:center;}
#copyright_jietu .jietu_file{ width:182px; height:110px;}
#copyright_jietu span.jietu_delete{float:left; width:180px; padding:0;}
#soft_ver img{widht:87px; height:27px;}
#soft_ver input{width:150px;}
#soft_ver a{color:#0041b9; text-decoration:underline; font-size:12px}
.agree{padding:10px 0 0 125px; font-size:12px;}
.agree a{color:#0041B9;}

#soft_upload3{padding:70px 0 30px;}
#soft_upload3 p{font-size:18px; text-align:center;}

/*soft_on*/



.soft_status{ display:block; width:95%; height:24px; margin:5px auto; background:#f0f0f0; border:1px solid #e5e5e5; position:relative; color:#ff9900;}
.soft_status b{ display:block; width:11px; height:8px; background:url(../images/soft_status_icon.png) no-repeat; position:absolute; left:38px; top:-8px;}

#down_data{width:590px; background:#f8f8f8; position:absolute; left:0; top:0; box-shadow: 0 0 21px rgba(0, 0, 0, 0.35); display:none; z-index:1000}
.down_list{width:590px; height:280px;}

#apply_off textarea{width:440px; height:60px; margin:10px 0 5px; padding:5px; border:1px solid #dcdcdc;}
#apply_off p{min-height:22px; _height:22px; color:#f17c41; line-height:22px; font-size:12px;}
#apply_off p span{color:#535353;}
#apply_off .open_close_btn a{float:left; margin-left:90px; display:inline;}

.reject_list{margin-top:10px;}
.reject_list li{float:left; width:440px; padding:10px 5px 5px; border-top:1px solid #dcdcdc;}
.reject_list li span{height:22px; color:#097c25;}
.reject_list li p{line-height:24px;}

#soft_detail{width:620px; height:480px;  overflow-y:auto; font-size:12px; color:#535353;}
#soft_detail .upload_apk_infor{padding-top:15px;}
#soft_detail .upload_apk_inforl em{width:55px;}
#soft_detail .upload_apk_inforl span{float:left; width:190px;}
#soft_detail .upload_apk_inforr span{float:left; width:211px;}
#soft_detail .upload_apk_icon{margin:0 10px 0 0;}
#soft_detail .upload_apk_txt{width:510px;}
#soft_detail .upload_apk_inforl,#soft_detail .upload_apk_inforr{width:255px;}
.soft_detail_box{ margin-top:10px; border-top:1px solid #dcdcdc;}

.soft_infor_more li{float:left; width:255px; height:22px; margin-top:5px;}
.soft_infor_more li.soft_infor_morel{width:337px;}
.soft_infor_more li span{font-size:14px;}
.soft_list1{position:relative; margin-top:5px;}
.soft_list1 h6{float:left; font-size:14px; color:#535353;}
.soft_list1_txt{float:right; width:550px; height:66px; line-height:22px; overflow:hidden}
.soft_list1_more{width:60px; height:22px; background:#F8F8F8; cursor:pointer; position:absolute; bottom:0; right:0; color:#055cc6; line-height:22px;}
.soft_jietulist h6{padding:10px 0 8px; font-size:14px; color:#535353;}
.soft_jietulist ul li{float:left; width:104px; height:80px; margin-right:10px; display:inline;}
.soft_jietulist ul li img{display:block; width:102px; height:78px; border:1px solid #dcdcdc;}


.soft_table thead th b.arrow_up{ background:url(../images/arrow_up.png) no-repeat;}

.soft_no_img{width:112px; margin:0 auto;}
.soft_no_img img{display:block; width:48px; height:57px; margin:0 auto;}
.soft_no_img p{ text-align:center;}

/*数据加载中提示*/
#loaddingtab { position:absolute;left:0px; top:0px;width:100%; z-index:999; background:#CCCCCC; filter:alpha(opacity=20);/* IE */ -moz-opacity:0.3; /* Moz + FF */ opacity: 0.3;/* 支持CSS3的浏览器（FF 1.5也支持）*/ display:none; }
.loading { width:32px; position:absolute; height:32px; z-index:10000; display:none;}
.zhuzuo_box {
    float: left;
    width: 586px;
}
.zhuzuo_box_top {
    height: 29px;
    width: 586px;
}
.upload_btn_zhuzuo {
    background: url("../images/upload_zhuzuo.png") no-repeat scroll 0 0 rgba(0, 0, 0, 0);
    cursor: pointer;
    float: left;
    height: 29px;
    position: relative;
    width: 96px;
}
.inputfile_zhuzuo {
    font-size: 16px;
    height: 29px;
    left: 0;
    opacity: 0;
    position: absolute;
    top: 0;
    width: 96px;
}
.zhuzuo_check {
    color: #959595;
    float: left;
    font-size: 12px;
    padding: 1px 0 0 15px;
}
.zhuzuo_box_bottom {
    min-height: 24px;
    width: 586px;
}
.zhuzuo_box_bottom ul {
    float: left;
    margin-top: 10px;
    width: 586px;
}
.zhuzuo_box_bottom li {
    color: #055CC6;
    float: left;
    margin-bottom: 10px;
    width: 596px;
}
.zhuzuo_box_bottom li span {
    background: none repeat scroll 0 0 #EEEEEE;
    float: left;
    font-size: 12px;
    height: 24px;
    padding-left: 10px;
    width: 536px;
}
.zhuzuo_box_bottom li a {
    color: #055CC6;
    float: left;
    text-align: right;
    width: 40px;
}

/*标签组*/
.tags_box{ float: left;}
.tags_hd{height: 30px; line-height: 30px;}
.tags_bd{ clear: both; overflow: hidden;}
.tags_bd li{ float: left;padding-right: 10px; list-style: none; position:relative;}
.tags_bd li span{ display: block; width: 93px;height:28px; line-height: 28px; border:1px solid #d8d8d8;text-align: center;}
.tags_bd li .tags_close{ position: absolute;right: 15px;top:1px;}
#add_select{ float: left;line-height: 17px;}