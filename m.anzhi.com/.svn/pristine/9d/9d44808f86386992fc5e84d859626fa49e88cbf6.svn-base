/* CSS Document */
/*公共样式*/
body,header,section,footer,nav,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
body{min-width:320px;max-width:640px;margin:0 auto;font-size:14px; font-family:Microsoft Yahei,"微软雅黑"; background-color: #ffffff; color: #ffffff;}
ol,ul{list-style-type:none;}
h1,h2,h3,h4,h5,h6{ font-weight: normal;}
var,em{font-style:normal;}
a,img,fieldset{border:none;}
img{vertical-align: bottom;}
a{text-decoration:none;color:#ffffff; font-size: 14px; outline: none;}
a,input,button{-webkit-tap-highlight-color:rgba(255,0,0,0); outline: none;}
.cl{overflow:hidden;}
.clear{clear:both;overflow:hidden;}


.cnt{text-align: center}
.wth{width:90%;margin:0 auto; }
.mbot{margin-bottom: 50px;}
.fl{float: left}
.fr{float: right}

section{background: url("../images/wrap_bg.png") repeat-y;background-size: 100% auto}
.banner{position: relative; width: 100%;min-width: 320px; margin:0 auto; text-align: center;}
.banner img{ width: 100%;vertical-align: bottom;}

/*公告*/
.notice{width:320px;height:116px;margin:0 auto;background: url(../images/notice_bg.png) 0 0 no-repeat;background-size:320px auto }
.notice_list{margin:17px 0px 0px 65px;line-height: 15px;color: #fff;}
.notice_list dt{width:195px;}
.notice_list dt.notice_no{margin-top: 30px;line-height: 25px;}
.slide_up{overflow:hidden;height:60px;position: relative;}

.wrap{ width: 100%; margin: 0 auto;padding-bottom:15px;}
.wrap_hd{padding-bottom: 15px;}
.wrap_hd img{width: 100%;}
.mod1{position:relative;margin:15px 14% 0px 10%;padding:10px;border: 3px solid #fff;background:rgba(72,26,102,0.5);}
.mod{position:relative;margin:-4% 14% -3% 14%;padding:20px 10px;border: 2px solid #fff;background:rgba(72,26,102,0.5);}
.mod_top{position:relative;}
.mod_top img{width: 100%;}
.prize_lot{position:relative;margin-bottom: -2%}
.prize_lot img{width: 100%;}
.prompt{width:315px;height:39px;padding-top:45px;padding-left:5px;background: url("../images/prompt_bg.png") 0 0 no-repeat;background-size:320px 84px;color: #fff;}
.prompt_01{margin:10px 5%;color: #fff;}
.con{width:100%;background: url("../images/con_bg.png") 0 0 no-repeat;background-size: 100% 100%;color: #fff;}
.con p{padding:50px 50px 40px 50px;text-align: center;line-height: 30px;}
.col{margin: 0px 10px;}
/*按钮*/
.btn_mod{color: #fff;}
.btn_mod a{color: #fff;}
.btn_mod a span.b_m{padding: 0px 10px;}
.btn_mod span{float: left;height: 52px;line-height: 44px;}
.btn_mod span.b_l{width: 20px;background: url("../images/btn_l.png") 0 0 no-repeat;background-size:20px auto ;}
.btn_mod span.b_m{background: url("../images/btn_m.png") 0 0 repeat-x;background-size:3px 52px}
.btn_mod span.b_r{width: 20px;background: url("../images/btn_r.png") 0 0 no-repeat;background-size:20px auto ;}
.btns{margin-top: 30px;}
.btns.mt10{margin-top:10px;}
.btns.mlr{margin-left: 10px;margin-right: 10px;}
.btns a{display:block; width:118px;height:36px;margin:0 auto;padding-top:12px;background: url("../images/common_btn.png") 0px -50px no-repeat; no-repeat; background-size: 200px 200px;color: #e1422c;text-align: center;font-size: 16px;}
.btns a.gray{background-position:0 -104px;color:#2e2e2e;cursor: default}
.btns a.upgrade_btn{ width: 160px;background-position:0px 0px;font-weight: bold;}
.btns a.btn_close{width:100px;height:32px;background-position:0 -156px;color: #fff;}
.btn_edit{position:absolute;right:10px;top:15px;display:block; width:56px;height:28px;margin:0 auto;padding-top:5px;background: url("../images/common_btn.png") -138px -52px no-repeat; no-repeat; background-size: 200px 200px;color: #e1422c;text-align: center;font-size: 16px;}
/*抽奖*/
.rotate {width: 100%;margin-top: 15px;}
.rotate-con {position: relative;width:100%;margin: 0 auto;overflow: hidden;}
.rotate-active{position: absolute;left: 0px;top: 0px;z-index: 2}
.rotate-pointer {position: absolute;top: 50%;left: 52%;width: 36%;height: 32%;margin-top: -16%;margin-left: -19.5%;cursor: pointer;-webkit-tap-highlight-color:rgba(255,0,0,0);-moz-tap-highlight-color:rgba(255,0,0,0);-o-tap-highlight-color:rgba(255,0,0,0); outline: none;z-index: 3}
.rotate-pointer.gray{background: url(../images/pointer_gray.png) no-repeat 0 0;background-size: 100% 100%;cursor: default}

/*结束页*/
.end{padding: 50px 15%;}
.end h4{color:#fff;text-align: center;font-weight: bold;}
.view{margin-top: 20px}
.view a{color:#fff;text-decoration: underline;}
.copyright{color: #fff;margin:70px 0px 0px 15px;}
/*软件*/
.app_hd{position: relative;margin-bottom: -55px;z-index: 2}
.app_hd img{width: 100%;}
.app_ft{position: relative;margin-top: -20px;z-index: 2}
.app_ft img{width: 100%;}
.btn_change{display: block;position: absolute;right: 2%;top:70%;width:11%;height: 100%;background: url("../images/btn_change.png") 0 0 no-repeat; background-size: 100% auto;text-indent: -9999px;cursor: pointer}
.soft-list{width: 100%; margin:25px auto;}
.soft-list li{ float: left; display: inline; width: 46%;margin:10px 2%;text-align: center;}
.soft-list li .soft-icon{float: left;}
.soft-list li .soft-icon img{ width: 55px; vertical-align: bottom;}
.soft-list li .soft-info{margin-left: 60px}
.soft-list li .soft-name{margin-top: 2px;height: 20px; line-height: 20px; overflow: hidden;}
.soft-list li .soft-name a{ color: #fff;}
.soft-list li a.downlode{ display: block; width:45px;height: 22px;line-height: 22px; margin: 5px auto 0px auto;background: url(../images/btn_app.png) 0 0 no-repeat;background-size: 45px 22px;
color:#ffffff;font-size: 14px;font-weight:bold;cursor: pointer;}

/*摇签*/
.lot_wrap{margin-top: 15px;}
.lot_top{}
.lot_mid{position:relative;background: url("../images/lot_mid.png") 0 0 repeat-y;background-size: 100% auto;text-align: center;}
.lot_mid img{width: 70%;}
.lot_prompt{}
.lot_prompt img{width: 100%;}
.lot_pot{position:relative;margin-top:20px;z-index: 2}
.lot_pot img{width:70%; }
.lot_bot{}
.lot_top img,.lot_bot img{width: 100%}
.btn_lot{position:relative;margin-top:25px;text-align: center;z-index: 4}
.btn_lot img{width: 50%;}
.lot_mid.active .lot_pot{animation:lot_pot_ac  0.3s ease 0s 6 alternate forwards;-webkit-animation:lot_pot_ac  0.3s ease 0s 6 alternate forwards;-moz-animation:lot_pot_ac  0.3s ease 0s 6 alternate forwards;}
@keyframes lot_pot_ac{
    0%{transform:rotate(0deg);}
    25%{transform:rotate(15deg);}
    50%{transform:rotate(0deg);}
    75%{transform:rotate(-15deg);}
    100%{transform:rotate(0deg);}
}
@-webkit-keyframes lot_pot_ac{
    0%{-webkit-transform:rotate(0deg);}
    25%{-webkit-transform:rotate(15deg);}
    50%{-webkit-transform:rotate(0deg);}
    75%{-webkit-transform:rotate(-15deg);}
    100%{-webkit-transform:rotate(0deg);}
}
@-moz-keyframes lot_pot_ac{
    0%{-moz-transform:rotate(0deg);}
    25%{-moz-transform:rotate(15deg);}
    50%{-moz-transform:rotate(0deg);}
    75%{-moz-transform:rotate(-15deg);}
    100%{-moz-transform:rotate(0deg);}
}
.light{position: absolute;left: 0px;top:0px;z-index: 3;opacity: 0}
.light img{width: 100%}
.lot_mid.active .light{animation:light_ac  0.3s ease 1.2s 1 normal forwards;-webkit-animation:light_ac  0.3s ease 1.2s 1 normal forwards;-moz-animation:light_ac  0.3s ease 1.2s 1 normal forwards; }
@keyframes light_ac{
    0%{transform:scale(0.0,0.0);opacity: 0;}
    100%{transform:scale(1.0,1.0);opacity: 1}
}
@-webkit-keyframes light_ac{
    0%{-webkit-transform:scale(0.0,0.0);opacity: 0;}
    100%{-webkit-transform:scale(1.0,1.0);opacity: 1}
}
@-moz-keyframes light_ac{
    0%{-moz-transform:scale(0.0,0.0);opacity: 0;}
    100%{-moz-transform:scale(1.0,1.0);opacity: 1}
}
/*活动规则*/
.rules{}
.rules h4{margin: 5px 0px;font-weight: bold}
.rules p{line-height: 22px;}
/*.rules dl{ line-height: 25px;}*/
/*.rules dl dt{float: left;width: 20px;}*/
/*.rules dl dd{width: auto;margin-left: 20px;}*/
/*提示框*/
#body-bg{position:absolute;top:0px;left:0px;width: 100%; height: 100%; background-color: #000;-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;z-index: 99998;}
.tip-box{ position:absolute;width: 300px;height:280px;left:50%;top:50%;margin-left:-150px;background:url("../images/tip_box.png") 0 0 no-repeat; background-size:100% 100%;z-index: 99999;}
.tip-icon{position: absolute;width: 250px; top:-30px;left:50%;margin-left:-125px;text-align: center;}
.tip-icon img{width:100%}
.tip-con{}
.tip-con h3{margin-top:8px;text-align: center;font-size: 18px;}
.tip-con p.prize_tip{margin-top:180px;padding: 0px 15px; line-height: 20px; text-align: center;color: #fffc00}
.tip-box .clo{ position: absolute;width:45px;height:45px;right: -10px;top:-7px;background: url("../images/close_icon.png") 0 0 no-repeat;background-size:45px 45px; text-indent: -9999px;z-index: 100000}

.contact-info{margin: 10px 20px}
.prize_hd{text-align: center;font-size: 16px; padding: 10px 0px;}
/*礼包奖品*/
.prize_gift{padding: 30px 20px 10px 20px }
.prize_app{float: left;width:80px;height: 80px;background: url("../images/app_bg.png") 0 0 no-repeat;background-size: 100% 100%;text-align: center; }
.prize_app img{width: 55px;height: 55px;padding-top: 13px;border-radius: 10px;}
.prize_info{margin-left: 95px;}
.prize_info h4{text-align: center;font-size: 18px;margin: 5px 0px 6px 0px;}
.prize_info p{line-height: 20px;}
.gift_code{margin: 10px 20px;font-size: 16px;}
/*联系信息*/
.form_items{}
.form_items li{ margin-top: 5px;clear: both;overflow: hidden;}
.form_items li label{float: left;width:55px;padding:10px 0px;}
.form_items li span{display:block;width: auto;margin-left: 55px;}
.form_items li input{width:90%;padding:10px 10px;border:none; color: #666;  font-family: Microsoft Yahei,"微软雅黑";background-color: #fff;border-radius: 10px;}
.prompt_submit{padding-left: 20px;color: #fffc00;}
.form_items.my_prize{margin-top: 70px;}
.form_items.my_prize li{margin-top: 20px;}
/*我的奖品页*/
.prize_list{}
.prize_list dt{margin-bottom:15px;}
.prize_list dt span{padding-left: 10px;}
.prize_list dd{line-height: 25px;word-break: break-all;}
.prize_list dd span{padding-left: 20px;}
.prize_no{padding:30px 0px; text-align: center;}

/*尾部*/
.footer{padding:50px 0px 20px 0px;background: url(../images/ft_bg.png) 0 0 no-repeat;background-size: 100% 100%;}
.btn_rule{margin-left:10px;position:relative;width:200px;height:100px;background:url("../images/btn_rule.png") 0 0 no-repeat;background-size: 200px 100px;}
.btn_rule a{display:block;width:90px; height:31px;position:absolute;top:42px;left: 49px;text-indent: -9999px;}
.footer p{margin-top:15px;text-align: center;color: #fff;}