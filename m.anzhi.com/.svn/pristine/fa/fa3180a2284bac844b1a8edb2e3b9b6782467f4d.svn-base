/* CSS Document */
body,div,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0; font-weight:normal}
fieldset,img {border: 0;}
a{text-decoration:none; color:#545454;}
.clear{clear:both;}
body{ font-family:'微软雅黑'; background:#fff; font-size:13px; color:#545454;}
.clear{clear:both;}
.padding_bom{padding-bottom:64px;}
.top_bar{width:100%; padding:9px 20px 9px 16px; height:32px; background:#7fc31e; border-bottom:2px solid #609d08; font-size:18px; color:#ffffff; line-height:33px; position:fixed; left:0; top:0; z-index:1000;}
.top_bar a:before{display:inline-block; vertical-align:-6px; width:28px; height:21px; background:url(/perfect/public/img/arrow.png) no-repeat 0 center; content:'';}
.top_bar a{display:inline-block; color:#ffffff;}

.banner{margin-top:-9px;}
.banner img{display:block; width:100%;}
.main{width:98%; margin:0 auto;}
.description{font-size:16px; line-height:22px; color:#5b5b5b; word-break:break-all; word-wrap:break-word;}
.description img{display:block; max-width:100%; margin:8px 0;}
.title_wrap{ margin:0 10px; padding:8px 0 7px; margin-top:7px; background:#fff;}
.title_txt{float:left; font-size:15px; color:#999999}
.title_more{float:right; color:#19d5fd;}
.title_more a{height:19px; color:#19d5fd; padding-top:1px; display:block; line-height:20px;}
.comment,.app{margin:0;}
.part{margin-top:5px; padding:9px 10px 4px; border-top:1px solid #e6e6e6; background:#fff;}
.cmnt_top{color:#a5a5a5;}
.cmnt_nick{float:left;}
.cmnt_ver{float:right; height:18px; line-height:21px;}
.cmnt_top i{float:right;  margin:3px 0 0 8px; display:inline;}
.stars{width:65px; height:12px; background:url(/perfect/public/img/stars.png) no-repeat 0 0;  overflow:hidden;}
.stars_0{background-position:0 0}
.stars_1{background-position:0 -12px}
.stars_2{background-position:0 -24px}
.stars_3{background-position:0 -36px}
.stars_4{background-position:0 -48px}
.stars_5{background-position:0 -60px}
.stars_6{background-position:0 -72px}
.stars_7{background-position:0 -84px}
.stars_8{background-position:0 -96px}
.stars_9{background-position:0 -108px}
.stars_10{background-position:0 -120px}
.cmnt_txt{padding:2px 0; line-height:22px; font-size:15px;word-wrap: break-word}
.cmnt_date{color:#a5a5a5; font-size:11px}

.app_list{border-bottom:1px solid #e6e6e6; padding-bottom:5px;}
.app_list li{position:relative; min-height:50px;}
.app_icon{float:left; width:48px; height:48px; margin-top:1px}
.app_icon img{display:block; width:48px; height:48px;}
.app_des{padding:0 0 0 62px;}
.app_des h2{min-height:20px; padding:5px 0 2px; font-size:15px; font-weight:normal;}
.app_des span{color:#a5a5a5; line-height:20px;}
.app_list li a.clickarea{position:absolute; left:0; top:0; bottom:0; right:0; display:block; z-index:999}

.footer_app{width:100%; height:64px; background:rgba(0, 0, 0, 0.85);position:fixed; bottom:0; z-index:1001}
.footer_img{position:absolute; left:10px; top:6px;}
.footer_img img{display:block; width:52px; height:52px;}
.footer_des{ padding:5px 0 0 75px;}
.footer_des h4{padding:0; color:#ffffff; font-size:16px;}
.footer_stars{height:12px; margin:4px 0 3px; overflow:hidden;}
.footer_des span{color:#c9cbca; font-size:12px}
.footer_down{position:absolute; right:10px; top:16px; z-index:1002; width:60px; padding:6px 0; background:#19d5fd ; border-radius:3px; color:#fff; text-align:center; font-size:16px;}
.mar_top59{margin-top:59px;}
.mar_top50{margin-top:50px;}

.load_more{width:98%; background:#eff2f2; border-radius:2px; margin:5px auto 0;}
.load_more a{display:block; width:160px; margin:0 auto; padding:10px 0 10px 35px; background:url(/perfect/public/img/arrow_down.png) no-repeat 136px center; color:#9a9a9a; font-size:16px; color:#808181;}
.loading{background:none;}
.loading a{width:140px; padding:4px 0 4px 35px; background:url(/perfect/public/img/loading.gif) no-repeat 94px center #d3d7d8; border-radius:4px; border-color:#d3d7d8; color:#ffffff; text-align:left}
.load_end a{background:none;}
.return_top a{display:block; width:200px; margin:20px auto; padding:5px 0; background:#19d5fd; border:1px solid #19d5fd; border-radius:4px; text-align:center; font-size:16px; color:#ffffff;}

