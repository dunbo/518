<script type="text/javascript">
    function check() {
        var download_limit = $("#download_limit").val();
        var reg = /^\s*\d+(\.\d+)?\s*$/;
        if (download_limit && !reg.test(download_limit)) {
            alert("请填写正确的下载限制");
            return false;
        }
        return true;
    }
</script>

<form action="__URL__/edit_packagename_end_with_config" method="post">
<table>
    <tbody>
        <tr>
            <td>关键字：</td>
            <td><textarea id="config_content" name="config_content" rows="10" cols="40">{$list.config_content}</textarea></td>
        </tr>
        <tr>
            <input type="hidden" id="config_code" name="config_code" value="{$list.config_code}"/>
            <td><input type="submit" value="提交" onclick="return check();"/></td>
            <td><input type="button" value="取消" onclick="tb_remove()"/></td>
        </tr>
    </tbody>
</table>
</form>
