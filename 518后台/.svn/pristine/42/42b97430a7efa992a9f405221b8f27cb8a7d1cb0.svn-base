function get_code_sec($num,&$code_sec){
		$str_5=substr($num,0,5);
		$str_5='0x0'.$str_5.'xx';
		$str_3=substr($num,0,3);
		$str_3='0x0'.$str_3.'xx'.substr($num,5,7);
		foreach($code_sec as $key=>$v){
			if($str_3==$key){
				return $v;
			}else if($str_5==$key){
				return $v;
			}
		}
}
