html{ font-family: tahoma,arial,'Hiragino Sans GB',\5b8b\4f53,sans-serif; -ms-text-size-adjust: 100%; /* 2 */
  -webkit-text-size-adjust: 100%; /* 2 */
  height: 100%; width: 100%; }
body{ margin: 0; background:#272822; height: 100%; width: 100%; overflow: hidden; }

#wrapbox{ width: 100%; height: 100%; background: #032234; }
.pages{ position: absolute; display:flex; top:0; left: 0; width: 100%; height: 100%; vertical-align: middle; overflow: hidden; z-index: 1; background: #032234; }

.page1{ z-index: 60; }
.page2{ z-index: 59; }
.page3{ z-index: 58; }
.page4{ z-index: 57; }
.page5{ z-index: 56; }
.page6{ z-index: 55; }
.page7{ z-index: 54; }
.page8{ z-index: 53; }
.page9{ z-index: 52; }
.page10{ z-index: 51; }
.page11{ z-index: 50; }
.page12{ z-index: 49; }
.page13{ z-index: 48; }
.page14{ z-index: 47; }
.page15{ z-index: 46; }
.page16{ z-index: 45; }
.page17{ z-index: 44; }
.page18{ z-index: 43; }
.page19{ z-index: 42; }

.wrap{ position: relative; vertical-align: middle; /* display:flex; */
  margin:auto; height: 960px; width: 100%; }
.title{ position: absolute; right: 20px; top: 25px; text-align: center; }

.content_img{ position: absolute; top: 24%; width: 100%; height: 50%; line-height: 100%; /* vertical-align: middle; */
  text-align: center; }
.text{ position: absolute; display: block; width: 100%; top: 120px; }

.page1 .title{ position: absolute; height: 87px; top: 0; }
.page1 .content_img{ position: absolute; top: 90px; }
.page2 .text{ top: 124px; height: 55px; line-height: 55px; text-align: center; }
.page2 .text img{ opacity: 0; margin: 0px; }
.page2 .text img.stop{ opacity: 1; }
.page3 .text{ height: 136px; text-align: center; top: 85px; }


/**  page4   */
.page4 .text{ }
.page4 .text img{ /* margin-left: -100%; */
  opacity: 0; }
.page4 #animate1{ /* display: none; */
  opacity: 0; position: absolute; left: 50px; top: -167px; z-index:1; }
.page4 #animate2{ /* display: none; */
  opacity: 0; position: absolute; right: 66px; top: -120px; z-index: 1; }
.page4 .content_img{ top: 45%; }

/*--  page5 animate  --*/

