<include file="../Public/header" />
<link rel="stylesheet" type="text/css" media="all"
	href="/Public/css/tablelist.css" />
<script type="text/javascript" src='/Public/js/jquery.js'></script>
<script type="text/javascript" src="/Public/js/My97DatePicker/WdatePicker.js"></script>
<link type="text/css" href="/Public/js/My97DatePicker/skin/WdatePicker.css" />
<script>
		$(document).ready(function(){
			$('.postionbtn').mouseover(function(){
				$(this).parent().find('.position_newbox').show();
			});
			$('.postionbtn').mouseout(function(){
				$(this).parent().find('.position_newbox').hide();
			});
		});
</script>
<div class="main" >
<div class="content">
<center>
<table width="90%" border="0" cellpadding="0" cellspacing="0" class="table_list">
  <tr>
    <td colspan="5" align="center"><h2>邮件发送记录</h2></td>
  </tr>
  <tr>
  <form action="__URL__/email_send_log" method='get'>
	<td align="center">邮件:<input name="email" value="{$email}" type="text"></td>
	<td align="center" colspan="2">起止日期:
						<input style="width:130px;cursor:hand;" type="text" value="{$edit_stm}" id="edit_stm" name="edit_stm" class="Wdate" onClick="WdatePicker({dateFmt:'yyyy-MM-dd HH:mm:ss'});" />
						<label>至</label>
						<input style="width:130px;cursor:hand;" type="text" value="{$edit_etm}" id="edit_etm" name="edit_etm" class="Wdate" onClick="WdatePicker({dateFmt:'yyyy-MM-dd HH:mm:ss'});" />
	</td>
	<td align="center">日志内容:<input name="content" value="{$content}" type="text"></td>
	<td align="center"><input value="搜索" type="submit"></td>
	</form>
  </tr>
  <tr>
	<th width="20%" align="center">邮件地址</td>
	<th width="20%" align="center">邮件内容</td>
	<th width="20%" align="center">应用场景</td>
	<th width="20%" align="center">发送时间</td>
	<th width="20%" align="center">发送状态</td>
  </tr>
  <volist name="email_list" id="vo">
  <tr>
	<td width="20%" align="center">{$vo.email_address}</td>
	<td width="20%" align="center">
	<div class="position_box">
		<div class="postionbtn">
		{$vo.content|substr=0,10}
		<div class="position_newbox">
							<p>{$vo.content}</p>
		</div>
		</div>	
	</div>
	</td>
	<td width="20%" align="center">
	    <if condition="$vo.app_sign eq 1">设备下载刷量</if>
		<if condition="$vo.app_sign eq 2">IP下载刷量</if>
		<if condition="$vo.app_sign eq 3">设备下载刷量 IP下载刷量</if>
		<if condition="$vo.app_sign eq 4">m.anzhi.com下载刷量</if>
		<if condition="$vo.app_sign eq 5">设备下载刷量 m.anzhi.com下载刷量</if>
		<if condition="$vo.app_sign eq 6">IP下载刷量 m.anzhi.com下载刷量</if>
		<if condition="$vo.app_sign eq 7">设备下载刷量 IP下载刷量 m.anzhi.com下载刷量</if>
		<if condition="$vo.app_sign eq 8">web下载刷量</if>
		<if condition="$vo.app_sign eq 9">设备下载刷量 web下载刷量</if>
		<if condition="$vo.app_sign eq 10">IP下载刷量 web下载刷量</if>
		<if condition="$vo.app_sign eq 11">设备下载刷量 IP下载刷量 web下载刷量</if>
		<if condition="$vo.app_sign eq 12">m.anzhi.com下载刷量 web下载刷量</if>
		<if condition="$vo.app_sign eq 13">设备下载刷量 m.anzhi.com下载刷量 web下载刷量</if>
		<if condition="$vo.app_sign eq 14">IP下载刷量 m.anzhi.com下载刷量 web下载刷量</if>
		<if condition="$vo.app_sign eq 15">设备下载刷量 IP下载刷量 m.anzhi.com下载刷量 web下载刷量</if>
	</td>
	<td width="20%" align="center">{$vo.send_time|date='Y-m-d H:i:s',###|default='&nbsp;'}</td>
	<td width="20%" align="center"><if condition="$vo.status eq 1">成功</if></td>
  </tr>
  </volist>
</table>
{$page}
</center>
</div>
</div>

